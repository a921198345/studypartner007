        <outline text="含义和形式" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%E5%92%8C%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="应收帐款变现金" _mubu_text="%3Cspan%3E%E5%BA%94%E6%94%B6%E5%B8%90%E6%AC%BE%E5%8F%98%E7%8E%B0%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="书面形式" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="虚构应收帐款的处理" _mubu_text="%3Cspan%3E%E8%99%9A%E6%9E%84%E5%BA%94%E6%94%B6%E5%B8%90%E6%AC%BE%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="通谋虚伪表示无效不得对抗善意第三人" _mubu_text="%3Cspan%3E%E9%80%9A%E8%B0%8B%E8%99%9A%E4%BC%AA%E8%A1%A8%E7%A4%BA%E6%97%A0%E6%95%88%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E6%84%8F%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="有追索权" _mubu_text="%3Cspan%3E%E6%9C%89%E8%BF%BD%E7%B4%A2%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="有担保权" _mubu_text="%3Cspan%3E%E6%9C%89%E6%8B%85%E4%BF%9D%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有选择权" _mubu_text="%3Cspan%3E%E6%9C%89%E9%80%89%E6%8B%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="无追索权" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%BD%E7%B4%A2%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="无担保权" _mubu_text="%3Cspan%3E%E6%97%A0%E6%8B%85%E4%BF%9D%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无选择权" _mubu_text="%3Cspan%3E%E6%97%A0%E9%80%89%E6%8B%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
