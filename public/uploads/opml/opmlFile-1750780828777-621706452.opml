          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%228XbITCLkWf%22%2C%22oh%22%3A720%2C%22ow%22%3A1600%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F24B6159E5AED4D5A1720352682.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
        <outline text="基本原理" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="缔约：在缔约过程中" _mubu_text="%3Cspan%3E%E7%BC%94%E7%BA%A6%EF%BC%9A%E5%9C%A8%E7%BC%94%E7%BA%A6%E8%BF%87%E7%A8%8B%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="如双方并未开始磋商（题目中关键词为：商谈、谈判、洽谈、接洽)，则不存在缔约过失责任问题。" _mubu_text="%3Cspan%3E%E5%A6%82%E5%8F%8C%E6%96%B9%E5%B9%B6%E6%9C%AA%E5%BC%80%E5%A7%8B%E7%A3%8B%E5%95%86%EF%BC%88%E9%A2%98%E7%9B%AE%E4%B8%AD%E5%85%B3%E9%94%AE%E8%AF%8D%E4%B8%BA%EF%BC%9A%E5%95%86%E8%B0%88%E3%80%81%E8%B0%88%E5%88%A4%E3%80%81%E6%B4%BD%E8%B0%88%E3%80%81%E6%8E%A5%E6%B4%BD)%EF%BC%8C%E5%88%99%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%E9%97%AE%E9%A2%98%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="过失：违反诚实信用原则" _mubu_text="%3Cspan%3E%E8%BF%87%E5%A4%B1%EF%BC%9A%E8%BF%9D%E5%8F%8D%E8%AF%9A%E5%AE%9E%E4%BF%A1%E7%94%A8%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="说明、告知、注意" _mubu_text="%3Cspan%3E%E8%AF%B4%E6%98%8E%E3%80%81%E5%91%8A%E7%9F%A5%E3%80%81%E6%B3%A8%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="责任：信赖利益的损失" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%EF%BC%9A%E4%BF%A1%E8%B5%96%E5%88%A9%E7%9B%8A%E7%9A%84%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="订约费用的损失" _mubu_text="%3Cspan%3E%E8%AE%A2%E7%BA%A6%E8%B4%B9%E7%94%A8%E7%9A%84%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="准备履行所需费用" _mubu_text="%3Cspan%3E%E5%87%86%E5%A4%87%E5%B1%A5%E8%A1%8C%E6%89%80%E9%9C%80%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="订约机会的损失" _mubu_text="%3Cspan%3E%E8%AE%A2%E7%BA%A6%E6%9C%BA%E4%BC%9A%E7%9A%84%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="因果：因果关系" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%9E%9C%EF%BC%9A%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="合同未成立型" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9C%AA%E6%88%90%E7%AB%8B%3C/span%3E%3Cspan%3E%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="恶意磋商" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E7%A3%8B%E5%95%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="欺诈" _mubu_text="%3Cspan%3E%E6%AC%BA%E8%AF%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="第三人缔约过失责任" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="第三人实施欺诈、胁迫行为，使当事人在违背真实意思的情况下订立合同，受到损失的当事人请求第三人承担赔偿责任的，人民法院依法予以支持；" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%AE%9E%E6%96%BD%E6%AC%BA%E8%AF%88%E3%80%81%E8%83%81%E8%BF%AB%E8%A1%8C%E4%B8%BA%EF%BC%8C%E4%BD%BF%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E8%BF%9D%E8%83%8C%E7%9C%9F%E5%AE%9E%E6%84%8F%E6%80%9D%E7%9A%84%E6%83%85%E5%86%B5%E4%B8%8B%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%EF%BC%8C%E5%8F%97%E5%88%B0%E6%8D%9F%E5%A4%B1%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E%3Cspan%20class=%22underline%22%3E%E8%AF%B7%E6%B1%82%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%E7%9A%84%3C/span%3E%3Cspan%3E%EF%BC%8C%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E4%BE%9D%E6%B3%95%E4%BA%88%E4%BB%A5%E6%94%AF%E6%8C%81%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="当事人亦有违背诚信原则的行为的，人民法院应当根据各自的过错确定相应的责任。" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E%3Cspan%20class=%22underline%22%3E%E4%BA%A6%E6%9C%89%E8%BF%9D%E8%83%8C%3C/span%3E%3Cspan%3E%E8%AF%9A%E4%BF%A1%E5%8E%9F%E5%88%99%E7%9A%84%E8%A1%8C%E4%B8%BA%E7%9A%84%EF%BC%8C%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%E6%A0%B9%E6%8D%AE%3C/span%3E%3Cspan%20class=%22underline%22%3E%E5%90%84%E8%87%AA%E7%9A%84%E8%BF%87%E9%94%99%3C/span%3E%3Cspan%3E%E7%A1%AE%E5%AE%9A%E7%9B%B8%E5%BA%94%E7%9A%84%E8%B4%A3%E4%BB%BB%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="但是，法律、司法解释对当事人与第三人的民事责任另有规定的，依照其规定。" _mubu_text="%3Cspan%3E%E4%BD%86%E6%98%AF%EF%BC%8C%3C/span%3E%3Cspan%20class=%22underline%22%3E%E6%B3%95%E5%BE%8B%E3%80%81%E5%8F%B8%E6%B3%95%E8%A7%A3%E9%87%8A%E5%AF%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8E%E7%AC%AC%E4%B8%89%E4%BA%BA%E7%9A%84%E6%B0%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%E5%8F%A6%E6%9C%89%E8%A7%84%E5%AE%9A%E7%9A%84%EF%BC%8C%E4%BE%9D%E7%85%A7%E5%85%B6%E8%A7%84%E5%AE%9A%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="侵犯商业秘密或保密信息" _mubu_text="%3Cspan%3E%E4%BE%B5%E7%8A%AF%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%E6%88%96%E4%BF%9D%E5%AF%86%E4%BF%A1%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="合同成立型" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%88%90%E7%AB%8B%3C/span%3E%3Cspan%3E%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="合同被撤销" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E8%A2%AB%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="合同被宣告无效（担保合同）" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E8%A2%AB%E5%AE%A3%E5%91%8A%E6%97%A0%E6%95%88%EF%BC%88%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="恶意不办理登记手续" _mubu_text="%3Cspan%20class=%22bold%20text-color-purple%22%3E%E6%81%B6%E6%84%8F%E4%B8%8D%E5%8A%9E%E7%90%86%E7%99%BB%E8%AE%B0%E6%89%8B%E7%BB%AD%3C/span%3E" _note="" _mubu_note="">
              <outline text="报批义务条款单独生效（独立性)，追究违约责任，非报批义务条款未生效，追究缔约过失责任。" _mubu_text="%3Cspan%3E%E6%8A%A5%E6%89%B9%E4%B9%89%E5%8A%A1%E6%9D%A1%E6%AC%BE%E5%8D%95%E7%8B%AC%E7%94%9F%E6%95%88%EF%BC%88%E7%8B%AC%E7%AB%8B%E6%80%A7)%EF%BC%8C%E8%BF%BD%E7%A9%B6%E8%BF%9D%E7%BA%A6%E8%B4%A3%E4%BB%BB%EF%BC%8C%E9%9D%9E%E6%8A%A5%E6%89%B9%E4%B9%89%E5%8A%A1%E6%9D%A1%E6%AC%BE%E6%9C%AA%E7%94%9F%E6%95%88%EF%BC%8C%E8%BF%BD%E7%A9%B6%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%E3%80%82%3C/span%3E" _note="批准生效合同的法律适用（采矿权、股权纠纷）" _mubu_note="%3Cspan%3E%E6%89%B9%E5%87%86%E7%94%9F%E6%95%88%E5%90%88%E5%90%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E9%80%82%E7%94%A8%EF%BC%88%E9%87%87%E7%9F%BF%E6%9D%83%E3%80%81%E8%82%A1%E6%9D%83%E7%BA%A0%E7%BA%B7%EF%BC%89%3C/span%3E" _mubu_images="%5B%7B%22id%22%3A%22GHTdVwcQPx%22%2C%22oh%22%3A615%2C%22ow%22%3A4000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F6D7F20E7532E47091715310592.jpg%22%2C%22w%22%3A400%7D%5D"/>
              <outline text="合同未生效可解除" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E6%9C%AA%E7%94%9F%E6%95%88%E5%8F%AF%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note=""/>
