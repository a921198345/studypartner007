              <outline text="指采用合同书形式订立合同，在签字或盖章之前，当事人一方已经履行主要义务，对方接受的，该合园成立。" _mubu_text="%3Cspan%3E%E6%8C%87%E9%87%87%E7%94%A8%E5%90%88%E5%90%8C%E4%B9%A6%E5%BD%A2%E5%BC%8F%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%EF%BC%8C%E5%9C%A8%E7%AD%BE%E5%AD%97%E6%88%96%E7%9B%96%E7%AB%A0%E4%B9%8B%E5%89%8D%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%80%E6%96%B9%E5%B7%B2%E7%BB%8F%E5%B1%A5%E8%A1%8C%E4%B8%BB%E8%A6%81%E4%B9%89%E5%8A%A1%EF%BC%8C%E5%AF%B9%E6%96%B9%E6%8E%A5%E5%8F%97%E7%9A%84%EF%BC%8C%E8%AF%A5%E5%90%88%E5%9B%AD%E6%88%90%E7%AB%8B%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="迟延：同新要约" _mubu_text="%3Cspan%3E%E8%BF%9F%E5%BB%B6%EF%BC%9A%E5%90%8C%E6%96%B0%E8%A6%81%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
