            <outline text="债权人承受债务人债务" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E6%89%BF%E5%8F%97%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
