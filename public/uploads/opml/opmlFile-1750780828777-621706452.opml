              <outline text="事实行为：无需意思表示" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%AE%9E%E8%A1%8C%E4%B8%BA%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%97%A0%E9%9C%80%3C/span%3E%3Cspan%3E%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%3C/span%3E" _note="侵权、创作" _mubu_note="%3Cspan%3E%E4%BE%B5%E6%9D%83%E3%80%81%E5%88%9B%E4%BD%9C%3C/span%3E"/>
            </outline>
            <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
              <outline text="民事法律行为：意定" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E6%B3%95%E5%BE%8B%E8%A1%8C%E4%B8%BA%EF%BC%9A%E6%84%8F%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="事实行为：法定" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%AE%9E%E8%A1%8C%E4%B8%BA%EF%BC%9A%E6%B3%95%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="民事行为能力" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
