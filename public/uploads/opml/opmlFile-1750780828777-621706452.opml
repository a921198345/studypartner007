        <outline text="基本原理" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="含义：加工、定作、修理" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%EF%BC%9A%E5%8A%A0%E5%B7%A5%E3%80%81%E5%AE%9A%E4%BD%9C%E3%80%81%E4%BF%AE%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="人身信赖关系" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E4%BF%A1%E8%B5%96%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="主要劳动成果亲自完成" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E5%8A%B3%E5%8A%A8%E6%88%90%E6%9E%9C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BA%B2%E8%87%AA%E5%AE%8C%E6%88%90%3C/span%3E" _note="" _mubu_note="">
