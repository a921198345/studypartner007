                <outline text="不同行为能力人从事同一法律行为的后果不同，因为依当事人意思自治追求的效果" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E4%BB%8E%E4%BA%8B%E5%90%8C%E4%B8%80%E6%B3%95%E5%BE%8B%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%90%8E%E6%9E%9C%E4%B8%8D%E5%90%8C%EF%BC%8C%E5%9B%A0%E4%B8%BA%E4%BE%9D%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%84%8F%E6%80%9D%E8%87%AA%E6%B2%BB%E8%BF%BD%E6%B1%82%E7%9A%84%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="事实行为：无需具有" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%AE%9E%E8%A1%8C%E4%B8%BA%EF%BC%9A%E6%97%A0%E9%9C%80%E5%85%B7%E6%9C%89%3C/span%3E" _note="" _mubu_note="">
