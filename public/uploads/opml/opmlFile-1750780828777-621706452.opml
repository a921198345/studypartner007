            <outline text="整体面部形象" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%95%B4%E4%BD%93%3C/span%3E%3Cspan%3E%E9%9D%A2%E9%83%A8%E5%BD%A2%E8%B1%A1%3C/span%3E" _note="" _mubu_note=""/>
