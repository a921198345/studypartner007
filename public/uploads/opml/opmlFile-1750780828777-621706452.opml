          <outline text="占有损害赔偿请求权" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E6%8D%9F%E5%AE%B3%E8%B5%94%E5%81%BF%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="占有保护请求权 VS 物权请求权" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E4%BF%9D%E6%8A%A4%E8%AF%B7%E6%B1%82%E6%9D%83%20VS%20%E7%89%A9%E6%9D%83%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="基础" _mubu_text="%3Cspan%3E%E5%9F%BA%E7%A1%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="占有保护请求权：占有的事实" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E4%BF%9D%E6%8A%A4%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%9A%E5%8D%A0%E6%9C%89%E7%9A%84%E4%BA%8B%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="物权请求权：物权" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%9A%E7%89%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="行权主体" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%9D%83%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="占有保护请求权：占有人" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E4%BF%9D%E6%8A%A4%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%9A%E5%8D%A0%E6%9C%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="物权请求权：物权人" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%9A%E7%89%A9%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="期间限制" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="占有保护请求权：1 年除斥期间" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E4%BF%9D%E6%8A%A4%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%9A1%20%E5%B9%B4%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="物权请求权" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="有登记制度：不受期间限制" _mubu_text="%3Cspan%3E%E6%9C%89%E7%99%BB%E8%AE%B0%E5%88%B6%E5%BA%A6%EF%BC%9A%E4%B8%8D%E5%8F%97%E6%9C%9F%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="无登记制度：诉讼时效" _mubu_text="%3Cspan%3E%E6%97%A0%E7%99%BB%E8%AE%B0%E5%88%B6%E5%BA%A6%EF%BC%9A%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="推定效力" _mubu_text="%3Cspan%3E%E6%8E%A8%E5%AE%9A%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="占有即推定为所有" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E5%8D%B3%E6%8E%A8%E5%AE%9A%E4%B8%BA%E6%89%80%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
