      <outline text="七种特殊主体责任" _mubu_text="%3Cspan%3E%E4%B8%83%E7%A7%8D%E7%89%B9%E6%AE%8A%E4%B8%BB%E4%BD%93%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="监护人责任" _mubu_text="%3Cspan%3E%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="责任主体" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则上由监护人承担侵权责任，有财产的无民事行为能力人、限制民事行为能力人造成他人损害的，从本人财产中支付赔偿费用;不足部分，由监护人赔偿。监护人责任的性质系“替代责任”，即为他人的侵权行为负责的责任。" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E7%94%B1%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%EF%BC%8C%E6%9C%89%E8%B4%A2%E4%BA%A7%E7%9A%84%E6%97%A0%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E9%80%A0%E6%88%90%E4%BB%96%E4%BA%BA%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%E4%BB%8E%E6%9C%AC%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%B8%AD%E6%94%AF%E4%BB%98%E8%B5%94%E5%81%BF%E8%B4%B9%E7%94%A8;%E4%B8%8D%E8%B6%B3%E9%83%A8%E5%88%86%EF%BC%8C%E7%94%B1%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B5%94%E5%81%BF%E3%80%82%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B4%A3%E4%BB%BB%E7%9A%84%E6%80%A7%E8%B4%A8%E7%B3%BB%E2%80%9C%E6%9B%BF%E4%BB%A3%E8%B4%A3%E4%BB%BB%E2%80%9D%EF%BC%8C%E5%8D%B3%E4%B8%BA%E4%BB%96%E4%BA%BA%E7%9A%84%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E8%B4%9F%E8%B4%A3%E7%9A%84%E8%B4%A3%E4%BB%BB%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="归责原则" _mubu_text="%3Cspan%3E%E5%BD%92%E8%B4%A3%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="相对无过错" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E6%97%A0%E8%BF%87%E9%94%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="替代责任" _mubu_text="%3Cspan%3E%E6%9B%BF%E4%BB%A3%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="免责事由和特殊情形" _mubu_text="%3Cspan%3E%E5%85%8D%E8%B4%A3%E4%BA%8B%E7%94%B1%E5%92%8C%E7%89%B9%E6%AE%8A%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="1.尽到监护责任，可以适当减轻" _mubu_text="%3Cspan%3E1.%E5%B0%BD%E5%88%B0%E7%9B%91%E6%8A%A4%E8%B4%A3%E4%BB%BB%EF%BC%8C%E5%8F%AF%E4%BB%A5%E9%80%82%E5%BD%93%E5%87%8F%E8%BD%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="2.被监护人有独立财产的，监护人承担补充赔偿责任" _mubu_text="%3Cspan%3E2.%E8%A2%AB%E7%9B%91%E6%8A%A4%E4%BA%BA%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E6%9C%89%E7%8B%AC%E7%AB%8B%E8%B4%A2%E4%BA%A7%3C/span%3E%3Cspan%3E%E7%9A%84%EF%BC%8C%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E8%A1%A5%E5%85%85%E8%B5%94%E5%81%BF%3C/span%3E%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="无民事行为能力人、限制民事行为能力人造成他人损害，被侵权人请求监护人承担侵权责任，或合并请求监护人和受托履行监护职责的人承担侵权责任的，法院应当将无民事行为能力人、限制民事行为能力人列为共同被告(《民法典侵权责任编解释(一)》第4条)。" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E9%80%A0%E6%88%90%E4%BB%96%E4%BA%BA%E6%8D%9F%E5%AE%B3%EF%BC%8C%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E8%AF%B7%E6%B1%82%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%EF%BC%8C%E6%88%96%E5%90%88%E5%B9%B6%E8%AF%B7%E6%B1%82%E7%9B%91%E6%8A%A4%E4%BA%BA%E5%92%8C%E5%8F%97%E6%89%98%E5%B1%A5%E8%A1%8C%E7%9B%91%E6%8A%A4%E8%81%8C%E8%B4%A3%E7%9A%84%E4%BA%BA%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%E5%B0%86%E6%97%A0%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E5%88%97%E4%B8%BA%E5%85%B1%E5%90%8C%E8%A2%AB%E5%91%8A%3C/span%3E%3Cspan%3E(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC4%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="被侵权人请求监护人承担侵权人应承担的全部责任的，法院应予支持，并在判决中明确，赔偿费用可以先从被监护人财产中支付，不足部分由监护人支付。监护人抗辩主张承担补充责任，或被侵权人、监护人主张法院判令有财产的无民事行为能力人、限制民事行为能力人承担赔偿责任的，法院不予支持。从被监护人财产中支付赔偿费用的，应当保留被监护人所必需的生活费和完成义务教育所必需的费用(《民法典侵权责任编解释(一)》第5条)。" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E8%AF%B7%E6%B1%82%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E%3Cspan%3E%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%BA%94%E6%89%BF%E6%8B%85%E7%9A%84%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E5%85%A8%E9%83%A8%E8%B4%A3%E4%BB%BB%3C/span%3E%3Cspan%3E%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E4%BA%88%E6%94%AF%E6%8C%81%EF%BC%8C%E5%B9%B6%E5%9C%A8%E5%88%A4%E5%86%B3%E4%B8%AD%E6%98%8E%E7%A1%AE%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E8%B5%94%E5%81%BF%E8%B4%B9%E7%94%A8%E5%8F%AF%E4%BB%A5%E5%85%88%E4%BB%8E%E8%A2%AB%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%B8%AD%E6%94%AF%E4%BB%98%EF%BC%8C%E4%B8%8D%E8%B6%B3%E9%83%A8%E5%88%86%E7%94%B1%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%94%AF%E4%BB%98%E3%80%82%3C/span%3E%3Cspan%3E%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%8A%97%E8%BE%A9%E4%B8%BB%E5%BC%A0%E6%89%BF%E6%8B%85%E8%A1%A5%E5%85%85%E8%B4%A3%E4%BB%BB%EF%BC%8C%E6%88%96%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E3%80%81%E7%9B%91%E6%8A%A4%E4%BA%BA%E4%B8%BB%E5%BC%A0%E6%B3%95%E9%99%A2%E5%88%A4%E4%BB%A4%E6%9C%89%E8%B4%A2%E4%BA%A7%E7%9A%84%E6%97%A0%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E4%B8%8D%E4%BA%88%E6%94%AF%E6%8C%81%E3%80%82%E4%BB%8E%E8%A2%AB%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%B8%AD%E6%94%AF%E4%BB%98%E8%B5%94%E5%81%BF%E8%B4%B9%E7%94%A8%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E4%BF%9D%E7%95%99%E8%A2%AB%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%80%E5%BF%85%E9%9C%80%E7%9A%84%E7%94%9F%E6%B4%BB%E8%B4%B9%E5%92%8C%E5%AE%8C%E6%88%90%E4%B9%89%E5%8A%A1%E6%95%99%E8%82%B2%E6%89%80%E5%BF%85%E9%9C%80%E7%9A%84%E8%B4%B9%E7%94%A8(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC5%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="3.侵权行为发生时不满 18 周岁，起诉时已满18 周岁。" _mubu_text="%3Cspan%3E3.%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E5%8F%91%E7%94%9F%E6%97%B6%E4%B8%8D%E6%BB%A1%2018%20%E5%91%A8%E5%B2%81%EF%BC%8C%E8%B5%B7%E8%AF%89%E6%97%B6%E5%B7%B2%E6%BB%A118%20%E5%91%A8%E5%B2%81%E3%80%82%3C/span%3E" _note="" _mubu_note="">
              <outline text="行为人在侵权行为发生时不满18周岁，被诉时已满18周岁的，被侵权人请求原监护人承担侵权人应承担的全部责任的，法院应予支持，并在判决中明确，赔偿费用可以先从被监护人财产中支付，不足部分由监护人支付。被侵权人仅起诉行为人的，法院应当向原告释明申请追加原监护人为共同被告(《民法典侵权责任编解释(一)》第6条)。" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E4%BA%BA%E5%9C%A8%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E5%8F%91%E7%94%9F%E6%97%B6%E4%B8%8D%E6%BB%A118%E5%91%A8%E5%B2%81%EF%BC%8C%E8%A2%AB%E8%AF%89%E6%97%B6%E5%B7%B2%E6%BB%A118%E5%91%A8%E5%B2%81%E7%9A%84%EF%BC%8C%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E8%AF%B7%E6%B1%82%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E5%8E%9F%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E%3Cspan%3E%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%BA%94%E6%89%BF%E6%8B%85%E7%9A%84%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E5%85%A8%E9%83%A8%E8%B4%A3%E4%BB%BB%3C/span%3E%3Cspan%3E%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E4%BA%88%E6%94%AF%E6%8C%81%EF%BC%8C%E5%B9%B6%E5%9C%A8%E5%88%A4%E5%86%B3%E4%B8%AD%E6%98%8E%E7%A1%AE%EF%BC%8C%E8%B5%94%E5%81%BF%E8%B4%B9%E7%94%A8%E5%8F%AF%E4%BB%A5%E5%85%88%E4%BB%8E%E8%A2%AB%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%B8%AD%E6%94%AF%E4%BB%98%EF%BC%8C%E4%B8%8D%E8%B6%B3%E9%83%A8%E5%88%86%E7%94%B1%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%94%AF%E4%BB%98%E3%80%82%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E4%BB%85%E8%B5%B7%E8%AF%89%E8%A1%8C%E4%B8%BA%E4%BA%BA%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%E5%90%91%E5%8E%9F%E5%91%8A%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E9%87%8A%E6%98%8E%3C/span%3E%3Cspan%3E%E7%94%B3%E8%AF%B7%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E8%BF%BD%E5%8A%A0%E5%8E%9F%E7%9B%91%E6%8A%A4%E4%BA%BA%E4%B8%BA%E5%85%B1%E5%90%8C%E8%A2%AB%E5%91%8A%3C/span%3E%3Cspan%3E(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC6%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="4.父母离异。" _mubu_text="%3Cspan%3E4.%E7%88%B6%E6%AF%8D%E7%A6%BB%E5%BC%82%E3%80%82%3C/span%3E" _note="" _mubu_note="">
              <outline text="夫妻离婚后，未成年子女造成他人损害，被侵权人可以请求离异夫妻共同承担侵权责任。一方不得以未与该子女共同生活为由主张不承担或少承担责任。离异夫妻之间的责任份额，可以由双方协议确定;协议不成的，法院可以根据双方履行监护职责的约定和实际履行情况等确定。实际承担责任超过自己责任份额的一方可以向另一方追偿(《民法典侵权责任编解释(一)》第8条)。" _mubu_text="%3Cspan%3E%E5%A4%AB%E5%A6%BB%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E7%A6%BB%E5%A9%9A%E5%90%8E%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%9C%AA%E6%88%90%E5%B9%B4%E5%AD%90%E5%A5%B3%E9%80%A0%E6%88%90%E4%BB%96%E4%BA%BA%E6%8D%9F%E5%AE%B3%EF%BC%8C%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%8F%AF%E4%BB%A5%E8%AF%B7%E6%B1%82%E7%A6%BB%E5%BC%82%E5%A4%AB%E5%A6%BB%E5%85%B1%E5%90%8C%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E3%80%82%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E4%B8%80%E6%96%B9%E4%B8%8D%E5%BE%97%E4%BB%A5%E6%9C%AA%E4%B8%8E%E8%AF%A5%E5%AD%90%E5%A5%B3%E5%85%B1%E5%90%8C%E7%94%9F%E6%B4%BB%E4%B8%BA%E7%94%B1%E4%B8%BB%E5%BC%A0%E4%B8%8D%E6%89%BF%E6%8B%85%E6%88%96%E5%B0%91%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%E3%80%82%3C/span%3E%3Cspan%3E%E7%A6%BB%E5%BC%82%E5%A4%AB%E5%A6%BB%E4%B9%8B%E9%97%B4%E7%9A%84%E8%B4%A3%E4%BB%BB%E4%BB%BD%E9%A2%9D%EF%BC%8C%E5%8F%AF%E4%BB%A5%E7%94%B1%E5%8F%8C%E6%96%B9%E5%8D%8F%E8%AE%AE%E7%A1%AE%E5%AE%9A;%E5%8D%8F%E8%AE%AE%E4%B8%8D%E6%88%90%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BB%A5%E6%A0%B9%E6%8D%AE%E5%8F%8C%E6%96%B9%E5%B1%A5%E8%A1%8C%E7%9B%91%E6%8A%A4%E8%81%8C%E8%B4%A3%E7%9A%84%E7%BA%A6%E5%AE%9A%E5%92%8C%E5%AE%9E%E9%99%85%E5%B1%A5%E8%A1%8C%E6%83%85%E5%86%B5%E7%AD%89%E7%A1%AE%E5%AE%9A%E3%80%82%E5%AE%9E%E9%99%85%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%E8%B6%85%E8%BF%87%E8%87%AA%E5%B7%B1%E8%B4%A3%E4%BB%BB%E4%BB%BD%E9%A2%9D%E7%9A%84%E4%B8%80%E6%96%B9%E5%8F%AF%E4%BB%A5%E5%90%91%E5%8F%A6%E4%B8%80%E6%96%B9%E8%BF%BD%E5%81%BF(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC8%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="5.无抚养教育关系的继父或继母" _mubu_text="%3Cspan%3E5.%E6%97%A0%E6%8A%9A%E5%85%BB%E6%95%99%E8%82%B2%E5%85%B3%E7%B3%BB%E7%9A%84%E7%BB%A7%E7%88%B6%E6%88%96%E7%BB%A7%E6%AF%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="未成年子女造成他人损害的，未与该子女形成抚养教育关系的继父或继母不承担监护人的侵权责任，由该子女的生父母承担侵权责任(《民法典侵权责任编解释(一)》第9条)。" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%88%90%E5%B9%B4%E5%AD%90%E5%A5%B3%E9%80%A0%E6%88%90%E4%BB%96%E4%BA%BA%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E6%9C%AA%E4%B8%8E%E8%AF%A5%E5%AD%90%E5%A5%B3%E5%BD%A2%E6%88%90%E6%8A%9A%E5%85%BB%E6%95%99%E8%82%B2%E5%85%B3%E7%B3%BB%E7%9A%84%E7%BB%A7%E7%88%B6%E6%88%96%E7%BB%A7%E6%AF%8D%E4%B8%8D%E6%89%BF%E6%8B%85%E7%9B%91%E6%8A%A4%E4%BA%BA%E7%9A%84%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%3C/span%3E%3Cspan%3E%EF%BC%8C%E7%94%B1%E8%AF%A5%E5%AD%90%E5%A5%B3%E7%9A%84%E7%94%9F%E7%88%B6%E6%AF%8D%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC9%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="6.受托监护" _mubu_text="%3Cspan%3E6.%E5%8F%97%E6%89%98%E7%9B%91%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="无民事行为能力人、限制民事行为能力人造成他人损害，被侵权人合并请求监护人和受托履行监护职责的人承担侵权责任的，监护人承担侵权人应承担的全部责任;受托人在过错范围内与监护人共同承担责任，但责任主体实际支付的赔偿费用总和不应超出被侵权人应受偿的损失数额。监护人承担责任后向受托人追偿的，法院可以参照《民法典》第929条的规定处理。仅有一般过失的无偿受托人承担责任后向监护人追偿的，法院应予支持(《民法典侵权责任编解释(一)》第10条)。" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E9%80%A0%E6%88%90%E4%BB%96%E4%BA%BA%E6%8D%9F%E5%AE%B3%EF%BC%8C%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%90%88%E5%B9%B6%E8%AF%B7%E6%B1%82%E7%9B%91%E6%8A%A4%E4%BA%BA%E5%92%8C%E5%8F%97%E6%89%98%E5%B1%A5%E8%A1%8C%E7%9B%91%E6%8A%A4%E8%81%8C%E8%B4%A3%E7%9A%84%E4%BA%BA%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%9A%84%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-purple%22%3E%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%BA%94%E6%89%BF%E6%8B%85%E7%9A%84%E5%85%A8%E9%83%A8%E8%B4%A3%E4%BB%BB;%E5%8F%97%E6%89%98%E4%BA%BA%E5%9C%A8%E8%BF%87%E9%94%99%E8%8C%83%E5%9B%B4%E5%86%85%E4%B8%8E%E7%9B%91%E6%8A%A4%E4%BA%BA%E5%85%B1%E5%90%8C%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%EF%BC%8C%3C/span%3E%3Cspan%3E%E4%BD%86%E8%B4%A3%E4%BB%BB%E4%B8%BB%E4%BD%93%E5%AE%9E%E9%99%85%E6%94%AF%E4%BB%98%E7%9A%84%E8%B5%94%E5%81%BF%E8%B4%B9%E7%94%A8%E6%80%BB%E5%92%8C%E4%B8%8D%E5%BA%94%E8%B6%85%E5%87%BA%E8%A2%AB%E4%BE%B5%E6%9D%83%E4%BA%BA%E5%BA%94%E5%8F%97%E5%81%BF%E7%9A%84%E6%8D%9F%E5%A4%B1%E6%95%B0%E9%A2%9D%E3%80%82%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%E5%90%8E%E5%90%91%E5%8F%97%E6%89%98%E4%BA%BA%E8%BF%BD%E5%81%BF%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BB%A5%E5%8F%82%E7%85%A7%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E3%80%8B%E7%AC%AC929%E6%9D%A1%E7%9A%84%E8%A7%84%E5%AE%9A%E5%A4%84%E7%90%86%E3%80%82%E4%BB%85%E6%9C%89%E4%B8%80%E8%88%AC%E8%BF%87%E5%A4%B1%E7%9A%84%E6%97%A0%E5%81%BF%E5%8F%97%E6%89%98%E4%BA%BA%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%E5%90%8E%E5%90%91%E7%9B%91%E6%8A%A4%E4%BA%BA%E8%BF%BD%E5%81%BF%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E4%BA%88%E6%94%AF%E6%8C%81(%E3%80%8A%E6%B0%91%E6%B3%95%E5%85%B8%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%E7%BC%96%E8%A7%A3%E9%87%8A(%E4%B8%80)%E3%80%8B%E7%AC%AC10%E6%9D%A1)%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
