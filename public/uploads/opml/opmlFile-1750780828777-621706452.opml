        <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="有权与无权" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9D%83%E4%B8%8E%E6%97%A0%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="本权有无" _mubu_text="%3Cspan%20class=%22underline%22%3E%E6%9C%AC%E6%9D%83%E6%9C%89%E6%97%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有权占有：基于合法原因" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9D%83%E5%8D%A0%E6%9C%89%EF%BC%9A%E5%9F%BA%E4%BA%8E%E5%90%88%E6%B3%95%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无权占有：非基于合法原因（盗赃物、遗失物）" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E5%8D%A0%E6%9C%89%EF%BC%9A%E9%9D%9E%E5%9F%BA%E4%BA%8E%E5%90%88%E6%B3%95%E5%8E%9F%E5%9B%A0%EF%BC%88%E7%9B%97%E8%B5%83%E7%89%A9%E3%80%81%E9%81%97%E5%A4%B1%E7%89%A9%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="是否误信" _mubu_text="%3Cspan%20class=%22underline%22%3E%E6%98%AF%E5%90%A6%3C/span%3E%3Cspan%20class=%22bold%20underline%22%3E%E8%AF%AF%E4%BF%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="善意占有：不知无本权" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%8D%A0%E6%9C%89%EF%BC%9A%E4%B8%8D%E7%9F%A5%E6%97%A0%E6%9C%AC%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="恶意占有：知道无本权" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%EF%BC%9A%E7%9F%A5%E9%81%93%E6%97%A0%E6%9C%AC%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="自主与他主" _mubu_text="%3Cspan%3E%E8%87%AA%E4%B8%BB%E4%B8%8E%E4%BB%96%E4%B8%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="是否以自己所有的意思占有" _mubu_text="%3Cspan%20class=%22underline%22%3E%E6%98%AF%E5%90%A6%E4%BB%A5%E8%87%AA%E5%B7%B1%E6%89%80%E6%9C%89%E7%9A%84%E6%84%8F%E6%80%9D%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="自主占有（盗贼对盗赃物的占有）" _mubu_text="%3Cspan%3E%E8%87%AA%E4%B8%BB%E5%8D%A0%E6%9C%89%EF%BC%88%E7%9B%97%E8%B4%BC%E5%AF%B9%E7%9B%97%E8%B5%83%E7%89%A9%E7%9A%84%E5%8D%A0%E6%9C%89%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="他主占有（租赁合同、借用合同）" _mubu_text="%3Cspan%3E%E4%BB%96%E4%B8%BB%E5%8D%A0%E6%9C%89%EF%BC%88%E7%A7%9F%E8%B5%81%E5%90%88%E5%90%8C%E3%80%81%E5%80%9F%E7%94%A8%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="直接与间接" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E4%B8%8E%E9%97%B4%E6%8E%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="是否直接管领其物（他主占有）" _mubu_text="%3Cspan%20class=%22underline%22%3E%E6%98%AF%E5%90%A6%E7%9B%B4%E6%8E%A5%E7%AE%A1%E9%A2%86%E5%85%B6%E7%89%A9%3C/span%3E%3Cspan%20class=%22bold%20underline%22%3E%EF%BC%88%E4%BB%96%E4%B8%BB%E5%8D%A0%E6%9C%89%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="直接占有：直接对物有事实上的管领力" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E5%8D%A0%E6%9C%89%EF%BC%9A%E7%9B%B4%E6%8E%A5%E5%AF%B9%E7%89%A9%E6%9C%89%E4%BA%8B%E5%AE%9E%E4%B8%8A%E7%9A%84%E7%AE%A1%E9%A2%86%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="间接占有：占有媒介关系" _mubu_text="%3Cspan%3E%E9%97%B4%E6%8E%A5%E5%8D%A0%E6%9C%89%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8D%A0%E6%9C%89%E5%AA%92%E4%BB%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="遗失物、盗赃物不存在占有媒介关系，因此不存在直接占有、间接占有问题" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%81%97%E5%A4%B1%E7%89%A9%E3%80%81%E7%9B%97%E8%B5%83%E7%89%A9%E4%B8%8D%E5%AD%98%E5%9C%A8%E5%8D%A0%E6%9C%89%E5%AA%92%E4%BB%8B%E5%85%B3%E7%B3%BB%EF%BC%8C%E5%9B%A0%E6%AD%A4%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%B8%8D%E5%AD%98%E5%9C%A8%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E7%9B%B4%E6%8E%A5%E5%8D%A0%E6%9C%89%E3%80%81%E9%97%B4%E6%8E%A5%E5%8D%A0%E6%9C%89%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="盗赃物 VS 遗失物" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%9B%97%E8%B5%83%E7%89%A9%20VS%20%E9%81%97%E5%A4%B1%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="盗赃物：无权占有、恶意占有、自主占有" _mubu_text="%3Cspan%3E%E7%9B%97%E8%B5%83%E7%89%A9%EF%BC%9A%E6%97%A0%E6%9D%83%E5%8D%A0%E6%9C%89%E3%80%81%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%E3%80%81%E8%87%AA%E4%B8%BB%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="遗失物：无权占有、恶意占有" _mubu_text="%3Cspan%3E%E9%81%97%E5%A4%B1%E7%89%A9%EF%BC%9A%E6%97%A0%E6%9D%83%E5%8D%A0%E6%9C%89%E3%80%81%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="有返还的意思：他主占有" _mubu_text="%3Cspan%3E%E6%9C%89%E8%BF%94%E8%BF%98%E7%9A%84%E6%84%8F%E6%80%9D%EF%BC%9A%E4%BB%96%E4%B8%BB%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无返还的意思：自主占有" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%94%E8%BF%98%E7%9A%84%E6%84%8F%E6%80%9D%EF%BC%9A%E8%87%AA%E4%B8%BB%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="善意占有 VS 恶意占有" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%96%84%E6%84%8F%E5%8D%A0%E6%9C%89%20VS%20%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="相同" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="都享有使用权" _mubu_text="%3Cspan%3E%E9%83%BD%E4%BA%AB%E6%9C%89%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="都承担侵权责任" _mubu_text="%3Cspan%3E%E9%83%BD%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="都需返还原物和孳息" _mubu_text="%3Cspan%3E%E9%83%BD%E9%9C%80%E8%BF%94%E8%BF%98%E5%8E%9F%E7%89%A9%E5%92%8C%E5%AD%B3%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="不同" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="损耗赔偿责任" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%8D%9F%E8%80%97%E8%B5%94%E5%81%BF%3C/span%3E%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="善意占有人：无需承担" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E6%97%A0%E9%9C%80%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="恶意占有人：需要承担" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E9%9C%80%E8%A6%81%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="请求必要费用" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BF%85%E8%A6%81%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="善意占有人：有权请求" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E6%9C%89%E6%9D%83%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="恶意占有人：无权请求" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E6%97%A0%E6%9D%83%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="承担风险" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%89%BF%E6%8B%85%E9%A3%8E%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="善意占有人：无需承担" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E6%97%A0%E9%9C%80%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="恶意占有人：需要承担" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E5%8D%A0%E6%9C%89%E4%BA%BA%EF%BC%9A%E9%9C%80%E8%A6%81%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="占有的保护" _mubu_text="%3Cspan%3E%E5%8D%A0%E6%9C%89%E7%9A%84%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="" _heading="3">
