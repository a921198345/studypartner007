              <outline text="权限" _mubu_text="%3Cspan%3E%E6%9D%83%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="期间" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="被代理人签章" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E7%AD%BE%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="基础法律关系 VS 代理权授予行为" _mubu_text="%3Cspan%3E%E5%9F%BA%E7%A1%80%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%20VS%20%E4%BB%A3%E7%90%86%E6%9D%83%E6%8E%88%E4%BA%88%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="基础法律关系：双方（委托合同、劳动合同）" _mubu_text="%3Cspan%3E%E5%9F%BA%E7%A1%80%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%EF%BC%9A%E5%8F%8C%E6%96%B9%EF%BC%88%E5%A7%94%E6%89%98%E5%90%88%E5%90%8C%E3%80%81%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="代理权授予行为：单方" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E6%9D%83%E6%8E%88%E4%BA%88%E8%A1%8C%E4%B8%BA%EF%BC%9A%E5%8D%95%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="委托 VS 代理" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%20VS%20%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="有委托不一定有代理（委托监护）" _mubu_text="%3Cspan%3E%E6%9C%89%E5%A7%94%E6%89%98%E4%B8%8D%E4%B8%80%E5%AE%9A%E6%9C%89%E4%BB%A3%E7%90%86%EF%BC%88%E5%A7%94%E6%89%98%E7%9B%91%E6%8A%A4%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有代理不一定有委托（法定监护）" _mubu_text="%3Cspan%3E%E6%9C%89%E4%BB%A3%E7%90%86%E4%B8%8D%E4%B8%80%E5%AE%9A%E6%9C%89%E5%A7%94%E6%89%98%EF%BC%88%E6%B3%95%E5%AE%9A%E7%9B%91%E6%8A%A4%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="代理权滥用" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E6%9D%83%E6%BB%A5%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="自己代理（一身二职）——合同相对无效" _mubu_text="%3Cspan%3E%E8%87%AA%E5%B7%B1%E4%BB%A3%E7%90%86%EF%BC%88%E4%B8%80%E8%BA%AB%E4%BA%8C%E8%81%8C%EF%BC%89%E2%80%94%E2%80%94%E5%90%88%E5%90%8C%E7%9B%B8%E5%AF%B9%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="被代理人同意或追认的，民事法律行为有效" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E5%90%8C%E6%84%8F%E6%88%96%E8%BF%BD%E8%AE%A4%E7%9A%84%EF%BC%8C%E6%B0%91%E4%BA%8B%E6%B3%95%E5%BE%8B%E8%A1%8C%E4%B8%BA%E6%9C%89%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="双方代理（一仆二主）——合同相对无效" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E4%BB%A3%E7%90%86%EF%BC%88%E4%B8%80%E4%BB%86%E4%BA%8C%E4%B8%BB%EF%BC%89%E2%80%94%E2%80%94%E5%90%88%E5%90%8C%E7%9B%B8%E5%AF%B9%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="恶意代理（吃里扒外）——合同绝对无效" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E4%BB%A3%E7%90%86%EF%BC%88%E5%90%83%E9%87%8C%E6%89%92%E5%A4%96%EF%BC%89%E2%80%94%E2%80%94%E5%90%88%E5%90%8C%E7%BB%9D%E5%AF%B9%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="代理人和相对人承担连带责任" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E4%BA%BA%E5%92%8C%E7%9B%B8%E5%AF%B9%E4%BA%BA%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="复代理" _mubu_text="%3Cspan%3E%E5%A4%8D%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="代理人以自己的名义选任" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E4%BA%BA%E4%BB%A5%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%87%AA%E5%B7%B1%E7%9A%84%E5%90%8D%E4%B9%89%3C/span%3E%3Cspan%3E%E9%80%89%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="复代理人是被代理人的代理人" _mubu_text="%3Cspan%3E%E5%A4%8D%E4%BB%A3%E7%90%86%E4%BA%BA%E6%98%AF%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E%3Cspan%3E%E7%9A%84%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="产生" _mubu_text="%3Cspan%3E%E4%BA%A7%E7%94%9F%3C/span%3E" _note="" _mubu_note="">
            <outline text="事前同意" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%89%8D%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="事后追认" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%90%8E%E8%BF%BD%E8%AE%A4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="紧急情况：急病、通讯中断，如不及时转托代理，会给被代理人造成利益损失" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%B4%A7%E6%80%A5%E6%83%85%E5%86%B5%EF%BC%9A%E6%80%A5%E7%97%85%E3%80%81%E9%80%9A%E8%AE%AF%E4%B8%AD%E6%96%AD%EF%BC%8C%E5%A6%82%E4%B8%8D%E5%8F%8A%E6%97%B6%E8%BD%AC%E6%89%98%E4%BB%A3%E7%90%86%EF%BC%8C%E4%BC%9A%E7%BB%99%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E9%80%A0%E6%88%90%E5%88%A9%E7%9B%8A%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="第三人可直接要求被代理人赔偿" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%8F%AF%E7%9B%B4%E6%8E%A5%E8%A6%81%E6%B1%82%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被代理人可向委托代理人追偿" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E5%8F%AF%E5%90%91%E5%A7%94%E6%89%98%E4%BB%A3%E7%90%86%E4%BA%BA%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="复代理人有过错，承担连带责任" _mubu_text="%3Cspan%3E%E5%A4%8D%E4%BB%A3%E7%90%86%E4%BA%BA%E6%9C%89%E8%BF%87%E9%94%99%EF%BC%8C%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="职务代理" _mubu_text="%3Cspan%3E%E8%81%8C%E5%8A%A1%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="被代理人是法人或非法人组织" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E6%98%AF%E6%B3%95%E4%BA%BA%E6%88%96%E9%9D%9E%E6%B3%95%E4%BA%BA%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="代理事项在职权范围内" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E4%BA%8B%E9%A1%B9%E5%9C%A8%E8%81%8C%E6%9D%83%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="职权范围内事项，发生效力" _mubu_text="%3Cspan%3E%E8%81%8C%E6%9D%83%E8%8C%83%E5%9B%B4%E5%86%85%E4%BA%8B%E9%A1%B9%EF%BC%8C%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="职权范围的限制，不得对抗善意相对人" _mubu_text="%3Cspan%3E%E8%81%8C%E6%9D%83%E8%8C%83%E5%9B%B4%E7%9A%84%E9%99%90%E5%88%B6%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E6%84%8F%E7%9B%B8%E5%AF%B9%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="无权代理 VS 表见代理" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%20VS%20%E8%A1%A8%E8%A7%81%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="判断" _mubu_text="%3Cspan%3E%E5%88%A4%E6%96%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="无权代理：第三人没有理由相信其有代理权" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%EF%BC%9A%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%B2%A1%E6%9C%89%E7%90%86%E7%94%B1%E7%9B%B8%E4%BF%A1%E5%85%B6%E6%9C%89%E4%BB%A3%E7%90%86%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="表见代理：第三人有理由相信其有代理权" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%A1%A8%E8%A7%81%E4%BB%A3%E7%90%86%EF%BC%9A%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%9C%89%E7%90%86%E7%94%B1%E7%9B%B8%E4%BF%A1%E5%85%B6%E6%9C%89%E4%BB%A3%E7%90%86%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="主观" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A7%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="善意：不知情" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%EF%BC%9A%E4%B8%8D%E7%9F%A5%E6%83%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="无过失：对授权委托书进行形式审查" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%87%E5%A4%B1%EF%BC%9A%E5%AF%B9%E6%8E%88%E6%9D%83%E5%A7%94%E6%89%98%E4%B9%A6%E8%BF%9B%E8%A1%8C%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="客观" _mubu_text="%3Cspan%3E%E5%AE%A2%E8%A7%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="介绍信" _mubu_text="%3Cspan%3E%E4%BB%8B%E7%BB%8D%E4%BF%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="盖有公章或合同专用章的空白合同书" _mubu_text="%3Cspan%3E%E7%9B%96%E6%9C%89%E5%85%AC%E7%AB%A0%E6%88%96%E5%90%88%E5%90%8C%E4%B8%93%E7%94%A8%E7%AB%A0%E7%9A%84%E7%A9%BA%E7%99%BD%E5%90%88%E5%90%8C%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="效力：公章 = 合同专用章" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%EF%BC%9A%E5%85%AC%E7%AB%A0%20=%20%E5%90%88%E5%90%8C%E4%B8%93%E7%94%A8%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="伪造章，构成无权代理" _mubu_text="%3Cspan%3E%E4%BC%AA%E9%80%A0%E7%AB%A0%EF%BC%8C%E6%9E%84%E6%88%90%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="交易习惯（经常、通常）" _mubu_text="%3Cspan%3E%E4%BA%A4%E6%98%93%E4%B9%A0%E6%83%AF%EF%BC%88%E7%BB%8F%E5%B8%B8%E3%80%81%E9%80%9A%E5%B8%B8%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="合同效力" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="无权代理：效力待定" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%EF%BC%9A%E6%95%88%E5%8A%9B%E5%BE%85%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="表见代理：合法有效" _mubu_text="%3Cspan%3E%E8%A1%A8%E8%A7%81%E4%BB%A3%E7%90%86%EF%BC%9A%E5%90%88%E6%B3%95%E6%9C%89%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="无权代理" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="本人追认：本人承担" _mubu_text="%3Cspan%3E%E6%9C%AC%E4%BA%BA%E8%BF%BD%E8%AE%A4%EF%BC%9A%E6%9C%AC%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="履行治愈规则：被代理人已经开始履行合同义务，视为对合同的追认" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%B1%A5%E8%A1%8C%E6%B2%BB%E6%84%88%E8%A7%84%E5%88%99%EF%BC%9A%E8%A2%AB%E4%BB%A3%E7%90%86%E4%BA%BA%E5%B7%B2%E7%BB%8F%E5%BC%80%E5%A7%8B%E5%B1%A5%E8%A1%8C%E5%90%88%E5%90%8C%E4%B9%89%E5%8A%A1%EF%BC%8C%E8%A7%86%E4%B8%BA%E5%AF%B9%E5%90%88%E5%90%8C%E7%9A%84%E8%BF%BD%E8%AE%A4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不追认：行为人承担" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%BF%BD%E8%AE%A4%EF%BC%9A%E8%A1%8C%E4%B8%BA%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="表见代理：承担责任" _mubu_text="%3Cspan%3E%E8%A1%A8%E8%A7%81%E4%BB%A3%E7%90%86%EF%BC%9A%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="表见代理vs表见代表" _mubu_text="%3Cspan%3E%E8%A1%A8%E8%A7%81%E4%BB%A3%E7%90%86vs%E8%A1%A8%E8%A7%81%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22mAsode1ltf%22%2C%22oh%22%3A999%2C%22ow%22%3A3407%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F7607641F747A45E51715387991.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
      </outline>
      <outline text="诉讼时效" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="" _heading="2">
