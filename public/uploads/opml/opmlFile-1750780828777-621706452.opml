          <outline text="中断" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%AD%E6%96%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="债权人主张" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E4%B8%BB%E5%BC%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="权利人提出履行请求" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E6%8F%90%E5%87%BA%E5%B1%A5%E8%A1%8C%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="主张权利文书送达对方" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%BC%A0%E6%9D%83%E5%88%A9%E6%96%87%E4%B9%A6%E9%80%81%E8%BE%BE%E5%AF%B9%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="信件或数据电文到达对方" _mubu_text="%3Cspan%3E%E4%BF%A1%E4%BB%B6%E6%88%96%E6%95%B0%E6%8D%AE%E7%94%B5%E6%96%87%E5%88%B0%E8%BE%BE%E5%AF%B9%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="金融机构从账户扣收欠款本息" _mubu_text="%3Cspan%3E%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E4%BB%8E%E8%B4%A6%E6%88%B7%E6%89%A3%E6%94%B6%E6%AC%A0%E6%AC%BE%E6%9C%AC%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="对方下落不明，在国家级或省级媒体刊登公告" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B9%E4%B8%8B%E8%90%BD%E4%B8%8D%E6%98%8E%EF%BC%8C%E5%9C%A8%E5%9B%BD%E5%AE%B6%E7%BA%A7%E6%88%96%E7%9C%81%E7%BA%A7%E5%AA%92%E4%BD%93%E5%88%8A%E7%99%BB%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="权利人提起诉讼、申请仲裁" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%E3%80%81%E7%94%B3%E8%AF%B7%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="与诉讼、仲裁同等效力" _mubu_text="%3Cspan%3E%E4%B8%8E%E8%AF%89%E8%AE%BC%E3%80%81%E4%BB%B2%E8%A3%81%E5%90%8C%E7%AD%89%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
                <outline text="为主张权利而申请义务人失踪或死亡" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%BA%E4%B8%BB%E5%BC%A0%E6%9D%83%E5%88%A9%E8%80%8C%E7%94%B3%E8%AF%B7%E4%B9%89%E5%8A%A1%E4%BA%BA%E5%A4%B1%E8%B8%AA%E6%88%96%E6%AD%BB%E4%BA%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在诉讼中主张抵消" _mubu_text="%3Cspan%3E%E5%9C%A8%E8%AF%89%E8%AE%BC%E4%B8%AD%E4%B8%BB%E5%BC%A0%E6%8A%B5%E6%B6%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="债务人认诺" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E8%AE%A4%E8%AF%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="分期履行" _mubu_text="%3Cspan%3E%E5%88%86%E6%9C%9F%E5%B1%A5%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="部分履行" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E5%B1%A5%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="提供担保" _mubu_text="%3Cspan%3E%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="请求延期履行" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E5%BB%B6%E6%9C%9F%E5%B1%A5%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="制定清偿债务计划" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%9A%E6%B8%85%E5%81%BF%E5%80%BA%E5%8A%A1%E8%AE%A1%E5%88%92%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法律后果：重新计算" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%90%8E%E6%9E%9C%EF%BC%9A%E9%87%8D%E6%96%B0%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="诉讼时效 VS 除斥期间" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%20VS%20%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="客体" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼时效：请求权（债权请求权）" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%88%E5%80%BA%E6%9D%83%E8%AF%B7%E6%B1%82%E6%9D%83%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="除斥期间：形成权：单方意思表示即可产生司法效果（撤销权、解除权）" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E5%BD%A2%E6%88%90%E6%9D%83%EF%BC%9A%E5%8D%95%E6%96%B9%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%E5%8D%B3%E5%8F%AF%E4%BA%A7%E7%94%9F%E5%8F%B8%E6%B3%95%E6%95%88%E6%9E%9C%EF%BC%88%E6%92%A4%E9%94%80%E6%9D%83%E3%80%81%E8%A7%A3%E9%99%A4%E6%9D%83%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼时效：永久性抗辩权" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A%E6%B0%B8%E4%B9%85%E6%80%A7%E6%8A%97%E8%BE%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="除斥期间：实体权利消灭" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E5%AE%9E%E4%BD%93%E6%9D%83%E5%88%A9%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="是否可变" _mubu_text="%3Cspan%3E%E6%98%AF%E5%90%A6%E5%8F%AF%E5%8F%98%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼时效：除最长诉讼时效，均可变" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A%E9%99%A4%E6%9C%80%E9%95%BF%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%8C%E5%9D%87%E5%8F%AF%E5%8F%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="除斥期间：不变期间" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E4%B8%8D%E5%8F%98%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="可否主动" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%A6%E4%B8%BB%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼时效：被动司法" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A%E8%A2%AB%E5%8A%A8%E5%8F%B8%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="除斥期间：可以审查" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E5%8F%AF%E4%BB%A5%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="起算" _mubu_text="%3Cspan%3E%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼时效：知道权利受到损害及义务人之日起算" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A%E7%9F%A5%E9%81%93%E6%9D%83%E5%88%A9%E5%8F%97%E5%88%B0%E6%8D%9F%E5%AE%B3%E5%8F%8A%E4%B9%89%E5%8A%A1%E4%BA%BA%E4%B9%8B%E6%97%A5%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="除斥期间：知道权利产生之日起算" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E7%9F%A5%E9%81%93%E6%9D%83%E5%88%A9%E4%BA%A7%E7%94%9F%E4%B9%8B%E6%97%A5%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="物权编" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%E7%BC%96%3C/span%3E" _note="" _mubu_note="" _heading="1">
      <outline text="物权的基本原理" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%E7%9A%84%E5%9F%BA%E6%9C%AC%E5%8E%9F%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="物的分类（客体）" _mubu_text="%3Cspan%3E%E7%89%A9%E7%9A%84%E5%88%86%E7%B1%BB%EF%BC%88%E5%AE%A2%E4%BD%93%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="不动产 VS 动产" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8A%A8%E4%BA%A7%20VS%20%E5%8A%A8%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="不动产：不能移动、移动会损害价值（土地、房屋）" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8A%A8%E4%BA%A7%EF%BC%9A%E4%B8%8D%E8%83%BD%E7%A7%BB%E5%8A%A8%E3%80%81%E7%A7%BB%E5%8A%A8%E4%BC%9A%E6%8D%9F%E5%AE%B3%E4%BB%B7%E5%80%BC%EF%BC%88%E5%9C%9F%E5%9C%B0%E3%80%81%E6%88%BF%E5%B1%8B%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="动产：能够移动、移动不损害价值（汽车、摄像机）" _mubu_text="%3Cspan%3E%E5%8A%A8%E4%BA%A7%EF%BC%9A%E8%83%BD%E5%A4%9F%E7%A7%BB%E5%8A%A8%E3%80%81%E7%A7%BB%E5%8A%A8%E4%B8%8D%E6%8D%9F%E5%AE%B3%E4%BB%B7%E5%80%BC%EF%BC%88%E6%B1%BD%E8%BD%A6%E3%80%81%E6%91%84%E5%83%8F%E6%9C%BA%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="原物 VS 孳息" _mubu_text="%3Cspan%3E%E5%8E%9F%E7%89%A9%20VS%20%E5%AD%B3%E6%81%AF%3C/span%3E" _note="" _mubu_note="">
            <outline text="原物：本体" _mubu_text="%3Cspan%3E%E5%8E%9F%E7%89%A9%EF%BC%9A%E6%9C%AC%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="孳息：原物产生的收益，出产并分离" _mubu_text="%3Cspan%3E%E5%AD%B3%E6%81%AF%EF%BC%9A%E5%8E%9F%E7%89%A9%E4%BA%A7%E7%94%9F%E7%9A%84%E6%94%B6%E7%9B%8A%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-yellow%22%3E%E5%87%BA%E4%BA%A7%3C/span%3E%3Cspan%3E%E5%B9%B6%3C/span%3E%3Cspan%20class=%22%20text-color-yellow%22%3E%E5%88%86%E7%A6%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="天然孳息" _mubu_text="%3Cspan%3E%E5%A4%A9%E7%84%B6%E5%AD%B3%E6%81%AF%3C/span%3E" _note="" _mubu_note="">
                <outline text="动物下的蛋" _mubu_text="%3Cspan%3E%E5%8A%A8%E7%89%A9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8B%3C/span%3E%3Cspan%3E%E7%9A%84%E8%9B%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="树上掉落的果实" _mubu_text="%3Cspan%3E%E6%A0%91%E4%B8%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8E%89%E8%90%BD%3C/span%3E%3Cspan%3E%E7%9A%84%E6%9E%9C%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法定孳息" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E5%AD%B3%E6%81%AF%3C/span%3E" _note="" _mubu_note="">
                <outline text="利息、租金" _mubu_text="%3Cspan%3E%E5%88%A9%E6%81%AF%E3%80%81%E7%A7%9F%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="射幸孳息（保险、彩票、捐款）" _mubu_text="%3Cspan%3E%E5%B0%84%E5%B9%B8%E5%AD%B3%E6%81%AF%EF%BC%88%E4%BF%9D%E9%99%A9%E3%80%81%E5%BD%A9%E7%A5%A8%E3%80%81%E6%8D%90%E6%AC%BE%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="主物vs从物" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%89%A9vs%E4%BB%8E%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="从物：非主物的构成成分；须辅助主物的使用；须与主物同属一人" _mubu_text="%3Cspan%3E%E4%BB%8E%E7%89%A9%EF%BC%9A%E9%9D%9E%E4%B8%BB%E7%89%A9%E7%9A%84%E6%9E%84%E6%88%90%E6%88%90%E5%88%86%EF%BC%9B%E9%A1%BB%E8%BE%85%E5%8A%A9%E4%B8%BB%E7%89%A9%E7%9A%84%E4%BD%BF%E7%94%A8%EF%BC%9B%E9%A1%BB%E4%B8%8E%E4%B8%BB%E7%89%A9%E5%90%8C%E5%B1%9E%E4%B8%80%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例，备胎+汽车、手机+充电器" _mubu_text="%3Cspan%3E%E4%BE%8B%EF%BC%8C%E5%A4%87%E8%83%8E+%E6%B1%BD%E8%BD%A6%E3%80%81%E6%89%8B%E6%9C%BA+%E5%85%85%E7%94%B5%E5%99%A8%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="货币" _mubu_text="%3Cspan%3E%E8%B4%A7%E5%B8%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则上占有即所有" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E5%8D%A0%E6%9C%89%E5%8D%B3%E6%89%80%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="数据和网络虚拟财产" _mubu_text="%3Cspan%3E%E6%95%B0%E6%8D%AE%E5%92%8C%E7%BD%91%E7%BB%9C%E8%99%9A%E6%8B%9F%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="可支配性、排他性" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%94%AF%E9%85%8D%E6%80%A7%E3%80%81%E6%8E%92%E4%BB%96%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="经济价值" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%B5%8E%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="效力" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="追及效力" _mubu_text="%3Cspan%3E%E8%BF%BD%E5%8F%8A%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="追及效力，是指物权成立后，不论其标的物辗转至何人之手，权利人均可以追及标的物之所在，并直接行使权利；只要在最后占有人处发现自己的物，即可子以取回。" _mubu_text="%3Cspan%3E%E8%BF%BD%E5%8F%8A%E6%95%88%E5%8A%9B%EF%BC%8C%E6%98%AF%E6%8C%87%E7%89%A9%E6%9D%83%E6%88%90%E7%AB%8B%E5%90%8E%EF%BC%8C%E4%B8%8D%E8%AE%BA%E5%85%B6%E6%A0%87%E7%9A%84%E7%89%A9%E8%BE%97%E8%BD%AC%E8%87%B3%E4%BD%95%E4%BA%BA%E4%B9%8B%E6%89%8B%EF%BC%8C%E6%9D%83%E5%88%A9%E4%BA%BA%E5%9D%87%E5%8F%AF%E4%BB%A5%E8%BF%BD%E5%8F%8A%E6%A0%87%E7%9A%84%E7%89%A9%E4%B9%8B%E6%89%80%E5%9C%A8%EF%BC%8C%E5%B9%B6%E7%9B%B4%E6%8E%A5%E8%A1%8C%E4%BD%BF%E6%9D%83%E5%88%A9%EF%BC%9B%E5%8F%AA%E8%A6%81%E5%9C%A8%E6%9C%80%E5%90%8E%E5%8D%A0%E6%9C%89%E4%BA%BA%E5%A4%84%E5%8F%91%E7%8E%B0%E8%87%AA%E5%B7%B1%E7%9A%84%E7%89%A9%EF%BC%8C%E5%8D%B3%E5%8F%AF%E5%AD%90%E4%BB%A5%E5%8F%96%E5%9B%9E%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外：善意取得" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E5%96%84%E6%84%8F%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="排他效力" _mubu_text="%3Cspan%3E%E6%8E%92%E4%BB%96%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="排他效力，是指同一物上不得同时存在两个所有权（即一物一权）或两个内容相冲突的他物权。" _mubu_text="%3Cspan%3E%E6%8E%92%E4%BB%96%E6%95%88%E5%8A%9B%EF%BC%8C%E6%98%AF%E6%8C%87%E5%90%8C%E4%B8%80%E7%89%A9%E4%B8%8A%E4%B8%8D%E5%BE%97%E5%90%8C%E6%97%B6%E5%AD%98%E5%9C%A8%E4%B8%A4%E4%B8%AA%E6%89%80%E6%9C%89%E6%9D%83%EF%BC%88%E5%8D%B3%E4%B8%80%E7%89%A9%E4%B8%80%E6%9D%83%EF%BC%89%E6%88%96%E4%B8%A4%E4%B8%AA%E5%86%85%E5%AE%B9%E7%9B%B8%E5%86%B2%E7%AA%81%E7%9A%84%E4%BB%96%E7%89%A9%E6%9D%83%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="优先效力" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BC%98%E5%85%88%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="优先效力，是指同一物之上存在数个互相冲突的权利时，效力较强的权利排斥效力较弱的权利而率先获得实现。" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E6%95%88%E5%8A%9B%EF%BC%8C%E6%98%AF%E6%8C%87%E5%90%8C%E4%B8%80%E7%89%A9%E4%B9%8B%E4%B8%8A%E5%AD%98%E5%9C%A8%E6%95%B0%E4%B8%AA%E4%BA%92%E7%9B%B8%E5%86%B2%E7%AA%81%E7%9A%84%E6%9D%83%E5%88%A9%E6%97%B6%EF%BC%8C%E6%95%88%E5%8A%9B%E8%BE%83%E5%BC%BA%E7%9A%84%E6%9D%83%E5%88%A9%E6%8E%92%E6%96%A5%E6%95%88%E5%8A%9B%E8%BE%83%E5%BC%B1%E7%9A%84%E6%9D%83%E5%88%A9%E8%80%8C%E7%8E%87%E5%85%88%E8%8E%B7%E5%BE%97%E5%AE%9E%E7%8E%B0%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对外效力" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：物权 &gt; 债权" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E7%89%A9%E6%9D%83%20&amp;gt;%20%E5%80%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="预告登记" _mubu_text="%3Cspan%3E%E9%A2%84%E5%91%8A%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="租赁权成立在先情形下的买卖不破租赁" _mubu_text="%3Cspan%3E%E7%A7%9F%E8%B5%81%E6%9D%83%E6%88%90%E7%AB%8B%E5%9C%A8%E5%85%88%E6%83%85%E5%BD%A2%E4%B8%8B%E7%9A%84%E4%B9%B0%E5%8D%96%E4%B8%8D%E7%A0%B4%E7%A7%9F%E8%B5%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="建设工程合同中的法定优先权" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E5%B7%A5%E7%A8%8B%E5%90%88%E5%90%8C%E4%B8%AD%E7%9A%84%E6%B3%95%E5%AE%9A%E4%BC%98%E5%85%88%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="对内效力" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%86%85%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="留置权 &gt; 登记抵押权 / 质押权 &gt; 未登记抵押权" _mubu_text="%3Cspan%3E%E7%95%99%E7%BD%AE%E6%9D%83%20&amp;gt;%20%E7%99%BB%E8%AE%B0%E6%8A%B5%E6%8A%BC%E6%9D%83%20/%20%E8%B4%A8%E6%8A%BC%E6%9D%83%20&amp;gt;%20%E6%9C%AA%E7%99%BB%E8%AE%B0%E6%8A%B5%E6%8A%BC%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="登记抵押权 VS 质押权：成立在先原则" _mubu_text="%3Cspan%3E%E7%99%BB%E8%AE%B0%E6%8A%B5%E6%8A%BC%E6%9D%83%20VS%20%E8%B4%A8%E6%8A%BC%E6%9D%83%EF%BC%9A%E6%88%90%E7%AB%8B%E5%9C%A8%E5%85%88%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
