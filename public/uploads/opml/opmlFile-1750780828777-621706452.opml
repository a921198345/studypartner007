            <outline text="第三人同时继承债权人和债务人" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%90%8C%E6%97%B6%E7%BB%A7%E6%89%BF%E5%80%BA%E6%9D%83%E4%BA%BA%E5%92%8C%E5%80%BA%E5%8A%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
