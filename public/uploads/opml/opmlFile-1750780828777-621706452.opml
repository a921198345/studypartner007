              <outline text="事实行为，对行为能力无限制" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%8B%E5%AE%9E%E8%A1%8C%E4%B8%BA%3C/span%3E%3Cspan%3E%EF%BC%8C%E5%AF%B9%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E6%97%A0%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="不违反本人明示或可推知的意思，但是本人真实意思违反法律或公序良俗的除外" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%BF%9D%E5%8F%8D%E6%9C%AC%E4%BA%BA%E6%98%8E%E7%A4%BA%E6%88%96%E5%8F%AF%E6%8E%A8%E7%9F%A5%E7%9A%84%E6%84%8F%E6%80%9D%EF%BC%8C%E4%BD%86%E6%98%AF%E6%9C%AC%E4%BA%BA%E7%9C%9F%E5%AE%9E%E6%84%8F%E6%80%9D%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E6%88%96%E5%85%AC%E5%BA%8F%E8%89%AF%E4%BF%97%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="权利" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="必要费用请求权" _mubu_text="%3Cspan%3E%E5%BF%85%E8%A6%81%E8%B4%B9%E7%94%A8%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="不享有报酬请求权" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%AB%E6%9C%89%E6%8A%A5%E9%85%AC%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="所付债务请求权" _mubu_text="%3Cspan%3E%E6%89%80%E4%BB%98%E5%80%BA%E5%8A%A1%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="所受损害请求权" _mubu_text="%3Cspan%3E%E6%89%80%E5%8F%97%E6%8D%9F%E5%AE%B3%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="可向受益人主张赔偿，受益人不得向管理人主张赔偿" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B8%BB%E5%BC%A0%E8%B5%94%E5%81%BF%EF%BC%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%90%91%E7%AE%A1%E7%90%86%E4%BA%BA%E4%B8%BB%E5%BC%A0%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="妥善管理" _mubu_text="%3Cspan%3E%E5%A6%A5%E5%96%84%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="期间限制：诉讼时效" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E9%99%90%E5%88%B6%EF%BC%9A%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="从无因管理行为结束并且管理人知道或应当知道本人之日起算" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BB%8E%E6%97%A0%E5%9B%A0%E7%AE%A1%E7%90%86%E8%A1%8C%E4%B8%BA%E7%BB%93%E6%9D%9F%E5%B9%B6%E4%B8%94%E7%AE%A1%E7%90%86%E4%BA%BA%E7%9F%A5%E9%81%93%E6%88%96%E5%BA%94%E5%BD%93%E7%9F%A5%E9%81%93%E6%9C%AC%E4%BA%BA%E4%B9%8B%E6%97%A5%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="不当得利" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="【体系框架】" _mubu_text="%3Cspan%3E%E3%80%90%E4%BD%93%E7%B3%BB%E6%A1%86%E6%9E%B6%E3%80%91%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22is7LaGIGK7%22%2C%22oh%22%3A669%2C%22ow%22%3A1601%2C%22uri%22%3A%22document_image%2FD2F1157E41004B761720268249.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="构成要件" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%E8%A6%81%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="没有法定或约定原因" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%E6%B3%95%E5%AE%9A%E6%88%96%E7%BA%A6%E5%AE%9A%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="自然债务不构成不当得利" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E5%80%BA%E5%8A%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%9E%84%E6%88%90%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="履行道德义务的给付不构成不当得利" _mubu_text="%3Cspan%3E%E5%B1%A5%E8%A1%8C%E9%81%93%E5%BE%B7%E4%B9%89%E5%8A%A1%E7%9A%84%E7%BB%99%E4%BB%98%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%9E%84%E6%88%90%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="一方获益" _mubu_text="%3Cspan%3E%E4%B8%80%E6%96%B9%E8%8E%B7%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="反射利益不构成不当得利（自然增值）" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%B0%84%E5%88%A9%E7%9B%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%9E%84%E6%88%90%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%EF%BC%88%E8%87%AA%E7%84%B6%E5%A2%9E%E5%80%BC%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="一方受损" _mubu_text="%3Cspan%3E%E4%B8%80%E6%96%B9%E5%8F%97%E6%8D%9F%3C/span%3E" _note="" _mubu_note="">
              <outline text="主动放弃利益不构成不当得利" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E6%94%BE%E5%BC%83%E5%88%A9%E7%9B%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%9E%84%E6%88%90%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="故意放弃利益不构成不当得利（强迫得利）" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F%E6%94%BE%E5%BC%83%E5%88%A9%E7%9B%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%9E%84%E6%88%90%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%E5%BE%97%E5%88%A9%EF%BC%88%E5%BC%BA%E8%BF%AB%E5%BE%97%E5%88%A9%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="二者有因果关系" _mubu_text="%3Cspan%3E%E4%BA%8C%E8%80%85%E6%9C%89%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="返还原物及孳息" _mubu_text="%3Cspan%3E%E8%BF%94%E8%BF%98%E5%8E%9F%E7%89%A9%E5%8F%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%AD%B3%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="善意得利人：返还“现存利益”，不存在的无须返还" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%BE%97%E5%88%A9%E4%BA%BA%EF%BC%9A%E8%BF%94%E8%BF%98%E2%80%9C%E7%8E%B0%E5%AD%98%E5%88%A9%E7%9B%8A%E2%80%9D%EF%BC%8C%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%9A%84%E6%97%A0%E9%A1%BB%E8%BF%94%E8%BF%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="恶意得利人：返还所受利益" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E5%BE%97%E5%88%A9%E4%BA%BA%EF%BC%9A%E8%BF%94%E8%BF%98%E6%89%80%E5%8F%97%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="债的学理分类体系" _mubu_text="%3Cspan%3E%E5%80%BA%E7%9A%84%E5%AD%A6%E7%90%86%E5%88%86%E7%B1%BB%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="【体系框架】" _mubu_text="%3Cspan%3E%E3%80%90%E4%BD%93%E7%B3%BB%E6%A1%86%E6%9E%B6%E3%80%91%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22ZQ9fOL27ZQ%22%2C%22oh%22%3A1000%2C%22ow%22%3A1136%2C%22uri%22%3A%22document_image%2FD022781E42B54DDD1720269414.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
        <outline text="主体（单一和多数人之债）" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%88%E5%8D%95%E4%B8%80%E5%92%8C%E5%A4%9A%E6%95%B0%E4%BA%BA%E4%B9%8B%E5%80%BA%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="单一之债：债权人、债务人均为一人" _mubu_text="%3Cspan%3E%E5%8D%95%E4%B8%80%E4%B9%8B%E5%80%BA%EF%BC%9A%E5%80%BA%E6%9D%83%E4%BA%BA%E3%80%81%E5%80%BA%E5%8A%A1%E4%BA%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9D%87%E4%B8%BA%3C/span%3E%3Cspan%3E%E4%B8%80%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="多数人之债" _mubu_text="%3Cspan%3E%E5%A4%9A%E6%95%B0%E4%BA%BA%E4%B9%8B%E5%80%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="按份之债：对外按份，有约从约，无内部追偿问题" _mubu_text="%3Cspan%3E%E6%8C%89%E4%BB%BD%E4%B9%8B%E5%80%BA%EF%BC%9A%E5%AF%B9%E5%A4%96%E6%8C%89%E4%BB%BD%EF%BC%8C%E6%9C%89%E7%BA%A6%E4%BB%8E%E7%BA%A6%EF%BC%8C%E6%97%A0%E5%86%85%E9%83%A8%E8%BF%BD%E5%81%BF%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="连带之债：对外连带，对内按份" _mubu_text="%3Cspan%3E%E8%BF%9E%E5%B8%A6%E4%B9%8B%E5%80%BA%EF%BC%9A%E5%AF%B9%E5%A4%96%E8%BF%9E%E5%B8%A6%EF%BC%8C%E5%AF%B9%E5%86%85%E6%8C%89%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="部分连带债务人的债务被债权人免除的，在应当承担的份额范围内，其他债务人对债权人的债务消灭" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E8%BF%9E%E5%B8%A6%E5%80%BA%E5%8A%A1%E4%BA%BA%E7%9A%84%E5%80%BA%E5%8A%A1%E8%A2%AB%E5%80%BA%E6%9D%83%E4%BA%BA%E5%85%8D%E9%99%A4%E7%9A%84%EF%BC%8C%E5%9C%A8%E5%BA%94%E5%BD%93%E6%89%BF%E6%8B%85%E7%9A%84%E4%BB%BD%E9%A2%9D%E8%8C%83%E5%9B%B4%E5%86%85%EF%BC%8C%E5%85%B6%E4%BB%96%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%AF%B9%E5%80%BA%E6%9D%83%E4%BA%BA%E7%9A%84%E5%80%BA%E5%8A%A1%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="标的物（财物、劳务和货币之债）" _mubu_text="%3Cspan%3E%E6%A0%87%E7%9A%84%E7%89%A9%EF%BC%88%E8%B4%A2%E7%89%A9%E3%80%81%E5%8A%B3%E5%8A%A1%E5%92%8C%E8%B4%A7%E5%B8%81%E4%B9%8B%E5%80%BA%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="财物之债" _mubu_text="%3Cspan%3E%E8%B4%A2%E7%89%A9%E4%B9%8B%E5%80%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="特定之债：无可替代性，交付前灭失，可免除继续履行的义务（自己的）" _mubu_text="%3Cspan%3E%E7%89%B9%E5%AE%9A%E4%B9%8B%E5%80%BA%EF%BC%9A%E6%97%A0%E5%8F%AF%E6%9B%BF%E4%BB%A3%E6%80%A7%EF%BC%8C%E4%BA%A4%E4%BB%98%E5%89%8D%E7%81%AD%E5%A4%B1%EF%BC%8C%E5%8F%AF%E5%85%8D%E9%99%A4%E7%BB%A7%E7%BB%AD%E5%B1%A5%E8%A1%8C%E7%9A%84%E4%B9%89%E5%8A%A1%EF%BC%88%E8%87%AA%E5%B7%B1%E7%9A%84%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="种类之债：有可替代性，交付前灭失，不免除继续履行的义务" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%E4%B9%8B%E5%80%BA%EF%BC%9A%E6%9C%89%E5%8F%AF%E6%9B%BF%E4%BB%A3%E6%80%A7%EF%BC%8C%E4%BA%A4%E4%BB%98%E5%89%8D%E7%81%AD%E5%A4%B1%EF%BC%8C%E4%B8%8D%E5%85%8D%E9%99%A4%E7%BB%A7%E7%BB%AD%E5%B1%A5%E8%A1%8C%E7%9A%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="劳务之债：不得强制执行" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E4%B9%8B%E5%80%BA%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%BE%97%3C/span%3E%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="货币之债：不存在履行不能的问题，只有货币之债之间才能法定抵销" _mubu_text="%3Cspan%3E%E8%B4%A7%E5%B8%81%E4%B9%8B%E5%80%BA%EF%BC%9A%E4%B8%8D%E5%AD%98%E5%9C%A8%E5%B1%A5%E8%A1%8C%E4%B8%8D%E8%83%BD%E7%9A%84%E9%97%AE%E9%A2%98%EF%BC%8C%E5%8F%AA%E6%9C%89%E8%B4%A7%E5%B8%81%E4%B9%8B%E5%80%BA%E4%B9%8B%E9%97%B4%E6%89%8D%E8%83%BD%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B3%95%E5%AE%9A%E6%8A%B5%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="标的物可选择性（简单之债和选择之债）" _mubu_text="%3Cspan%3E%E6%A0%87%E7%9A%84%E7%89%A9%E5%8F%AF%E9%80%89%E6%8B%A9%E6%80%A7%EF%BC%88%E7%AE%80%E5%8D%95%E4%B9%8B%E5%80%BA%E5%92%8C%E9%80%89%E6%8B%A9%E4%B9%8B%E5%80%BA%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="简单之债" _mubu_text="%3Cspan%3E%E7%AE%80%E5%8D%95%E4%B9%8B%E5%80%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="选择之债" _mubu_text="%3Cspan%3E%E9%80%89%E6%8B%A9%E4%B9%8B%E5%80%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="选择权：先债务人后债权人" _mubu_text="%3Cspan%3E%E9%80%89%E6%8B%A9%E6%9D%83%EF%BC%9A%E5%85%88%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%90%8E%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="【技术流】判断债的种类" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E6%8A%80%E6%9C%AF%E6%B5%81%E3%80%91%E5%88%A4%E6%96%AD%E5%80%BA%E7%9A%84%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="第一步：找人数——单一之债或多数人之债（按份或连带）；" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%80%E6%AD%A5%EF%BC%9A%E6%89%BE%E4%BA%BA%E6%95%B0%E2%80%94%E2%80%94%E5%8D%95%E4%B8%80%E4%B9%8B%E5%80%BA%E6%88%96%E5%A4%9A%E6%95%B0%E4%BA%BA%E4%B9%8B%E5%80%BA%EF%BC%88%E6%8C%89%E4%BB%BD%E6%88%96%E8%BF%9E%E5%B8%A6%EF%BC%89%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="第二步：找标的——财物之债（特定或种类）、劳务之债或货币之债；" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%BA%8C%E6%AD%A5%EF%BC%9A%E6%89%BE%E6%A0%87%E7%9A%84%E2%80%94%E2%80%94%E8%B4%A2%E7%89%A9%E4%B9%8B%E5%80%BA%EF%BC%88%E7%89%B9%E5%AE%9A%E6%88%96%E7%A7%8D%E7%B1%BB%EF%BC%89%E3%80%81%E5%8A%B3%E5%8A%A1%E4%B9%8B%E5%80%BA%E6%88%96%E8%B4%A7%E5%B8%81%E4%B9%8B%E5%80%BA%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="第三步：找原因——意定之债、法定之债或自然之债；" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E6%AD%A5%EF%BC%9A%E6%89%BE%E5%8E%9F%E5%9B%A0%E2%80%94%E2%80%94%E6%84%8F%E5%AE%9A%E4%B9%8B%E5%80%BA%E3%80%81%E6%B3%95%E5%AE%9A%E4%B9%8B%E5%80%BA%E6%88%96%E8%87%AA%E7%84%B6%E4%B9%8B%E5%80%BA%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="第四步．找标的可选否——简单之债或选择之债。" _mubu_text="%3Cspan%3E%E7%AC%AC%E5%9B%9B%E6%AD%A5%EF%BC%8E%E6%89%BE%E6%A0%87%E7%9A%84%E5%8F%AF%E9%80%89%E5%90%A6%E2%80%94%E2%80%94%E7%AE%80%E5%8D%95%E4%B9%8B%E5%80%BA%E6%88%96%E9%80%89%E6%8B%A9%E4%B9%8B%E5%80%BA%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
    </outline>
    <outline text="合同编通则" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E7%BC%96%E9%80%9A%E5%88%99%3C/span%3E" _note="" _mubu_note="" _heading="1">
      <outline text="合同的基本原理" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E7%9A%84%E5%9F%BA%E6%9C%AC%E5%8E%9F%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22Mr6quj6Gaq%22%2C%22oh%22%3A462%2C%22ow%22%3A1600%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2FFA0F460B34C54F851720341947.jpg%22%2C%22w%22%3A400%7D%5D"/>
          <outline text="限于「财产合同」" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E3%80%8C%E8%B4%A2%E4%BA%A7%E5%90%88%E5%90%8C%E3%80%8D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不含「身份协议」" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%AB%E3%80%8C%E8%BA%AB%E4%BB%BD%E5%8D%8F%E8%AE%AE%E3%80%8D%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="学理分类" _mubu_text="%3Cspan%3E%E5%AD%A6%E7%90%86%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="单务合同 VS 双务合同" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E5%8D%95%E5%8A%A1%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%20VS%20%E5%8F%8C%E5%8A%A1%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="单务合同：无对待给付义务（赠与合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8D%95%E5%8A%A1%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%97%A0%E5%AF%B9%E5%BE%85%E7%BB%99%E4%BB%98%E4%B9%89%E5%8A%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%EF%BC%88%E8%B5%A0%E4%B8%8E%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="双务合同：有对待给付义务（买卖合同）" _mubu_text="%3Cspan%3E%E5%8F%8C%E5%8A%A1%E5%90%88%E5%90%8C%EF%BC%9A%E6%9C%89%E5%AF%B9%E5%BE%85%E7%BB%99%E4%BB%98%E4%B9%89%E5%8A%A1%EF%BC%88%E4%B9%B0%E5%8D%96%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="有名合同 VS 无名合同" _mubu_text="%3Cspan%3E%E6%9C%89%E5%90%8D%E5%90%88%E5%90%8C%20VS%20%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E6%97%A0%E5%90%8D%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="有名合同：法律有规定（买水赠款租，承建融技输，纪委管居储）" _mubu_text="%3Cspan%3E%E6%9C%89%E5%90%8D%E5%90%88%E5%90%8C%EF%BC%9A%E6%B3%95%E5%BE%8B%E6%9C%89%E8%A7%84%E5%AE%9A%EF%BC%88%E4%B9%B0%E6%B0%B4%E8%B5%A0%E6%AC%BE%E7%A7%9F%EF%BC%8C%E6%89%BF%E5%BB%BA%E8%9E%8D%E6%8A%80%E8%BE%93%EF%BC%8C%E7%BA%AA%E5%A7%94%E7%AE%A1%E5%B1%85%E5%82%A8%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无名合同：法律未规定，类推适用（借用合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%97%A0%E5%90%8D%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%B3%95%E5%BE%8B%E6%9C%AA%E8%A7%84%E5%AE%9A%EF%BC%8C%E7%B1%BB%E6%8E%A8%E9%80%82%E7%94%A8%EF%BC%88%E5%80%9F%E7%94%A8%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="有偿合同 VS 无偿合同" _mubu_text="%3Cspan%3E%E6%9C%89%E5%81%BF%E5%90%88%E5%90%8C%20VS%20%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E6%97%A0%E5%81%BF%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="有偿合同：有对价（买卖合同）" _mubu_text="%3Cspan%3E%E6%9C%89%E5%81%BF%E5%90%88%E5%90%8C%EF%BC%9A%E6%9C%89%E5%AF%B9%E4%BB%B7%EF%BC%88%E4%B9%B0%E5%8D%96%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无偿合同：无对价（赠与合同、借用合同、保证合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%97%A0%E5%81%BF%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%97%A0%E5%AF%B9%E4%BB%B7%EF%BC%88%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%B5%A0%E4%B8%8E%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%E3%80%81%E5%80%9F%E7%94%A8%E5%90%88%E5%90%8C%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BF%9D%E8%AF%81%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可有偿可无偿（委托合同、保管合同、自然人之间借贷合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E6%9C%89%E5%81%BF%E5%8F%AF%E6%97%A0%E5%81%BF%3C/span%3E%3Cspan%3E%EF%BC%88%E5%A7%94%E6%89%98%E5%90%88%E5%90%8C%E3%80%81%E4%BF%9D%E7%AE%A1%E5%90%88%E5%90%8C%E3%80%81%E8%87%AA%E7%84%B6%E4%BA%BA%E4%B9%8B%E9%97%B4%E5%80%9F%E8%B4%B7%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="委托合同无特别约定推定为有偿" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E5%90%88%E5%90%8C%E6%97%A0%E7%89%B9%E5%88%AB%E7%BA%A6%E5%AE%9A%E6%8E%A8%E5%AE%9A%E4%B8%BA%E6%9C%89%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="诺成性合同 VS 实践性合同" _mubu_text="%3Cspan%3E%E8%AF%BA%E6%88%90%E6%80%A7%E5%90%88%E5%90%8C%20VS%20%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E5%AE%9E%E8%B7%B5%E6%80%A7%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="诺成性合同：意思表示一致" _mubu_text="%3Cspan%3E%E8%AF%BA%E6%88%90%E6%80%A7%E5%90%88%E5%90%8C%EF%BC%9A%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="实践性合同：交付标的物" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B7%B5%E6%80%A7%E5%90%88%E5%90%8C%EF%BC%9A%E4%BA%A4%E4%BB%98%E6%A0%87%E7%9A%84%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="与钱有关：定金合同、自然人之间借贷合同" _mubu_text="%3Cspan%3E%E4%B8%8E%E9%92%B1%E6%9C%89%E5%85%B3%EF%BC%9A%E5%AE%9A%E9%87%91%E5%90%88%E5%90%8C%E3%80%81%E8%87%AA%E7%84%B6%E4%BA%BA%E4%B9%8B%E9%97%B4%E5%80%9F%E8%B4%B7%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="与物有关：保管合同、借用合同" _mubu_text="%3Cspan%3E%E4%B8%8E%E7%89%A9%E6%9C%89%E5%85%B3%EF%BC%9A%E4%BF%9D%E7%AE%A1%E5%90%88%E5%90%8C%E3%80%81%E5%80%9F%E7%94%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="要式合同 VS 不要式合同" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E8%A6%81%E5%BC%8F%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%20VS%20%E4%B8%8D%E8%A6%81%E5%BC%8F%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="要式合同：法律要求具备一定形式（担保合同、保证合同、定金合同）" _mubu_text="%3Cspan%3E%E8%A6%81%E5%BC%8F%E5%90%88%E5%90%8C%EF%BC%9A%E6%B3%95%E5%BE%8B%E8%A6%81%E6%B1%82%E5%85%B7%E5%A4%87%E4%B8%80%E5%AE%9A%E5%BD%A2%E5%BC%8F%EF%BC%88%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E3%80%81%E4%BF%9D%E8%AF%81%E5%90%88%E5%90%8C%E3%80%81%E5%AE%9A%E9%87%91%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不要式合同：法律不要求具备一定形式（买卖合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%A6%81%E5%BC%8F%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%B3%95%E5%BE%8B%E4%B8%8D%E8%A6%81%E6%B1%82%E5%85%B7%E5%A4%87%E4%B8%80%E5%AE%9A%E5%BD%A2%E5%BC%8F%3C/span%3E%3Cspan%20class=%22bold%22%3E%EF%BC%88%E4%B9%B0%E5%8D%96%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="主合同 VS 从合同" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%90%88%E5%90%8C%20VS%20%E4%BB%8E%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="主合同（借款合同）" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%90%88%E5%90%8C%EF%BC%88%E5%80%9F%E6%AC%BE%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="从合同（担保合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BB%8E%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%88%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="本约 VS 预约" _mubu_text="%3Cspan%3E%E6%9C%AC%E7%BA%A6%20VS%20%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E9%A2%84%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="本约：履行预约" _mubu_text="%3Cspan%3E%E6%9C%AC%E7%BA%A6%EF%BC%9A%E5%B1%A5%E8%A1%8C%E9%A2%84%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="预约：约定将来" _mubu_text="%3Cspan%20class=%22bold%20text-color-purple%22%3E%E9%A2%84%E7%BA%A6%EF%BC%9A%E7%BA%A6%E5%AE%9A%E5%B0%86%E6%9D%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="违反预约构成违约，但不得强制履行本约" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E9%A2%84%E7%BA%A6%E6%9E%84%E6%88%90%E8%BF%9D%E7%BA%A6%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E4%BD%86%E4%B8%8D%E5%BE%97%E5%BC%BA%E5%88%B6%E5%B1%A5%E8%A1%8C%E6%9C%AC%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="预约合同的认定" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E9%A2%84%E7%BA%A6%E5%90%88%E5%90%8C%E7%9A%84%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="具有在将来一定期限内订立合同的意思表示" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E5%9C%A8%3C/span%3E%3Cspan%20class=%22underline%22%3E%E5%B0%86%E6%9D%A5%E4%B8%80%E5%AE%9A%E6%9C%9F%E9%99%90%E5%86%85%3C/span%3E%3Cspan%3E%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%E7%9A%84%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="能够确定将来所要订立合同的主体、标的物" _mubu_text="%3Cspan%3E%E8%83%BD%E5%A4%9F%E7%A1%AE%E5%AE%9A%E5%B0%86%E6%9D%A5%E6%89%80%E8%A6%81%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%E7%9A%84%3C/span%3E%3Cspan%20class=%22underline%22%3E%E4%B8%BB%E4%BD%93%E3%80%81%E6%A0%87%E7%9A%84%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="违反预约合同的认定" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E8%BF%9D%E5%8F%8D%E9%A2%84%E7%BA%A6%E5%90%88%E5%90%8C%E7%9A%84%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="(1）磋商说，即预约合同生效后，当事人双方负有磋商签订本约的义务。" _mubu_text="%3Cspan%3E(1%EF%BC%89%E7%A3%8B%E5%95%86%E8%AF%B4%EF%BC%8C%E5%8D%B3%E9%A2%84%E7%BA%A6%E5%90%88%E5%90%8C%E7%94%9F%E6%95%88%E5%90%8E%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%8C%E6%96%B9%E8%B4%9F%E6%9C%89%E7%A3%8B%E5%95%86%E7%AD%BE%E8%AE%A2%E6%9C%AC%E7%BA%A6%E7%9A%84%E4%B9%89%E5%8A%A1%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="(2） 必须缔约说，即预约合同生效后，无论如何，双方当事人都必须签订（达成）本约。" _mubu_text="%3Cspan%3E(2%EF%BC%89%20%E5%BF%85%E9%A1%BB%E7%BC%94%E7%BA%A6%E8%AF%B4%EF%BC%8C%E5%8D%B3%E9%A2%84%E7%BA%A6%E5%90%88%E5%90%8C%E7%94%9F%E6%95%88%E5%90%8E%EF%BC%8C%E6%97%A0%E8%AE%BA%E5%A6%82%E4%BD%95%EF%BC%8C%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%83%BD%E5%BF%85%E9%A1%BB%E7%AD%BE%E8%AE%A2%EF%BC%88%E8%BE%BE%E6%88%90%EF%BC%89%E6%9C%AC%E7%BA%A6%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="本条采“磋商说”，即以诚信原则产生的磋商义务为基础来认定违反预约的行为，是恰当的" _mubu_text="%3Cspan%3E%E6%9C%AC%E6%9D%A1%E9%87%87%E2%80%9C%E7%A3%8B%E5%95%86%E8%AF%B4%E2%80%9D%EF%BC%8C%E5%8D%B3%3C/span%3E%3Cspan%20class=%22underline%22%3E%E4%BB%A5%E8%AF%9A%E4%BF%A1%E5%8E%9F%E5%88%99%E4%BA%A7%E7%94%9F%E7%9A%84%E7%A3%8B%E5%95%86%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%9F%BA%E7%A1%80%3C/span%3E%3Cspan%3E%E6%9D%A5%E8%AE%A4%E5%AE%9A%E8%BF%9D%E5%8F%8D%E9%A2%84%E7%BA%A6%E7%9A%84%E8%A1%8C%E4%B8%BA%EF%BC%8C%E6%98%AF%E6%81%B0%E5%BD%93%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="违反预约合同的违约责任" _mubu_text="%3Cspan%20class=%22%20text-color-purple%22%3E%E8%BF%9D%E5%8F%8D%E9%A2%84%E7%BA%A6%E5%90%88%E5%90%8C%E7%9A%84%E8%BF%9D%E7%BA%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="折中说：综合考虑合同成熟度在“信赖利益”和“履行利益”之间进行“弹性酌定”" _mubu_text="%3Cspan%3E%E6%8A%98%E4%B8%AD%E8%AF%B4%EF%BC%9A%E7%BB%BC%E5%90%88%E8%80%83%E8%99%91%3C/span%3E%3Cspan%20class=%22underline%22%3E%E5%90%88%E5%90%8C%E6%88%90%E7%86%9F%E5%BA%A6%3C/span%3E%3Cspan%3E%E5%9C%A8%E2%80%9C%E4%BF%A1%E8%B5%96%E5%88%A9%E7%9B%8A%E2%80%9D%E5%92%8C%E2%80%9C%E5%B1%A5%E8%A1%8C%E5%88%A9%E7%9B%8A%E2%80%9D%E4%B9%8B%E9%97%B4%E8%BF%9B%E8%A1%8C%3C/span%3E%3Cspan%20class=%22underline%22%3E%E2%80%9C%E5%BC%B9%E6%80%A7%E9%85%8C%E5%AE%9A%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="束己合同 VS 涉他合同" _mubu_text="%3Cspan%3E%E6%9D%9F%E5%B7%B1%E5%90%88%E5%90%8C%20VS%20%E6%B6%89%E4%BB%96%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="束己合同：仅限当事人" _mubu_text="%3Cspan%3E%E6%9D%9F%E5%B7%B1%E5%90%88%E5%90%8C%EF%BC%9A%E4%BB%85%E9%99%90%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="涉他合同：涉及第三人" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%B6%89%E4%BB%96%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%B6%89%E5%8F%8A%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="射幸合同 VS 确定合同" _mubu_text="%3Cspan%3E%E5%B0%84%E5%B9%B8%E5%90%88%E5%90%8C%20VS%20%E7%A1%AE%E5%AE%9A%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="射幸合同：发生与否不确定（保险合同）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B0%84%E5%B9%B8%E5%90%88%E5%90%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%E5%8F%91%E7%94%9F%E4%B8%8E%E5%90%A6%E4%B8%8D%E7%A1%AE%E5%AE%9A%EF%BC%88%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="确定合同：发生与否确定" _mubu_text="%3Cspan%3E%E7%A1%AE%E5%AE%9A%E5%90%88%E5%90%8C%EF%BC%9A%E5%8F%91%E7%94%9F%E4%B8%8E%E5%90%A6%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="一时性合同 VS 继续性合同" _mubu_text="%3Cspan%3E%E4%B8%80%E6%97%B6%E6%80%A7%E5%90%88%E5%90%8C%20VS%20%E7%BB%A7%E7%BB%AD%E6%80%A7%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="一时性合同（买卖合同）" _mubu_text="%3Cspan%3E%E4%B8%80%E6%97%B6%E6%80%A7%E5%90%88%E5%90%8C%EF%BC%88%E4%B9%B0%E5%8D%96%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="继续性合同（供水电气热力合同）" _mubu_text="%3Cspan%3E%E7%BB%A7%E7%BB%AD%E6%80%A7%E5%90%88%E5%90%8C%EF%BC%88%E4%BE%9B%E6%B0%B4%E7%94%B5%E6%B0%94%E7%83%AD%E5%8A%9B%E5%90%88%E5%90%8C%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="缔约过失责任" _mubu_text="%3Cspan%3E%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="" _heading="2">
