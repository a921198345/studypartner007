        <outline text="民事法律行为的效力" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E6%B3%95%E5%BE%8B%E8%A1%8C%E4%B8%BA%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="有效" _mubu_text="%3Cspan%3E%E6%9C%89%E6%95%88%3C/span%3E" _note="" _mubu_note="">
