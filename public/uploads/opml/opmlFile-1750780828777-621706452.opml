        <outline text="用工责任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%94%A8%E5%B7%A5%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="单位用工" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E7%94%A8%E5%B7%A5%3C/span%3E" _note="" _mubu_note="">
