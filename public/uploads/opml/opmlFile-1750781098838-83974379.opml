              </outline>
            </outline>
          </outline>
          <outline text="基本养老金" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%85%BB%E8%80%81%E9%87%91%3C/span%3E" _note="" _mubu_note="">
            <outline text="由统筹养老金和个人账户养老金组成" _mubu_text="%3Cspan%3E%E7%94%B1%E7%BB%9F%E7%AD%B9%E5%85%BB%E8%80%81%E9%87%91%E5%92%8C%E4%B8%AA%E4%BA%BA%E8%B4%A6%E6%88%B7%E5%85%BB%E8%80%81%E9%87%91%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领取条件" _mubu_text="%3Cspan%3E%E9%A2%86%E5%8F%96%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="法定退休年龄＋累计15年" _mubu_note="%3Cspan%3E%E6%B3%95%E5%AE%9A%E9%80%80%E4%BC%91%E5%B9%B4%E9%BE%84%EF%BC%8B%E7%B4%AF%E8%AE%A115%E5%B9%B4%3C/span%3E">
              <outline text="达到法定退休年龄时" _mubu_text="%3Cspan%3E%E8%BE%BE%E5%88%B0%E6%B3%95%E5%AE%9A%E9%80%80%E4%BC%91%E5%B9%B4%E9%BE%84%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="累计缴费满15年的" _mubu_text="%3Cspan%3E%E7%B4%AF%E8%AE%A1%E7%BC%B4%E8%B4%B9%E6%BB%A115%E5%B9%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="按月领取基本养老金" _mubu_text="%3Cspan%3E%E6%8C%89%E6%9C%88%E9%A2%86%E5%8F%96%E5%9F%BA%E6%9C%AC%E5%85%BB%E8%80%81%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="累计缴费不足15年的" _mubu_text="%3Cspan%3E%E7%B4%AF%E8%AE%A1%E7%BC%B4%E8%B4%B9%E4%B8%8D%E8%B6%B315%E5%B9%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="可以缴费至满15年，按月领取基本养老金" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E7%BC%B4%E8%B4%B9%E8%87%B3%E6%BB%A115%E5%B9%B4%EF%BC%8C%E6%8C%89%E6%9C%88%E9%A2%86%E5%8F%96%E5%9F%BA%E6%9C%AC%E5%85%BB%E8%80%81%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="也可转入新型农村社会养老保险或城镇居民社会养老保险" _mubu_text="%3Cspan%3E%E4%B9%9F%E5%8F%AF%E8%BD%AC%E5%85%A5%E6%96%B0%E5%9E%8B%E5%86%9C%E6%9D%91%E7%A4%BE%E4%BC%9A%E5%85%BB%E8%80%81%E4%BF%9D%E9%99%A9%E6%88%96%E5%9F%8E%E9%95%87%E5%B1%85%E6%B0%91%E7%A4%BE%E4%BC%9A%E5%85%BB%E8%80%81%E4%BF%9D%E9%99%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="统筹地区就业" _mubu_text="%3Cspan%3E%E7%BB%9F%E7%AD%B9%E5%9C%B0%E5%8C%BA%E5%B0%B1%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="基本养老保险关系随本人转移，缴费年限累计计算" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%85%BB%E8%80%81%E4%BF%9D%E9%99%A9%E5%85%B3%E7%B3%BB%E9%9A%8F%E6%9C%AC%E4%BA%BA%E8%BD%AC%E7%A7%BB%EF%BC%8C%E7%BC%B4%E8%B4%B9%E5%B9%B4%E9%99%90%E7%B4%AF%E8%AE%A1%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="基本医疗保险" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8C%BB%E7%96%97%E4%BF%9D%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
          <outline text="职工基本医疗保险的缴纳" _mubu_text="%3Cspan%3E%E8%81%8C%E5%B7%A5%E5%9F%BA%E6%9C%AC%E5%8C%BB%E7%96%97%E4%BF%9D%E9%99%A9%E7%9A%84%E7%BC%B4%E7%BA%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="由用人单位和职工个人共同缴纳" _mubu_text="%3Cspan%3E%E7%94%B1%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%92%8C%E8%81%8C%E5%B7%A5%E4%B8%AA%E4%BA%BA%E5%85%B1%E5%90%8C%E7%BC%B4%E7%BA%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="单位缴费计入" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E7%BC%B4%E8%B4%B9%E8%AE%A1%E5%85%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="一部分建立统筹基金，另一部分划入个人账户" _mubu_text="%3Cspan%3E%E4%B8%80%E9%83%A8%E5%88%86%E5%BB%BA%E7%AB%8B%E7%BB%9F%E7%AD%B9%E5%9F%BA%E9%87%91%EF%BC%8C%E5%8F%A6%E4%B8%80%E9%83%A8%E5%88%86%E5%88%92%E5%85%A5%E4%B8%AA%E4%BA%BA%E8%B4%A6%E6%88%B7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="享受条件" _mubu_text="%3Cspan%3E%E4%BA%AB%E5%8F%97%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
