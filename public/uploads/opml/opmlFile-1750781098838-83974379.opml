            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="票据债务人对未履行直接债务的持票人抗辩" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%AF%B9%E6%9C%AA%E5%B1%A5%E8%A1%8C%E7%9B%B4%E6%8E%A5%E5%80%BA%E5%8A%A1%E7%9A%84%E6%8C%81%E7%A5%A8%E4%BA%BA%E6%8A%97%E8%BE%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="持票人明知票据债务人对其前手有抗辩事由" _mubu_text="%3Cspan%3E%E6%8C%81%E7%A5%A8%E4%BA%BA%E6%98%8E%E7%9F%A5%E7%A5%A8%E6%8D%AE%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%AF%B9%E5%85%B6%E5%89%8D%E6%89%8B%E6%9C%89%E6%8A%97%E8%BE%A9%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
