          <outline text="和解协议的通过" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E7%9A%84%E9%80%9A%E8%BF%87%3C/span%3E" _note="1/2+2/3" _mubu_note="%3Cspan%3E1/2+2/3%3C/span%3E">
            <outline text="＞出席有表决权的债权人的1/2" _mubu_text="%3Cspan%3E%EF%BC%9E%E5%87%BA%E5%B8%AD%E6%9C%89%E8%A1%A8%E5%86%B3%E6%9D%83%E7%9A%84%E5%80%BA%E6%9D%83%E4%BA%BA%E7%9A%841/2%3C/span%3E" _note="" _mubu_note=""/>
