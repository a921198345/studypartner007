                <outline text="由全体股东在决定文件上签名、盖章" _mubu_text="%3Cspan%3E%E7%94%B1%E5%85%A8%E4%BD%93%E8%82%A1%E4%B8%9C%E5%9C%A8%E5%86%B3%E5%AE%9A%E6%96%87%E4%BB%B6%E4%B8%8A%E7%AD%BE%E5%90%8D%E3%80%81%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
