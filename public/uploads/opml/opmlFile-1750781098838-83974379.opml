                <outline text="公开播送作品" _mubu_text="%3Cspan%3E%E5%85%AC%E5%BC%80%E6%92%AD%E9%80%81%E4%BD%9C%E5%93%81%3C/span%3E" _note="机械表演" _mubu_note="%3Cspan%3E%E6%9C%BA%E6%A2%B0%E8%A1%A8%E6%BC%94%3C/span%3E">
                  <outline text="征得词曲作者同意" _mubu_text="%3Cspan%3E%E5%BE%81%E5%BE%97%E8%AF%8D%E6%9B%B2%E4%BD%9C%E8%80%85%E5%90%8C%E6%84%8F%3C/span%3E" _note="而非表演者" _mubu_note="%3Cspan%3E%E8%80%8C%E9%9D%9E%E8%A1%A8%E6%BC%94%E8%80%85%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="放映权" _mubu_text="%3Cspan%3E%E6%94%BE%E6%98%A0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="广播权" _mubu_text="%3Cspan%3E%E5%B9%BF%E6%92%AD%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="≠广播组织权" _mubu_text="%3Cspan%3E%E2%89%A0%E5%B9%BF%E6%92%AD%E7%BB%84%E7%BB%87%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="属于邻接权" _mubu_text="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E9%82%BB%E6%8E%A5%E6%9D%83%3C/span%3E" _note="传播者权" _mubu_note="%3Cspan%3E%E4%BC%A0%E6%92%AD%E8%80%85%E6%9D%83%3C/span%3E"/>
                  <outline text="是广播电台、电视台对其播放内容享有的权利" _mubu_text="%3Cspan%3E%E6%98%AF%E5%B9%BF%E6%92%AD%E7%94%B5%E5%8F%B0%E3%80%81%E7%94%B5%E8%A7%86%E5%8F%B0%E5%AF%B9%E5%85%B6%E6%92%AD%E6%94%BE%E5%86%85%E5%AE%B9%E4%BA%AB%E6%9C%89%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="信息网络传播权" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%81%AF%E7%BD%91%E7%BB%9C%E4%BC%A0%E6%92%AD%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="公众可以在其个人选定的时间和地点获得作品" _mubu_text="%3Cspan%3E%E5%85%AC%E4%BC%97%E5%8F%AF%E4%BB%A5%E5%9C%A8%E5%85%B6%E4%B8%AA%E4%BA%BA%E9%80%89%E5%AE%9A%E7%9A%84%E6%97%B6%E9%97%B4%E5%92%8C%E5%9C%B0%E7%82%B9%E8%8E%B7%E5%BE%97%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="四类主体享有该权" _mubu_text="%3Cspan%3E%E5%9B%9B%E7%B1%BB%E4%B8%BB%E4%BD%93%E4%BA%AB%E6%9C%89%E8%AF%A5%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="作者、表演者、录音录像制作者、广播电台和电视台" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%80%85%E3%80%81%E8%A1%A8%E6%BC%94%E8%80%85%E3%80%81%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%E5%88%B6%E4%BD%9C%E8%80%85%E3%80%81%E5%B9%BF%E6%92%AD%E7%94%B5%E5%8F%B0%E5%92%8C%E7%94%B5%E8%A7%86%E5%8F%B0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="摄制权" _mubu_text="%3Cspan%3E%E6%91%84%E5%88%B6%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="改编权" _mubu_text="%3Cspan%3E%E6%94%B9%E7%BC%96%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="改变作品的表现形式或用途" _mubu_text="%3Cspan%3E%E6%94%B9%E5%8F%98%E4%BD%9C%E5%93%81%E7%9A%84%E8%A1%A8%E7%8E%B0%E5%BD%A2%E5%BC%8F%E6%88%96%E7%94%A8%E9%80%94%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="创作出具有独创性新作品" _mubu_text="%3Cspan%3E%E5%88%9B%E4%BD%9C%E5%87%BA%E5%85%B7%E6%9C%89%E7%8B%AC%E5%88%9B%E6%80%A7%E6%96%B0%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="翻译权" _mubu_text="%3Cspan%3E%E7%BF%BB%E8%AF%91%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="汇编权" _mubu_text="%3Cspan%3E%E6%B1%87%E7%BC%96%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="许可权" _mubu_text="%3Cspan%3E%E8%AE%B8%E5%8F%AF%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="转让权" _mubu_text="%3Cspan%3E%E8%BD%AC%E8%AE%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="出质权" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B4%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="著作权的取得" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E7%9A%84%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note="">
            <outline text="中国自然人、单位" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E8%87%AA%E7%84%B6%E4%BA%BA%E3%80%81%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="自作品创作完成之日起自动有著作权" _mubu_text="%3Cspan%3E%E8%87%AA%E4%BD%9C%E5%93%81%E5%88%9B%E4%BD%9C%E5%AE%8C%E6%88%90%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%87%AA%E5%8A%A8%E6%9C%89%E8%91%97%E4%BD%9C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="完成，既包括整部作品最终完成，也包括某一部分" _mubu_text="%3Cspan%3E%E5%AE%8C%E6%88%90%EF%BC%8C%E6%97%A2%E5%8C%85%E6%8B%AC%E6%95%B4%E9%83%A8%E4%BD%9C%E5%93%81%E6%9C%80%E7%BB%88%E5%AE%8C%E6%88%90%EF%BC%8C%E4%B9%9F%E5%8C%85%E6%8B%AC%E6%9F%90%E4%B8%80%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="外国人、无国籍人" _mubu_text="%3Cspan%3E%E5%A4%96%E5%9B%BD%E4%BA%BA%E3%80%81%E6%97%A0%E5%9B%BD%E7%B1%8D%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="有协议／条约" _mubu_text="%3Cspan%3E%E6%9C%89%E5%8D%8F%E8%AE%AE%EF%BC%8F%E6%9D%A1%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="享有的著作权，受我国《著作权法》保护" _mubu_text="%3Cspan%3E%E4%BA%AB%E6%9C%89%E7%9A%84%E8%91%97%E4%BD%9C%E6%9D%83%EF%BC%8C%E5%8F%97%E6%88%91%E5%9B%BD%E3%80%8A%E8%91%97%E4%BD%9C%E6%9D%83%E6%B3%95%E3%80%8B%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="首次在我国出版" _mubu_text="%3Cspan%3E%E9%A6%96%E6%AC%A1%E5%9C%A8%E6%88%91%E5%9B%BD%E5%87%BA%E7%89%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="其著作权自首次出版之日起受保护" _mubu_text="%3Cspan%3E%E5%85%B6%E8%91%97%E4%BD%9C%E6%9D%83%E8%87%AA%E9%A6%96%E6%AC%A1%E5%87%BA%E7%89%88%E4%B9%8B%E6%97%A5%E8%B5%B7%E5%8F%97%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="无协议／条约" _mubu_text="%3Cspan%3E%E6%97%A0%E5%8D%8F%E8%AE%AE%EF%BC%8F%E6%9D%A1%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="首次在中国参加的国际条约的成员国出版的，或者在成员国和非成员国同时出版的，受我国《著作权法》保护" _mubu_text="%3Cspan%3E%E9%A6%96%E6%AC%A1%E5%9C%A8%E4%B8%AD%E5%9B%BD%E5%8F%82%E5%8A%A0%E7%9A%84%E5%9B%BD%E9%99%85%E6%9D%A1%E7%BA%A6%E7%9A%84%E6%88%90%E5%91%98%E5%9B%BD%E5%87%BA%E7%89%88%E7%9A%84%EF%BC%8C%E6%88%96%E8%80%85%E5%9C%A8%E6%88%90%E5%91%98%E5%9B%BD%E5%92%8C%E9%9D%9E%E6%88%90%E5%91%98%E5%9B%BD%E5%90%8C%E6%97%B6%E5%87%BA%E7%89%88%E7%9A%84%EF%BC%8C%E5%8F%97%E6%88%91%E5%9B%BD%E3%80%8A%E8%91%97%E4%BD%9C%E6%9D%83%E6%B3%95%E3%80%8B%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="“同时出版”" _mubu_text="%3Cspan%3E%E2%80%9C%E5%90%8C%E6%97%B6%E5%87%BA%E7%89%88%E2%80%9D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="是指一个作品在首次出版后30日内在两个或两个以上国家内出版，则该作品应视为同时在几个国家内出版" _mubu_text="%3Cspan%3E%E6%98%AF%E6%8C%87%E4%B8%80%E4%B8%AA%E4%BD%9C%E5%93%81%E5%9C%A8%E9%A6%96%E6%AC%A1%E5%87%BA%E7%89%88%E5%90%8E30%E6%97%A5%E5%86%85%E5%9C%A8%E4%B8%A4%E4%B8%AA%E6%88%96%E4%B8%A4%E4%B8%AA%E4%BB%A5%E4%B8%8A%E5%9B%BD%E5%AE%B6%E5%86%85%E5%87%BA%E7%89%88%EF%BC%8C%E5%88%99%E8%AF%A5%E4%BD%9C%E5%93%81%E5%BA%94%E8%A7%86%E4%B8%BA%E5%90%8C%E6%97%B6%E5%9C%A8%E5%87%A0%E4%B8%AA%E5%9B%BD%E5%AE%B6%E5%86%85%E5%87%BA%E7%89%88%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="一般规定" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="自然人作者" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E4%BA%BA%E4%BD%9C%E8%80%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="创作作品的自然人是作者" _mubu_text="%3Cspan%3E%E5%88%9B%E4%BD%9C%E4%BD%9C%E5%93%81%E7%9A%84%E8%87%AA%E7%84%B6%E4%BA%BA%E6%98%AF%E4%BD%9C%E8%80%85%3C/span%3E" _note="进行其他辅助工作不视为创作" _mubu_note="%3Cspan%3E%E8%BF%9B%E8%A1%8C%E5%85%B6%E4%BB%96%E8%BE%85%E5%8A%A9%E5%B7%A5%E4%BD%9C%E4%B8%8D%E8%A7%86%E4%B8%BA%E5%88%9B%E4%BD%9C%3C/span%3E"/>
            </outline>
            <outline text="单位视为作者的情形" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E8%A7%86%E4%B8%BA%E4%BD%9C%E8%80%85%E7%9A%84%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="单位主持" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E4%B8%BB%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="单位意志创作" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%84%8F%E5%BF%97%E5%88%9B%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="单位担责" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%8B%85%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="署名者推定为作者" _mubu_text="%3Cspan%3E%E7%BD%B2%E5%90%8D%E8%80%85%E6%8E%A8%E5%AE%9A%E4%B8%BA%E4%BD%9C%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="他人执笔的报告、讲话" _mubu_text="%3Cspan%3E%E4%BB%96%E4%BA%BA%E6%89%A7%E7%AC%94%E7%9A%84%E6%8A%A5%E5%91%8A%E3%80%81%E8%AE%B2%E8%AF%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="著作权归报告人或讲话人享有" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E5%BD%92%E6%8A%A5%E5%91%8A%E4%BA%BA%E6%88%96%E8%AE%B2%E8%AF%9D%E4%BA%BA%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="特殊的自传体作品" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E7%9A%84%E8%87%AA%E4%BC%A0%E4%BD%93%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="有约从约" _mubu_text="%3Cspan%3E%E6%9C%89%E7%BA%A6%E4%BB%8E%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无约，著作权归该特定人物享有" _mubu_text="%3Cspan%3E%E6%97%A0%E7%BA%A6%EF%BC%8C%E8%91%97%E4%BD%9C%E6%9D%83%E5%BD%92%E8%AF%A5%E7%89%B9%E5%AE%9A%E4%BA%BA%E7%89%A9%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="著作权集体管理组织" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E9%9B%86%E4%BD%93%E7%AE%A1%E7%90%86%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note="">
              <outline text="著作权人和有关权利人可授权其行使著作权或有关权利" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E4%BA%BA%E5%92%8C%E6%9C%89%E5%85%B3%E6%9D%83%E5%88%A9%E4%BA%BA%E5%8F%AF%E6%8E%88%E6%9D%83%E5%85%B6%E8%A1%8C%E4%BD%BF%E8%91%97%E4%BD%9C%E6%9D%83%E6%88%96%E6%9C%89%E5%85%B3%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以自己的名义为著作权人和有关权利人主张权利" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%87%AA%E5%B7%B1%E7%9A%84%E5%90%8D%E4%B9%89%E4%B8%BA%E8%91%97%E4%BD%9C%E6%9D%83%E4%BA%BA%E5%92%8C%E6%9C%89%E5%85%B3%E6%9D%83%E5%88%A9%E4%BA%BA%E4%B8%BB%E5%BC%A0%E6%9D%83%E5%88%A9%3C/span%3E" _note="诉讼、仲裁、调解活动" _mubu_note="%3Cspan%3E%E8%AF%89%E8%AE%BC%E3%80%81%E4%BB%B2%E8%A3%81%E3%80%81%E8%B0%83%E8%A7%A3%E6%B4%BB%E5%8A%A8%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="演绎作品" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%BC%94%E7%BB%8E%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="改编、翻译、注释、整理" _mubu_text="%3Cspan%3E%E6%94%B9%E7%BC%96%E3%80%81%E7%BF%BB%E8%AF%91%E3%80%81%E6%B3%A8%E9%87%8A%E3%80%81%E6%95%B4%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="须征得原作品著作权人的同意" _mubu_text="%3Cspan%3E%E9%A1%BB%E5%BE%81%E5%BE%97%E5%8E%9F%E4%BD%9C%E5%93%81%E8%91%97%E4%BD%9C%E6%9D%83%E4%BA%BA%E7%9A%84%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="使用演绎作品的双重许可(图)" _mubu_text="%3Cspan%3E%E4%BD%BF%E7%94%A8%E6%BC%94%E7%BB%8E%E4%BD%9C%E5%93%81%E7%9A%84%E5%8F%8C%E9%87%8D%E8%AE%B8%E5%8F%AF(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
