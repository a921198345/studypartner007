                  <outline text="1/3以上的董事" _mubu_text="%3Cspan%3E1/3%E4%BB%A5%E4%B8%8A%E7%9A%84%E8%91%A3%E4%BA%8B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="监事(会)" _mubu_text="%3Cspan%3E%E7%9B%91%E4%BA%8B(%E4%BC%9A)%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="提议召开临时会议的，应当召开临时会议" _mubu_text="%3Cspan%3E%E6%8F%90%E8%AE%AE%E5%8F%AC%E5%BC%80%E4%B8%B4%E6%97%B6%E4%BC%9A%E8%AE%AE%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%8F%AC%E5%BC%80%E4%B8%B4%E6%97%B6%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
