<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <head>
    <title>商经知</title>
  </head>
  <body>
    <outline text="商法" _mubu_text="%3Cspan%3E%E5%95%86%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="公司法" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="基础理论" _mubu_text="%3Cspan%3E%E5%9F%BA%E7%A1%80%E7%90%86%E8%AE%BA%3C/span%3E" _note="" _mubu_note="">
          <outline text="股东的有限责任" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E7%9A%84%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="公司以全部财产对其债务承担责任" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%BB%A5%E5%85%A8%E9%83%A8%E8%B4%A2%E4%BA%A7%E5%AF%B9%E5%85%B6%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="股东仅以其认缴的出资额对公司债务承担责任" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%BB%85%E4%BB%A5%E5%85%B6%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="为实际出资到位的应在其未出范围内担责" _mubu_note="%3Cspan%3E%E4%B8%BA%E5%AE%9E%E9%99%85%E5%87%BA%E8%B5%84%E5%88%B0%E4%BD%8D%E7%9A%84%E5%BA%94%E5%9C%A8%E5%85%B6%E6%9C%AA%E5%87%BA%E8%8C%83%E5%9B%B4%E5%86%85%E6%8B%85%E8%B4%A3%3C/span%3E">
              <outline text="股东最大责任范围是其认缴的出资额" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%9C%80%E5%A4%A7%E8%B4%A3%E4%BB%BB%E8%8C%83%E5%9B%B4%E6%98%AF%E5%85%B6%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="公司的独立法人地位" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E7%8B%AC%E7%AB%8B%E6%B3%95%E4%BA%BA%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="名义独立" _mubu_text="%3Cspan%3E%E5%90%8D%E4%B9%89%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="以独立的名义为民事法律行为" _mubu_text="%3Cspan%3E%E4%BB%A5%E7%8B%AC%E7%AB%8B%E7%9A%84%E5%90%8D%E4%B9%89%E4%B8%BA%E6%B0%91%E4%BA%8B%E6%B3%95%E5%BE%8B%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="设立过程中" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E8%BF%87%E7%A8%8B%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="发起人可自己的名义对外" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%8F%AF%E8%87%AA%E5%B7%B1%E7%9A%84%E5%90%8D%E4%B9%89%E5%AF%B9%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="公司成立后" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="发起人或股东均无权对外代表公司" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E6%88%96%E8%82%A1%E4%B8%9C%E5%9D%87%E6%97%A0%E6%9D%83%E5%AF%B9%E5%A4%96%E4%BB%A3%E8%A1%A8%E5%85%AC%E5%8F%B8%3C/span%3E" _note="法定代表人以自己名义代表公司" _mubu_note="%3Cspan%3E%E6%B3%95%E5%AE%9A%E4%BB%A3%E8%A1%A8%E4%BA%BA%E4%BB%A5%E8%87%AA%E5%B7%B1%E5%90%8D%E4%B9%89%E4%BB%A3%E8%A1%A8%E5%85%AC%E5%8F%B8%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="财产独立" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="股东向公司履行的出资归公司所有" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%90%91%E5%85%AC%E5%8F%B8%E5%B1%A5%E8%A1%8C%E7%9A%84%E5%87%BA%E8%B5%84%E5%BD%92%E5%85%AC%E5%8F%B8%E6%89%80%E6%9C%89%3C/span%3E" _note="股东不得取回或擅自使用" _mubu_note="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%B8%8D%E5%BE%97%E5%8F%96%E5%9B%9E%E6%88%96%E6%93%85%E8%87%AA%E4%BD%BF%E7%94%A8%3C/span%3E"/>
              <outline text="经营取得的利润归属于公司" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E5%8F%96%E5%BE%97%E7%9A%84%E5%88%A9%E6%B6%A6%E5%BD%92%E5%B1%9E%E4%BA%8E%E5%85%AC%E5%8F%B8%3C/span%3E" _note="未经法定的利润分配程序，股东不得擅自占有" _mubu_note="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E6%B3%95%E5%AE%9A%E7%9A%84%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E7%A8%8B%E5%BA%8F%EF%BC%8C%E8%82%A1%E4%B8%9C%E4%B8%8D%E5%BE%97%E6%93%85%E8%87%AA%E5%8D%A0%E6%9C%89%3C/span%3E"/>
            </outline>
            <outline text="责任独立" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="是公司独立性最重要的体现" _mubu_text="%3Cspan%3E%E6%98%AF%E5%85%AC%E5%8F%B8%E7%8B%AC%E7%AB%8B%E6%80%A7%E6%9C%80%E9%87%8D%E8%A6%81%E7%9A%84%E4%BD%93%E7%8E%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="通常情况下债权人不能向公司背后的股东追索" _mubu_text="%3Cspan%3E%E9%80%9A%E5%B8%B8%E6%83%85%E5%86%B5%E4%B8%8B%E5%80%BA%E6%9D%83%E4%BA%BA%E4%B8%8D%E8%83%BD%E5%90%91%E5%85%AC%E5%8F%B8%E8%83%8C%E5%90%8E%E7%9A%84%E8%82%A1%E4%B8%9C%E8%BF%BD%E7%B4%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="公司法人人格否认制度" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%B3%95%E4%BA%BA%E4%BA%BA%E6%A0%BC%E5%90%A6%E8%AE%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="制度要点" _mubu_text="%3Cspan%3E%E5%88%B6%E5%BA%A6%E8%A6%81%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="前提" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="股东滥用公司法人独立地位和股东有限责任，逃避债务" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%BB%A5%E7%94%A8%E5%85%AC%E5%8F%B8%E6%B3%95%E4%BA%BA%E7%8B%AC%E7%AB%8B%E5%9C%B0%E4%BD%8D%E5%92%8C%E8%82%A1%E4%B8%9C%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%EF%BC%8C%E9%80%83%E9%81%BF%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="影响" _mubu_text="%3Cspan%3E%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="严重损害公司债权人利益" _mubu_text="%3Cspan%3E%E4%B8%A5%E9%87%8D%E6%8D%9F%E5%AE%B3%E5%85%AC%E5%8F%B8%E5%80%BA%E6%9D%83%E4%BA%BA%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="股东对公司债务承担连带责任" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="股东利用其控制的两个以上公司实施上述行为的，各公司应当对任一公司的债务承担连带责任" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%88%A9%E7%94%A8%E5%85%B6%E6%8E%A7%E5%88%B6%E7%9A%84%E4%B8%A4%E4%B8%AA%E4%BB%A5%E4%B8%8A%E5%85%AC%E5%8F%B8%E5%AE%9E%E6%96%BD%E4%B8%8A%E8%BF%B0%E8%A1%8C%E4%B8%BA%E7%9A%84%EF%BC%8C%E5%90%84%E5%85%AC%E5%8F%B8%E5%BA%94%E5%BD%93%E5%AF%B9%E4%BB%BB%E4%B8%80%E5%85%AC%E5%8F%B8%E7%9A%84%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="适用情形" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="人格混同" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%A0%BC%E6%B7%B7%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司不再具有独立意思和独立财产" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%86%8D%E5%85%B7%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%84%8F%E6%80%9D%E5%92%8C%E7%8B%AC%E7%AB%8B%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="过度支配与控制" _mubu_text="%3Cspan%3E%E8%BF%87%E5%BA%A6%E6%94%AF%E9%85%8D%E4%B8%8E%E6%8E%A7%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司完全丧失独立性，沦为控制股东的工具或躯壳" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%AE%8C%E5%85%A8%E4%B8%A7%E5%A4%B1%E7%8B%AC%E7%AB%8B%E6%80%A7%EF%BC%8C%E6%B2%A6%E4%B8%BA%E6%8E%A7%E5%88%B6%E8%82%A1%E4%B8%9C%E7%9A%84%E5%B7%A5%E5%85%B7%E6%88%96%E8%BA%AF%E5%A3%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="资本显著不足" _mubu_text="%3Cspan%3E%E8%B5%84%E6%9C%AC%E6%98%BE%E8%91%97%E4%B8%8D%E8%B6%B3%3C/span%3E" _note="" _mubu_note="">
                <outline text="股东实际投入公司的资本数额与公司经营所隐含的风险相比明显不匹配" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%AE%9E%E9%99%85%E6%8A%95%E5%85%A5%E5%85%AC%E5%8F%B8%E7%9A%84%E8%B5%84%E6%9C%AC%E6%95%B0%E9%A2%9D%E4%B8%8E%E5%85%AC%E5%8F%B8%E7%BB%8F%E8%90%A5%E6%89%80%E9%9A%90%E5%90%AB%E7%9A%84%E9%A3%8E%E9%99%A9%E7%9B%B8%E6%AF%94%E6%98%8E%E6%98%BE%E4%B8%8D%E5%8C%B9%E9%85%8D%3C/span%3E" _note="实质是恶意利用公司独立人格和股东有限责任把投资风险转嫁给债权人" _mubu_note="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E6%98%AF%E6%81%B6%E6%84%8F%E5%88%A9%E7%94%A8%E5%85%AC%E5%8F%B8%E7%8B%AC%E7%AB%8B%E4%BA%BA%E6%A0%BC%E5%92%8C%E8%82%A1%E4%B8%9C%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%E6%8A%8A%E6%8A%95%E8%B5%84%E9%A3%8E%E9%99%A9%E8%BD%AC%E5%AB%81%E7%BB%99%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="制度特点" _mubu_text="%3Cspan%3E%E5%88%B6%E5%BA%A6%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="法人人格否认制度是例外情形" _mubu_text="%3Cspan%3E%E6%B3%95%E4%BA%BA%E4%BA%BA%E6%A0%BC%E5%90%A6%E8%AE%A4%E5%88%B6%E5%BA%A6%E6%98%AF%E4%BE%8B%E5%A4%96%E6%83%85%E5%BD%A2%3C/span%3E" _note="原则上是股东有限责任和公司独立法人地位" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E6%98%AF%E8%82%A1%E4%B8%9C%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%E5%92%8C%E5%85%AC%E5%8F%B8%E7%8B%AC%E7%AB%8B%E6%B3%95%E4%BA%BA%E5%9C%B0%E4%BD%8D%3C/span%3E"/>
              <outline text="特定股东承担连带责任" _mubu_text="%3Cspan%3E%E7%89%B9%E5%AE%9A%E8%82%A1%E4%B8%9C%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="无关者不承担" _mubu_note="%3Cspan%3E%E6%97%A0%E5%85%B3%E8%80%85%E4%B8%8D%E6%89%BF%E6%8B%85%3C/span%3E"/>
              <outline text="个案适用" _mubu_text="%3Cspan%3E%E4%B8%AA%E6%A1%88%E9%80%82%E7%94%A8%3C/span%3E" _note="具体案例具体分析，要细分到单笔债务" _mubu_note="%3Cspan%3E%E5%85%B7%E4%BD%93%E6%A1%88%E4%BE%8B%E5%85%B7%E4%BD%93%E5%88%86%E6%9E%90%EF%BC%8C%E8%A6%81%E7%BB%86%E5%88%86%E5%88%B0%E5%8D%95%E7%AC%94%E5%80%BA%E5%8A%A1%3C/span%3E"/>
              <outline text="当事人主张适用" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BB%E5%BC%A0%E9%80%82%E7%94%A8%3C/span%3E" _note="法院、仲裁不能主动适用" _mubu_note="%3Cspan%3E%E6%B3%95%E9%99%A2%E3%80%81%E4%BB%B2%E8%A3%81%E4%B8%8D%E8%83%BD%E4%B8%BB%E5%8A%A8%E9%80%82%E7%94%A8%3C/span%3E"/>
            </outline>
            <outline text="举证责任分配" _mubu_text="%3Cspan%3E%E4%B8%BE%E8%AF%81%E8%B4%A3%E4%BB%BB%E5%88%86%E9%85%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="一般由受损害的公司债权人举证" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%94%B1%E5%8F%97%E6%8D%9F%E5%AE%B3%E7%9A%84%E5%85%AC%E5%8F%B8%E5%80%BA%E6%9D%83%E4%BA%BA%E4%B8%BE%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="举证责任倒置情形" _mubu_text="%3Cspan%3E%E4%B8%BE%E8%AF%81%E8%B4%A3%E4%BB%BB%E5%80%92%E7%BD%AE%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="一人公司的股东" _mubu_text="%3Cspan%3E%E4%B8%80%E4%BA%BA%E5%85%AC%E5%8F%B8%E7%9A%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
