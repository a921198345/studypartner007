            <outline text="重整失败" _mubu_text="%3Cspan%3E%E9%87%8D%E6%95%B4%E5%A4%B1%E8%B4%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="不(能)执行重整计划，经管理人或利害关系人请求，裁定终止重整执行，并宣告债务人破产" _mubu_text="%3Cspan%3E%E4%B8%8D(%E8%83%BD)%E6%89%A7%E8%A1%8C%E9%87%8D%E6%95%B4%E8%AE%A1%E5%88%92%EF%BC%8C%E7%BB%8F%E7%AE%A1%E7%90%86%E4%BA%BA%E6%88%96%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E8%AF%B7%E6%B1%82%EF%BC%8C%E8%A3%81%E5%AE%9A%E7%BB%88%E6%AD%A2%E9%87%8D%E6%95%B4%E6%89%A7%E8%A1%8C%EF%BC%8C%E5%B9%B6%E5%AE%A3%E5%91%8A%E5%80%BA%E5%8A%A1%E4%BA%BA%E7%A0%B4%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
