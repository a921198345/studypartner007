              <outline text="支付后有权向第三人追偿" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E5%90%8E%E6%9C%89%E6%9D%83%E5%90%91%E7%AC%AC%E4%B8%89%E4%BA%BA%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="应当由公共卫生负担的" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E7%94%B1%E5%85%AC%E5%85%B1%E5%8D%AB%E7%94%9F%E8%B4%9F%E6%8B%85%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="在境外就医的" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%A2%83%E5%A4%96%E5%B0%B1%E5%8C%BB%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
