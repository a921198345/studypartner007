          <outline text="汇编作品" _mubu_text="%3Cspan%3E%E6%B1%87%E7%BC%96%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="在汇编材料属于作品时" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%B1%87%E7%BC%96%E6%9D%90%E6%96%99%E5%B1%9E%E4%BA%8E%E4%BD%9C%E5%93%81%E6%97%B6%3C/span%3E" _note="" _mubu_note="">
