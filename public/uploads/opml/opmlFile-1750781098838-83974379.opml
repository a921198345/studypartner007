                </outline>
                <outline text="股份公司发行记名股票的，应当置备股东名册" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%E5%8F%91%E8%A1%8C%E8%AE%B0%E5%90%8D%E8%82%A1%E7%A5%A8%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E7%BD%AE%E5%A4%87%E8%82%A1%E4%B8%9C%E5%90%8D%E5%86%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="登记" _mubu_text="%3Cspan%3E%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="变更的应登记" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E7%9A%84%E5%BA%94%E7%99%BB%E8%AE%B0%3C/span%3E" _note="未登记不得对抗善三" _mubu_note="%3Cspan%3E%E6%9C%AA%E7%99%BB%E8%AE%B0%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E4%B8%89%3C/span%3E">
