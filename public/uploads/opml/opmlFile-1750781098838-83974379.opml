        <outline text="股份有限公司" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
          <outline text="设立" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="发起人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="人数" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="200人≥发起人≥2人" _mubu_text="%3Cspan%3E200%E4%BA%BA%E2%89%A5%E5%8F%91%E8%B5%B7%E4%BA%BA%E2%89%A52%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="且半数以上在中国境内有住所" _mubu_text="%3Cspan%3E%E4%B8%94%E5%8D%8A%E6%95%B0%E4%BB%A5%E4%B8%8A%E5%9C%A8%E4%B8%AD%E5%9B%BD%E5%A2%83%E5%86%85%E6%9C%89%E4%BD%8F%E6%89%80%3C/span%3E" _note="非国籍要求" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%9B%BD%E7%B1%8D%E8%A6%81%E6%B1%82%3C/span%3E"/>
              </outline>
              <outline text="应当签订发起人协议" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E7%AD%BE%E8%AE%A2%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="承担公司筹办事务" _mubu_text="%3Cspan%3E%E6%89%BF%E6%8B%85%E5%85%AC%E5%8F%B8%E7%AD%B9%E5%8A%9E%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="发起设立" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="全部股份由发起人自行认购" _mubu_text="%3Cspan%3E%E5%85%A8%E9%83%A8%E8%82%A1%E4%BB%BD%E7%94%B1%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%87%AA%E8%A1%8C%E8%AE%A4%E8%B4%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="同有限" _mubu_text="%3Cspan%3E%E5%90%8C%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="注册资本" _mubu_text="%3Cspan%3E%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%3C/span%3E" _note="" _mubu_note="">
              <outline text="为在公司登记机关登记的已发行股份的股本总额" _mubu_text="%3Cspan%3E%E4%B8%BA%E5%9C%A8%E5%85%AC%E5%8F%B8%E7%99%BB%E8%AE%B0%E6%9C%BA%E5%85%B3%E7%99%BB%E8%AE%B0%E7%9A%84%E5%B7%B2%E5%8F%91%E8%A1%8C%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E6%9C%AC%E6%80%BB%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="在发起人认购的股份缴足前，不得向他人募集股份" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%AE%A4%E8%B4%AD%E7%9A%84%E8%82%A1%E4%BB%BD%E7%BC%B4%E8%B6%B3%E5%89%8D%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%90%91%E4%BB%96%E4%BA%BA%E5%8B%9F%E9%9B%86%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="实缴出资" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%BC%B4%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="发起人应当在公司成立前按照其认购的股份全额缴纳股款" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%BA%94%E5%BD%93%E5%9C%A8%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E5%89%8D%E6%8C%89%E7%85%A7%E5%85%B6%E8%AE%A4%E8%B4%AD%E7%9A%84%E8%82%A1%E4%BB%BD%E5%85%A8%E9%A2%9D%E7%BC%B4%E7%BA%B3%E8%82%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="募集设立" _mubu_text="%3Cspan%3E%E5%8B%9F%E9%9B%86%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="目前募集设立仅存在于有限变股份时" _mubu_text="%3Cspan%3E%E7%9B%AE%E5%89%8D%E5%8B%9F%E9%9B%86%E8%AE%BE%E7%AB%8B%E4%BB%85%E5%AD%98%E5%9C%A8%E4%BA%8E%E6%9C%89%E9%99%90%E5%8F%98%E8%82%A1%E4%BB%BD%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="实缴注册资本" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%BC%B4%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="发起人认购公司应发行股份的一部分" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%AE%A4%E8%B4%AD%E5%85%AC%E5%8F%B8%E5%BA%94%E5%8F%91%E8%A1%8C%E8%82%A1%E4%BB%BD%E7%9A%84%E4%B8%80%E9%83%A8%E5%88%86%3C/span%3E" _note="≥35%" _mubu_note="%3Cspan%3E%E2%89%A535%25%3C/span%3E"/>
              <outline text="其余股份向社会公开募集/向特定对象募集" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BD%99%E8%82%A1%E4%BB%BD%E5%90%91%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%BC%80%E5%8B%9F%E9%9B%86/%E5%90%91%E7%89%B9%E5%AE%9A%E5%AF%B9%E8%B1%A1%E5%8B%9F%E9%9B%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="成立大会" _mubu_text="%3Cspan%3E%E6%88%90%E7%AB%8B%E5%A4%A7%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="时间" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="股款缴足之日起30日内" _mubu_text="%3Cspan%3E%E8%82%A1%E6%AC%BE%E7%BC%B4%E8%B6%B3%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="组成" _mubu_text="%3Cspan%3E%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="发起人主持召开" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%B8%BB%E6%8C%81%E5%8F%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="代表股份总数过半数的发起人、认股人出席" _mubu_text="%3Cspan%3E%E4%BB%A3%E8%A1%A8%E8%82%A1%E4%BB%BD%E6%80%BB%E6%95%B0%E8%BF%87%E5%8D%8A%E6%95%B0%E7%9A%84%E5%8F%91%E8%B5%B7%E4%BA%BA%E3%80%81%E8%AE%A4%E8%82%A1%E4%BA%BA%E5%87%BA%E5%B8%AD%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="决议" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="经出席会议的认股人所持表决权过半数通过" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%87%BA%E5%B8%AD%E4%BC%9A%E8%AE%AE%E7%9A%84%E8%AE%A4%E8%82%A1%E4%BA%BA%E6%89%80%E6%8C%81%E8%A1%A8%E5%86%B3%E6%9D%83%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E" _note="发起人回避" _mubu_note="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%9B%9E%E9%81%BF%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="股东大会" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%A4%A7%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="定期会议" _mubu_text="%3Cspan%3E%E5%AE%9A%E6%9C%9F%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="每年召开1次" _mubu_text="%3Cspan%3E%E6%AF%8F%E5%B9%B4%E5%8F%AC%E5%BC%801%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="会议召开事项，于召开20日前通知各股东" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E5%8F%AC%E5%BC%80%E4%BA%8B%E9%A1%B9%EF%BC%8C%E4%BA%8E%E5%8F%AC%E5%BC%8020%E6%97%A5%E5%89%8D%E9%80%9A%E7%9F%A5%E5%90%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="发行无记名股票的应30日前" _mubu_note="%3Cspan%3E%E5%8F%91%E8%A1%8C%E6%97%A0%E8%AE%B0%E5%90%8D%E8%82%A1%E7%A5%A8%E7%9A%84%E5%BA%9430%E6%97%A5%E5%89%8D%3C/span%3E"/>
            </outline>
            <outline text="临时会议" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="出现特定情形，应在2个月内召开" _mubu_text="%3Cspan%3E%E5%87%BA%E7%8E%B0%E7%89%B9%E5%AE%9A%E6%83%85%E5%BD%A2%EF%BC%8C%E5%BA%94%E5%9C%A82%E4%B8%AA%E6%9C%88%E5%86%85%E5%8F%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="董事人数不足规定2/3" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BA%BA%E6%95%B0%E4%B8%8D%E8%B6%B3%E8%A7%84%E5%AE%9A2/3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公司未弥补的亏损达实收股本总额1/3" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%9C%AA%E5%BC%A5%E8%A1%A5%E7%9A%84%E4%BA%8F%E6%8D%9F%E8%BE%BE%E5%AE%9E%E6%94%B6%E8%82%A1%E6%9C%AC%E6%80%BB%E9%A2%9D1/3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="持有公司10％以上股份的股东请求" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E5%85%AC%E5%8F%B810%EF%BC%85%E4%BB%A5%E4%B8%8A%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E4%B8%9C%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="董事会认为必要时" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E8%AE%A4%E4%B8%BA%E5%BF%85%E8%A6%81%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="监事会提议召开时" _mubu_text="%3Cspan%3E%E7%9B%91%E4%BA%8B%E4%BC%9A%E6%8F%90%E8%AE%AE%E5%8F%AC%E5%BC%80%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="单独或合计持10%以上股份的股东请求开临时股东会" _mubu_text="%3Cspan%3E%E5%8D%95%E7%8B%AC%E6%88%96%E5%90%88%E8%AE%A1%E6%8C%8110%25%E4%BB%A5%E4%B8%8A%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E4%B8%9C%E8%AF%B7%E6%B1%82%E5%BC%80%E4%B8%B4%E6%97%B6%E8%82%A1%E4%B8%9C%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="董事会、监事会应当在收到请求起10日内作出是否召开的决定，并书面答复股东" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E3%80%81%E7%9B%91%E4%BA%8B%E4%BC%9A%E5%BA%94%E5%BD%93%E5%9C%A8%E6%94%B6%E5%88%B0%E8%AF%B7%E6%B1%82%E8%B5%B710%E6%97%A5%E5%86%85%E4%BD%9C%E5%87%BA%E6%98%AF%E5%90%A6%E5%8F%AC%E5%BC%80%E7%9A%84%E5%86%B3%E5%AE%9A%EF%BC%8C%E5%B9%B6%E4%B9%A6%E9%9D%A2%E7%AD%94%E5%A4%8D%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="会议召开15日前通知各股东" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E5%8F%AC%E5%BC%8015%E6%97%A5%E5%89%8D%E9%80%9A%E7%9F%A5%E5%90%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="会议不得对通知中未列明的事项作出决议" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E4%B8%8D%E5%BE%97%E5%AF%B9%E9%80%9A%E7%9F%A5%E4%B8%AD%E6%9C%AA%E5%88%97%E6%98%8E%E7%9A%84%E4%BA%8B%E9%A1%B9%E4%BD%9C%E5%87%BA%E5%86%B3%E8%AE%AE%3C/span%3E" _note="即使通过也是可撤销" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%BD%BF%E9%80%9A%E8%BF%87%E4%B9%9F%E6%98%AF%E5%8F%AF%E6%92%A4%E9%94%80%3C/span%3E"/>
            <outline text="召集与主持" _mubu_text="%3Cspan%3E%E5%8F%AC%E9%9B%86%E4%B8%8E%E4%B8%BB%E6%8C%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="董事会召集，董事长/副董事长/推选董事主持" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E5%8F%AC%E9%9B%86%EF%BC%8C%E8%91%A3%E4%BA%8B%E9%95%BF/%E5%89%AF%E8%91%A3%E4%BA%8B%E9%95%BF/%E6%8E%A8%E9%80%89%E8%91%A3%E4%BA%8B%E4%B8%BB%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不能，监事会召集和主持" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%EF%BC%8C%E7%9B%91%E4%BA%8B%E4%BC%9A%E5%8F%AC%E9%9B%86%E5%92%8C%E4%B8%BB%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不能，连续90日以上持有10%股份的股东自行召集和主持" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%EF%BC%8C%E8%BF%9E%E7%BB%AD90%E6%97%A5%E4%BB%A5%E4%B8%8A%E6%8C%81%E6%9C%8910%25%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E4%B8%9C%E8%87%AA%E8%A1%8C%E5%8F%AC%E9%9B%86%E5%92%8C%E4%B8%BB%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="“由内而外，依次补位”" _mubu_text="%3Cspan%3E%E2%80%9C%E7%94%B1%E5%86%85%E8%80%8C%E5%A4%96%EF%BC%8C%E4%BE%9D%E6%AC%A1%E8%A1%A5%E4%BD%8D%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="表决" _mubu_text="%3Cspan%3E%E8%A1%A8%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="一股一票" _mubu_text="%3Cspan%3E%E4%B8%80%E8%82%A1%E4%B8%80%E7%A5%A8%3C/span%3E" _note="公司自持的股份无表决权" _mubu_note="%3Cspan%3E%E5%85%AC%E5%8F%B8%E8%87%AA%E6%8C%81%E7%9A%84%E8%82%A1%E4%BB%BD%E6%97%A0%E8%A1%A8%E5%86%B3%E6%9D%83%3C/span%3E"/>
              <outline text="一般事项" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="出席股东表决权过半数" _mubu_text="%3Cspan%3E%E5%87%BA%E5%B8%AD%E8%82%A1%E4%B8%9C%E8%A1%A8%E5%86%B3%E6%9D%83%E8%BF%87%E5%8D%8A%E6%95%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="特别事项" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="同有限" _mubu_text="%3Cspan%3E%E5%90%8C%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="≥2/3" _mubu_text="%3Cspan%3E%E2%89%A52/3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="临时提案" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E6%8F%90%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="单独或合计持公司1%以上股份的股东，可在股东会召开10日前提出临时提案并书面提交董事会" _mubu_text="%3Cspan%3E%E5%8D%95%E7%8B%AC%E6%88%96%E5%90%88%E8%AE%A1%E6%8C%81%E5%85%AC%E5%8F%B81%25%E4%BB%A5%E4%B8%8A%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E4%B8%9C%EF%BC%8C%E5%8F%AF%E5%9C%A8%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%8F%AC%E5%BC%8010%E6%97%A5%E5%89%8D%E6%8F%90%E5%87%BA%E4%B8%B4%E6%97%B6%E6%8F%90%E6%A1%88%E5%B9%B6%E4%B9%A6%E9%9D%A2%E6%8F%90%E4%BA%A4%E8%91%A3%E4%BA%8B%E4%BC%9A%3C/span%3E" _note="公司不得提高该持股比例" _mubu_note="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%BE%97%E6%8F%90%E9%AB%98%E8%AF%A5%E6%8C%81%E8%82%A1%E6%AF%94%E4%BE%8B%3C/span%3E"/>
                <outline text="股东会应在收到2日内通知其他股东，并将该临时提案提交股东会审议" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%BA%94%E5%9C%A8%E6%94%B6%E5%88%B02%E6%97%A5%E5%86%85%E9%80%9A%E7%9F%A5%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%EF%BC%8C%E5%B9%B6%E5%B0%86%E8%AF%A5%E4%B8%B4%E6%97%B6%E6%8F%90%E6%A1%88%E6%8F%90%E4%BA%A4%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%AE%A1%E8%AE%AE%3C/span%3E" _note="但违反法律、章程或不属于股东会职权的除外" _mubu_note="%3Cspan%3E%E4%BD%86%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E3%80%81%E7%AB%A0%E7%A8%8B%E6%88%96%E4%B8%8D%E5%B1%9E%E4%BA%8E%E8%82%A1%E4%B8%9C%E4%BC%9A%E8%81%8C%E6%9D%83%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="公开发行股份的公司应以公告方式作出上述通知" _mubu_text="%3Cspan%3E%E5%85%AC%E5%BC%80%E5%8F%91%E8%A1%8C%E8%82%A1%E4%BB%BD%E7%9A%84%E5%85%AC%E5%8F%B8%E5%BA%94%E4%BB%A5%E5%85%AC%E5%91%8A%E6%96%B9%E5%BC%8F%E4%BD%9C%E5%87%BA%E4%B8%8A%E8%BF%B0%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
