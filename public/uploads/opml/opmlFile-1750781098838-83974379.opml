              <outline text="母公司" _mubu_text="%3Cspan%3E%E6%AF%8D%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="持有子公司的股权(股份)" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%E8%82%A1%E6%9D%83(%E8%82%A1%E4%BB%BD)%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="或根据协议，能够控制、支配子公司重要事项" _mubu_text="%3Cspan%3E%E6%88%96%E6%A0%B9%E6%8D%AE%E5%8D%8F%E8%AE%AE%EF%BC%8C%E8%83%BD%E5%A4%9F%E6%8E%A7%E5%88%B6%E3%80%81%E6%94%AF%E9%85%8D%E5%AD%90%E5%85%AC%E5%8F%B8%E9%87%8D%E8%A6%81%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="母公司本质上是子公司的股东" _mubu_text="%3Cspan%3E%E6%AF%8D%E5%85%AC%E5%8F%B8%E6%9C%AC%E8%B4%A8%E4%B8%8A%E6%98%AF%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="子公司" _mubu_text="%3Cspan%3E%E5%AD%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="子公司具有法人资格，独立承担民事责任" _mubu_text="%3Cspan%3E%E5%AD%90%E5%85%AC%E5%8F%B8%E5%85%B7%E6%9C%89%E6%B3%95%E4%BA%BA%E8%B5%84%E6%A0%BC%EF%BC%8C%E7%8B%AC%E7%AB%8B%E6%89%BF%E6%8B%85%E6%B0%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="两者关系" _mubu_text="%3Cspan%3E%E4%B8%A4%E8%80%85%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="两者均具有独立法人地位，均独立担责" _mubu_text="%3Cspan%3E%E4%B8%A4%E8%80%85%E5%9D%87%E5%85%B7%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%B3%95%E4%BA%BA%E5%9C%B0%E4%BD%8D%EF%BC%8C%E5%9D%87%E7%8B%AC%E7%AB%8B%E6%8B%85%E8%B4%A3%3C/span%3E" _note="实质为两个主体" _mubu_note="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E4%B8%BA%E4%B8%A4%E4%B8%AA%E4%B8%BB%E4%BD%93%3C/span%3E"/>
                <outline text="母公司本质上是子公司的股东" _mubu_text="%3Cspan%3E%E6%AF%8D%E5%85%AC%E5%8F%B8%E6%9C%AC%E8%B4%A8%E4%B8%8A%E6%98%AF%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="公司的能力" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
          <outline text="权利能力与行为能力" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E8%83%BD%E5%8A%9B%E4%B8%8E%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="权利能力" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="存续" _mubu_text="%3Cspan%3E%E5%AD%98%E7%BB%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="始于设立登记" _mubu_text="%3Cspan%3E%E5%A7%8B%E4%BA%8E%E8%AE%BE%E7%AB%8B%E7%99%BB%E8%AE%B0%3C/span%3E" _note="营业执照签发之日为公司成立之日" _mubu_note="%3Cspan%3E%E8%90%A5%E4%B8%9A%E6%89%A7%E7%85%A7%E7%AD%BE%E5%8F%91%E4%B9%8B%E6%97%A5%E4%B8%BA%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E4%B9%8B%E6%97%A5%3C/span%3E"/>
                <outline text="终于公司注销登记" _mubu_text="%3Cspan%3E%E7%BB%88%E4%BA%8E%E5%85%AC%E5%8F%B8%E6%B3%A8%E9%94%80%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="公司的权利能力不平等" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E6%9D%83%E5%88%A9%E8%83%BD%E5%8A%9B%E4%B8%8D%E5%B9%B3%E7%AD%89%3C/span%3E" _note="受营业范围、特许经营、经营状况等因素的影响" _mubu_note="%3Cspan%3E%E5%8F%97%E8%90%A5%E4%B8%9A%E8%8C%83%E5%9B%B4%E3%80%81%E7%89%B9%E8%AE%B8%E7%BB%8F%E8%90%A5%E3%80%81%E7%BB%8F%E8%90%A5%E7%8A%B6%E5%86%B5%E7%AD%89%E5%9B%A0%E7%B4%A0%E7%9A%84%E5%BD%B1%E5%93%8D%3C/span%3E"/>
            </outline>
            <outline text="行为能力" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="通过公司的法人机关来形成和表示" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E5%85%AC%E5%8F%B8%E7%9A%84%E6%B3%95%E4%BA%BA%E6%9C%BA%E5%85%B3%E6%9D%A5%E5%BD%A2%E6%88%90%E5%92%8C%E8%A1%A8%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="两者同生同灭" _mubu_text="%3Cspan%3E%E4%B8%A4%E8%80%85%E5%90%8C%E7%94%9F%E5%90%8C%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="公司的经营范围" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E7%BB%8F%E8%90%A5%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="经营范围自治" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%8C%83%E5%9B%B4%E8%87%AA%E6%B2%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="由公司章程规定，并依法登记" _mubu_text="%3Cspan%3E%E7%94%B1%E5%85%AC%E5%8F%B8%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%EF%BC%8C%E5%B9%B6%E4%BE%9D%E6%B3%95%E7%99%BB%E8%AE%B0%3C/span%3E" _note="可修改，但应变更登记" _mubu_note="%3Cspan%3E%E5%8F%AF%E4%BF%AE%E6%94%B9%EF%BC%8C%E4%BD%86%E5%BA%94%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%3C/span%3E"/>
              <outline text="须经批准的，应依法批准" _mubu_text="%3Cspan%3E%E9%A1%BB%E7%BB%8F%E6%89%B9%E5%87%86%E7%9A%84%EF%BC%8C%E5%BA%94%E4%BE%9D%E6%B3%95%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="经营范围的效力" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%8C%83%E5%9B%B4%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="对内" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="对外" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="对外签订合同并非当然无效" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E7%AD%BE%E8%AE%A2%E5%90%88%E5%90%8C%E5%B9%B6%E9%9D%9E%E5%BD%93%E7%84%B6%E6%97%A0%E6%95%88%3C/span%3E" _note="除非为特许行业因无许可而无效" _mubu_note="%3Cspan%3E%E9%99%A4%E9%9D%9E%E4%B8%BA%E7%89%B9%E8%AE%B8%E8%A1%8C%E4%B8%9A%E5%9B%A0%E6%97%A0%E8%AE%B8%E5%8F%AF%E8%80%8C%E6%97%A0%E6%95%88%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="公司的对外投资能力" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E5%AF%B9%E5%A4%96%E6%8A%95%E8%B5%84%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="公司的转投资自由" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E8%BD%AC%E6%8A%95%E8%B5%84%E8%87%AA%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
              <outline text="公司可以向其他企业投资" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%8F%AF%E4%BB%A5%E5%90%91%E5%85%B6%E4%BB%96%E4%BC%81%E4%B8%9A%E6%8A%95%E8%B5%84%3C/span%3E" _note="法律另规除外" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%8F%A6%E8%A7%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              <outline text="公司投资范围不受限制" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%8A%95%E8%B5%84%E8%8C%83%E5%9B%B4%E4%B8%8D%E5%8F%97%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公司可以成为普通合伙人" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%8F%AF%E4%BB%A5%E6%88%90%E4%B8%BA%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="对合伙企业债务承担无限连带责任" _mubu_note="%3Cspan%3E%E5%AF%B9%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E">
                <outline text="例外：国有独资、国有企业、上市公司、公益性事业单位、社会团体" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E5%9B%BD%E6%9C%89%E7%8B%AC%E8%B5%84%E3%80%81%E5%9B%BD%E6%9C%89%E4%BC%81%E4%B8%9A%E3%80%81%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E3%80%81%E5%85%AC%E7%9B%8A%E6%80%A7%E4%BA%8B%E4%B8%9A%E5%8D%95%E4%BD%8D%E3%80%81%E7%A4%BE%E4%BC%9A%E5%9B%A2%E4%BD%93%3C/span%3E" _note="即不对所投资企业债务承担连带责任" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%B8%8D%E5%AF%B9%E6%89%80%E6%8A%95%E8%B5%84%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="公司转投资的比例不存在法定限制" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E8%BD%AC%E6%8A%95%E8%B5%84%E7%9A%84%E6%AF%94%E4%BE%8B%E4%B8%8D%E5%AD%98%E5%9C%A8%E6%B3%95%E5%AE%9A%E9%99%90%E5%88%B6%3C/span%3E" _note="依照公司章程" _mubu_note="%3Cspan%3E%E4%BE%9D%E7%85%A7%E5%85%AC%E5%8F%B8%E7%AB%A0%E7%A8%8B%3C/span%3E"/>
          </outline>
          <outline text="公司的担保能力" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E6%8B%85%E4%BF%9D%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="担保金额不存在法定限制" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E9%87%91%E9%A2%9D%E4%B8%8D%E5%AD%98%E5%9C%A8%E6%B3%95%E5%AE%9A%E9%99%90%E5%88%B6%3C/span%3E" _note="依照公司章程" _mubu_note="%3Cspan%3E%E4%BE%9D%E7%85%A7%E5%85%AC%E5%8F%B8%E7%AB%A0%E7%A8%8B%3C/span%3E"/>
            <outline text="公司担保的决策" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%8B%85%E4%BF%9D%E7%9A%84%E5%86%B3%E7%AD%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="为关联方提供担保" _mubu_text="%3Cspan%3E%E4%B8%BA%E5%85%B3%E8%81%94%E6%96%B9%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="公司为其股东或实际控制人提供担保" _mubu_note="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%BA%E5%85%B6%E8%82%A1%E4%B8%9C%E6%88%96%E5%AE%9E%E9%99%85%E6%8E%A7%E5%88%B6%E4%BA%BA%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E">
                <outline text="必须由股东(大)会决议" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E7%94%B1%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E" _note="不能是董事会" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%98%AF%E8%91%A3%E4%BA%8B%E4%BC%9A%3C/span%3E"/>
                <outline text="接受担保的人不得参加表决" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%8F%97%E6%8B%85%E4%BF%9D%E7%9A%84%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%8F%82%E5%8A%A0%E8%A1%A8%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="由出席会议的其他股东所持表决权的过半数通过" _mubu_text="%3Cspan%3E%E7%94%B1%E5%87%BA%E5%B8%AD%E4%BC%9A%E8%AE%AE%E7%9A%84%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%89%80%E6%8C%81%E8%A1%A8%E5%86%B3%E6%9D%83%E7%9A%84%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="为非关联方提供担保" _mubu_text="%3Cspan%3E%E4%B8%BA%E9%9D%9E%E5%85%B3%E8%81%94%E6%96%B9%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="由董事会或股东(大)会决议" _mubu_text="%3Cspan%3E%E7%94%B1%E8%91%A3%E4%BA%8B%E4%BC%9A%E6%88%96%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="未经决策担保有效情形" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E5%86%B3%E7%AD%96%E6%8B%85%E4%BF%9D%E6%9C%89%E6%95%88%E6%83%85%E5%BD%A2%3C/span%3E" _note="即直接认定有效" _mubu_note="%3Cspan%3E%E5%8D%B3%E7%9B%B4%E6%8E%A5%E8%AE%A4%E5%AE%9A%E6%9C%89%E6%95%88%3C/span%3E">
                <outline text="金融机构开立保函或者担保公司提供担保" _mubu_text="%3Cspan%3E%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E5%BC%80%E7%AB%8B%E4%BF%9D%E5%87%BD%E6%88%96%E8%80%85%E6%8B%85%E4%BF%9D%E5%85%AC%E5%8F%B8%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="有理由对其业务的信任而相信" _mubu_note="%3Cspan%3E%E6%9C%89%E7%90%86%E7%94%B1%E5%AF%B9%E5%85%B6%E4%B8%9A%E5%8A%A1%E7%9A%84%E4%BF%A1%E4%BB%BB%E8%80%8C%E7%9B%B8%E4%BF%A1%3C/span%3E"/>
                <outline text="公司为其全资子公司开展经营活动提供担保" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%BA%E5%85%B6%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%E5%BC%80%E5%B1%95%E7%BB%8F%E8%90%A5%E6%B4%BB%E5%8A%A8%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="受益唯一，理所当然" _mubu_note="%3Cspan%3E%E5%8F%97%E7%9B%8A%E5%94%AF%E4%B8%80%EF%BC%8C%E7%90%86%E6%89%80%E5%BD%93%E7%84%B6%3C/span%3E"/>
                <outline text="担保合同系由持有公司2/3 以上对担保事项有表决权的股东签字同意" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E7%B3%BB%E7%94%B1%E6%8C%81%E6%9C%89%E5%85%AC%E5%8F%B82/3%20%E4%BB%A5%E4%B8%8A%E5%AF%B9%E6%8B%85%E4%BF%9D%E4%BA%8B%E9%A1%B9%E6%9C%89%E8%A1%A8%E5%86%B3%E6%9D%83%E7%9A%84%E8%82%A1%E4%B8%9C%E7%AD%BE%E5%AD%97%E5%90%8C%E6%84%8F%3C/span%3E" _note="即直接在担保合同上签字" _mubu_note="%3Cspan%3E%E5%8D%B3%E7%9B%B4%E6%8E%A5%E5%9C%A8%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E4%B8%8A%E7%AD%BE%E5%AD%97%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="越权担保" _mubu_text="%3Cspan%3E%E8%B6%8A%E6%9D%83%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="公司法代越权代表公司与相对人订立担保合同" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%B3%95%E4%BB%A3%E8%B6%8A%E6%9D%83%E4%BB%A3%E8%A1%A8%E5%85%AC%E5%8F%B8%E4%B8%8E%E7%9B%B8%E5%AF%B9%E4%BA%BA%E8%AE%A2%E7%AB%8B%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公司责任" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="善意认定" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="指相对人在订立担保合同时不(应)知道法代越权" _mubu_text="%3Cspan%3E%E6%8C%87%E7%9B%B8%E5%AF%B9%E4%BA%BA%E5%9C%A8%E8%AE%A2%E7%AB%8B%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E6%97%B6%E4%B8%8D(%E5%BA%94)%E7%9F%A5%E9%81%93%E6%B3%95%E4%BB%A3%E8%B6%8A%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或有证据证明已对公司决议进行了合理审查" _mubu_text="%3Cspan%3E%E6%88%96%E6%9C%89%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E5%B7%B2%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%86%B3%E8%AE%AE%E8%BF%9B%E8%A1%8C%E4%BA%86%E5%90%88%E7%90%86%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="形式审查" _mubu_note="%3Cspan%3E%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E"/>
                  <outline text="常见考点" _mubu_text="%3Cspan%3E%E5%B8%B8%E8%A7%81%E8%80%83%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
                    <outline text="相对人没有审查任何决议的，非善意" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E4%BA%BA%E6%B2%A1%E6%9C%89%E5%AE%A1%E6%9F%A5%E4%BB%BB%E4%BD%95%E5%86%B3%E8%AE%AE%E7%9A%84%EF%BC%8C%E9%9D%9E%E5%96%84%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="越权＋对关联方担保，相对人对董事会决议进行审查的，非善意" _mubu_text="%3Cspan%3E%E8%B6%8A%E6%9D%83%EF%BC%8B%E5%AF%B9%E5%85%B3%E8%81%94%E6%96%B9%E6%8B%85%E4%BF%9D%EF%BC%8C%E7%9B%B8%E5%AF%B9%E4%BA%BA%E5%AF%B9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%3C/span%3E%3Cspan%3E%E5%86%B3%E8%AE%AE%E8%BF%9B%E8%A1%8C%E5%AE%A1%E6%9F%A5%E7%9A%84%EF%BC%8C%E9%9D%9E%E5%96%84%E6%84%8F%3C/span%3E" _note="应股东(大)会决议" _mubu_note="%3Cspan%3E%E5%BA%94%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E"/>
                    <outline text="越权＋对关联方担保，法代伪造股东会决议，&quot;全体股东&quot;签章，非善意" _mubu_text="%3Cspan%3E%E8%B6%8A%E6%9D%83%EF%BC%8B%E5%AF%B9%E5%85%B3%E8%81%94%E6%96%B9%E6%8B%85%E4%BF%9D%EF%BC%8C%E6%B3%95%E4%BB%A3%E4%BC%AA%E9%80%A0%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%22%E5%85%A8%E4%BD%93%E8%82%A1%E4%B8%9C%22%E7%AD%BE%E7%AB%A0%3C/span%3E%3Cspan%3E%EF%BC%8C%E9%9D%9E%E5%96%84%E6%84%8F%3C/span%3E" _note="该回避未回避" _mubu_note="%3Cspan%3E%E8%AF%A5%E5%9B%9E%E9%81%BF%E6%9C%AA%E5%9B%9E%E9%81%BF%3C/span%3E"/>
                  </outline>
                </outline>
                <outline text="相对人善意" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E4%BA%BA%E5%96%84%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="担保合同对公司发生效力" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="相对人非善意" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E4%BA%BA%E9%9D%9E%E5%96%84%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="担保合同对公司不发生效力" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E5%AF%B9%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="公司应承担赔偿责任" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%BA%94%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="缔约过失责任" _mubu_note="%3Cspan%3E%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="善意认定" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="已进行合理审查" _mubu_text="%3Cspan%3E%E5%B7%B2%E8%BF%9B%E8%A1%8C%E5%90%88%E7%90%86%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法代赔偿责任" _mubu_text="%3Cspan%3E%E6%B3%95%E4%BB%A3%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="民商结合：相对人非善意，担保合同对公司不发生效力时，&quot;担保人&quot;的责任" _mubu_text="%3Cspan%3E%E6%B0%91%E5%95%86%E7%BB%93%E5%90%88%EF%BC%9A%E7%9B%B8%E5%AF%B9%E4%BA%BA%E9%9D%9E%E5%96%84%E6%84%8F%EF%BC%8C%E6%8B%85%E4%BF%9D%E5%90%88%E5%90%8C%E5%AF%B9%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%E6%97%B6%EF%BC%8C%22%E6%8B%85%E4%BF%9D%E4%BA%BA%22%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="公司的设立" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="发起设立" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="全部股份均由发起人自行认购而设立" _mubu_text="%3Cspan%3E%E5%85%A8%E9%83%A8%E8%82%A1%E4%BB%BD%E5%9D%87%E7%94%B1%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%87%AA%E8%A1%8C%E8%AE%A4%E8%B4%AD%E8%80%8C%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有限公司：只能采取发起设立的方式" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8%EF%BC%9A%E5%8F%AA%E8%83%BD%E9%87%87%E5%8F%96%E5%8F%91%E8%B5%B7%E8%AE%BE%E7%AB%8B%E7%9A%84%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="募集设立" _mubu_text="%3Cspan%3E%E5%8B%9F%E9%9B%86%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="部分由发起人认购，其余股份公开或向特定对象募集" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E7%94%B1%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%AE%A4%E8%B4%AD%EF%BC%8C%E5%85%B6%E4%BD%99%E8%82%A1%E4%BB%BD%E5%85%AC%E5%BC%80%E6%88%96%E5%90%91%E7%89%B9%E5%AE%9A%E5%AF%B9%E8%B1%A1%E5%8B%9F%E9%9B%86%3C/span%3E" _note="≥35%" _mubu_note="%3Cspan%3E%E2%89%A535%25%3C/span%3E"/>
              <outline text="股份公司：可发起设立，也可募集设立" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%EF%BC%9A%E5%8F%AF%E5%8F%91%E8%B5%B7%E8%AE%BE%E7%AB%8B%EF%BC%8C%E4%B9%9F%E5%8F%AF%E5%8B%9F%E9%9B%86%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="发起人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="发起人or股东" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BAor%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note="">
              <outline text="发起人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司成立后，当然成为公司股东" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E5%90%8E%EF%BC%8C%E5%BD%93%E7%84%B6%E6%88%90%E4%B8%BA%E5%85%AC%E5%8F%B8%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="股东" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="包括参与公司设立的发起人" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E5%8F%82%E4%B8%8E%E5%85%AC%E5%8F%B8%E8%AE%BE%E7%AB%8B%E7%9A%84%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="也包括公司成立后通过受让股权或增资取得股权的股东" _mubu_text="%3Cspan%3E%E4%B9%9F%E5%8C%85%E6%8B%AC%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E5%90%8E%E9%80%9A%E8%BF%87%E5%8F%97%E8%AE%A9%E8%82%A1%E6%9D%83%E6%88%96%E5%A2%9E%E8%B5%84%E5%8F%96%E5%BE%97%E8%82%A1%E6%9D%83%E7%9A%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="发起人一定是股东，股东不一定是发起人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%B8%80%E5%AE%9A%E6%98%AF%E8%82%A1%E4%B8%9C%EF%BC%8C%E8%82%A1%E4%B8%9C%E4%B8%8D%E4%B8%80%E5%AE%9A%E6%98%AF%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="发起人的要求" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E7%9A%84%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
              <outline text="有限公司" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="1~50人" _mubu_text="%3Cspan%3E1~50%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="股份公司" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="1~200人" _mubu_text="%3Cspan%3E1~200%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="半数以上的发起人在中国境内有住所" _mubu_text="%3Cspan%3E%E5%8D%8A%E6%95%B0%E4%BB%A5%E4%B8%8A%E7%9A%84%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%9C%A8%E4%B8%AD%E5%9B%BD%E5%A2%83%E5%86%85%E6%9C%89%E4%BD%8F%E6%89%80%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="发起人责任与公司的责任" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%B4%A3%E4%BB%BB%E4%B8%8E%E5%85%AC%E5%8F%B8%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="从设立开始到公司成立" _mubu_text="%3Cspan%3E%E4%BB%8E%E8%AE%BE%E7%AB%8B%E5%BC%80%E5%A7%8B%E5%88%B0%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%3C/span%3E" _note="“设立中公司”" _mubu_note="%3Cspan%3E%E2%80%9C%E8%AE%BE%E7%AB%8B%E4%B8%AD%E5%85%AC%E5%8F%B8%E2%80%9D%3C/span%3E">
                <outline text="发起人之间属于民事合伙关系" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%B9%8B%E9%97%B4%E5%B1%9E%E4%BA%8E%E6%B0%91%E4%BA%8B%E5%90%88%E4%BC%99%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="发起人以自己名义对外签订合同" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%BB%A5%E8%87%AA%E5%B7%B1%E5%90%8D%E4%B9%89%E5%AF%B9%E5%A4%96%E7%AD%BE%E8%AE%A2%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="合同相对人有权选择要求该发起人或公司承担合同责任" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E7%9B%B8%E5%AF%B9%E4%BA%BA%E6%9C%89%E6%9D%83%E9%80%89%E6%8B%A9%E8%A6%81%E6%B1%82%E8%AF%A5%E5%8F%91%E8%B5%B7%E4%BA%BA%E6%88%96%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%E5%90%88%E5%90%8C%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="非连带" _mubu_note="%3Cspan%3E%E9%9D%9E%E8%BF%9E%E5%B8%A6%3C/span%3E"/>
              </outline>
              <outline text="发起人以设立中公司名义对外签订合同" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%BB%A5%E8%AE%BE%E7%AB%8B%E4%B8%AD%E5%85%AC%E5%8F%B8%E5%90%8D%E4%B9%89%E5%AF%B9%E5%A4%96%E7%AD%BE%E8%AE%A2%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：公司承担" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="发起人为自己的利益" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E4%B8%BA%E8%87%AA%E5%B7%B1%E7%9A%84%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="且合同相对人知情" _mubu_text="%3Cspan%3E%E4%B8%94%E5%90%88%E5%90%8C%E7%9B%B8%E5%AF%B9%E4%BA%BA%E7%9F%A5%E6%83%85%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="公司可免除责任" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%8F%AF%E5%85%8D%E9%99%A4%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="设立过程中，发起人行为致人损害" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E8%BF%87%E7%A8%8B%E4%B8%AD%EF%BC%8C%E5%8F%91%E8%B5%B7%E4%BA%BA%E8%A1%8C%E4%B8%BA%E8%87%B4%E4%BA%BA%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note="">
                <outline text="职务行为：公司承担" _mubu_text="%3Cspan%3E%E8%81%8C%E5%8A%A1%E8%A1%8C%E4%B8%BA%EF%BC%9A%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="个人行为：个人承担" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BA%BA%E8%A1%8C%E4%B8%BA%EF%BC%9A%E4%B8%AA%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="设立过程中，发起人给公司造成损客" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E8%BF%87%E7%A8%8B%E4%B8%AD%EF%BC%8C%E5%8F%91%E8%B5%B7%E4%BA%BA%E7%BB%99%E5%85%AC%E5%8F%B8%E9%80%A0%E6%88%90%E6%8D%9F%E5%AE%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="发起人对公司承担赔偿责任" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%AF%B9%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="公司不能成立时" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%8D%E8%83%BD%E6%88%90%E7%AB%8B%E6%97%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="发起人对公司设立过程中的费用、债务承担连带责任" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%E5%AF%B9%E5%85%AC%E5%8F%B8%E8%AE%BE%E7%AB%8B%E8%BF%87%E7%A8%8B%E4%B8%AD%E7%9A%84%E8%B4%B9%E7%94%A8%E3%80%81%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="内部追偿" _mubu_note="%3Cspan%3E%E5%86%85%E9%83%A8%E8%BF%BD%E5%81%BF%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="股东出资制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%82%A1%E4%B8%9C%E5%87%BA%E8%B5%84%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="出资形式" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="货币出资" _mubu_text="%3Cspan%3E%E8%B4%A7%E5%B8%81%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="应将货币对公转账" _mubu_text="%3Cspan%3E%E5%BA%94%E5%B0%86%E8%B4%A7%E5%B8%81%E5%AF%B9%E5%85%AC%E8%BD%AC%E8%B4%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="货币来源无限制" _mubu_text="%3Cspan%3E%E8%B4%A7%E5%B8%81%E6%9D%A5%E6%BA%90%E6%97%A0%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="违法犯罪所得的货币出资依然有效" _mubu_text="%3Cspan%3E%E8%BF%9D%E6%B3%95%E7%8A%AF%E7%BD%AA%E6%89%80%E5%BE%97%E7%9A%84%E8%B4%A7%E5%B8%81%E5%87%BA%E8%B5%84%E4%BE%9D%E7%84%B6%E6%9C%89%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="对违法犯罪行为予以追究、处罚" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%BF%9D%E6%B3%95%E7%8A%AF%E7%BD%AA%E8%A1%8C%E4%B8%BA%E4%BA%88%E4%BB%A5%E8%BF%BD%E7%A9%B6%E3%80%81%E5%A4%84%E7%BD%9A%3C/span%3E" _note="" _mubu_note="">
                    <outline text="出资后取得股权的，应拍/变卖方式处置其股权" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E5%90%8E%E5%8F%96%E5%BE%97%E8%82%A1%E6%9D%83%E7%9A%84%EF%BC%8C%E5%BA%94%E6%8B%8D/%E5%8F%98%E5%8D%96%E6%96%B9%E5%BC%8F%E5%A4%84%E7%BD%AE%E5%85%B6%E8%82%A1%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="非货币财产出资" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%B4%A7%E5%B8%81%E8%B4%A2%E4%BA%A7%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="实物、知识产权、土地使用权、债权等" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%89%A9%E3%80%81%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%E3%80%81%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E3%80%81%E5%80%BA%E6%9D%83%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="可用货币估价" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%A8%E8%B4%A7%E5%B8%81%E4%BC%B0%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应当评估作价，核实财产" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E8%AF%84%E4%BC%B0%E4%BD%9C%E4%BB%B7%EF%BC%8C%E6%A0%B8%E5%AE%9E%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="未评估的处理" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%AF%84%E4%BC%B0%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                      <outline text="法院补评估即可" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%A1%A5%E8%AF%84%E4%BC%B0%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                  <outline text="可依法转让" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BE%9D%E6%B3%95%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="以无处分权的财产出资" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%97%A0%E5%A4%84%E5%88%86%E6%9D%83%E7%9A%84%E8%B4%A2%E4%BA%A7%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="以善意取得规则予以认定" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%96%84%E6%84%8F%E5%8F%96%E5%BE%97%E8%A7%84%E5%88%99%E4%BA%88%E4%BB%A5%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="无权处分" _mubu_note="%3Cspan%3E%E6%97%A0%E6%9D%83%E5%A4%84%E5%88%86%3C/span%3E"/>
                </outline>
                <outline text="以房屋、土地使用权或需办权属登记的知识产权等财产出资" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%88%BF%E5%B1%8B%E3%80%81%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E6%88%96%E9%9C%80%E5%8A%9E%E6%9D%83%E5%B1%9E%E7%99%BB%E8%AE%B0%E7%9A%84%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%E7%AD%89%E8%B4%A2%E4%BA%A7%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="已交付，但未办变更登记" _mubu_text="%3Cspan%3E%E5%B7%B2%E4%BA%A4%E4%BB%98%EF%BC%8C%E4%BD%86%E6%9C%AA%E5%8A%9E%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应在指定合理期间内办理变更手续" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E6%8C%87%E5%AE%9A%E5%90%88%E7%90%86%E6%9C%9F%E9%97%B4%E5%86%85%E5%8A%9E%E7%90%86%E5%8F%98%E6%9B%B4%E6%89%8B%E7%BB%AD%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="已办变更登记，但未交付" _mubu_text="%3Cspan%3E%E5%B7%B2%E5%8A%9E%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%EF%BC%8C%E4%BD%86%E6%9C%AA%E4%BA%A4%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应向公司交付" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E5%85%AC%E5%8F%B8%E4%BA%A4%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="实际交付前，不享有相应股东权利" _mubu_text="%3Cspan%3E%E5%AE%9E%E9%99%85%E4%BA%A4%E4%BB%98%E5%89%8D%EF%BC%8C%E4%B8%8D%E4%BA%AB%E6%9C%89%E7%9B%B8%E5%BA%94%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="土地使用权出资" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="应以“出让”土地使用权出资" _mubu_text="%3Cspan%3E%E5%BA%94%E4%BB%A5%E2%80%9C%E5%87%BA%E8%AE%A9%E2%80%9D%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="有瑕疵" _mubu_text="%3Cspan%3E%E6%9C%89%E7%91%95%E7%96%B5%3C/span%3E" _note="" _mubu_note="">
                    <outline text="以“划拨”土地使用权出资的" _mubu_text="%3Cspan%3E%E4%BB%A5%E2%80%9C%E5%88%92%E6%8B%A8%E2%80%9D%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%B5%84%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                      <outline text="合理期间内办理土地变更手续" _mubu_text="%3Cspan%3E%E5%90%88%E7%90%86%E6%9C%9F%E9%97%B4%E5%86%85%E5%8A%9E%E7%90%86%E5%9C%9F%E5%9C%B0%E5%8F%98%E6%9B%B4%E6%89%8B%E7%BB%AD%3C/span%3E" _note="划拨→出让" _mubu_note="%3Cspan%3E%E5%88%92%E6%8B%A8%E2%86%92%E5%87%BA%E8%AE%A9%3C/span%3E"/>
                      <outline text="逾期未办理，认定未依法全面履行出资义务" _mubu_text="%3Cspan%3E%E9%80%BE%E6%9C%9F%E6%9C%AA%E5%8A%9E%E7%90%86%EF%BC%8C%E8%AE%A4%E5%AE%9A%E6%9C%AA%E4%BE%9D%E6%B3%95%E5%85%A8%E9%9D%A2%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="以设定权利负担的土地使用权出资的" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%AE%BE%E5%AE%9A%E6%9D%83%E5%88%A9%E8%B4%9F%E6%8B%85%E7%9A%84%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%B5%84%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                      <outline text="合理期限内解除权利负担" _mubu_text="%3Cspan%3E%E5%90%88%E7%90%86%E6%9C%9F%E9%99%90%E5%86%85%E8%A7%A3%E9%99%A4%E6%9D%83%E5%88%A9%E8%B4%9F%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="逾期未办理，认定未依法全面履行出资义务" _mubu_text="%3Cspan%3E%E9%80%BE%E6%9C%9F%E6%9C%AA%E5%8A%9E%E7%90%86%EF%BC%8C%E8%AE%A4%E5%AE%9A%E6%9C%AA%E4%BE%9D%E6%B3%95%E5%85%A8%E9%9D%A2%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                </outline>
                <outline text="股权出资" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="出资人合法持有并依法可以转让" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E4%BA%BA%E5%90%88%E6%B3%95%E6%8C%81%E6%9C%89%E5%B9%B6%E4%BE%9D%E6%B3%95%E5%8F%AF%E4%BB%A5%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="股权无权利瑕疵或者权利负担" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E6%97%A0%E6%9D%83%E5%88%A9%E7%91%95%E7%96%B5%E6%88%96%E8%80%85%E6%9D%83%E5%88%A9%E8%B4%9F%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="出资人已履行关于股权转让的法定手续" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E4%BA%BA%E5%B7%B2%E5%B1%A5%E8%A1%8C%E5%85%B3%E4%BA%8E%E8%82%A1%E6%9D%83%E8%BD%AC%E8%AE%A9%E7%9A%84%E6%B3%95%E5%AE%9A%E6%89%8B%E7%BB%AD%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="股权已依法进行了价值评估" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E5%B7%B2%E4%BE%9D%E6%B3%95%E8%BF%9B%E8%A1%8C%E4%BA%86%E4%BB%B7%E5%80%BC%E8%AF%84%E4%BC%B0%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="不符条件的" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AC%A6%E6%9D%A1%E4%BB%B6%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="合理期间内补正即可" _mubu_text="%3Cspan%3E%E5%90%88%E7%90%86%E6%9C%9F%E9%97%B4%E5%86%85%E8%A1%A5%E6%AD%A3%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="逾期未补正，认定未依法全面履行出资义务" _mubu_text="%3Cspan%3E%E9%80%BE%E6%9C%9F%E6%9C%AA%E8%A1%A5%E6%AD%A3%EF%BC%8C%E8%AE%A4%E5%AE%9A%E6%9C%AA%E4%BE%9D%E6%B3%95%E5%85%A8%E9%9D%A2%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="债权出资" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="本质上是债权让与" _mubu_text="%3Cspan%3E%E6%9C%AC%E8%B4%A8%E4%B8%8A%E6%98%AF%E5%80%BA%E6%9D%83%E8%AE%A9%E4%B8%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="禁止的出资形式" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E7%9A%84%E5%87%BA%E8%B5%84%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="劳务、信用、自然人姓名、商誉、特许经营权、设担保的财产等" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E3%80%81%E4%BF%A1%E7%94%A8%E3%80%81%E8%87%AA%E7%84%B6%E4%BA%BA%E5%A7%93%E5%90%8D%E3%80%81%E5%95%86%E8%AA%89%E3%80%81%E7%89%B9%E8%AE%B8%E7%BB%8F%E8%90%A5%E6%9D%83%E3%80%81%E8%AE%BE%E6%8B%85%E4%BF%9D%E7%9A%84%E8%B4%A2%E4%BA%A7%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="出资方式" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="取认缴资本制" _mubu_text="%3Cspan%3E%E5%8F%96%E8%AE%A4%E7%BC%B4%E8%B5%84%E6%9C%AC%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="无最低注册资本" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%80%E4%BD%8E%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%3C/span%3E" _note="" _mubu_note="">
                  <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                    <outline text="金融机构" _mubu_text="%3Cspan%3E%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="上市公司" _mubu_text="%3Cspan%3E%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="出资期限" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%87%BA%E8%B5%84%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="可分期出资" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%88%86%E6%9C%9F%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="按章程规定自公司成立之日起5年内缴足" _mubu_text="%3Cspan%3E%E6%8C%89%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%E8%87%AA%E5%85%AC%E5%8F%B8%E6%88%90%E7%AB%8B%E4%B9%8B%E6%97%A5%E8%B5%B75%E5%B9%B4%E5%86%85%E7%BC%B4%E8%B6%B3%3C/span%3E" _note="期限届满，股东享有出资的期限利益" _mubu_note="%3Cspan%3E%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%EF%BC%8C%E8%82%A1%E4%B8%9C%E4%BA%AB%E6%9C%89%E5%87%BA%E8%B5%84%E7%9A%84%E6%9C%9F%E9%99%90%E5%88%A9%E7%9B%8A%3C/span%3E"/>
                  </outline>
                  <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                    <outline text="金融机构、股份公司" _mubu_text="%3Cspan%3E%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E3%80%81%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%3C/span%3E" _note="要求实缴注册资本，即股东应一次性缴纳全部出资" _mubu_note="%3Cspan%3E%E8%A6%81%E6%B1%82%E5%AE%9E%E7%BC%B4%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%EF%BC%8C%E5%8D%B3%E8%82%A1%E4%B8%9C%E5%BA%94%E4%B8%80%E6%AC%A1%E6%80%A7%E7%BC%B4%E7%BA%B3%E5%85%A8%E9%83%A8%E5%87%BA%E8%B5%84%3C/span%3E"/>
                  </outline>
                </outline>
                <outline text="无法定的验资要求" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E5%AE%9A%E7%9A%84%E9%AA%8C%E8%B5%84%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                  <outline text="募集设立的股份公司应当验资" _mubu_text="%3Cspan%3E%E5%8B%9F%E9%9B%86%E8%AE%BE%E7%AB%8B%E7%9A%84%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%E5%BA%94%E5%BD%93%E9%AA%8C%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="出资责任" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="对内" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="未按期足额缴纳出资" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%9C%AA%E6%8C%89%E6%9C%9F%E8%B6%B3%E9%A2%9D%E7%BC%B4%E7%BA%B3%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                    <outline text="迟延履行出资义务" _mubu_text="%3Cspan%3E%E8%BF%9F%E5%BB%B6%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="货币出资不足额" _mubu_text="%3Cspan%3E%E8%B4%A7%E5%B8%81%E5%87%BA%E8%B5%84%E4%B8%8D%E8%B6%B3%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="不出资" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="非货币财产的实际价额显著低于所认缴的出资额" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%B4%A7%E5%B8%81%E8%B4%A2%E4%BA%A7%E7%9A%84%E5%AE%9E%E9%99%85%E4%BB%B7%E9%A2%9D%E6%98%BE%E8%91%97%E4%BD%8E%E4%BA%8E%E6%89%80%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                      <outline text="因市场变化或者其他客观因素导致出资财产贬值，不认定为出资不实" _mubu_text="%3Cspan%3E%E5%9B%A0%E5%B8%82%E5%9C%BA%E5%8F%98%E5%8C%96%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E5%AE%A2%E8%A7%82%E5%9B%A0%E7%B4%A0%E5%AF%BC%E8%87%B4%E5%87%BA%E8%B5%84%E8%B4%A2%E4%BA%A7%E8%B4%AC%E5%80%BC%EF%BC%8C%E4%B8%8D%E8%AE%A4%E5%AE%9A%E4%B8%BA%E5%87%BA%E8%B5%84%E4%B8%8D%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                  <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                    <outline text="该股东向公司补足差额" _mubu_text="%3Cspan%3E%E8%AF%A5%E8%82%A1%E4%B8%9C%E5%90%91%E5%85%AC%E5%8F%B8%E8%A1%A5%E8%B6%B3%E5%B7%AE%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="设立时的其他股东与该股东在出资不足的范围内向公司承担连带任" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E6%97%B6%E7%9A%84%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E4%B8%8E%E8%AF%A5%E8%82%A1%E4%B8%9C%E5%9C%A8%E5%87%BA%E8%B5%84%E4%B8%8D%E8%B6%B3%E7%9A%84%E8%8C%83%E5%9B%B4%E5%86%85%E5%90%91%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="抽逃出资" _mubu_text="%3Cspan%3E%E6%8A%BD%E9%80%83%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="要损害公司权益" _mubu_text="%3Cspan%3E%E8%A6%81%E6%8D%9F%E5%AE%B3%E5%85%AC%E5%8F%B8%E6%9D%83%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
                    <outline text="公司、股东或公司债权人均可请求认定" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E3%80%81%E8%82%A1%E4%B8%9C%E6%88%96%E5%85%AC%E5%8F%B8%E5%80%BA%E6%9D%83%E4%BA%BA%E5%9D%87%E5%8F%AF%E8%AF%B7%E6%B1%82%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                    <outline text="制作虚假财务会计报表虚增利润进行分配" _mubu_text="%3Cspan%3E%E5%88%B6%E4%BD%9C%E8%99%9A%E5%81%87%E8%B4%A2%E5%8A%A1%E4%BC%9A%E8%AE%A1%E6%8A%A5%E8%A1%A8%E8%99%9A%E5%A2%9E%E5%88%A9%E6%B6%A6%E8%BF%9B%E8%A1%8C%E5%88%86%E9%85%8D%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="虚构债权债务关系将其出资转出" _mubu_text="%3Cspan%3E%E8%99%9A%E6%9E%84%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E5%85%B3%E7%B3%BB%E5%B0%86%E5%85%B6%E5%87%BA%E8%B5%84%E8%BD%AC%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="利用关联交易将出资转出" _mubu_text="%3Cspan%3E%E5%88%A9%E7%94%A8%E5%85%B3%E8%81%94%E4%BA%A4%E6%98%93%E5%B0%86%E5%87%BA%E8%B5%84%E8%BD%AC%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                    <outline text="返还抽逃的出资" _mubu_text="%3Cspan%3E%E8%BF%94%E8%BF%98%E6%8A%BD%E9%80%83%E7%9A%84%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                      <outline text="抽逃出资的数额其出资的数额为限" _mubu_text="%3Cspan%3E%E6%8A%BD%E9%80%83%E5%87%BA%E8%B5%84%E7%9A%84%E6%95%B0%E9%A2%9D%E5%85%B6%E5%87%BA%E8%B5%84%E7%9A%84%E6%95%B0%E9%A2%9D%E4%B8%BA%E9%99%90%3C/span%3E" _note="超出部分按侵害公司财产权处理" _mubu_note="%3Cspan%3E%E8%B6%85%E5%87%BA%E9%83%A8%E5%88%86%E6%8C%89%E4%BE%B5%E5%AE%B3%E5%85%AC%E5%8F%B8%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%A4%84%E7%90%86%3C/span%3E"/>
                    </outline>
                    <outline text="负有责任的董监高承担连带责任" _mubu_text="%3Cspan%3E%E8%B4%9F%E6%9C%89%E8%B4%A3%E4%BB%BB%E7%9A%84%E8%91%A3%E7%9B%91%E9%AB%98%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="对外" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="未(全面)履行出资义务股东的对外责任" _mubu_text="%3Cspan%3E%E6%9C%AA(%E5%85%A8%E9%9D%A2)%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%B9%89%E5%8A%A1%E8%82%A1%E4%B8%9C%E7%9A%84%E5%AF%B9%E5%A4%96%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="将股东的对内责任转化为对外责任" _mubu_note="%3Cspan%3E%E5%B0%86%E8%82%A1%E4%B8%9C%E7%9A%84%E5%AF%B9%E5%86%85%E8%B4%A3%E4%BB%BB%E8%BD%AC%E5%8C%96%E4%B8%BA%E5%AF%B9%E5%A4%96%E8%B4%A3%E4%BB%BB%3C/span%3E">
                  <outline text="股东对外责任" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%AF%B9%E5%A4%96%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                    <outline text="有限责任：在未出资本息范围内" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%EF%BC%9A%E5%9C%A8%E6%9C%AA%E5%87%BA%E8%B5%84%E6%9C%AC%E6%81%AF%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="补充责任：对公司债务不能清偿的部分承担" _mubu_text="%3Cspan%3E%E8%A1%A5%E5%85%85%E8%B4%A3%E4%BB%BB%EF%BC%9A%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%80%BA%E5%8A%A1%E4%B8%8D%E8%83%BD%E6%B8%85%E5%81%BF%E7%9A%84%E9%83%A8%E5%88%86%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
