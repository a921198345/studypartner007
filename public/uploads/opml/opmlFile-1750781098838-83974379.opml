                <outline text="发行人、上市公司的董监高和其他直接责任人员" _mubu_text="%3Cspan%3E%E5%8F%91%E8%A1%8C%E4%BA%BA%E3%80%81%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E7%9A%84%E8%91%A3%E7%9B%91%E9%AB%98%E5%92%8C%E5%85%B6%E4%BB%96%E7%9B%B4%E6%8E%A5%E8%B4%A3%E4%BB%BB%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="保荐人、承销的证券公司" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%8D%90%E4%BA%BA%E3%80%81%E6%89%BF%E9%94%80%E7%9A%84%E8%AF%81%E5%88%B8%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="发行人的控股股东、实际控制人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%A1%8C%E4%BA%BA%E7%9A%84%E6%8E%A7%E8%82%A1%E8%82%A1%E4%B8%9C%E3%80%81%E5%AE%9E%E9%99%85%E6%8E%A7%E5%88%B6%E4%BA%BA%3C/span%3E" _note="与发行人承担连带赔偿责任" _mubu_note="%3Cspan%3E%E4%B8%8E%E5%8F%91%E8%A1%8C%E4%BA%BA%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="上市公司的收购" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E7%9A%84%E6%94%B6%E8%B4%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="收购方式" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%AD%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="要约收购" _mubu_text="%3Cspan%3E%E8%A6%81%E7%BA%A6%E6%94%B6%E8%B4%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="协议收购" _mubu_text="%3Cspan%3E%E5%8D%8F%E8%AE%AE%E6%94%B6%E8%B4%AD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="收购中的信息披露" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%AD%E4%B8%AD%E7%9A%84%E4%BF%A1%E6%81%AF%E6%8A%AB%E9%9C%B2%3C/span%3E" _note="" _mubu_note="">
              <outline text="持有达到5％" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E8%BE%BE%E5%88%B05%EF%BC%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="应在该事实发生之日起3日内" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E8%AF%A5%E4%BA%8B%E5%AE%9E%E5%8F%91%E7%94%9F%E4%B9%8B%E6%97%A5%E8%B5%B73%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="该期限内，不得再行买卖该上市公司的股票" _mubu_text="%3Cspan%3E%E8%AF%A5%E6%9C%9F%E9%99%90%E5%86%85%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%86%8D%E8%A1%8C%E4%B9%B0%E5%8D%96%E8%AF%A5%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E7%9A%84%E8%82%A1%E7%A5%A8%3C/span%3E" _note="冷静期" _mubu_note="%3Cspan%3E%E5%86%B7%E9%9D%99%E6%9C%9F%3C/span%3E"/>
                </outline>
                <outline text="向证监会、交易所作出书面报告" _mubu_text="%3Cspan%3E%E5%90%91%E8%AF%81%E7%9B%91%E4%BC%9A%E3%80%81%E4%BA%A4%E6%98%93%E6%89%80%E4%BD%9C%E5%87%BA%E4%B9%A6%E9%9D%A2%E6%8A%A5%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="通知该上市公司" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%E8%AF%A5%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="并予公告" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%BA%88%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="达到5％后，每加或减5%" _mubu_text="%3Cspan%3E%E8%BE%BE%E5%88%B05%EF%BC%85%E5%90%8E%EF%BC%8C%E6%AF%8F%E5%8A%A0%E6%88%96%E5%87%8F5%25%3C/span%3E" _note="" _mubu_note="">
                <outline text="报告+公告" _mubu_text="%3Cspan%3E%E6%8A%A5%E5%91%8A+%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="冷静期" _mubu_text="%3Cspan%3E%E5%86%B7%E9%9D%99%E6%9C%9F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="该事实发生之日起至公告后3日内" _mubu_text="%3Cspan%3E%E8%AF%A5%E4%BA%8B%E5%AE%9E%E5%8F%91%E7%94%9F%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%87%B3%E5%85%AC%E5%91%8A%E5%90%8E3%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="达到5％后，每加或减1%" _mubu_text="%3Cspan%3E%E8%BE%BE%E5%88%B05%EF%BC%85%E5%90%8E%EF%BC%8C%E6%AF%8F%E5%8A%A0%E6%88%96%E5%87%8F1%25%3C/span%3E" _note="" _mubu_note="">
                <outline text="该事实发生的次日" _mubu_text="%3Cspan%3E%E8%AF%A5%E4%BA%8B%E5%AE%9E%E5%8F%91%E7%94%9F%E7%9A%84%E6%AC%A1%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="通知该上市公司" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%E8%AF%A5%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="并予公告" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%BA%88%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%227nZwsYXgGg%22%2C%22oh%22%3A300%2C%22ow%22%3A548%2C%22uri%22%3A%22document_image%2F5c3156a8-0110-4c55-9c69-4a32acdb9348-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
            </outline>
            <outline text="要约收购" _mubu_text="%3Cspan%3E%E8%A6%81%E7%BA%A6%E6%94%B6%E8%B4%AD%3C/span%3E" _note="" _mubu_note="">
              <outline text="强制要约收购" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E8%A6%81%E7%BA%A6%E6%94%B6%E8%B4%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="持有达有表决权股份的30%，并继续收购的" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E8%BE%BE%E6%9C%89%E8%A1%A8%E5%86%B3%E6%9D%83%E8%82%A1%E4%BB%BD%E7%9A%8430%25%EF%BC%8C%E5%B9%B6%E7%BB%A7%E7%BB%AD%E6%94%B6%E8%B4%AD%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="应向该上市公司其他所有股东发出收购要约" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E8%AF%A5%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E5%85%B6%E4%BB%96%E6%89%80%E6%9C%89%E8%82%A1%E4%B8%9C%E5%8F%91%E5%87%BA%E6%94%B6%E8%B4%AD%E8%A6%81%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="30日≤≤60日" _mubu_text="%3Cspan%3E30%E6%97%A5%E2%89%A4%E2%89%A460%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="期限届满" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不符合上市交易的应终止上市交易" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AC%A6%E5%90%88%E4%B8%8A%E5%B8%82%E4%BA%A4%E6%98%93%E7%9A%84%E5%BA%94%E7%BB%88%E6%AD%A2%E4%B8%8A%E5%B8%82%E4%BA%A4%E6%98%93%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其余持有股东有权向收购人以要约同等条件出售" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BD%99%E6%8C%81%E6%9C%89%E8%82%A1%E4%B8%9C%E6%9C%89%E6%9D%83%E5%90%91%E6%94%B6%E8%B4%AD%E4%BA%BA%E4%BB%A5%E8%A6%81%E7%BA%A6%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E5%87%BA%E5%94%AE%3C/span%3E" _note="收购人应当收购" _mubu_note="%3Cspan%3E%E6%94%B6%E8%B4%AD%E4%BA%BA%E5%BA%94%E5%BD%93%E6%94%B6%E8%B4%AD%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="要约不得撤回" _mubu_text="%3Cspan%3E%E8%A6%81%E7%BA%A6%E4%B8%8D%E5%BE%97%E6%92%A4%E5%9B%9E%3C/span%3E" _note="" _mubu_note="">
                <outline text="但可变更" _mubu_text="%3Cspan%3E%E4%BD%86%E5%8F%AF%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="应及时公告" _mubu_text="%3Cspan%3E%E5%BA%94%E5%8F%8A%E6%97%B6%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="条件不得变得更坏" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%E4%B8%8D%E5%BE%97%E5%8F%98%E5%BE%97%E6%9B%B4%E5%9D%8F%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="条件公平对待所有股东" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%E5%85%AC%E5%B9%B3%E5%AF%B9%E5%BE%85%E6%89%80%E6%9C%89%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可就全部股份，也可就部分" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%B0%B1%E5%85%A8%E9%83%A8%E8%82%A1%E4%BB%BD%EF%BC%8C%E4%B9%9F%E5%8F%AF%E5%B0%B1%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="限制" _mubu_text="%3Cspan%3E%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="收购人收购期限内，不得卖出" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%AD%E4%BA%BA%E6%94%B6%E8%B4%AD%E6%9C%9F%E9%99%90%E5%86%85%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%8D%96%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得采取要约以外形式和超出要约条件" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E9%87%87%E5%8F%96%E8%A6%81%E7%BA%A6%E4%BB%A5%E5%A4%96%E5%BD%A2%E5%BC%8F%E5%92%8C%E8%B6%85%E5%87%BA%E8%A6%81%E7%BA%A6%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="收购完成后18个月内不得转让" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%AD%E5%AE%8C%E6%88%90%E5%90%8E18%E4%B8%AA%E6%9C%88%E5%86%85%E4%B8%8D%E5%BE%97%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="投资者保护" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E8%80%85%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="普通投资者与证券公司纠纷" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E6%8A%95%E8%B5%84%E8%80%85%E4%B8%8E%E8%AF%81%E5%88%B8%E5%85%AC%E5%8F%B8%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note="">
              <outline text="证券公司应证明其合规，不存在误导、欺诈" _mubu_text="%3Cspan%3E%E8%AF%81%E5%88%B8%E5%85%AC%E5%8F%B8%E5%BA%94%E8%AF%81%E6%98%8E%E5%85%B6%E5%90%88%E8%A7%84%EF%BC%8C%E4%B8%8D%E5%AD%98%E5%9C%A8%E8%AF%AF%E5%AF%BC%E3%80%81%E6%AC%BA%E8%AF%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="征集股东权利" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%BE%81%E9%9B%86%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="征集人" _mubu_text="%3Cspan%3E%E5%BE%81%E9%9B%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="上市公司董事会" _mubu_text="%3Cspan%3E%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E8%91%A3%E4%BA%8B%E4%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="独立董事" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E8%91%A3%E4%BA%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="持有1％以上有表决权股份的股东" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%891%EF%BC%85%E4%BB%A5%E4%B8%8A%E6%9C%89%E8%A1%A8%E5%86%B3%E6%9D%83%E8%82%A1%E4%BB%BD%E7%9A%84%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="投资者保护机构" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E8%80%85%E4%BF%9D%E6%8A%A4%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="行使" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="自行或者委托证券公司、证券服务机构" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%E6%88%96%E8%80%85%E5%A7%94%E6%89%98%E8%AF%81%E5%88%B8%E5%85%AC%E5%8F%B8%E3%80%81%E8%AF%81%E5%88%B8%E6%9C%8D%E5%8A%A1%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公开请求上市公司股东委托其代为出席股东大会，并代为行使提案权、表决权等股东权利" _mubu_text="%3Cspan%3E%E5%85%AC%E5%BC%80%E8%AF%B7%E6%B1%82%E4%B8%8A%E5%B8%82%E5%85%AC%E5%8F%B8%E8%82%A1%E4%B8%9C%E5%A7%94%E6%89%98%E5%85%B6%E4%BB%A3%E4%B8%BA%E5%87%BA%E5%B8%AD%E8%82%A1%E4%B8%9C%E5%A4%A7%E4%BC%9A%EF%BC%8C%E5%B9%B6%E4%BB%A3%E4%B8%BA%E8%A1%8C%E4%BD%BF%E6%8F%90%E6%A1%88%E6%9D%83%E3%80%81%E8%A1%A8%E5%86%B3%E6%9D%83%E7%AD%89%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="禁止" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="禁止以有偿或者变相有偿的方式公开征集股东权利" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E4%BB%A5%E6%9C%89%E5%81%BF%E6%88%96%E8%80%85%E5%8F%98%E7%9B%B8%E6%9C%89%E5%81%BF%E7%9A%84%E6%96%B9%E5%BC%8F%E5%85%AC%E5%BC%80%E5%BE%81%E9%9B%86%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="代表人诉讼制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BB%A3%E8%A1%A8%E4%BA%BA%E8%AF%89%E8%AE%BC%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="证券民事赔偿诉讼" _mubu_text="%3Cspan%3E%E8%AF%81%E5%88%B8%E6%B0%91%E4%BA%8B%E8%B5%94%E5%81%BF%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="标的是同一种类" _mubu_text="%3Cspan%3E%E6%A0%87%E7%9A%84%E6%98%AF%E5%90%8C%E4%B8%80%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="当事人一方人数众多" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%80%E6%96%B9%E4%BA%BA%E6%95%B0%E4%BC%97%E5%A4%9A%3C/span%3E" _note="＞10" _mubu_note="%3Cspan%3E%EF%BC%9E10%3C/span%3E"/>
              </outline>
              <outline text="法院发出公告，通知投资者在一定期间向法院登记" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%91%E5%87%BA%E5%85%AC%E5%91%8A%EF%BC%8C%E9%80%9A%E7%9F%A5%E6%8A%95%E8%B5%84%E8%80%85%E5%9C%A8%E4%B8%80%E5%AE%9A%E6%9C%9F%E9%97%B4%E5%90%91%E6%B3%95%E9%99%A2%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院判裁，对参加登记的投资者发生效力" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%88%A4%E8%A3%81%EF%BC%8C%E5%AF%B9%E5%8F%82%E5%8A%A0%E7%99%BB%E8%AE%B0%E7%9A%84%E6%8A%95%E8%B5%84%E8%80%85%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="投资者保护机构" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E8%80%85%E4%BF%9D%E6%8A%A4%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="其受50名以上投资者委托，可以作为代表人参加诉讼" _mubu_text="%3Cspan%3E%E5%85%B6%E5%8F%9750%E5%90%8D%E4%BB%A5%E4%B8%8A%E6%8A%95%E8%B5%84%E8%80%85%E5%A7%94%E6%89%98%EF%BC%8C%E5%8F%AF%E4%BB%A5%E4%BD%9C%E4%B8%BA%E4%BB%A3%E8%A1%A8%E4%BA%BA%E5%8F%82%E5%8A%A0%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="其为确认的权利人向法院登记，但明确表示不愿参加该诉讼的除外" _mubu_text="%3Cspan%3E%E5%85%B6%E4%B8%BA%E7%A1%AE%E8%AE%A4%E7%9A%84%E6%9D%83%E5%88%A9%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E7%99%BB%E8%AE%B0%EF%BC%8C%E4%BD%86%E6%98%8E%E7%A1%AE%E8%A1%A8%E7%A4%BA%E4%B8%8D%E6%84%BF%E5%8F%82%E5%8A%A0%E8%AF%A5%E8%AF%89%E8%AE%BC%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="默示加入，明示退出" _mubu_note="%3Cspan%3E%E9%BB%98%E7%A4%BA%E5%8A%A0%E5%85%A5%EF%BC%8C%E6%98%8E%E7%A4%BA%E9%80%80%E5%87%BA%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="证券投资基金法" _mubu_text="%3Cspan%3E%E8%AF%81%E5%88%B8%E6%8A%95%E8%B5%84%E5%9F%BA%E9%87%91%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="基金分类" _mubu_text="%3Cspan%3E%E5%9F%BA%E9%87%91%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="公募基金" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8B%9F%E5%9F%BA%E9%87%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="必须经证监会注册" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E7%BB%8F%E8%AF%81%E7%9B%91%E4%BC%9A%E6%B3%A8%E5%86%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="(不)特定对象超过200人" _mubu_text="%3Cspan%3E(%E4%B8%8D)%E7%89%B9%E5%AE%9A%E5%AF%B9%E8%B1%A1%E8%B6%85%E8%BF%87200%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="基金合同" _mubu_text="%3Cspan%3E%E5%9F%BA%E9%87%91%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="投资人缴纳认购款时成立" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E4%BA%BA%E7%BC%B4%E7%BA%B3%E8%AE%A4%E8%B4%AD%E6%AC%BE%E6%97%B6%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="管理人向证监会备案后生效" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E4%BA%BA%E5%90%91%E8%AF%81%E7%9B%91%E4%BC%9A%E5%A4%87%E6%A1%88%E5%90%8E%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="设立失败" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E5%A4%B1%E8%B4%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="管理人承担债务及费用" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E4%BA%BA%E6%89%BF%E6%8B%85%E5%80%BA%E5%8A%A1%E5%8F%8A%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="募集期满30日内向投资人返还款项并加算利息" _mubu_text="%3Cspan%3E%E5%8B%9F%E9%9B%86%E6%9C%9F%E6%BB%A130%E6%97%A5%E5%86%85%E5%90%91%E6%8A%95%E8%B5%84%E4%BA%BA%E8%BF%94%E8%BF%98%E6%AC%BE%E9%A1%B9%E5%B9%B6%E5%8A%A0%E7%AE%97%E5%88%A9%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不得买卖其他基金份额" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%B9%B0%E5%8D%96%E5%85%B6%E4%BB%96%E5%9F%BA%E9%87%91%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="私募基金" _mubu_text="%3Cspan%3E%E7%A7%81%E5%8B%9F%E5%9F%BA%E9%87%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="向合格投资者募集" _mubu_text="%3Cspan%3E%E5%90%91%E5%90%88%E6%A0%BC%E6%8A%95%E8%B5%84%E8%80%85%E5%8B%9F%E9%9B%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="≤200人" _mubu_text="%3Cspan%3E%E2%89%A4200%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不得向不特定对象宣传推介" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%90%91%E4%B8%8D%E7%89%B9%E5%AE%9A%E5%AF%B9%E8%B1%A1%E5%AE%A3%E4%BC%A0%E6%8E%A8%E4%BB%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可以买卖其他基金份额" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B9%B0%E5%8D%96%E5%85%B6%E4%BB%96%E5%9F%BA%E9%87%91%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="法律关系" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="管理人" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="私募" _mubu_text="%3Cspan%3E%E7%A7%81%E5%8B%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="基金管理人可以是合伙企业" _mubu_text="%3Cspan%3E%E5%9F%BA%E9%87%91%E7%AE%A1%E7%90%86%E4%BA%BA%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="应当向证券投资基金协会登记" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%90%91%E8%AF%81%E5%88%B8%E6%8A%95%E8%B5%84%E5%9F%BA%E9%87%91%E5%8D%8F%E4%BC%9A%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="公募" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8B%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="由基金管理公司或经证监会核准的其他机构担任" _mubu_text="%3Cspan%3E%E7%94%B1%E5%9F%BA%E9%87%91%E7%AE%A1%E7%90%86%E5%85%AC%E5%8F%B8%E6%88%96%E7%BB%8F%E8%AF%81%E7%9B%91%E4%BC%9A%E6%A0%B8%E5%87%86%E7%9A%84%E5%85%B6%E4%BB%96%E6%9C%BA%E6%9E%84%E6%8B%85%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="设立的注册资本不低于1亿元人民币，且必须为实缴货币资本" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E7%9A%84%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%E4%B8%8D%E4%BD%8E%E4%BA%8E1%E4%BA%BF%E5%85%83%E4%BA%BA%E6%B0%91%E5%B8%81%EF%BC%8C%E4%B8%94%E5%BF%85%E9%A1%BB%E4%B8%BA%E5%AE%9E%E7%BC%B4%E8%B4%A7%E5%B8%81%E8%B5%84%E6%9C%AC%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="托管人" _mubu_text="%3Cspan%3E%E6%89%98%E7%AE%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="依法设立的商业银行或者其他金融机构担任" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E8%AE%BE%E7%AB%8B%E7%9A%84%E5%95%86%E4%B8%9A%E9%93%B6%E8%A1%8C%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E6%8B%85%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="基金托管人与基金管理人不得为同一人，不得相互出资或者持有股份" _mubu_text="%3Cspan%3E%E5%9F%BA%E9%87%91%E6%89%98%E7%AE%A1%E4%BA%BA%E4%B8%8E%E5%9F%BA%E9%87%91%E7%AE%A1%E7%90%86%E4%BA%BA%E4%B8%8D%E5%BE%97%E4%B8%BA%E5%90%8C%E4%B8%80%E4%BA%BA%EF%BC%8C%E4%B8%8D%E5%BE%97%E7%9B%B8%E4%BA%92%E5%87%BA%E8%B5%84%E6%88%96%E8%80%85%E6%8C%81%E6%9C%89%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="份额持有人" _mubu_text="%3Cspan%3E%E4%BB%BD%E9%A2%9D%E6%8C%81%E6%9C%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="公募" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8B%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="收益风险按其持有份额" _mubu_text="%3Cspan%3E%E6%94%B6%E7%9B%8A%E9%A3%8E%E9%99%A9%E6%8C%89%E5%85%B6%E6%8C%81%E6%9C%89%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="私募" _mubu_text="%3Cspan%3E%E7%A7%81%E5%8B%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="收益风险按基金合同约定" _mubu_text="%3Cspan%3E%E6%94%B6%E7%9B%8A%E9%A3%8E%E9%99%A9%E6%8C%89%E5%9F%BA%E9%87%91%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="保险法" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="基本原则" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="保险利益原则" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E5%88%A9%E7%9B%8A%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="投保人或者被保险人对保险标的具有的法律上承认的利益" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%88%96%E8%80%85%E8%A2%AB%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%AF%B9%E4%BF%9D%E9%99%A9%E6%A0%87%E7%9A%84%E5%85%B7%E6%9C%89%E7%9A%84%E6%B3%95%E5%BE%8B%E4%B8%8A%E6%89%BF%E8%AE%A4%E7%9A%84%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="最大诚信原则" _mubu_text="%3Cspan%3E%E6%9C%80%E5%A4%A7%E8%AF%9A%E4%BF%A1%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="投保人" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="保险人" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="近因原则" _mubu_text="%3Cspan%3E%E8%BF%91%E5%9B%A0%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="造成标的损害的最直接、最有效的原因" _mubu_text="%3Cspan%3E%E9%80%A0%E6%88%90%E6%A0%87%E7%9A%84%E6%8D%9F%E5%AE%B3%E7%9A%84%E6%9C%80%E7%9B%B4%E6%8E%A5%E3%80%81%E6%9C%80%E6%9C%89%E6%95%88%E7%9A%84%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="是否在保险责任范围内" _mubu_note="%3Cspan%3E%E6%98%AF%E5%90%A6%E5%9C%A8%E4%BF%9D%E9%99%A9%E8%B4%A3%E4%BB%BB%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E"/>
          </outline>
          <outline text="合法经营原则；自愿原则；公序良俗原则" _mubu_text="%3Cspan%3E%E5%90%88%E6%B3%95%E7%BB%8F%E8%90%A5%E5%8E%9F%E5%88%99%EF%BC%9B%E8%87%AA%E6%84%BF%E5%8E%9F%E5%88%99%EF%BC%9B%E5%85%AC%E5%BA%8F%E8%89%AF%E4%BF%97%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="保险合同" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="不要式合同" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E5%BC%8F%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="订立" _mubu_text="%3Cspan%3E%E8%AE%A2%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="投保人的如实告知义务" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E7%9A%84%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="保险人询问，投保人应如实告知" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E8%AF%A2%E9%97%AE%EF%BC%8C%E6%8A%95%E4%BF%9D%E4%BA%BA%E5%BA%94%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%3C/span%3E" _note="限于保险人询问范围" _mubu_note="%3Cspan%3E%E9%99%90%E4%BA%8E%E4%BF%9D%E9%99%A9%E4%BA%BA%E8%AF%A2%E9%97%AE%E8%8C%83%E5%9B%B4%3C/span%3E">
                <outline text="保险人有举证义务" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E6%9C%89%E4%B8%BE%E8%AF%81%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="体检不能替代如实告知" _mubu_text="%3Cspan%3E%E4%BD%93%E6%A3%80%E4%B8%8D%E8%83%BD%E6%9B%BF%E4%BB%A3%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="保险人的解除权" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E7%9A%84%E8%A7%A3%E9%99%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="投保人故意或者因重大过失未履行如实告知义务，足以影响保险人决定是否同意承保或者提高保险费率的，保险人有权解除合同" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%95%85%E6%84%8F%E6%88%96%E8%80%85%E5%9B%A0%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%E6%9C%AA%E5%B1%A5%E8%A1%8C%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E4%B9%89%E5%8A%A1%EF%BC%8C%E8%B6%B3%E4%BB%A5%E5%BD%B1%E5%93%8D%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%86%B3%E5%AE%9A%E6%98%AF%E5%90%A6%E5%90%8C%E6%84%8F%E6%89%BF%E4%BF%9D%E6%88%96%E8%80%85%E6%8F%90%E9%AB%98%E4%BF%9D%E9%99%A9%E8%B4%B9%E7%8E%87%E7%9A%84%EF%BC%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E6%9C%89%E6%9D%83%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="解除权的限制" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E6%9D%83%E7%9A%84%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="自保险人知道有解除事由之日起，超过30日不行使而消灭" _mubu_text="%3Cspan%3E%E8%87%AA%E4%BF%9D%E9%99%A9%E4%BA%BA%E7%9F%A5%E9%81%93%E6%9C%89%E8%A7%A3%E9%99%A4%E4%BA%8B%E7%94%B1%E4%B9%8B%E6%97%A5%E8%B5%B7%EF%BC%8C%E8%B6%85%E8%BF%8730%E6%97%A5%E4%B8%8D%E8%A1%8C%E4%BD%BF%E8%80%8C%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="自合同成立之日起超过2年的，保险人不得解除合同" _mubu_text="%3Cspan%3E%E8%87%AA%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%B6%85%E8%BF%872%E5%B9%B4%E7%9A%84%EF%BC%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%B8%8D%E5%BE%97%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="保险人在合同订立时已经知道投保人未如实告知的情况的，保险人不得解除合同" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%9C%A8%E5%90%88%E5%90%8C%E8%AE%A2%E7%AB%8B%E6%97%B6%E5%B7%B2%E7%BB%8F%E7%9F%A5%E9%81%93%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%AA%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E7%9A%84%E6%83%85%E5%86%B5%E7%9A%84%EF%BC%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%B8%8D%E5%BE%97%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="保险人在保险合同成立后知道或者应当知道投保人未履行如实告知义务，仍然收取保险费，不得主张解除合同" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%9C%A8%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%E5%90%8E%E7%9F%A5%E9%81%93%E6%88%96%E8%80%85%E5%BA%94%E5%BD%93%E7%9F%A5%E9%81%93%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%AA%E5%B1%A5%E8%A1%8C%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E4%B9%89%E5%8A%A1%EF%BC%8C%E4%BB%8D%E7%84%B6%E6%94%B6%E5%8F%96%E4%BF%9D%E9%99%A9%E8%B4%B9%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%B8%BB%E5%BC%A0%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="故意/重大过失未如实告知" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F/%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%E6%9C%AA%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="足以影响保险承保/提高费率" _mubu_text="%3Cspan%3E%E8%B6%B3%E4%BB%A5%E5%BD%B1%E5%93%8D%E4%BF%9D%E9%99%A9%E6%89%BF%E4%BF%9D/%E6%8F%90%E9%AB%98%E8%B4%B9%E7%8E%87%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="保险人有解除权" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E6%9C%89%E8%A7%A3%E9%99%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="限制" _mubu_text="%3Cspan%3E%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="时效" _mubu_text="%3Cspan%3E%E6%97%B6%E6%95%88%3C/span%3E" _note="“30已满2年”" _mubu_note="%3Cspan%3E%E2%80%9C30%E5%B7%B2%E6%BB%A12%E5%B9%B4%E2%80%9D%3C/span%3E">
                      <outline text="自知道有解除事由之日起，超过30日不行使，解除权消灭" _mubu_text="%3Cspan%3E%E8%87%AA%E7%9F%A5%E9%81%93%E6%9C%89%E8%A7%A3%E9%99%A4%E4%BA%8B%E7%94%B1%E4%B9%8B%E6%97%A5%E8%B5%B7%EF%BC%8C%E8%B6%85%E8%BF%8730%E6%97%A5%E4%B8%8D%E8%A1%8C%E4%BD%BF%EF%BC%8C%E8%A7%A3%E9%99%A4%E6%9D%83%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="自合同成立之日起超过2年的，保险人不得解除合同" _mubu_text="%3Cspan%3E%E8%87%AA%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%B6%85%E8%BF%872%E5%B9%B4%E7%9A%84%EF%BC%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%B8%8D%E5%BE%97%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="明知" _mubu_text="%3Cspan%3E%E6%98%8E%E7%9F%A5%3C/span%3E" _note="”订立知道仍收费“" _mubu_note="%3Cspan%3E%E2%80%9D%E8%AE%A2%E7%AB%8B%E7%9F%A5%E9%81%93%E4%BB%8D%E6%94%B6%E8%B4%B9%E2%80%9C%3C/span%3E">
                      <outline text="保险人在合同订立时己经知道投保人未如实告知的情况的" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%9C%A8%E5%90%88%E5%90%8C%E8%AE%A2%E7%AB%8B%E6%97%B6%E5%B7%B1%E7%BB%8F%E7%9F%A5%E9%81%93%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%AA%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E7%9A%84%E6%83%85%E5%86%B5%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="保险人在订立合同后知道，而收取保费的" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%9C%A8%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%E5%90%8E%E7%9F%A5%E9%81%93%EF%BC%8C%E8%80%8C%E6%94%B6%E5%8F%96%E4%BF%9D%E8%B4%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                  <outline text="赔偿与退费" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E4%B8%8E%E9%80%80%E8%B4%B9%3C/span%3E" _note="" _mubu_note="">
                    <outline text="故意" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
                      <outline text="不赔，不退" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%B5%94%EF%BC%8C%E4%B8%8D%E9%80%80%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="重大过失" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
                      <outline text="不赔，应退" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%B5%94%EF%BC%8C%E5%BA%94%E9%80%80%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="拒绝赔偿应先行使解除权" _mubu_text="%3Cspan%3E%E6%8B%92%E7%BB%9D%E8%B5%94%E5%81%BF%E5%BA%94%E5%85%88%E8%A1%8C%E4%BD%BF%E8%A7%A3%E9%99%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="效力" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="通常，成立即生效" _mubu_text="%3Cspan%3E%E9%80%9A%E5%B8%B8%EF%BC%8C%E6%88%90%E7%AB%8B%E5%8D%B3%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="成立后，投保人按约定交付保险费" _mubu_text="%3Cspan%3E%E6%88%90%E7%AB%8B%E5%90%8E%EF%BC%8C%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%8C%89%E7%BA%A6%E5%AE%9A%E4%BA%A4%E4%BB%98%E4%BF%9D%E9%99%A9%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="保险人代签字" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%BB%A3%E7%AD%BE%E5%AD%97%3C/span%3E" _note="" _mubu_note="">
              <outline text="对投保人不生效" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%8A%95%E4%BF%9D%E4%BA%BA%E4%B8%8D%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="视为追认" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E8%BF%BD%E8%AE%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="投保人已经交纳保险费的" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E5%B7%B2%E7%BB%8F%E4%BA%A4%E7%BA%B3%E4%BF%9D%E9%99%A9%E8%B4%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="保险人代填保险单" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%BB%A3%E5%A1%AB%E4%BF%9D%E9%99%A9%E5%8D%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="经投保人签字或者盖章确认的" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%8A%95%E4%BF%9D%E4%BA%BA%E7%AD%BE%E5%AD%97%E6%88%96%E8%80%85%E7%9B%96%E7%AB%A0%E7%A1%AE%E8%AE%A4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="视为投保人的真实意思表示" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E6%8A%95%E4%BF%9D%E4%BA%BA%E7%9A%84%E7%9C%9F%E5%AE%9E%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合同审查期间事故" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E5%AE%A1%E6%9F%A5%E6%9C%9F%E9%97%B4%E4%BA%8B%E6%95%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="尚未作出是否承保的意思表示" _mubu_text="%3Cspan%3E%E5%B0%9A%E6%9C%AA%E4%BD%9C%E5%87%BA%E6%98%AF%E5%90%A6%E6%89%BF%E4%BF%9D%E7%9A%84%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="符合承保条件的，应当赔偿" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E6%89%BF%E4%BF%9D%E6%9D%A1%E4%BB%B6%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不符合的，不承担保险责任，但应退还已经收取的保险费" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AC%A6%E5%90%88%E7%9A%84%EF%BC%8C%E4%B8%8D%E6%89%BF%E6%8B%85%E4%BF%9D%E9%99%A9%E8%B4%A3%E4%BB%BB%EF%BC%8C%E4%BD%86%E5%BA%94%E9%80%80%E8%BF%98%E5%B7%B2%E7%BB%8F%E6%94%B6%E5%8F%96%E7%9A%84%E4%BF%9D%E9%99%A9%E8%B4%B9%3C/span%3E" _note="保险人应承担举证责任" _mubu_note="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%BA%94%E6%89%BF%E6%8B%85%E4%B8%BE%E8%AF%81%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
            </outline>
            <outline text="保险合同冲突处理规则" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%E5%86%B2%E7%AA%81%E5%A4%84%E7%90%86%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则上，以投保单为准" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%EF%BC%8C%E4%BB%A5%E6%8A%95%E4%BF%9D%E5%8D%95%E4%B8%BA%E5%87%86%3C/span%3E" _note="内容系经保险人说明并经投保人同意的除外" _mubu_note="%3Cspan%3E%E5%86%85%E5%AE%B9%E7%B3%BB%E7%BB%8F%E4%BF%9D%E9%99%A9%E4%BA%BA%E8%AF%B4%E6%98%8E%E5%B9%B6%E7%BB%8F%E6%8A%95%E4%BF%9D%E4%BA%BA%E5%90%8C%E6%84%8F%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              <outline text="以非格式条款为准" _mubu_text="%3Cspan%3E%E4%BB%A5%E9%9D%9E%E6%A0%BC%E5%BC%8F%E6%9D%A1%E6%AC%BE%E4%B8%BA%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以形成时间在后的为准" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%BD%A2%E6%88%90%E6%97%B6%E9%97%B4%E5%9C%A8%E5%90%8E%E7%9A%84%E4%B8%BA%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以双方签字、盖章的手写部分的内容为准" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%8F%8C%E6%96%B9%E7%AD%BE%E5%AD%97%E3%80%81%E7%9B%96%E7%AB%A0%E7%9A%84%E6%89%8B%E5%86%99%E9%83%A8%E5%88%86%E7%9A%84%E5%86%85%E5%AE%B9%E4%B8%BA%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="格式条款解释规则" _mubu_text="%3Cspan%3E%E6%A0%BC%E5%BC%8F%E6%9D%A1%E6%AC%BE%E8%A7%A3%E9%87%8A%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无效条款" _mubu_text="%3Cspan%3E%E6%97%A0%E6%95%88%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="理赔" _mubu_text="%3Cspan%3E%E7%90%86%E8%B5%94%3C/span%3E" _note="" _mubu_note="">
            <outline text="保险事故发生后的通知义务" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%8B%E6%95%85%E5%8F%91%E7%94%9F%E5%90%8E%E7%9A%84%E9%80%9A%E7%9F%A5%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="投保人、被保险人或者受益人知道保险事故发生后，应及时通知保险人" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E3%80%81%E8%A2%AB%E4%BF%9D%E9%99%A9%E4%BA%BA%E6%88%96%E8%80%85%E5%8F%97%E7%9B%8A%E4%BA%BA%E7%9F%A5%E9%81%93%E4%BF%9D%E9%99%A9%E4%BA%8B%E6%95%85%E5%8F%91%E7%94%9F%E5%90%8E%EF%BC%8C%E5%BA%94%E5%8F%8A%E6%97%B6%E9%80%9A%E7%9F%A5%E4%BF%9D%E9%99%A9%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="故意或重大过失未及时通知" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F%E6%88%96%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%E6%9C%AA%E5%8F%8A%E6%97%B6%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="致使保险事故的性质、原因、损失程度等难以确定的" _mubu_text="%3Cspan%3E%E8%87%B4%E4%BD%BF%E4%BF%9D%E9%99%A9%E4%BA%8B%E6%95%85%E7%9A%84%E6%80%A7%E8%B4%A8%E3%80%81%E5%8E%9F%E5%9B%A0%E3%80%81%E6%8D%9F%E5%A4%B1%E7%A8%8B%E5%BA%A6%E7%AD%89%E9%9A%BE%E4%BB%A5%E7%A1%AE%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="保险人对无法确定的部分不担责" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%AF%B9%E6%97%A0%E6%B3%95%E7%A1%AE%E5%AE%9A%E7%9A%84%E9%83%A8%E5%88%86%E4%B8%8D%E6%8B%85%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="但保险人(应)及时知道保险事故发生的除外" _mubu_text="%3Cspan%3E%E4%BD%86%E4%BF%9D%E9%99%A9%E4%BA%BA(%E5%BA%94)%E5%8F%8A%E6%97%B6%E7%9F%A5%E9%81%93%E4%BF%9D%E9%99%A9%E4%BA%8B%E6%95%85%E5%8F%91%E7%94%9F%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="证明与资料的提供" _mubu_text="%3Cspan%3E%E8%AF%81%E6%98%8E%E4%B8%8E%E8%B5%84%E6%96%99%E7%9A%84%E6%8F%90%E4%BE%9B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="诉讼时效" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="(应)知道事故发生之日起算" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E4%BA%8B%E6%95%85%E5%8F%91%E7%94%9F%E4%B9%8B%E6%97%A5%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="人寿保险" _mubu_text="%3Cspan%3E%E4%BA%BA%E5%AF%BF%E4%BF%9D%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="5年" _mubu_text="%3Cspan%3E5%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="其他" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%3C/span%3E" _note="财产、其他人身" _mubu_note="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E3%80%81%E5%85%B6%E4%BB%96%E4%BA%BA%E8%BA%AB%3C/span%3E">
              <outline text="3年" _mubu_text="%3Cspan%3E3%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="解除" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="投保人有任意解除权" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%89%E4%BB%BB%E6%84%8F%E8%A7%A3%E9%99%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="保险人的解除权" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E4%BA%BA%E7%9A%84%E8%A7%A3%E9%99%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="未如实告知" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="谎报事故" _mubu_text="%3Cspan%3E%E8%B0%8E%E6%8A%A5%E4%BA%8B%E6%95%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="故意制造事故" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F%E5%88%B6%E9%80%A0%E4%BA%8B%E6%95%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未尽安全注意义务" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%B0%BD%E5%AE%89%E5%85%A8%E6%B3%A8%E6%84%8F%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="保险标的危险程度增加" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E6%A0%87%E7%9A%84%E5%8D%B1%E9%99%A9%E7%A8%8B%E5%BA%A6%E5%A2%9E%E5%8A%A0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="投保人身险时谎报年龄" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E8%BA%AB%E9%99%A9%E6%97%B6%E8%B0%8E%E6%8A%A5%E5%B9%B4%E9%BE%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="人身保险合同" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="人的寿命和身体为保险标的" _mubu_text="%3Cspan%3E%E4%BA%BA%E7%9A%84%E5%AF%BF%E5%91%BD%E5%92%8C%E8%BA%AB%E4%BD%93%E4%B8%BA%E4%BF%9D%E9%99%A9%E6%A0%87%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
            <outline text="标的人格化，不得转让" _mubu_text="%3Cspan%3E%E6%A0%87%E7%9A%84%E4%BA%BA%E6%A0%BC%E5%8C%96%EF%BC%8C%E4%B8%8D%E5%BE%97%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="保险金定额支付" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E9%87%91%E5%AE%9A%E9%A2%9D%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="无法直接以金钱来衡量" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E7%9B%B4%E6%8E%A5%E4%BB%A5%E9%87%91%E9%92%B1%E6%9D%A5%E8%A1%A1%E9%87%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只能由投保人和保险人协商确定一个固定的数额" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E7%94%B1%E6%8A%95%E4%BF%9D%E4%BA%BA%E5%92%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%8D%8F%E5%95%86%E7%A1%AE%E5%AE%9A%E4%B8%80%E4%B8%AA%E5%9B%BA%E5%AE%9A%E7%9A%84%E6%95%B0%E9%A2%9D%3C/span%3E" _note="以此为保险人给付保险金的最高限额" _mubu_note="%3Cspan%3E%E4%BB%A5%E6%AD%A4%E4%B8%BA%E4%BF%9D%E9%99%A9%E4%BA%BA%E7%BB%99%E4%BB%98%E4%BF%9D%E9%99%A9%E9%87%91%E7%9A%84%E6%9C%80%E9%AB%98%E9%99%90%E9%A2%9D%3C/span%3E"/>
            </outline>
            <outline text="具有储蓄性" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E5%82%A8%E8%93%84%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="特别是人寿险" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E6%98%AF%E4%BA%BA%E5%AF%BF%E9%99%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以备急需时或年老时使用" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%A4%87%E6%80%A5%E9%9C%80%E6%97%B6%E6%88%96%E5%B9%B4%E8%80%81%E6%97%B6%E4%BD%BF%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="人身保险合同并非债权债务关系" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E4%BF%9D%E9%99%A9%E5%90%88%E5%90%8C%E5%B9%B6%E9%9D%9E%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="人不得要求投保人承担违约责任" _mubu_text="%3Cspan%3E%E4%BA%BA%E4%B8%8D%E5%BE%97%E8%A6%81%E6%B1%82%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%89%BF%E6%8B%85%E8%BF%9D%E7%BA%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得以诉讼方式要求投保人支付保费" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E8%AF%89%E8%AE%BC%E6%96%B9%E5%BC%8F%E8%A6%81%E6%B1%82%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%94%AF%E4%BB%98%E4%BF%9D%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="不适用代位求偿" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%E4%BB%A3%E4%BD%8D%E6%B1%82%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="保险利益" _mubu_text="%3Cspan%3E%E4%BF%9D%E9%99%A9%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="投保人对被保险人" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E5%AF%B9%E8%A2%AB%E4%BF%9D%E9%99%A9%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="本人" _mubu_text="%3Cspan%3E%E6%9C%AC%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="配偶、子女、父母" _mubu_text="%3Cspan%3E%E9%85%8D%E5%81%B6%E3%80%81%E5%AD%90%E5%A5%B3%E3%80%81%E7%88%B6%E6%AF%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="其他与投保人有抚养、赡养或者扶养关系的家庭其他成员、近亲属" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E4%B8%8E%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%89%E6%8A%9A%E5%85%BB%E3%80%81%E8%B5%A1%E5%85%BB%E6%88%96%E8%80%85%E6%89%B6%E5%85%BB%E5%85%B3%E7%B3%BB%E7%9A%84%E5%AE%B6%E5%BA%AD%E5%85%B6%E4%BB%96%E6%88%90%E5%91%98%E3%80%81%E8%BF%91%E4%BA%B2%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="与投保人有劳动关系的劳动者" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%8A%95%E4%BF%9D%E4%BA%BA%E6%9C%89%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E7%9A%84%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="被保险人同意投保人为其订立合同的，视为具有保险利益" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BF%9D%E9%99%A9%E4%BA%BA%E5%90%8C%E6%84%8F%E6%8A%95%E4%BF%9D%E4%BA%BA%E4%B8%BA%E5%85%B6%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%E7%9A%84%EF%BC%8C%E8%A7%86%E4%B8%BA%E5%85%B7%E6%9C%89%E4%BF%9D%E9%99%A9%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="在合同订立时" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%90%88%E5%90%8C%E8%AE%A2%E7%AB%8B%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="应有保险利益" _mubu_text="%3Cspan%3E%E5%BA%94%E6%9C%89%E4%BF%9D%E9%99%A9%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="无，保险人不承保" _mubu_text="%3Cspan%3E%E6%97%A0%EF%BC%8C%E4%BF%9D%E9%99%A9%E4%BA%BA%E4%B8%8D%E6%89%BF%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="承保的，合同无效" _mubu_text="%3Cspan%3E%E6%89%BF%E4%BF%9D%E7%9A%84%EF%BC%8C%E5%90%88%E5%90%8C%E6%97%A0%E6%95%88%3C/span%3E" _note="扣手续费后，退还保费" _mubu_note="%3Cspan%3E%E6%89%A3%E6%89%8B%E7%BB%AD%E8%B4%B9%E5%90%8E%EF%BC%8C%E9%80%80%E8%BF%98%E4%BF%9D%E8%B4%B9%3C/span%3E"/>
              <outline text="法院审理时应主动审查" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%AE%A1%E7%90%86%E6%97%B6%E5%BA%94%E4%B8%BB%E5%8A%A8%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="中止与复效" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%E4%B8%8E%E5%A4%8D%E6%95%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="中止(图)" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
              <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22Ie3kX9d9lQ%22%2C%22oh%22%3A229%2C%22ow%22%3A833%2C%22uri%22%3A%22document_image%2F19dbf562-1972-4386-b8f3-a78dc2053e54-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
            <outline text="复效" _mubu_text="%3Cspan%3E%E5%A4%8D%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="补交保险费" _mubu_text="%3Cspan%3E%E8%A1%A5%E4%BA%A4%E4%BF%9D%E9%99%A9%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="投保人主动请求" _mubu_text="%3Cspan%3E%E6%8A%95%E4%BF%9D%E4%BA%BA%E4%B8%BB%E5%8A%A8%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
