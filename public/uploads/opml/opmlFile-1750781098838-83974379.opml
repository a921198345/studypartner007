        <outline text="股东的资格与权利" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E7%9A%84%E8%B5%84%E6%A0%BC%E4%B8%8E%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
