          <outline text="合作作品" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%90%88%E4%BD%9C%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="著作权由合作作者共同享有" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E7%94%B1%E5%90%88%E4%BD%9C%E4%BD%9C%E8%80%85%E5%85%B1%E5%90%8C%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="著作权行使规则" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E8%A1%8C%E4%BD%BF%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="先协商一致行使" _mubu_text="%3Cspan%3E%E5%85%88%E5%8D%8F%E5%95%86%E4%B8%80%E8%87%B4%E8%A1%8C%E4%BD%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="协商不成，无正当理由任何一方不得阻碍他方行使" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%95%86%E4%B8%8D%E6%88%90%EF%BC%8C%E6%97%A0%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E4%BB%BB%E4%BD%95%E4%B8%80%E6%96%B9%E4%B8%8D%E5%BE%97%E9%98%BB%E7%A2%8D%E4%BB%96%E6%96%B9%E8%A1%8C%E4%BD%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="转让" _mubu_text="%3Cspan%3E%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="许可他人专有使用" _mubu_text="%3Cspan%3E%E8%AE%B8%E5%8F%AF%E4%BB%96%E4%BA%BA%E4%B8%93%E6%9C%89%E4%BD%BF%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="出质" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B4%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="所得收益应合理分配给所有合作作者" _mubu_text="%3Cspan%3E%E6%89%80%E5%BE%97%E6%94%B6%E7%9B%8A%E5%BA%94%E5%90%88%E7%90%86%E5%88%86%E9%85%8D%E7%BB%99%E6%89%80%E6%9C%89%E5%90%88%E4%BD%9C%E4%BD%9C%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="可分割使用的" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%88%86%E5%89%B2%E4%BD%BF%E7%94%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="对各自创作的部分可以单独享有著作权" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%90%84%E8%87%AA%E5%88%9B%E4%BD%9C%E7%9A%84%E9%83%A8%E5%88%86%E5%8F%AF%E4%BB%A5%E5%8D%95%E7%8B%AC%E4%BA%AB%E6%9C%89%E8%91%97%E4%BD%9C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="行使时不得侵犯作品整体的著作权" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E6%97%B6%E4%B8%8D%E5%BE%97%E4%BE%B5%E7%8A%AF%E4%BD%9C%E5%93%81%E6%95%B4%E4%BD%93%E7%9A%84%E8%91%97%E4%BD%9C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="委托作品" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%A7%94%E6%89%98%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
