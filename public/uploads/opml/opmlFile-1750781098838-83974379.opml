            <outline text="申请破产后，宣告破产前" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E7%A0%B4%E4%BA%A7%E5%90%8E%EF%BC%8C%E5%AE%A3%E5%91%8A%E7%A0%B4%E4%BA%A7%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
