              <outline text="之后追偿" _mubu_text="%3Cspan%3E%E4%B9%8B%E5%90%8E%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="食品经营者免于处罚的情形" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E7%BB%8F%E8%90%A5%E8%80%85%E5%85%8D%E4%BA%8E%E5%A4%84%E7%BD%9A%E7%9A%84%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="证据证明其不知道不符合标准，并能如实说明其进货来源的" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E5%85%B6%E4%B8%8D%E7%9F%A5%E9%81%93%E4%B8%8D%E7%AC%A6%E5%90%88%E6%A0%87%E5%87%86%EF%BC%8C%E5%B9%B6%E8%83%BD%E5%A6%82%E5%AE%9E%E8%AF%B4%E6%98%8E%E5%85%B6%E8%BF%9B%E8%B4%A7%E6%9D%A5%E6%BA%90%E7%9A%84%3C/span%3E" _note="免予行政处罚" _mubu_note="%3Cspan%3E%E5%85%8D%E4%BA%88%E8%A1%8C%E6%94%BF%E5%A4%84%E7%BD%9A%3C/span%3E"/>
              <outline text="造成人身、财产或者其他损害的，仍依法承担赔偿责任" _mubu_text="%3Cspan%3E%E9%80%A0%E6%88%90%E4%BA%BA%E8%BA%AB%E3%80%81%E8%B4%A2%E4%BA%A7%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%E4%BB%8D%E4%BE%9D%E6%B3%95%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="惩罚性赔偿" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%83%A9%E7%BD%9A%E6%80%A7%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="适用情形" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="生产不符合标准的" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E4%B8%8D%E7%AC%A6%E5%90%88%E6%A0%87%E5%87%86%E7%9A%84%3C/span%3E" _note="不用明知" _mubu_note="%3Cspan%3E%E4%B8%8D%E7%94%A8%E6%98%8E%E7%9F%A5%3C/span%3E"/>
                <outline text="销售明知是不符合标准的" _mubu_text="%3Cspan%3E%E9%94%80%E5%94%AE%E6%98%8E%E7%9F%A5%E6%98%AF%E4%B8%8D%E7%AC%A6%E5%90%88%E6%A0%87%E5%87%86%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="赔偿标准" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="赔偿损失" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="+向生产/销售者主张支付价款10倍或损失3倍的赔偿金" _mubu_text="%3Cspan%3E+%E5%90%91%E7%94%9F%E4%BA%A7/%E9%94%80%E5%94%AE%E8%80%85%E4%B8%BB%E5%BC%A0%E6%94%AF%E4%BB%98%E4%BB%B7%E6%AC%BE10%E5%80%8D%E6%88%96%E6%8D%9F%E5%A4%B13%E5%80%8D%E7%9A%84%E8%B5%94%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="增加赔偿＜1000元的，为1000元" _mubu_text="%3Cspan%3E%E5%A2%9E%E5%8A%A0%E8%B5%94%E5%81%BF%EF%BC%9C1000%E5%85%83%E7%9A%84%EF%BC%8C%E4%B8%BA1000%E5%85%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="食品标签、说明书有不影响食品安全且不会对消费者造成误导的瑕疵" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E6%A0%87%E7%AD%BE%E3%80%81%E8%AF%B4%E6%98%8E%E4%B9%A6%E6%9C%89%E4%B8%8D%E5%BD%B1%E5%93%8D%E9%A3%9F%E5%93%81%E5%AE%89%E5%85%A8%E4%B8%94%E4%B8%8D%E4%BC%9A%E5%AF%B9%E6%B6%88%E8%B4%B9%E8%80%85%E9%80%A0%E6%88%90%E8%AF%AF%E5%AF%BC%E7%9A%84%E7%91%95%E7%96%B5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="无效抗辩事由" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%97%A0%E6%95%88%E6%8A%97%E8%BE%A9%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
              <outline text="明知抗辩无效" _mubu_text="%3Cspan%3E%E6%98%8E%E7%9F%A5%E6%8A%97%E8%BE%A9%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="购买者明知食品、药品存在质量问题而仍然购买为由进行抗辩" _mubu_text="%3Cspan%3E%E8%B4%AD%E4%B9%B0%E8%80%85%E6%98%8E%E7%9F%A5%E9%A3%9F%E5%93%81%E3%80%81%E8%8D%AF%E5%93%81%E5%AD%98%E5%9C%A8%E8%B4%A8%E9%87%8F%E9%97%AE%E9%A2%98%E8%80%8C%E4%BB%8D%E7%84%B6%E8%B4%AD%E4%B9%B0%E4%B8%BA%E7%94%B1%E8%BF%9B%E8%A1%8C%E6%8A%97%E8%BE%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="赠品抗辩无效" _mubu_text="%3Cspan%3E%E8%B5%A0%E5%93%81%E6%8A%97%E8%BE%A9%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="消费者未对赠品支付对价为由进行免责抗辩" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E6%9C%AA%E5%AF%B9%E8%B5%A0%E5%93%81%E6%94%AF%E4%BB%98%E5%AF%B9%E4%BB%B7%E4%B8%BA%E7%94%B1%E8%BF%9B%E8%A1%8C%E5%85%8D%E8%B4%A3%E6%8A%97%E8%BE%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="举证责任" _mubu_text="%3Cspan%3E%E4%B8%BE%E8%AF%81%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="生产者与销售者" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E8%80%85%E4%B8%8E%E9%94%80%E5%94%AE%E8%80%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="食品符合质量标准" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E7%AC%A6%E5%90%88%E8%B4%A8%E9%87%8F%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="消费者" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="受到损害" _mubu_text="%3Cspan%3E%E5%8F%97%E5%88%B0%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="初步证明存在因果关系" _mubu_text="%3Cspan%3E%E5%88%9D%E6%AD%A5%E8%AF%81%E6%98%8E%E5%AD%98%E5%9C%A8%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="连带责任" _mubu_text="%3Cspan%3E%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="虚假广告中" _mubu_text="%3Cspan%3E%E8%99%9A%E5%81%87%E5%B9%BF%E5%91%8A%E4%B8%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检验、认证机构" _mubu_text="%3Cspan%3E%E6%A3%80%E9%AA%8C%E3%80%81%E8%AE%A4%E8%AF%81%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="网络食品第三方平台" _mubu_text="%3Cspan%3E%E7%BD%91%E7%BB%9C%E9%A3%9F%E5%93%81%E7%AC%AC%E4%B8%89%E6%96%B9%E5%B9%B3%E5%8F%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="银行法" _mubu_text="%3Cspan%3E%E9%93%B6%E8%A1%8C%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="商业银行法" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E9%93%B6%E8%A1%8C%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="设立" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="国务院银行业监督管理机构审批" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E9%93%B6%E8%A1%8C%E4%B8%9A%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%E6%9C%BA%E6%9E%84%E5%AE%A1%E6%89%B9%3C/span%3E" _note="银保监会" _mubu_note="%3Cspan%3E%E9%93%B6%E4%BF%9D%E7%9B%91%E4%BC%9A%3C/span%3E"/>
            <outline text="实缴资本制" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%BC%B4%E8%B5%84%E6%9C%AC%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="分支机构" _mubu_text="%3Cspan%3E%E5%88%86%E6%94%AF%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="银保监会审查批准" _mubu_text="%3Cspan%3E%E9%93%B6%E4%BF%9D%E7%9B%91%E4%BC%9A%E5%AE%A1%E6%9F%A5%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不按行政区划设立" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%8C%89%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%88%92%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不是独立法人，但可作诉讼主体" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%98%AF%E7%8B%AC%E7%AB%8B%E6%B3%95%E4%BA%BA%EF%BC%8C%E4%BD%86%E5%8F%AF%E4%BD%9C%E8%AF%89%E8%AE%BC%E4%B8%BB%E4%BD%93%3C/span%3E" _note="责任承担上，不足部分，由总行承担" _mubu_note="%3Cspan%3E%E8%B4%A3%E4%BB%BB%E6%89%BF%E6%8B%85%E4%B8%8A%EF%BC%8C%E4%B8%8D%E8%B6%B3%E9%83%A8%E5%88%86%EF%BC%8C%E7%94%B1%E6%80%BB%E8%A1%8C%E6%89%BF%E6%8B%85%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="组织机构" _mubu_text="%3Cspan%3E%E7%BB%84%E7%BB%87%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用《公司法》的规定" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E3%80%8A%E5%85%AC%E5%8F%B8%E6%B3%95%E3%80%8B%E7%9A%84%E8%A7%84%E5%AE%9A%3C/span%3E" _note="组织形式、组织机构、分立、合并等" _mubu_note="%3Cspan%3E%E7%BB%84%E7%BB%87%E5%BD%A2%E5%BC%8F%E3%80%81%E7%BB%84%E7%BB%87%E6%9C%BA%E6%9E%84%E3%80%81%E5%88%86%E7%AB%8B%E3%80%81%E5%90%88%E5%B9%B6%E7%AD%89%3C/span%3E"/>
          </outline>
          <outline text="业务规则" _mubu_text="%3Cspan%3E%E4%B8%9A%E5%8A%A1%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="贷款业务" _mubu_text="%3Cspan%3E%E8%B4%B7%E6%AC%BE%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="签订书面合同" _mubu_text="%3Cspan%3E%E7%AD%BE%E8%AE%A2%E4%B9%A6%E9%9D%A2%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="坚持审贷分离、分级审批" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E5%AE%A1%E8%B4%B7%E5%88%86%E7%A6%BB%E3%80%81%E5%88%86%E7%BA%A7%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="分为信用贷款和担保贷款" _mubu_text="%3Cspan%3E%E5%88%86%E4%B8%BA%E4%BF%A1%E7%94%A8%E8%B4%B7%E6%AC%BE%E5%92%8C%E6%8B%85%E4%BF%9D%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="关系人贷款" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%B3%E7%B3%BB%E4%BA%BA%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="关系人" _mubu_text="%3Cspan%3E%E5%85%B3%E7%B3%BB%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="商业银行的董事、监事、管理人员、信贷业务人员及其近亲属" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E9%93%B6%E8%A1%8C%E7%9A%84%E8%91%A3%E4%BA%8B%E3%80%81%E7%9B%91%E4%BA%8B%E3%80%81%E7%AE%A1%E7%90%86%E4%BA%BA%E5%91%98%E3%80%81%E4%BF%A1%E8%B4%B7%E4%B8%9A%E5%8A%A1%E4%BA%BA%E5%91%98%E5%8F%8A%E5%85%B6%E8%BF%91%E4%BA%B2%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="及其投资或担任高管的其他经济组织" _mubu_text="%3Cspan%3E%E5%8F%8A%E5%85%B6%E6%8A%95%E8%B5%84%E6%88%96%E6%8B%85%E4%BB%BB%E9%AB%98%E7%AE%A1%E7%9A%84%E5%85%B6%E4%BB%96%E7%BB%8F%E6%B5%8E%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="禁止向其发放信用贷款" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E5%90%91%E5%85%B6%E5%8F%91%E6%94%BE%E4%BF%A1%E7%94%A8%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="向其发放担保贷款的条件不得优于其他" _mubu_text="%3Cspan%3E%E5%90%91%E5%85%B6%E5%8F%91%E6%94%BE%E6%8B%85%E4%BF%9D%E8%B4%B7%E6%AC%BE%E7%9A%84%E6%9D%A1%E4%BB%B6%E4%B8%8D%E5%BE%97%E4%BC%98%E4%BA%8E%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="禁止利用贷款从事的行为" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E5%88%A9%E7%94%A8%E8%B4%B7%E6%AC%BE%E4%BB%8E%E4%BA%8B%E7%9A%84%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="从事股本权益性投资" _mubu_text="%3Cspan%3E%E4%BB%8E%E4%BA%8B%E8%82%A1%E6%9C%AC%E6%9D%83%E7%9B%8A%E6%80%A7%E6%8A%95%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在有价证券、期货方面进行投机" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%89%E4%BB%B7%E8%AF%81%E5%88%B8%E3%80%81%E6%9C%9F%E8%B4%A7%E6%96%B9%E9%9D%A2%E8%BF%9B%E8%A1%8C%E6%8A%95%E6%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="从事房地产业务" _mubu_text="%3Cspan%3E%E4%BB%8E%E4%BA%8B%E6%88%BF%E5%9C%B0%E4%BA%A7%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="有经营房地产资格的除外" _mubu_note="%3Cspan%3E%E6%9C%89%E7%BB%8F%E8%90%A5%E6%88%BF%E5%9C%B0%E4%BA%A7%E8%B5%84%E6%A0%BC%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="进行借贷谋取非法收入" _mubu_text="%3Cspan%3E%E8%BF%9B%E8%A1%8C%E5%80%9F%E8%B4%B7%E8%B0%8B%E5%8F%96%E9%9D%9E%E6%B3%95%E6%94%B6%E5%85%A5%3C/span%3E" _note="转贷" _mubu_note="%3Cspan%3E%E8%BD%AC%E8%B4%B7%3C/span%3E"/>
              </outline>
              <outline text="不得强令银行发放贷款" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%BC%BA%E4%BB%A4%E9%93%B6%E8%A1%8C%E5%8F%91%E6%94%BE%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不良贷款" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%89%AF%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="呆账贷款、呆滞贷款、逾期贷款" _mubu_text="%3Cspan%3E%E5%91%86%E8%B4%A6%E8%B4%B7%E6%AC%BE%E3%80%81%E5%91%86%E6%BB%9E%E8%B4%B7%E6%AC%BE%E3%80%81%E9%80%BE%E6%9C%9F%E8%B4%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="拆借业务" _mubu_text="%3Cspan%3E%E6%8B%86%E5%80%9F%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="银行因临时资金不足向其他银行及金融机构的临时借款" _mubu_text="%3Cspan%3E%E9%93%B6%E8%A1%8C%E5%9B%A0%E4%B8%B4%E6%97%B6%E8%B5%84%E9%87%91%E4%B8%8D%E8%B6%B3%E5%90%91%E5%85%B6%E4%BB%96%E9%93%B6%E8%A1%8C%E5%8F%8A%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E7%9A%84%E4%B8%B4%E6%97%B6%E5%80%9F%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="拆出资金" _mubu_text="%3Cspan%3E%E6%8B%86%E5%87%BA%E8%B5%84%E9%87%91%3C/span%3E" _note="" _mubu_note="">
                <outline text="限于交足、留足和归还后的闲置资金" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E4%BA%A4%E8%B6%B3%E3%80%81%E7%95%99%E8%B6%B3%E5%92%8C%E5%BD%92%E8%BF%98%E5%90%8E%E7%9A%84%E9%97%B2%E7%BD%AE%E8%B5%84%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="拆入资金" _mubu_text="%3Cspan%3E%E6%8B%86%E5%85%A5%E8%B5%84%E9%87%91%3C/span%3E" _note="" _mubu_note="">
                <outline text="仅能用于弥补和解决临时性周转资金的需要" _mubu_text="%3Cspan%3E%E4%BB%85%E8%83%BD%E7%94%A8%E4%BA%8E%E5%BC%A5%E8%A1%A5%E5%92%8C%E8%A7%A3%E5%86%B3%E4%B8%B4%E6%97%B6%E6%80%A7%E5%91%A8%E8%BD%AC%E8%B5%84%E9%87%91%E7%9A%84%E9%9C%80%E8%A6%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="禁止利用拆入资金发放固定资产贷款或者用于投资" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E5%88%A9%E7%94%A8%E6%8B%86%E5%85%A5%E8%B5%84%E9%87%91%E5%8F%91%E6%94%BE%E5%9B%BA%E5%AE%9A%E8%B5%84%E4%BA%A7%E8%B4%B7%E6%AC%BE%E6%88%96%E8%80%85%E7%94%A8%E4%BA%8E%E6%8A%95%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="投资业务" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="投资范围" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="注重安全性、流动性" _mubu_text="%3Cspan%3E%E6%B3%A8%E9%87%8D%E5%AE%89%E5%85%A8%E6%80%A7%E3%80%81%E6%B5%81%E5%8A%A8%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="主要是投资低风险的" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E6%98%AF%E6%8A%95%E8%B5%84%E4%BD%8E%E9%A3%8E%E9%99%A9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="禁止混业经营" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%B7%B7%E4%B8%9A%E7%BB%8F%E8%90%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得从事信托投资和证券经营业务" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%8E%E4%BA%8B%E4%BF%A1%E6%89%98%E6%8A%95%E8%B5%84%E5%92%8C%E8%AF%81%E5%88%B8%E7%BB%8F%E8%90%A5%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="禁止投资领域" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%8A%95%E8%B5%84%E9%A2%86%E5%9F%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="非自用不动产投资" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%87%AA%E7%94%A8%E4%B8%8D%E5%8A%A8%E4%BA%A7%E6%8A%95%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="非银行金融机构和企业投资" _mubu_text="%3Cspan%3E%E9%9D%9E%E9%93%B6%E8%A1%8C%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%E5%92%8C%E4%BC%81%E4%B8%9A%E6%8A%95%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不动产、股权的处置" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8A%A8%E4%BA%A7%E3%80%81%E8%82%A1%E6%9D%83%E7%9A%84%E5%A4%84%E7%BD%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="因行使抵押权、质权而取得的不动产或者股权" _mubu_text="%3Cspan%3E%E5%9B%A0%E8%A1%8C%E4%BD%BF%E6%8A%B5%E6%8A%BC%E6%9D%83%E3%80%81%E8%B4%A8%E6%9D%83%E8%80%8C%E5%8F%96%E5%BE%97%E7%9A%84%E4%B8%8D%E5%8A%A8%E4%BA%A7%E6%88%96%E8%80%85%E8%82%A1%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="应自取得之日起2年内予以处分" _mubu_text="%3Cspan%3E%E5%BA%94%E8%87%AA%E5%8F%96%E5%BE%97%E4%B9%8B%E6%97%A5%E8%B5%B72%E5%B9%B4%E5%86%85%E4%BA%88%E4%BB%A5%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="存款业务" _mubu_text="%3Cspan%3E%E5%AD%98%E6%AC%BE%E4%B8%9A%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="国务院银行业监督管理机构的职责" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E9%93%B6%E8%A1%8C%E4%B8%9A%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%E6%9C%BA%E6%9E%84%E7%9A%84%E8%81%8C%E8%B4%A3%3C/span%3E" _note="银保监会" _mubu_note="%3Cspan%3E%E9%93%B6%E4%BF%9D%E7%9B%91%E4%BC%9A%3C/span%3E">
            <outline text="行政监督管理" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="组织机构、业务范围内容的设立与变更" _mubu_text="%3Cspan%3E%E7%BB%84%E7%BB%87%E6%9C%BA%E6%9E%84%E3%80%81%E4%B8%9A%E5%8A%A1%E8%8C%83%E5%9B%B4%E5%86%85%E5%AE%B9%E7%9A%84%E8%AE%BE%E7%AB%8B%E4%B8%8E%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="中国人民银行的职责" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E4%BA%BA%E6%B0%91%E9%93%B6%E8%A1%8C%E7%9A%84%E8%81%8C%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="制定和执行货币政策" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%9A%E5%92%8C%E6%89%A7%E8%A1%8C%E8%B4%A7%E5%B8%81%E6%94%BF%E7%AD%96%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="商业银行接受中国人民银行的业务指导" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E9%93%B6%E8%A1%8C%E6%8E%A5%E5%8F%97%E4%B8%AD%E5%9B%BD%E4%BA%BA%E6%B0%91%E9%93%B6%E8%A1%8C%E7%9A%84%E4%B8%9A%E5%8A%A1%E6%8C%87%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="破产" _mubu_text="%3Cspan%3E%E7%A0%B4%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="破产原因" _mubu_text="%3Cspan%3E%E7%A0%B4%E4%BA%A7%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="不能支付到期债务" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%94%AF%E4%BB%98%E5%88%B0%E6%9C%9F%E5%80%BA%E5%8A%A1%3C/span%3E" _note="单一破产原因" _mubu_note="%3Cspan%3E%E5%8D%95%E4%B8%80%E7%A0%B4%E4%BA%A7%E5%8E%9F%E5%9B%A0%3C/span%3E"/>
            </outline>
            <outline text="前置程序" _mubu_text="%3Cspan%3E%E5%89%8D%E7%BD%AE%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="经银保监会同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E9%93%B6%E4%BF%9D%E7%9B%91%E4%BC%9A%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="债务清偿顺序" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E6%B8%85%E5%81%BF%E9%A1%BA%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="1、清算费用" _mubu_text="%3Cspan%3E1%E3%80%81%E6%B8%85%E7%AE%97%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="2、职工工资和劳保费用" _mubu_text="%3Cspan%3E2%E3%80%81%E8%81%8C%E5%B7%A5%E5%B7%A5%E8%B5%84%E5%92%8C%E5%8A%B3%E4%BF%9D%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="3、优先支付个人存款本息" _mubu_text="%3Cspan%3E3%E3%80%81%E4%BC%98%E5%85%88%E6%94%AF%E4%BB%98%E4%B8%AA%E4%BA%BA%E5%AD%98%E6%AC%BE%E6%9C%AC%E6%81%AF%3C/span%3E" _note="企业之后" _mubu_note="%3Cspan%3E%E4%BC%81%E4%B8%9A%E4%B9%8B%E5%90%8E%3C/span%3E"/>
              <outline text="4、其他" _mubu_text="%3Cspan%3E4%E3%80%81%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="银行业监督管理法" _mubu_text="%3Cspan%3E%E9%93%B6%E8%A1%8C%E4%B8%9A%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="银行业金融机构" _mubu_text="%3Cspan%3E%E9%93%B6%E8%A1%8C%E4%B8%9A%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="非银行业金融机构" _mubu_text="%3Cspan%3E%E9%9D%9E%E9%93%B6%E8%A1%8C%E4%B8%9A%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="措施" _mubu_text="%3Cspan%3E%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
            <outline text="强制信息披露" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E4%BF%A1%E6%81%AF%E6%8A%AB%E9%9C%B2%3C/span%3E" _note="重大事项" _mubu_note="%3Cspan%3E%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%3C/span%3E"/>
            <outline text="强制整改措施" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%95%B4%E6%94%B9%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="机构违反审慎经营规则" _mubu_text="%3Cspan%3E%E6%9C%BA%E6%9E%84%E8%BF%9D%E5%8F%8D%E5%AE%A1%E6%85%8E%E7%BB%8F%E8%90%A5%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="责令限期改正" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%A4%E9%99%90%E6%9C%9F%E6%94%B9%E6%AD%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="逾期未改正" _mubu_text="%3Cspan%3E%E9%80%BE%E6%9C%9F%E6%9C%AA%E6%94%B9%E6%AD%A3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="或其行为严重危及自身和客户合法权益" _mubu_text="%3Cspan%3E%E6%88%96%E5%85%B6%E8%A1%8C%E4%B8%BA%E4%B8%A5%E9%87%8D%E5%8D%B1%E5%8F%8A%E8%87%AA%E8%BA%AB%E5%92%8C%E5%AE%A2%E6%88%B7%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="接管" _mubu_text="%3Cspan%3E%E6%8E%A5%E7%AE%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="已经或可能发生信用危机" _mubu_text="%3Cspan%3E%E5%B7%B2%E7%BB%8F%E6%88%96%E5%8F%AF%E8%83%BD%E5%8F%91%E7%94%9F%E4%BF%A1%E7%94%A8%E5%8D%B1%E6%9C%BA%3C/span%3E" _note="挤兑" _mubu_note="%3Cspan%3E%E6%8C%A4%E5%85%91%3C/span%3E"/>
              <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="最长不超过2年" _mubu_text="%3Cspan%3E%E6%9C%80%E9%95%BF%E4%B8%8D%E8%B6%85%E8%BF%872%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="对相关人员" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%9B%B8%E5%85%B3%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="直接负责的董高和其他人员" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%B4%9F%E8%B4%A3%E7%9A%84%E8%91%A3%E9%AB%98%E5%92%8C%E5%85%B6%E4%BB%96%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="出境将对国家利益造成重大损失的，通知出境管理机关依法阻止其出境" _mubu_text="%3Cspan%3E%E5%87%BA%E5%A2%83%E5%B0%86%E5%AF%B9%E5%9B%BD%E5%AE%B6%E5%88%A9%E7%9B%8A%E9%80%A0%E6%88%90%E9%87%8D%E5%A4%A7%E6%8D%9F%E5%A4%B1%E7%9A%84%EF%BC%8C%E9%80%9A%E7%9F%A5%E5%87%BA%E5%A2%83%E7%AE%A1%E7%90%86%E6%9C%BA%E5%85%B3%E4%BE%9D%E6%B3%95%E9%98%BB%E6%AD%A2%E5%85%B6%E5%87%BA%E5%A2%83%3C/span%3E" _note="" _mubu_note=""/>
