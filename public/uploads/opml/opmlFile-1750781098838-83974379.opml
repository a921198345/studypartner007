                <outline text="被保证人的债务因汇票记载事项欠缺而无效" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BF%9D%E8%AF%81%E4%BA%BA%E7%9A%84%E5%80%BA%E5%8A%A1%E5%9B%A0%E6%B1%87%E7%A5%A8%E8%AE%B0%E8%BD%BD%E4%BA%8B%E9%A1%B9%E6%AC%A0%E7%BC%BA%E8%80%8C%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="保证人的代位权" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E4%BA%BA%E7%9A%84%E4%BB%A3%E4%BD%8D%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="保证人清偿票据供务后" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E4%BA%BA%E6%B8%85%E5%81%BF%E7%A5%A8%E6%8D%AE%E4%BE%9B%E5%8A%A1%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可行使持票人对被保证人及其前手的追索权" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%A1%8C%E4%BD%BF%E6%8C%81%E7%A5%A8%E4%BA%BA%E5%AF%B9%E8%A2%AB%E4%BF%9D%E8%AF%81%E4%BA%BA%E5%8F%8A%E5%85%B6%E5%89%8D%E6%89%8B%E7%9A%84%E8%BF%BD%E7%B4%A2%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="附条件保证" _mubu_text="%3Cspan%3E%E9%99%84%E6%9D%A1%E4%BB%B6%E4%BF%9D%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="保证有效，所附条件无效" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E6%9C%89%E6%95%88%EF%BC%8C%E6%89%80%E9%99%84%E6%9D%A1%E4%BB%B6%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="附条件的票据行为总结(图)" _mubu_text="%3Cspan%3E%E9%99%84%E6%9D%A1%E4%BB%B6%E7%9A%84%E7%A5%A8%E6%8D%AE%E8%A1%8C%E4%B8%BA%E6%80%BB%E7%BB%93(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
                  <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22a7OZaEnPrm%22%2C%22oh%22%3A133%2C%22ow%22%3A1145%2C%22uri%22%3A%22document_image%2F60aca98f-1f1f-40ba-98bf-3f288099a190-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
              </outline>
            </outline>
            <outline text="付款" _mubu_text="%3Cspan%3E%E4%BB%98%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
              <outline text="付款人责任" _mubu_text="%3Cspan%3E%E4%BB%98%E6%AC%BE%E4%BA%BA%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="恶意或重大过失付款" _mubu_text="%3Cspan%3E%E6%81%B6%E6%84%8F%E6%88%96%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%E4%BB%98%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="期前付款" _mubu_text="%3Cspan%3E%E6%9C%9F%E5%89%8D%E4%BB%98%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="自行担责" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%E6%8B%85%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="代理" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="代理人应在票据上签章" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%90%86%E4%BA%BA%E5%BA%94%E5%9C%A8%E7%A5%A8%E6%8D%AE%E4%B8%8A%E7%AD%BE%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应表明代理关系" _mubu_text="%3Cspan%3E%E5%BA%94%E8%A1%A8%E6%98%8E%E4%BB%A3%E7%90%86%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无权代理" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="由签章人承担票据责任" _mubu_text="%3Cspan%3E%E7%94%B1%E7%AD%BE%E7%AB%A0%E4%BA%BA%E6%89%BF%E6%8B%85%E7%A5%A8%E6%8D%AE%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="无权代理人" _mubu_note="%3Cspan%3E%E6%97%A0%E6%9D%83%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E"/>
              </outline>
              <outline text="越权代理" _mubu_text="%3Cspan%3E%E8%B6%8A%E6%9D%83%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="就其超越权限的部分承担票据责任" _mubu_text="%3Cspan%3E%E5%B0%B1%E5%85%B6%E8%B6%85%E8%B6%8A%E6%9D%83%E9%99%90%E7%9A%84%E9%83%A8%E5%88%86%E6%89%BF%E6%8B%85%E7%A5%A8%E6%8D%AE%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="本票" _mubu_text="%3Cspan%3E%E6%9C%AC%E7%A5%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="本人出票，本人付款" _mubu_text="%3Cspan%3E%E6%9C%AC%E4%BA%BA%E5%87%BA%E7%A5%A8%EF%BC%8C%E6%9C%AC%E4%BA%BA%E4%BB%98%E6%AC%BE%3C/span%3E" _note="出票人只能是银行" _mubu_note="%3Cspan%3E%E5%87%BA%E7%A5%A8%E4%BA%BA%E5%8F%AA%E8%83%BD%E6%98%AF%E9%93%B6%E8%A1%8C%3C/span%3E"/>
          <outline text="基本当事人" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="出票人、收款人" _mubu_text="%3Cspan%3E%E5%87%BA%E7%A5%A8%E4%BA%BA%E3%80%81%E6%94%B6%E6%AC%BE%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="除另约，适用汇票规定" _mubu_text="%3Cspan%3E%E9%99%A4%E5%8F%A6%E7%BA%A6%EF%BC%8C%E9%80%82%E7%94%A8%E6%B1%87%E7%A5%A8%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="支票" _mubu_text="%3Cspan%3E%E6%94%AF%E7%A5%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="付款人仅限于银行或其他金融机构" _mubu_text="%3Cspan%3E%E4%BB%98%E6%AC%BE%E4%BA%BA%E4%BB%85%E9%99%90%E4%BA%8E%E9%93%B6%E8%A1%8C%E6%88%96%E5%85%B6%E4%BB%96%E9%87%91%E8%9E%8D%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="限于即期票据" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E5%8D%B3%E6%9C%9F%E7%A5%A8%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不具备信用" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%B7%E5%A4%87%E4%BF%A1%E7%94%A8%3C/span%3E" _note="和汇票的本质区别" _mubu_note="%3Cspan%3E%E5%92%8C%E6%B1%87%E7%A5%A8%E7%9A%84%E6%9C%AC%E8%B4%A8%E5%8C%BA%E5%88%AB%3C/span%3E"/>
          <outline text="开立支票存款账户" _mubu_text="%3Cspan%3E%E5%BC%80%E7%AB%8B%E6%94%AF%E7%A5%A8%E5%AD%98%E6%AC%BE%E8%B4%A6%E6%88%B7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="禁止签发空头支票" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E7%AD%BE%E5%8F%91%E7%A9%BA%E5%A4%B4%E6%94%AF%E7%A5%A8%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="金额、收款人名称可经出票人授权补记" _mubu_text="%3Cspan%3E%E9%87%91%E9%A2%9D%E3%80%81%E6%94%B6%E6%AC%BE%E4%BA%BA%E5%90%8D%E7%A7%B0%E5%8F%AF%E7%BB%8F%E5%87%BA%E7%A5%A8%E4%BA%BA%E6%8E%88%E6%9D%83%E8%A1%A5%E8%AE%B0%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="票据权利" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
          <outline text="请求支付权" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E6%94%AF%E4%BB%98%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="追索权" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="期后追索" _mubu_text="%3Cspan%3E%E6%9C%9F%E5%90%8E%E8%BF%BD%E7%B4%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="汇票到期被拒绝付款的" _mubu_text="%3Cspan%3E%E6%B1%87%E7%A5%A8%E5%88%B0%E6%9C%9F%E8%A2%AB%E6%8B%92%E7%BB%9D%E4%BB%98%E6%AC%BE%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="期前追索" _mubu_text="%3Cspan%3E%E6%9C%9F%E5%89%8D%E8%BF%BD%E7%B4%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="汇票到期日前" _mubu_text="%3Cspan%3E%E6%B1%87%E7%A5%A8%E5%88%B0%E6%9C%9F%E6%97%A5%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有不可能实现付款请求权的" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%8D%E5%8F%AF%E8%83%BD%E5%AE%9E%E7%8E%B0%E4%BB%98%E6%AC%BE%E8%AF%B7%E6%B1%82%E6%9D%83%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="追索义务人" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E4%B9%89%E5%8A%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="出票人、背书人、保证人、承兑人" _mubu_text="%3Cspan%3E%E5%87%BA%E7%A5%A8%E4%BA%BA%E3%80%81%E8%83%8C%E4%B9%A6%E4%BA%BA%E3%80%81%E4%BF%9D%E8%AF%81%E4%BA%BA%E3%80%81%E6%89%BF%E5%85%91%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="各追索对象为连带债务人" _mubu_text="%3Cspan%3E%E5%90%84%E8%BF%BD%E7%B4%A2%E5%AF%B9%E8%B1%A1%E4%B8%BA%E8%BF%9E%E5%B8%A6%E5%80%BA%E5%8A%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有选择性、变更性、代位性" _mubu_text="%3Cspan%3E%E6%9C%89%E9%80%89%E6%8B%A9%E6%80%A7%E3%80%81%E5%8F%98%E6%9B%B4%E6%80%A7%E3%80%81%E4%BB%A3%E4%BD%8D%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="取得原则" _mubu_text="%3Cspan%3E%E5%8F%96%E5%BE%97%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="善意原则" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%8E%9F%E5%88%99%3C/span%3E" _note="持票人善意" _mubu_note="%3Cspan%3E%E6%8C%81%E7%A5%A8%E4%BA%BA%E5%96%84%E6%84%8F%3C/span%3E"/>
            <outline text="对价原则" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%AF%B9%E4%BB%B7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：持票人取得票据必须给付对价" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E6%8C%81%E7%A5%A8%E4%BA%BA%E5%8F%96%E5%BE%97%E7%A5%A8%E6%8D%AE%E5%BF%85%E9%A1%BB%E7%BB%99%E4%BB%98%E5%AF%B9%E4%BB%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="税收、继承、赠与" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E3%80%81%E7%BB%A7%E6%89%BF%E3%80%81%E8%B5%A0%E4%B8%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="但所享有的票据权利不得优于其前手的权利" _mubu_text="%3Cspan%3E%E4%BD%86%E6%89%80%E4%BA%AB%E6%9C%89%E7%9A%84%E7%A5%A8%E6%8D%AE%E6%9D%83%E5%88%A9%E4%B8%8D%E5%BE%97%E4%BC%98%E4%BA%8E%E5%85%B6%E5%89%8D%E6%89%8B%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="前手无权利则其也无权利" _mubu_note="%3Cspan%3E%E5%89%8D%E6%89%8B%E6%97%A0%E6%9D%83%E5%88%A9%E5%88%99%E5%85%B6%E4%B9%9F%E6%97%A0%E6%9D%83%E5%88%A9%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="失票后的救济" _mubu_text="%3Cspan%3E%E5%A4%B1%E7%A5%A8%E5%90%8E%E7%9A%84%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="挂失止付" _mubu_text="%3Cspan%3E%E6%8C%82%E5%A4%B1%E6%AD%A2%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="公示催告" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="提起诉讼" _mubu_text="%3Cspan%3E%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="票据效力" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%A5%A8%E6%8D%AE%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
          <outline text="非完全民事行为能力人签章" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%AE%8C%E5%85%A8%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E7%AD%BE%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
            <outline text="其签章无效" _mubu_text="%3Cspan%3E%E5%85%B6%E7%AD%BE%E7%AB%A0%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="但不影响其他签章的效力" _mubu_text="%3Cspan%3E%E4%BD%86%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%85%B6%E4%BB%96%E7%AD%BE%E7%AB%A0%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="伪造签章" _mubu_text="%3Cspan%3E%E4%BC%AA%E9%80%A0%E7%AD%BE%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
            <outline text="不影响其他真实签章效力" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%85%B6%E4%BB%96%E7%9C%9F%E5%AE%9E%E7%AD%BE%E7%AB%A0%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被伪造者、伪造者都不承担票据法律责任" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BC%AA%E9%80%A0%E8%80%85%E3%80%81%E4%BC%AA%E9%80%A0%E8%80%85%E9%83%BD%E4%B8%8D%E6%89%BF%E6%8B%85%E7%A5%A8%E6%8D%AE%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="伪造者承担民事赔偿责任" _mubu_note="%3Cspan%3E%E4%BC%AA%E9%80%A0%E8%80%85%E6%89%BF%E6%8B%85%E6%B0%91%E4%BA%8B%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
          </outline>
          <outline text="更改" _mubu_text="%3Cspan%3E%E6%9B%B4%E6%94%B9%3C/span%3E" _note="" _mubu_note="">
            <outline text="票据金额、日期、收款人名称不得更改" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E9%87%91%E9%A2%9D%E3%80%81%E6%97%A5%E6%9C%9F%E3%80%81%E6%94%B6%E6%AC%BE%E4%BA%BA%E5%90%8D%E7%A7%B0%E4%B8%8D%E5%BE%97%E6%9B%B4%E6%94%B9%3C/span%3E" _note="更改的票据无效" _mubu_note="%3Cspan%3E%E6%9B%B4%E6%94%B9%E7%9A%84%E7%A5%A8%E6%8D%AE%E6%97%A0%E6%95%88%3C/span%3E"/>
            <outline text="其他，原记载人可更改，应由其签章证明" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%EF%BC%8C%E5%8E%9F%E8%AE%B0%E8%BD%BD%E4%BA%BA%E5%8F%AF%E6%9B%B4%E6%94%B9%EF%BC%8C%E5%BA%94%E7%94%B1%E5%85%B6%E7%AD%BE%E7%AB%A0%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="变造" _mubu_text="%3Cspan%3E%E5%8F%98%E9%80%A0%3C/span%3E" _note="" _mubu_note="">
            <outline text="对签章以外事项的变造" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%AD%BE%E7%AB%A0%E4%BB%A5%E5%A4%96%E4%BA%8B%E9%A1%B9%E7%9A%84%E5%8F%98%E9%80%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="并不会导致票据无效" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%B8%8D%E4%BC%9A%E5%AF%BC%E8%87%B4%E7%A5%A8%E6%8D%AE%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="负责" _mubu_text="%3Cspan%3E%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
              <outline text="变造前签章的人，对原记载事项负责" _mubu_text="%3Cspan%3E%E5%8F%98%E9%80%A0%E5%89%8D%E7%AD%BE%E7%AB%A0%E7%9A%84%E4%BA%BA%EF%BC%8C%E5%AF%B9%E5%8E%9F%E8%AE%B0%E8%BD%BD%E4%BA%8B%E9%A1%B9%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="变造后签章的人，对变造后记载事项负责" _mubu_text="%3Cspan%3E%E5%8F%98%E9%80%A0%E5%90%8E%E7%AD%BE%E7%AB%A0%E7%9A%84%E4%BA%BA%EF%BC%8C%E5%AF%B9%E5%8F%98%E9%80%A0%E5%90%8E%E8%AE%B0%E8%BD%BD%E4%BA%8B%E9%A1%B9%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不能辨认的，视同在变造之前签章" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E8%BE%A8%E8%AE%A4%E7%9A%84%EF%BC%8C%E8%A7%86%E5%90%8C%E5%9C%A8%E5%8F%98%E9%80%A0%E4%B9%8B%E5%89%8D%E7%AD%BE%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="信托法" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="信托财产转移给受托人" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E8%BD%AC%E7%A7%BB%E7%BB%99%E5%8F%97%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="信托财产具有独立性" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E5%85%B7%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%80%A7%3C/span%3E" _note="独立于委托人、受托人和受益人" _mubu_note="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%A7%94%E6%89%98%E4%BA%BA%E3%80%81%E5%8F%97%E6%89%98%E4%BA%BA%E5%92%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E"/>
          <outline text="委托人和受托人之间具有信义关系" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E5%92%8C%E5%8F%97%E6%89%98%E4%BA%BA%E4%B9%8B%E9%97%B4%E5%85%B7%E6%9C%89%E4%BF%A1%E4%B9%89%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="受益人与委托人的关系" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B8%8E%E5%A7%94%E6%89%98%E4%BA%BA%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="自益信托" _mubu_text="%3Cspan%3E%E8%87%AA%E7%9B%8A%E4%BF%A1%E6%89%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="受益人和委托人是同一人" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%92%8C%E5%A7%94%E6%89%98%E4%BA%BA%E6%98%AF%E5%90%8C%E4%B8%80%E4%BA%BA%3C/span%3E" _note="委托人设立信托是为了自己的利益" _mubu_note="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E8%AE%BE%E7%AB%8B%E4%BF%A1%E6%89%98%E6%98%AF%E4%B8%BA%E4%BA%86%E8%87%AA%E5%B7%B1%E7%9A%84%E5%88%A9%E7%9B%8A%3C/span%3E"/>
            </outline>
            <outline text="他益信托" _mubu_text="%3Cspan%3E%E4%BB%96%E7%9B%8A%E4%BF%A1%E6%89%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="受益人是委托人之外的第三人" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E6%98%AF%E5%A7%94%E6%89%98%E4%BA%BA%E4%B9%8B%E5%A4%96%E7%9A%84%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="或委托人与其他第三人共同成为受益人" _mubu_text="%3Cspan%3E%E6%88%96%E5%A7%94%E6%89%98%E4%BA%BA%E4%B8%8E%E5%85%B6%E4%BB%96%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%85%B1%E5%90%8C%E6%88%90%E4%B8%BA%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="委托人是否有权终止／撤销信托关系" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E6%98%AF%E5%90%A6%E6%9C%89%E6%9D%83%E7%BB%88%E6%AD%A2%EF%BC%8F%E6%92%A4%E9%94%80%E4%BF%A1%E6%89%98%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="可撤销信托" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%92%A4%E9%94%80%E4%BF%A1%E6%89%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="自益信托一般为可撤销信托" _mubu_text="%3Cspan%3E%E8%87%AA%E7%9B%8A%E4%BF%A1%E6%89%98%E4%B8%80%E8%88%AC%E4%B8%BA%E5%8F%AF%E6%92%A4%E9%94%80%E4%BF%A1%E6%89%98%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="不可撤销信托" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E6%92%A4%E9%94%80%E4%BF%A1%E6%89%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="他益信托原则上不可撤销" _mubu_text="%3Cspan%3E%E4%BB%96%E7%9B%8A%E4%BF%A1%E6%89%98%E5%8E%9F%E5%88%99%E4%B8%8A%E4%B8%8D%E5%8F%AF%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="设立" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="“三确定”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%89%E7%A1%AE%E5%AE%9A%E2%80%9D%3C/span%3E" _note="" _mubu_note="">
            <outline text="信托财产确定" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="受益人确定" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="信托目的确定" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E7%9B%AE%E7%9A%84%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="委托人的债权人对信托的撤销" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E7%9A%84%E5%80%BA%E6%9D%83%E4%BA%BA%E5%AF%B9%E4%BF%A1%E6%89%98%E7%9A%84%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note="">
            <outline text="委托人设立信托损害其债权人利益" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E8%AE%BE%E7%AB%8B%E4%BF%A1%E6%89%98%E6%8D%9F%E5%AE%B3%E5%85%B6%E5%80%BA%E6%9D%83%E4%BA%BA%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="申请法院撤销" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%B3%95%E9%99%A2%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="(应)知道撤销原因之日起1年内" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E6%92%A4%E9%94%80%E5%8E%9F%E5%9B%A0%E4%B9%8B%E6%97%A5%E8%B5%B71%E5%B9%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="撤销的，不影响善意受益人已取得的信托利益" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%96%84%E6%84%8F%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%B7%B2%E5%8F%96%E5%BE%97%E7%9A%84%E4%BF%A1%E6%89%98%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="信托财产" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
          <outline text="范围" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="信托而取得" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E8%80%8C%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="管理运用、处分信托财产而取得" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E8%BF%90%E7%94%A8%E3%80%81%E5%A4%84%E5%88%86%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E8%80%8C%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="禁止流通物" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%B5%81%E9%80%9A%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得作为" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="限制流通物" _mubu_text="%3Cspan%3E%E9%99%90%E5%88%B6%E6%B5%81%E9%80%9A%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="批准后可作为" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%E5%90%8E%E5%8F%AF%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="独立性" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="独立于委托人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%A7%94%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="独立于委托人未设立信托的其他财产" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%A7%94%E6%89%98%E4%BA%BA%E6%9C%AA%E8%AE%BE%E7%AB%8B%E4%BF%A1%E6%89%98%E7%9A%84%E5%85%B6%E4%BB%96%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="独立于委托人的遗产或清算财产" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%A7%94%E6%89%98%E4%BA%BA%E7%9A%84%E9%81%97%E4%BA%A7%E6%88%96%E6%B8%85%E7%AE%97%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：不作为" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%B8%8D%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="委托人是共同受益人之一" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E6%98%AF%E5%85%B1%E5%90%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B9%8B%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="委托人是唯一的受益人" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E6%98%AF%E5%94%AF%E4%B8%80%E7%9A%84%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="独立于委托的债权人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%A7%94%E6%89%98%E7%9A%84%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：不得对其主张强制执行" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%B8%8D%E5%BE%97%E5%AF%B9%E5%85%B6%E4%B8%BB%E5%BC%A0%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="设立信托前债权人已对该信托财产享有优先受偿的权利" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E4%BF%A1%E6%89%98%E5%89%8D%E5%80%BA%E6%9D%83%E4%BA%BA%E5%B7%B2%E5%AF%B9%E8%AF%A5%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%BA%AB%E6%9C%89%E4%BC%98%E5%85%88%E5%8F%97%E5%81%BF%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="独立于受托人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="独立于受托人的固有财产" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E6%89%98%E4%BA%BA%E7%9A%84%E5%9B%BA%E6%9C%89%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="信托财产与其个人债务之间不能用于相互清偿" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%B8%8E%E5%85%B6%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%E4%B9%8B%E9%97%B4%E4%B8%8D%E8%83%BD%E7%94%A8%E4%BA%8E%E7%9B%B8%E4%BA%92%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="独立于受托人的遗产或清算财产" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E6%89%98%E4%BA%BA%E7%9A%84%E9%81%97%E4%BA%A7%E6%88%96%E6%B8%85%E7%AE%97%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="独立于受托人的债权人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E6%89%98%E4%BA%BA%E7%9A%84%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：不得对其主张强制执行" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%B8%8D%E5%BE%97%E5%AF%B9%E5%85%B6%E4%B8%BB%E5%BC%A0%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="处理信托事务所产生的债务" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%E4%BF%A1%E6%89%98%E4%BA%8B%E5%8A%A1%E6%89%80%E4%BA%A7%E7%94%9F%E7%9A%84%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="独立于受益人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="受益人对信托财产不享有直接的支配、处分、所有权" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%AF%B9%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%B8%8D%E4%BA%AB%E6%9C%89%E7%9B%B4%E6%8E%A5%E7%9A%84%E6%94%AF%E9%85%8D%E3%80%81%E5%A4%84%E5%88%86%E3%80%81%E6%89%80%E6%9C%89%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="受益人享有信托受益权" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%BA%AB%E6%9C%89%E4%BF%A1%E6%89%98%E5%8F%97%E7%9B%8A%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="独立于受益人的债权人" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E4%BA%8E%E5%8F%97%E7%9B%8A%E4%BA%BA%E7%9A%84%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：不得对其主张强制执行" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%B8%8D%E5%BE%97%E5%AF%B9%E5%85%B6%E4%B8%BB%E5%BC%A0%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="爱益人可以信托受益权清偿个人债务" _mubu_text="%3Cspan%3E%E7%88%B1%E7%9B%8A%E4%BA%BA%E5%8F%AF%E4%BB%A5%E4%BF%A1%E6%89%98%E5%8F%97%E7%9B%8A%E6%9D%83%E6%B8%85%E5%81%BF%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="信托当事人" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
          <outline text="委托人" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="具完民行能的自然人、(非)法人" _mubu_text="%3Cspan%3E%E5%85%B7%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%E7%9A%84%E8%87%AA%E7%84%B6%E4%BA%BA%E3%80%81(%E9%9D%9E)%E6%B3%95%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一个或多个" _mubu_text="%3Cspan%3E%E4%B8%80%E4%B8%AA%E6%88%96%E5%A4%9A%E4%B8%AA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可为唯一受益人或共同受益人之一" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%BA%E5%94%AF%E4%B8%80%E5%8F%97%E7%9B%8A%E4%BA%BA%E6%88%96%E5%85%B1%E5%90%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B9%8B%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对受托人" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%8F%97%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="行为的撤销权" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E7%9A%84%E6%92%A4%E9%94%80%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="违反目的处分、处理不当致损的" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E7%9B%AE%E7%9A%84%E5%A4%84%E5%88%86%E3%80%81%E5%A4%84%E7%90%86%E4%B8%8D%E5%BD%93%E8%87%B4%E6%8D%9F%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="申请法院撤销" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%B3%95%E9%99%A2%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="(应)知道撤销原因之日起1年内" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E6%92%A4%E9%94%80%E5%8E%9F%E5%9B%A0%E4%B9%8B%E6%97%A5%E8%B5%B71%E5%B9%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="对财产受让人" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B4%A2%E4%BA%A7%E5%8F%97%E8%AE%A9%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="明知是违反仍接受，应返还或赔偿" _mubu_text="%3Cspan%3E%E6%98%8E%E7%9F%A5%E6%98%AF%E8%BF%9D%E5%8F%8D%E4%BB%8D%E6%8E%A5%E5%8F%97%EF%BC%8C%E5%BA%94%E8%BF%94%E8%BF%98%E6%88%96%E8%B5%94%E5%81%BF%3C/span%3E" _note="即恶意；善意除外" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%81%B6%E6%84%8F%EF%BC%9B%E5%96%84%E6%84%8F%E9%99%A4%E5%A4%96%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="解任权" _mubu_text="%3Cspan%3E%E8%A7%A3%E4%BB%BB%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="违反信托目的处分" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E4%BF%A1%E6%89%98%E7%9B%AE%E7%9A%84%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="管理运用处分有重大过失" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E8%BF%90%E7%94%A8%E5%A4%84%E5%88%86%E6%9C%89%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="申请法院解任" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%B3%95%E9%99%A2%E8%A7%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="变更受益人、处分信托受益权" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E5%8F%97%E7%9B%8A%E4%BA%BA%E3%80%81%E5%A4%84%E5%88%86%E4%BF%A1%E6%89%98%E5%8F%97%E7%9B%8A%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="受益人对委托人/其他共同受益人有重大侵权行为" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%AF%B9%E5%A7%94%E6%89%98%E4%BA%BA/%E5%85%B6%E4%BB%96%E5%85%B1%E5%90%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E6%9C%89%E9%87%8D%E5%A4%A7%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="经受益人同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="受托人" _mubu_text="%3Cspan%3E%E5%8F%97%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="具完民行能的自然人、法人" _mubu_text="%3Cspan%3E%E5%85%B7%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%E7%9A%84%E8%87%AA%E7%84%B6%E4%BA%BA%E3%80%81%E6%B3%95%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一个或多个" _mubu_text="%3Cspan%3E%E4%B8%80%E4%B8%AA%E6%88%96%E5%A4%9A%E4%B8%AA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可为受益人之一" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%BA%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B9%8B%E4%B8%80%3C/span%3E" _note="不可为唯一受益人" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E4%B8%BA%E5%94%AF%E4%B8%80%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E"/>
            <outline text="亲自处理信托事务" _mubu_text="%3Cspan%3E%E4%BA%B2%E8%87%AA%E5%A4%84%E7%90%86%E4%BF%A1%E6%89%98%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="另规或有不得已事由除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E8%A7%84%E6%88%96%E6%9C%89%E4%B8%8D%E5%BE%97%E5%B7%B2%E4%BA%8B%E7%94%B1%E9%99%A4%E5%A4%96%3C/span%3E"/>
            <outline text="分别管理义务" _mubu_text="%3Cspan%3E%E5%88%86%E5%88%AB%E7%AE%A1%E7%90%86%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="将信托财产与其固有财产" _mubu_text="%3Cspan%3E%E5%B0%86%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%B8%8E%E5%85%B6%E5%9B%BA%E6%9C%89%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不同委托人的信托财产" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%E5%A7%94%E6%89%98%E4%BA%BA%E7%9A%84%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="分别管理、分别记账、公平对待" _mubu_text="%3Cspan%3E%E5%88%86%E5%88%AB%E7%AE%A1%E7%90%86%E3%80%81%E5%88%86%E5%88%AB%E8%AE%B0%E8%B4%A6%E3%80%81%E5%85%AC%E5%B9%B3%E5%AF%B9%E5%BE%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="禁止自我交易" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E8%87%AA%E6%88%91%E4%BA%A4%E6%98%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="其固有财产与信托财产之间" _mubu_text="%3Cspan%3E%E5%85%B6%E5%9B%BA%E6%9C%89%E8%B4%A2%E4%BA%A7%E4%B8%8E%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不同委托人的信托财产之间" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%E5%A7%94%E6%89%98%E4%BA%BA%E7%9A%84%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="经委托人或者受益人同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%A7%94%E6%89%98%E4%BA%BA%E6%88%96%E8%80%85%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="或有另约" _mubu_note="%3Cspan%3E%E6%88%96%E6%9C%89%E5%8F%A6%E7%BA%A6%3C/span%3E"/>
                <outline text="并以公平的市场价格进行" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%BB%A5%E5%85%AC%E5%B9%B3%E7%9A%84%E5%B8%82%E5%9C%BA%E4%BB%B7%E6%A0%BC%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="职责终止" _mubu_text="%3Cspan%3E%E8%81%8C%E8%B4%A3%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="职责终止，不意味着信托终止" _mubu_text="%3Cspan%3E%E8%81%8C%E8%B4%A3%E7%BB%88%E6%AD%A2%EF%BC%8C%E4%B8%8D%E6%84%8F%E5%91%B3%E7%9D%80%E4%BF%A1%E6%89%98%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="其继承人或者遗产管理人、监护人、清算人应当妥善保管信托财产，协助新受托人接管信托事务" _mubu_text="%3Cspan%3E%E5%85%B6%E7%BB%A7%E6%89%BF%E4%BA%BA%E6%88%96%E8%80%85%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%E3%80%81%E7%9B%91%E6%8A%A4%E4%BA%BA%E3%80%81%E6%B8%85%E7%AE%97%E4%BA%BA%E5%BA%94%E5%BD%93%E5%A6%A5%E5%96%84%E4%BF%9D%E7%AE%A1%E4%BF%A1%E6%89%98%E8%B4%A2%E4%BA%A7%EF%BC%8C%E5%8D%8F%E5%8A%A9%E6%96%B0%E5%8F%97%E6%89%98%E4%BA%BA%E6%8E%A5%E7%AE%A1%E4%BF%A1%E6%89%98%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="选任新受托人" _mubu_text="%3Cspan%3E%E9%80%89%E4%BB%BB%E6%96%B0%E5%8F%97%E6%89%98%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="1、依信托文件规定" _mubu_text="%3Cspan%3E1%E3%80%81%E4%BE%9D%E4%BF%A1%E6%89%98%E6%96%87%E4%BB%B6%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="2、无，由委托人选任" _mubu_text="%3Cspan%3E2%E3%80%81%E6%97%A0%EF%BC%8C%E7%94%B1%E5%A7%94%E6%89%98%E4%BA%BA%E9%80%89%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="3、无，由受益人选任" _mubu_text="%3Cspan%3E3%E3%80%81%E6%97%A0%EF%BC%8C%E7%94%B1%E5%8F%97%E7%9B%8A%E4%BA%BA%E9%80%89%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="4、无，由受益人监护人代行选任" _mubu_text="%3Cspan%3E4%E3%80%81%E6%97%A0%EF%BC%8C%E7%94%B1%E5%8F%97%E7%9B%8A%E4%BA%BA%E7%9B%91%E6%8A%A4%E4%BA%BA%E4%BB%A3%E8%A1%8C%E9%80%89%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="受益人" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="自然人(无行能要求)、法人、其他组织" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E4%BA%BA(%E6%97%A0%E8%A1%8C%E8%83%BD%E8%A6%81%E6%B1%82)%E3%80%81%E6%B3%95%E4%BA%BA%E3%80%81%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一个或多个" _mubu_text="%3Cspan%3E%E4%B8%80%E4%B8%AA%E6%88%96%E5%A4%9A%E4%B8%AA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="身份竞合" _mubu_text="%3Cspan%3E%E8%BA%AB%E4%BB%BD%E7%AB%9E%E5%90%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="委托人可以是受益人，也可以是唯一的受益人" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BA%BA%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%8F%97%E7%9B%8A%E4%BA%BA%EF%BC%8C%E4%B9%9F%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%94%AF%E4%B8%80%E7%9A%84%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="受托人可以是受益人，但不能是唯一的受益人" _mubu_text="%3Cspan%3E%E5%8F%97%E6%89%98%E4%BA%BA%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%8F%97%E7%9B%8A%E4%BA%BA%EF%BC%8C%E4%BD%86%E4%B8%8D%E8%83%BD%E6%98%AF%E5%94%AF%E4%B8%80%E7%9A%84%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="受益权" _mubu_text="%3Cspan%3E%E5%8F%97%E7%9B%8A%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="信托文件未规定的，各受益人按均等比例享受" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E6%96%87%E4%BB%B6%E6%9C%AA%E8%A7%84%E5%AE%9A%E7%9A%84%EF%BC%8C%E5%90%84%E5%8F%97%E7%9B%8A%E4%BA%BA%E6%8C%89%E5%9D%87%E7%AD%89%E6%AF%94%E4%BE%8B%E4%BA%AB%E5%8F%97%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="放弃" _mubu_text="%3Cspan%3E%E6%94%BE%E5%BC%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="全体放弃" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BD%93%E6%94%BE%E5%BC%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="信托终止" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="部分放弃" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E6%94%BE%E5%BC%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="该部分归属" _mubu_text="%3Cspan%3E%E8%AF%A5%E9%83%A8%E5%88%86%E5%BD%92%E5%B1%9E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="1、文件规定" _mubu_text="%3Cspan%3E1%E3%80%81%E6%96%87%E4%BB%B6%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="2、无，其他受益人" _mubu_text="%3Cspan%3E2%E3%80%81%E6%97%A0%EF%BC%8C%E5%85%B6%E4%BB%96%E5%8F%97%E7%9B%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="3、无，委托人或者其继承人" _mubu_text="%3Cspan%3E3%E3%80%81%E6%97%A0%EF%BC%8C%E5%A7%94%E6%89%98%E4%BA%BA%E6%88%96%E8%80%85%E5%85%B6%E7%BB%A7%E6%89%BF%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="处分" _mubu_text="%3Cspan%3E%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="可清偿受益人个人债务" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%B8%85%E5%81%BF%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="可被强制执行" _mubu_note="%3Cspan%3E%E5%8F%AF%E8%A2%AB%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E"/>
                <outline text="可转让和继承" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%BD%AC%E8%AE%A9%E5%92%8C%E7%BB%A7%E6%89%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="撤销权、解任权" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E6%9D%83%E3%80%81%E8%A7%A3%E4%BB%BB%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="对受托人行为撤销权，对受托人解任权" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%8F%97%E6%89%98%E4%BA%BA%E8%A1%8C%E4%B8%BA%E6%92%A4%E9%94%80%E6%9D%83%EF%BC%8C%E5%AF%B9%E5%8F%97%E6%89%98%E4%BA%BA%E8%A7%A3%E4%BB%BB%E6%9D%83%3C/span%3E" _note="与委托人意见不一致时，可申请法院裁定" _mubu_note="%3Cspan%3E%E4%B8%8E%E5%A7%94%E6%89%98%E4%BA%BA%E6%84%8F%E8%A7%81%E4%B8%8D%E4%B8%80%E8%87%B4%E6%97%B6%EF%BC%8C%E5%8F%AF%E7%94%B3%E8%AF%B7%E6%B3%95%E9%99%A2%E8%A3%81%E5%AE%9A%3C/span%3E"/>
              <outline text="共同受益人之一申请的，裁定对全体有效" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E4%B9%8B%E4%B8%80%E7%94%B3%E8%AF%B7%E7%9A%84%EF%BC%8C%E8%A3%81%E5%AE%9A%E5%AF%B9%E5%85%A8%E4%BD%93%E6%9C%89%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="信托的终止" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%89%98%E7%9A%84%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="连续性" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="不因委托人或受托人失格情形而终止" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%9B%A0%E5%A7%94%E6%89%98%E4%BA%BA%E6%88%96%E5%8F%97%E6%89%98%E4%BA%BA%E5%A4%B1%E6%A0%BC%E6%83%85%E5%BD%A2%E8%80%8C%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="财产归属" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%B4%A2%E4%BA%A7%E5%BD%92%E5%B1%9E%3C/span%3E" _note="" _mubu_note="">
            <outline text="1、文件规定" _mubu_text="%3Cspan%3E1%E3%80%81%E6%96%87%E4%BB%B6%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="2、无，受益人或其继承人" _mubu_text="%3Cspan%3E2%E3%80%81%E6%97%A0%EF%BC%8C%E5%8F%97%E7%9B%8A%E4%BA%BA%E6%88%96%E5%85%B6%E7%BB%A7%E6%89%BF%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="3、无，委托人或其继承人" _mubu_text="%3Cspan%3E3%E3%80%81%E6%97%A0%EF%BC%8C%E5%A7%94%E6%89%98%E4%BA%BA%E6%88%96%E5%85%B6%E7%BB%A7%E6%89%BF%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="经济法" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%B5%8E%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="竞争法" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%BA%89%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="反垄断法" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%9E%84%E6%96%AD%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="概述" _mubu_text="%3Cspan%3E%E6%A6%82%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="境内" _mubu_text="%3Cspan%3E%E5%A2%83%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="境外，对境内产生排除、限制影响" _mubu_text="%3Cspan%3E%E5%A2%83%E5%A4%96%EF%BC%8C%E5%AF%B9%E5%A2%83%E5%86%85%E4%BA%A7%E7%94%9F%E6%8E%92%E9%99%A4%E3%80%81%E9%99%90%E5%88%B6%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不适用" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="知识产权" _mubu_text="%3Cspan%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="农业生产的联合或协同行为" _mubu_text="%3Cspan%3E%E5%86%9C%E4%B8%9A%E7%94%9F%E4%BA%A7%E7%9A%84%E8%81%94%E5%90%88%E6%88%96%E5%8D%8F%E5%90%8C%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="建立健全公平竞争审查制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%BB%BA%E7%AB%8B%E5%81%A5%E5%85%A8%E5%85%AC%E5%B9%B3%E7%AB%9E%E4%BA%89%E5%AE%A1%E6%9F%A5%E5%88%B6%E5%BA%A6%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
              <outline text="进行抽象行政行为时" _mubu_text="%3Cspan%3E%E8%BF%9B%E8%A1%8C%E6%8A%BD%E8%B1%A1%E8%A1%8C%E6%94%BF%E8%A1%8C%E4%B8%BA%E6%97%B6%3C/span%3E" _note="制定相关规定" _mubu_note="%3Cspan%3E%E5%88%B6%E5%AE%9A%E7%9B%B8%E5%85%B3%E8%A7%84%E5%AE%9A%3C/span%3E"/>
              <outline text="应进行公平竞争审查" _mubu_text="%3Cspan%3E%E5%BA%94%E8%BF%9B%E8%A1%8C%E5%85%AC%E5%B9%B3%E7%AB%9E%E4%BA%89%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="清理有限制排除竞争的内容" _mubu_note="%3Cspan%3E%E6%B8%85%E7%90%86%E6%9C%89%E9%99%90%E5%88%B6%E6%8E%92%E9%99%A4%E7%AB%9E%E4%BA%89%E7%9A%84%E5%86%85%E5%AE%B9%3C/span%3E"/>
            </outline>
            <outline text="数字经济领域反垄断原则" _mubu_text="%3Cspan%3E%E6%95%B0%E5%AD%97%E7%BB%8F%E6%B5%8E%E9%A2%86%E5%9F%9F%E5%8F%8D%E5%9E%84%E6%96%AD%E5%8E%9F%E5%88%99%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
              <outline text="利用数据和算法、技术、资本优势以及平台规则等" _mubu_text="%3Cspan%3E%E5%88%A9%E7%94%A8%E6%95%B0%E6%8D%AE%E5%92%8C%E7%AE%97%E6%B3%95%E3%80%81%E6%8A%80%E6%9C%AF%E3%80%81%E8%B5%84%E6%9C%AC%E4%BC%98%E5%8A%BF%E4%BB%A5%E5%8F%8A%E5%B9%B3%E5%8F%B0%E8%A7%84%E5%88%99%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="反垄断机构" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%9E%84%E6%96%AD%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="反垄断委员会" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%9E%84%E6%96%AD%E5%A7%94%E5%91%98%E4%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="反垄断执法机构" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%9E%84%E6%96%AD%E6%89%A7%E6%B3%95%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="国家市场监督管理总局" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%B8%82%E5%9C%BA%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%E6%80%BB%E5%B1%80%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="根据工作需要，可授权到省一级" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E5%B7%A5%E4%BD%9C%E9%9C%80%E8%A6%81%EF%BC%8C%E5%8F%AF%E6%8E%88%E6%9D%83%E5%88%B0%E7%9C%81%E4%B8%80%E7%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="垄断协议" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="横向垄断协议" _mubu_text="%3Cspan%3E%E6%A8%AA%E5%90%91%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="具有竞争关系的经营者" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E7%AB%9E%E4%BA%89%E5%85%B3%E7%B3%BB%E7%9A%84%E7%BB%8F%E8%90%A5%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="纵向垄断协议" _mubu_text="%3Cspan%3E%E7%BA%B5%E5%90%91%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="经营者与交易相对人" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E4%B8%8E%E4%BA%A4%E6%98%93%E7%9B%B8%E5%AF%B9%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="经营者能证明其不具有排除、限制竞争效果的，不禁止" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E8%83%BD%E8%AF%81%E6%98%8E%E5%85%B6%E4%B8%8D%E5%85%B7%E6%9C%89%E6%8E%92%E9%99%A4%E3%80%81%E9%99%90%E5%88%B6%E7%AB%9E%E4%BA%89%E6%95%88%E6%9E%9C%E7%9A%84%EF%BC%8C%E4%B8%8D%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="安全港制度" _mubu_text="%3Cspan%3E%E5%AE%89%E5%85%A8%E6%B8%AF%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="经营者能够证明" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E8%83%BD%E5%A4%9F%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其在相关市场的市场份额低于国务院反垄断执法机构规定的标准" _mubu_text="%3Cspan%3E%E5%85%B6%E5%9C%A8%E7%9B%B8%E5%85%B3%E5%B8%82%E5%9C%BA%E7%9A%84%E5%B8%82%E5%9C%BA%E4%BB%BD%E9%A2%9D%E4%BD%8E%E4%BA%8E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%8D%E5%9E%84%E6%96%AD%E6%89%A7%E6%B3%95%E6%9C%BA%E6%9E%84%E8%A7%84%E5%AE%9A%E7%9A%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="并符合国务院反垄断执法机构规定的其他条件" _mubu_text="%3Cspan%3E%E5%B9%B6%E7%AC%A6%E5%90%88%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%8D%E5%9E%84%E6%96%AD%E6%89%A7%E6%B3%95%E6%9C%BA%E6%9E%84%E8%A7%84%E5%AE%9A%E7%9A%84%E5%85%B6%E4%BB%96%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="不予禁止" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="轴辐协议" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%BD%B4%E8%BE%90%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
              <outline text="组织型垄断协议" _mubu_text="%3Cspan%3E%E7%BB%84%E7%BB%87%E5%9E%8B%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="经营者不得组织其他经营者达成垄断协议" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E4%B8%8D%E5%BE%97%E7%BB%84%E7%BB%87%E5%85%B6%E4%BB%96%E7%BB%8F%E8%90%A5%E8%80%85%E8%BE%BE%E6%88%90%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="帮助型垄断协议" _mubu_text="%3Cspan%3E%E5%B8%AE%E5%8A%A9%E5%9E%8B%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="经营者不得为其他经营者达成垄断协议提供实质性帮助" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E4%B8%8D%E5%BE%97%E4%B8%BA%E5%85%B6%E4%BB%96%E7%BB%8F%E8%90%A5%E8%80%85%E8%BE%BE%E6%88%90%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%E6%8F%90%E4%BE%9B%E5%AE%9E%E8%B4%A8%E6%80%A7%E5%B8%AE%E5%8A%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="以组织帮助者为轴，其他与其达成协议" _mubu_text="%3Cspan%3E%E4%BB%A5%E7%BB%84%E7%BB%87%E5%B8%AE%E5%8A%A9%E8%80%85%E4%B8%BA%E8%BD%B4%EF%BC%8C%E5%85%B6%E4%BB%96%E4%B8%8E%E5%85%B6%E8%BE%BE%E6%88%90%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="其他之间有横向垄断协议的效果" _mubu_note="%3Cspan%3E%E5%85%B6%E4%BB%96%E4%B9%8B%E9%97%B4%E6%9C%89%E6%A8%AA%E5%90%91%E5%9E%84%E6%96%AD%E5%8D%8F%E8%AE%AE%E7%9A%84%E6%95%88%E6%9E%9C%3C/span%3E"/>
            </outline>
            <outline text="豁免" _mubu_text="%3Cspan%3E%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="目的正当行为+2个效果" _mubu_text="%3Cspan%3E%E7%9B%AE%E7%9A%84%E6%AD%A3%E5%BD%93%E8%A1%8C%E4%B8%BA+2%E4%B8%AA%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="不会严重限制相关市场的竞争" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BC%9A%E4%B8%A5%E9%87%8D%E9%99%90%E5%88%B6%E7%9B%B8%E5%85%B3%E5%B8%82%E5%9C%BA%E7%9A%84%E7%AB%9E%E4%BA%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="且能使消费者分享由此产生的利益" _mubu_text="%3Cspan%3E%E4%B8%94%E8%83%BD%E4%BD%BF%E6%B6%88%E8%B4%B9%E8%80%85%E5%88%86%E4%BA%AB%E7%94%B1%E6%AD%A4%E4%BA%A7%E7%94%9F%E7%9A%84%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="对外贸易" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E8%B4%B8%E6%98%93%3C/span%3E" _note="" _mubu_note="">
                <outline text="为保障外贸和对外经济合作中的正当利益" _mubu_text="%3Cspan%3E%E4%B8%BA%E4%BF%9D%E9%9A%9C%E5%A4%96%E8%B4%B8%E5%92%8C%E5%AF%B9%E5%A4%96%E7%BB%8F%E6%B5%8E%E5%90%88%E4%BD%9C%E4%B8%AD%E7%9A%84%E6%AD%A3%E5%BD%93%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="对经营者负责人的处罚" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%BB%8F%E8%90%A5%E8%80%85%E8%B4%9F%E8%B4%A3%E4%BA%BA%E7%9A%84%E5%A4%84%E7%BD%9A%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E"/>
              <outline text="加倍处罚" _mubu_text="%3Cspan%3E%E5%8A%A0%E5%80%8D%E5%A4%84%E7%BD%9A%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="罚款数额2~5倍" _mubu_text="%3Cspan%3E%E7%BD%9A%E6%AC%BE%E6%95%B0%E9%A2%9D2~5%E5%80%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="滥用市场支配地位" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%BB%A5%E7%94%A8%E5%B8%82%E5%9C%BA%E6%94%AF%E9%85%8D%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="认定" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="以市场份额为主、兼顾反映企业综合经济实力的其他因素" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%B8%82%E5%9C%BA%E4%BB%BD%E9%A2%9D%E4%B8%BA%E4%B8%BB%E3%80%81%E5%85%BC%E9%A1%BE%E5%8F%8D%E6%98%A0%E4%BC%81%E4%B8%9A%E7%BB%BC%E5%90%88%E7%BB%8F%E6%B5%8E%E5%AE%9E%E5%8A%9B%E7%9A%84%E5%85%B6%E4%BB%96%E5%9B%A0%E7%B4%A0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="推定" _mubu_text="%3Cspan%3E%E6%8E%A8%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="1个经营者在相关市场的市场份额达1/2的" _mubu_text="%3Cspan%3E1%E4%B8%AA%E7%BB%8F%E8%90%A5%E8%80%85%E5%9C%A8%E7%9B%B8%E5%85%B3%E5%B8%82%E5%9C%BA%E7%9A%84%E5%B8%82%E5%9C%BA%E4%BB%BD%E9%A2%9D%E8%BE%BE1/2%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="2个合计达2/3的" _mubu_text="%3Cspan%3E2%E4%B8%AA%E5%90%88%E8%AE%A1%E8%BE%BE2/3%E7%9A%84%3C/span%3E" _note="其中有的经营者市场份额不足1/10的除外" _mubu_note="%3Cspan%3E%E5%85%B6%E4%B8%AD%E6%9C%89%E7%9A%84%E7%BB%8F%E8%90%A5%E8%80%85%E5%B8%82%E5%9C%BA%E4%BB%BD%E9%A2%9D%E4%B8%8D%E8%B6%B31/10%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="3个合计达3/4的" _mubu_text="%3Cspan%3E3%E4%B8%AA%E5%90%88%E8%AE%A1%E8%BE%BE3/4%E7%9A%84%3C/span%3E" _note="其中有的经营者市场份额不足1/10的除外" _mubu_note="%3Cspan%3E%E5%85%B6%E4%B8%AD%E6%9C%89%E7%9A%84%E7%BB%8F%E8%90%A5%E8%80%85%E5%B8%82%E5%9C%BA%E4%BB%BD%E9%A2%9D%E4%B8%8D%E8%B6%B31/10%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="有证据证明不具有市场支配地位的除外" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E4%B8%8D%E5%85%B7%E6%9C%89%E5%B8%82%E5%9C%BA%E6%94%AF%E9%85%8D%E5%9C%B0%E4%BD%8D%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="责令停止" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%A4%E5%81%9C%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="没收违法所得" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%94%B6%E8%BF%9D%E6%B3%95%E6%89%80%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="并处罚款" _mubu_text="%3Cspan%3E%E5%B9%B6%E5%A4%84%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="上一年度销售额1％~10％" _mubu_text="%3Cspan%3E%E4%B8%8A%E4%B8%80%E5%B9%B4%E5%BA%A6%E9%94%80%E5%94%AE%E9%A2%9D1%EF%BC%85~10%EF%BC%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="加倍处罚" _mubu_text="%3Cspan%3E%E5%8A%A0%E5%80%8D%E5%A4%84%E7%BD%9A%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="罚款数额2~5倍" _mubu_text="%3Cspan%3E%E7%BD%9A%E6%AC%BE%E6%95%B0%E9%A2%9D2~5%E5%80%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="经营者集中" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E9%9B%86%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="申报" _mubu_text="%3Cspan%3E%E7%94%B3%E6%8A%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="应事先申报，不得事后补报" _mubu_text="%3Cspan%3E%E5%BA%94%E4%BA%8B%E5%85%88%E7%94%B3%E6%8A%A5%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BA%8B%E5%90%8E%E8%A1%A5%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="申报标准" _mubu_text="%3Cspan%3E%E7%94%B3%E6%8A%A5%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="需在国内外达到一定营业额体量" _mubu_text="%3Cspan%3E%E9%9C%80%E5%9C%A8%E5%9B%BD%E5%86%85%E5%A4%96%E8%BE%BE%E5%88%B0%E4%B8%80%E5%AE%9A%E8%90%A5%E4%B8%9A%E9%A2%9D%E4%BD%93%E9%87%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未达标准" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%9C%AA%E8%BE%BE%E6%A0%87%E5%87%86%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                  <outline text="有证据证明可能具有排除、限制竞争效果的" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E5%8F%AF%E8%83%BD%E5%85%B7%E6%9C%89%E6%8E%92%E9%99%A4%E3%80%81%E9%99%90%E5%88%B6%E7%AB%9E%E4%BA%89%E6%95%88%E6%9E%9C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="可要求其申报" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%A6%81%E6%B1%82%E5%85%B6%E7%94%B3%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="未申报后果" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%94%B3%E6%8A%A5%E5%90%8E%E6%9E%9C%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="国务院反垄断机构依法进行调查" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%8D%E5%9E%84%E6%96%AD%E6%9C%BA%E6%9E%84%E4%BE%9D%E6%B3%95%E8%BF%9B%E8%A1%8C%E8%B0%83%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="母公司对子公司的整合" _mubu_text="%3Cspan%3E%E6%AF%8D%E5%85%AC%E5%8F%B8%E5%AF%B9%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%E6%95%B4%E5%90%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="兄弟公司之间的整合" _mubu_text="%3Cspan%3E%E5%85%84%E5%BC%9F%E5%85%AC%E5%8F%B8%E4%B9%8B%E9%97%B4%E7%9A%84%E6%95%B4%E5%90%88%3C/span%3E" _note="属同一母公司" _mubu_note="%3Cspan%3E%E5%B1%9E%E5%90%8C%E4%B8%80%E6%AF%8D%E5%85%AC%E5%8F%B8%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="审查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="初步审查" _mubu_text="%3Cspan%3E%E5%88%9D%E6%AD%A5%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="进一步审查" _mubu_text="%3Cspan%3E%E8%BF%9B%E4%B8%80%E6%AD%A5%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="90+60" _mubu_text="%3Cspan%3E90+60%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="停表制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%81%9C%E8%A1%A8%E5%88%B6%E5%BA%A6%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="中止计算审查期限" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%E8%AE%A1%E7%AE%97%E5%AE%A1%E6%9F%A5%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="分级审查制度" _mubu_text="%3Cspan%3E%E5%88%86%E7%BA%A7%E5%AE%A1%E6%9F%A5%E5%88%B6%E5%BA%A6%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="分类分级审查" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%E5%88%86%E7%BA%A7%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="提高审查质量和效率" _mubu_text="%3Cspan%3E%E6%8F%90%E9%AB%98%E5%AE%A1%E6%9F%A5%E8%B4%A8%E9%87%8F%E5%92%8C%E6%95%88%E7%8E%87%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="审查后决定" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E5%90%8E%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="禁止集中" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E9%9B%86%E4%B8%AD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不禁止" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="附加限制性条件" _mubu_text="%3Cspan%3E%E9%99%84%E5%8A%A0%E9%99%90%E5%88%B6%E6%80%A7%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="将禁止的向社会公布" _mubu_text="%3Cspan%3E%E5%B0%86%E7%A6%81%E6%AD%A2%E7%9A%84%E5%90%91%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%B8%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="(可能)具有排除竞争效果的" _mubu_text="%3Cspan%3E(%E5%8F%AF%E8%83%BD)%E5%85%B7%E6%9C%89%E6%8E%92%E9%99%A4%E7%AB%9E%E4%BA%89%E6%95%88%E6%9E%9C%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="恢复到集中前状态" _mubu_text="%3Cspan%3E%E6%81%A2%E5%A4%8D%E5%88%B0%E9%9B%86%E4%B8%AD%E5%89%8D%E7%8A%B6%E6%80%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="≤上年度销售额10%罚款" _mubu_text="%3Cspan%3E%E2%89%A4%E4%B8%8A%E5%B9%B4%E5%BA%A6%E9%94%80%E5%94%AE%E9%A2%9D10%25%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不具有排除竞争效果的" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%B7%E6%9C%89%E6%8E%92%E9%99%A4%E7%AB%9E%E4%BA%89%E6%95%88%E6%9E%9C%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="≤500万罚款" _mubu_text="%3Cspan%3E%E2%89%A4500%E4%B8%87%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="加倍处罚" _mubu_text="%3Cspan%3E%E5%8A%A0%E5%80%8D%E5%A4%84%E7%BD%9A%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                <outline text="罚款数额2~5倍" _mubu_text="%3Cspan%3E%E7%BD%9A%E6%AC%BE%E6%95%B0%E9%A2%9D2~5%E5%80%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="滥用行政权力排除、限制竞争行为" _mubu_text="%3Cspan%3E%E6%BB%A5%E7%94%A8%E8%A1%8C%E6%94%BF%E6%9D%83%E5%8A%9B%E6%8E%92%E9%99%A4%E3%80%81%E9%99%90%E5%88%B6%E7%AB%9E%E4%BA%89%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="反垄断公益诉讼" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%9E%84%E6%96%AD%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
              <outline text="设区的市级以上检察院向法院提起民事公益诉讼" _mubu_text="%3Cspan%3E%E8%AE%BE%E5%8C%BA%E7%9A%84%E5%B8%82%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%A3%80%E5%AF%9F%E9%99%A2%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E8%B5%B7%E6%B0%91%E4%BA%8B%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="计入信用记录，向社会公示" _mubu_text="%3Cspan%3E%E8%AE%A1%E5%85%A5%E4%BF%A1%E7%94%A8%E8%AE%B0%E5%BD%95%EF%BC%8C%E5%90%91%E7%A4%BE%E4%BC%9A%E5%85%AC%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="刑事责任" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="反不正当竞争法" _mubu_text="%3Cspan%3E%E5%8F%8D%E4%B8%8D%E6%AD%A3%E5%BD%93%E7%AB%9E%E4%BA%89%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="商业混淆行为" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%95%86%E4%B8%9A%E6%B7%B7%E6%B7%86%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="“蹭热度”" _mubu_note="%3Cspan%3E%E2%80%9C%E8%B9%AD%E7%83%AD%E5%BA%A6%E2%80%9D%3C/span%3E">
            <outline text="与他人商品标识相混淆" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%BB%96%E4%BA%BA%E5%95%86%E5%93%81%E6%A0%87%E8%AF%86%E7%9B%B8%E6%B7%B7%E6%B7%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="与他人名称或姓名相混淆" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%BB%96%E4%BA%BA%E5%90%8D%E7%A7%B0%E6%88%96%E5%A7%93%E5%90%8D%E7%9B%B8%E6%B7%B7%E6%B7%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="其他商业混淆行为" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%95%86%E4%B8%9A%E6%B7%B7%E6%B7%86%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="应当及时办理名称变更登记" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%8F%8A%E6%97%B6%E5%8A%9E%E7%90%86%E5%90%8D%E7%A7%B0%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="变更前，由原企业登记机关以统一社会信用代码代替其名称" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E5%89%8D%EF%BC%8C%E7%94%B1%E5%8E%9F%E4%BC%81%E4%B8%9A%E7%99%BB%E8%AE%B0%E6%9C%BA%E5%85%B3%E4%BB%A5%E7%BB%9F%E4%B8%80%E7%A4%BE%E4%BC%9A%E4%BF%A1%E7%94%A8%E4%BB%A3%E7%A0%81%E4%BB%A3%E6%9B%BF%E5%85%B6%E5%90%8D%E7%A7%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="虚假宣传行为" _mubu_text="%3Cspan%3E%E8%99%9A%E5%81%87%E5%AE%A3%E4%BC%A0%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="虚假的商业宣传" _mubu_text="%3Cspan%3E%E8%99%9A%E5%81%87%E7%9A%84%E5%95%86%E4%B8%9A%E5%AE%A3%E4%BC%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="引人误解的商业宣传" _mubu_text="%3Cspan%3E%E5%BC%95%E4%BA%BA%E8%AF%AF%E8%A7%A3%E7%9A%84%E5%95%86%E4%B8%9A%E5%AE%A3%E4%BC%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="组织虚假交易等" _mubu_text="%3Cspan%3E%E7%BB%84%E7%BB%87%E8%99%9A%E5%81%87%E4%BA%A4%E6%98%93%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="请求赔偿损失，应举证其受到损失" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E8%B5%94%E5%81%BF%E6%8D%9F%E5%A4%B1%EF%BC%8C%E5%BA%94%E4%B8%BE%E8%AF%81%E5%85%B6%E5%8F%97%E5%88%B0%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="商业贿赂行为" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E8%B4%BF%E8%B5%82%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="谋取交易机会或竞争优势" _mubu_text="%3Cspan%3E%E8%B0%8B%E5%8F%96%E4%BA%A4%E6%98%93%E6%9C%BA%E4%BC%9A%E6%88%96%E7%AB%9E%E4%BA%89%E4%BC%98%E5%8A%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="可以明示方式向交易相对方支付折扣，或者向中间人支付佣金" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E6%98%8E%E7%A4%BA%E6%96%B9%E5%BC%8F%E5%90%91%E4%BA%A4%E6%98%93%E7%9B%B8%E5%AF%B9%E6%96%B9%E6%94%AF%E4%BB%98%E6%8A%98%E6%89%A3%EF%BC%8C%E6%88%96%E8%80%85%E5%90%91%E4%B8%AD%E9%97%B4%E4%BA%BA%E6%94%AF%E4%BB%98%E4%BD%A3%E9%87%91%3C/span%3E" _note="应当如实人账" _mubu_note="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%A6%82%E5%AE%9E%E4%BA%BA%E8%B4%A6%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="侵犯商业秘密" _mubu_text="%3Cspan%3E%E4%BE%B5%E7%8A%AF%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="长期稳定的交易关系不属于商业秘密" _mubu_text="%3Cspan%3E%E9%95%BF%E6%9C%9F%E7%A8%B3%E5%AE%9A%E7%9A%84%E4%BA%A4%E6%98%93%E5%85%B3%E7%B3%BB%E4%B8%8D%E5%B1%9E%E4%BA%8E%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="如特定客户稳定的交易关系" _mubu_note="%3Cspan%3E%E5%A6%82%E7%89%B9%E5%AE%9A%E5%AE%A2%E6%88%B7%E7%A8%B3%E5%AE%9A%E7%9A%84%E4%BA%A4%E6%98%93%E5%85%B3%E7%B3%BB%3C/span%3E"/>
            <outline text="自行研制和反向工程不是侵犯商业秘密" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%E7%A0%94%E5%88%B6%E5%92%8C%E5%8F%8D%E5%90%91%E5%B7%A5%E7%A8%8B%E4%B8%8D%E6%98%AF%E4%BE%B5%E7%8A%AF%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="诋毁他人商誉的行为" _mubu_text="%3Cspan%3E%E8%AF%8B%E6%AF%81%E4%BB%96%E4%BA%BA%E5%95%86%E8%AA%89%E7%9A%84%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="行为" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="编造、传播虚假信息或者误导性信息" _mubu_text="%3Cspan%3E%E7%BC%96%E9%80%A0%E3%80%81%E4%BC%A0%E6%92%AD%E8%99%9A%E5%81%87%E4%BF%A1%E6%81%AF%E6%88%96%E8%80%85%E8%AF%AF%E5%AF%BC%E6%80%A7%E4%BF%A1%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="传播他人编造的虚假信息或者误导性信息意" _mubu_text="%3Cspan%3E%E4%BC%A0%E6%92%AD%E4%BB%96%E4%BA%BA%E7%BC%96%E9%80%A0%E7%9A%84%E8%99%9A%E5%81%87%E4%BF%A1%E6%81%AF%E6%88%96%E8%80%85%E8%AF%AF%E5%AF%BC%E6%80%A7%E4%BF%A1%E6%81%AF%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对象" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="一个或多个特定竞争对手" _mubu_text="%3Cspan%3E%E4%B8%80%E4%B8%AA%E6%88%96%E5%A4%9A%E4%B8%AA%E7%89%B9%E5%AE%9A%E7%AB%9E%E4%BA%89%E5%AF%B9%E6%89%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="对比性宣传是以同行业所有经营者为竞争对手而进行贬低宣传" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%AF%94%E6%80%A7%E5%AE%A3%E4%BC%A0%E6%98%AF%E4%BB%A5%E5%90%8C%E8%A1%8C%E4%B8%9A%E6%89%80%E6%9C%89%E7%BB%8F%E8%90%A5%E8%80%85%E4%B8%BA%E7%AB%9E%E4%BA%89%E5%AF%B9%E6%89%8B%E8%80%8C%E8%BF%9B%E8%A1%8C%E8%B4%AC%E4%BD%8E%E5%AE%A3%E4%BC%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="媒体或具体销售人员若受人指使有上述行为，仅侵犯他人名誉权的" _mubu_text="%3Cspan%3E%E5%AA%92%E4%BD%93%E6%88%96%E5%85%B7%E4%BD%93%E9%94%80%E5%94%AE%E4%BA%BA%E5%91%98%E8%8B%A5%E5%8F%97%E4%BA%BA%E6%8C%87%E4%BD%BF%E6%9C%89%E4%B8%8A%E8%BF%B0%E8%A1%8C%E4%B8%BA%EF%BC%8C%E4%BB%85%E4%BE%B5%E7%8A%AF%E4%BB%96%E4%BA%BA%E5%90%8D%E8%AA%89%E6%9D%83%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="不正当有奖销售行为" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%AD%A3%E5%BD%93%E6%9C%89%E5%A5%96%E9%94%80%E5%94%AE%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="有奖销售信息不明确，影响兑奖的" _mubu_text="%3Cspan%3E%E6%9C%89%E5%A5%96%E9%94%80%E5%94%AE%E4%BF%A1%E6%81%AF%E4%B8%8D%E6%98%8E%E7%A1%AE%EF%BC%8C%E5%BD%B1%E5%93%8D%E5%85%91%E5%A5%96%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="称有奖或者故意让内定人员中奖的欺骗方式" _mubu_text="%3Cspan%3E%E7%A7%B0%E6%9C%89%E5%A5%96%E6%88%96%E8%80%85%E6%95%85%E6%84%8F%E8%AE%A9%E5%86%85%E5%AE%9A%E4%BA%BA%E5%91%98%E4%B8%AD%E5%A5%96%E7%9A%84%E6%AC%BA%E9%AA%97%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="抽奖式的有奖销售，最高奖的金额超过5万元" _mubu_text="%3Cspan%3E%E6%8A%BD%E5%A5%96%E5%BC%8F%E7%9A%84%E6%9C%89%E5%A5%96%E9%94%80%E5%94%AE%EF%BC%8C%E6%9C%80%E9%AB%98%E5%A5%96%E7%9A%84%E9%87%91%E9%A2%9D%E8%B6%85%E8%BF%875%E4%B8%87%E5%85%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="网络领域不正当竞争" _mubu_text="%3Cspan%3E%E7%BD%91%E7%BB%9C%E9%A2%86%E5%9F%9F%E4%B8%8D%E6%AD%A3%E5%BD%93%E7%AB%9E%E4%BA%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="消费者法" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="消费者权益保护法" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E6%9D%83%E7%9B%8A%E4%BF%9D%E6%8A%A4%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用对象" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="消费者只能是个人，不包括团体、社会组织、单位" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E5%8F%AA%E8%83%BD%E6%98%AF%E4%B8%AA%E4%BA%BA%EF%BC%8C%E4%B8%8D%E5%8C%85%E6%8B%AC%E5%9B%A2%E4%BD%93%E3%80%81%E7%A4%BE%E4%BC%9A%E7%BB%84%E7%BB%87%E3%80%81%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="限于为生活消费需要" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E4%B8%BA%E7%94%9F%E6%B4%BB%E6%B6%88%E8%B4%B9%E9%9C%80%E8%A6%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="不包括个人购买生产资料" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8C%85%E6%8B%AC%E4%B8%AA%E4%BA%BA%E8%B4%AD%E4%B9%B0%E7%94%9F%E4%BA%A7%E8%B5%84%E6%96%99%3C/span%3E" _note="农民购买、使用直接用于农业生产的生产资料，参照消保法执行" _mubu_note="%3Cspan%3E%E5%86%9C%E6%B0%91%E8%B4%AD%E4%B9%B0%E3%80%81%E4%BD%BF%E7%94%A8%E7%9B%B4%E6%8E%A5%E7%94%A8%E4%BA%8E%E5%86%9C%E4%B8%9A%E7%94%9F%E4%BA%A7%E7%9A%84%E7%94%9F%E4%BA%A7%E8%B5%84%E6%96%99%EF%BC%8C%E5%8F%82%E7%85%A7%E6%B6%88%E4%BF%9D%E6%B3%95%E6%89%A7%E8%A1%8C%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="经营者义务" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="安全保障义务" _mubu_text="%3Cspan%3E%E5%AE%89%E5%85%A8%E4%BF%9D%E9%9A%9C%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="召回义务" _mubu_text="%3Cspan%3E%E5%8F%AC%E5%9B%9E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="商品或者服务存在缺陷，有危及人身、财产安全危险的" _mubu_text="%3Cspan%3E%E5%95%86%E5%93%81%E6%88%96%E8%80%85%E6%9C%8D%E5%8A%A1%E5%AD%98%E5%9C%A8%E7%BC%BA%E9%99%B7%EF%BC%8C%E6%9C%89%E5%8D%B1%E5%8F%8A%E4%BA%BA%E8%BA%AB%E3%80%81%E8%B4%A2%E4%BA%A7%E5%AE%89%E5%85%A8%E5%8D%B1%E9%99%A9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="报告+告知" _mubu_text="%3Cspan%3E%E6%8A%A5%E5%91%8A+%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="经营者应当承担消费者因商品被召回支出的必要费用" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E5%BA%94%E5%BD%93%E6%89%BF%E6%8B%85%E6%B6%88%E8%B4%B9%E8%80%85%E5%9B%A0%E5%95%86%E5%93%81%E8%A2%AB%E5%8F%AC%E5%9B%9E%E6%94%AF%E5%87%BA%E7%9A%84%E5%BF%85%E8%A6%81%E8%B4%B9%E7%94%A8%3C/span%3E" _note="非所有费用" _mubu_note="%3Cspan%3E%E9%9D%9E%E6%89%80%E6%9C%89%E8%B4%B9%E7%94%A8%3C/span%3E"/>
            </outline>
            <outline text="保证商品质量义务" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E5%95%86%E5%93%81%E8%B4%A8%E9%87%8F%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="耐用商品或者装饰装修等服务，消费者自接受商品或者服务之日起6个月内发现瑕疵，发生争议的，由经营者承担有关瑕疵的举证责任" _mubu_text="%3Cspan%3E%E8%80%90%E7%94%A8%E5%95%86%E5%93%81%E6%88%96%E8%80%85%E8%A3%85%E9%A5%B0%E8%A3%85%E4%BF%AE%E7%AD%89%E6%9C%8D%E5%8A%A1%EF%BC%8C%E6%B6%88%E8%B4%B9%E8%80%85%E8%87%AA%E6%8E%A5%E5%8F%97%E5%95%86%E5%93%81%E6%88%96%E8%80%85%E6%9C%8D%E5%8A%A1%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%E5%86%85%E5%8F%91%E7%8E%B0%E7%91%95%E7%96%B5%EF%BC%8C%E5%8F%91%E7%94%9F%E4%BA%89%E8%AE%AE%E7%9A%84%EF%BC%8C%E7%94%B1%E7%BB%8F%E8%90%A5%E8%80%85%E6%89%BF%E6%8B%85%E6%9C%89%E5%85%B3%E7%91%95%E7%96%B5%E7%9A%84%E4%B8%BE%E8%AF%81%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="机动车、计算机、电视机、电冰箱、空调器、洗衣机等" _mubu_note="%3Cspan%3E%E6%9C%BA%E5%8A%A8%E8%BD%A6%E3%80%81%E8%AE%A1%E7%AE%97%E6%9C%BA%E3%80%81%E7%94%B5%E8%A7%86%E6%9C%BA%E3%80%81%E7%94%B5%E5%86%B0%E7%AE%B1%E3%80%81%E7%A9%BA%E8%B0%83%E5%99%A8%E3%80%81%E6%B4%97%E8%A1%A3%E6%9C%BA%E7%AD%89%3C/span%3E"/>
            </outline>
            <outline text="修理、更换、退货义务" _mubu_text="%3Cspan%3E%E4%BF%AE%E7%90%86%E3%80%81%E6%9B%B4%E6%8D%A2%E3%80%81%E9%80%80%E8%B4%A7%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="有国家规定或当事人有约定的，从之" _mubu_text="%3Cspan%3E%E6%9C%89%E5%9B%BD%E5%AE%B6%E8%A7%84%E5%AE%9A%E6%88%96%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%89%E7%BA%A6%E5%AE%9A%E7%9A%84%EF%BC%8C%E4%BB%8E%E4%B9%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="没有国家规定和当事人约定的" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%E5%9B%BD%E5%AE%B6%E8%A7%84%E5%AE%9A%E5%92%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%BA%A6%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="可自收到商品之日起7日内退货" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%87%AA%E6%94%B6%E5%88%B0%E5%95%86%E5%93%81%E4%B9%8B%E6%97%A5%E8%B5%B77%E6%97%A5%E5%86%85%E9%80%80%E8%B4%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="符合法定解除合同条件的" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E6%B3%95%E5%AE%9A%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%E6%9D%A1%E4%BB%B6%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="可及时退货" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%8F%8A%E6%97%B6%E9%80%80%E8%B4%A7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="不符合法定解除合同条件的" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AC%A6%E5%90%88%E6%B3%95%E5%AE%9A%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%E6%9D%A1%E4%BB%B6%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="可要求经营者履行更换、修理等义务" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%A6%81%E6%B1%82%E7%BB%8F%E8%90%A5%E8%80%85%E5%B1%A5%E8%A1%8C%E6%9B%B4%E6%8D%A2%E3%80%81%E4%BF%AE%E7%90%86%E7%AD%89%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="费用承担" _mubu_text="%3Cspan%3E%E8%B4%B9%E7%94%A8%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="经营者应当承担运输等必要费用" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E5%BA%94%E5%BD%93%E6%89%BF%E6%8B%85%E8%BF%90%E8%BE%93%E7%AD%89%E5%BF%85%E8%A6%81%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="提供商品真实信息的义务" _mubu_text="%3Cspan%3E%E6%8F%90%E4%BE%9B%E5%95%86%E5%93%81%E7%9C%9F%E5%AE%9E%E4%BF%A1%E6%81%AF%E7%9A%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="应当明码标价" _mubu_note="%3Cspan%3E%E5%BA%94%E5%BD%93%E6%98%8E%E7%A0%81%E6%A0%87%E4%BB%B7%3C/span%3E"/>
            <outline text="网络购物的七天无理由退货" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%BD%91%E7%BB%9C%E8%B4%AD%E7%89%A9%E7%9A%84%E4%B8%83%E5%A4%A9%E6%97%A0%E7%90%86%E7%94%B1%E9%80%80%E8%B4%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="自收到商品之日起7日内退货，且无须说明理由" _mubu_text="%3Cspan%3E%E8%87%AA%E6%94%B6%E5%88%B0%E5%95%86%E5%93%81%E4%B9%8B%E6%97%A5%E8%B5%B77%E6%97%A5%E5%86%85%E9%80%80%E8%B4%A7%EF%BC%8C%E4%B8%94%E6%97%A0%E9%A1%BB%E8%AF%B4%E6%98%8E%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不适用" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="根据商品性质并经消费者在购买时确认不宜退货的" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E5%95%86%E5%93%81%E6%80%A7%E8%B4%A8%E5%B9%B6%E7%BB%8F%E6%B6%88%E8%B4%B9%E8%80%85%E5%9C%A8%E8%B4%AD%E4%B9%B0%E6%97%B6%E7%A1%AE%E8%AE%A4%E4%B8%8D%E5%AE%9C%E9%80%80%E8%B4%A7%E7%9A%84%3C/span%3E" _note="退回去不好再买了" _mubu_note="%3Cspan%3E%E9%80%80%E5%9B%9E%E5%8E%BB%E4%B8%8D%E5%A5%BD%E5%86%8D%E4%B9%B0%E4%BA%86%3C/span%3E"/>
              </outline>
              <outline text="要求" _mubu_text="%3Cspan%3E%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="消费者退货的商品应当完好" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E9%80%80%E8%B4%A7%E7%9A%84%E5%95%86%E5%93%81%E5%BA%94%E5%BD%93%E5%AE%8C%E5%A5%BD%3C/span%3E" _note="非外包装完好" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%A4%96%E5%8C%85%E8%A3%85%E5%AE%8C%E5%A5%BD%3C/span%3E"/>
                <outline text="退款经营者应自收到退回商品之日7日内返还价款" _mubu_text="%3Cspan%3E%E9%80%80%E6%AC%BE%E7%BB%8F%E8%90%A5%E8%80%85%E5%BA%94%E8%87%AA%E6%94%B6%E5%88%B0%E9%80%80%E5%9B%9E%E5%95%86%E5%93%81%E4%B9%8B%E6%97%A57%E6%97%A5%E5%86%85%E8%BF%94%E8%BF%98%E4%BB%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="退回商品的运费由消费者承担" _mubu_text="%3Cspan%3E%E9%80%80%E5%9B%9E%E5%95%86%E5%93%81%E7%9A%84%E8%BF%90%E8%B4%B9%E7%94%B1%E6%B6%88%E8%B4%B9%E8%80%85%E6%89%BF%E6%8B%85%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="保护消费者信息" _mubu_text="%3Cspan%3E%E4%BF%9D%E6%8A%A4%E6%B6%88%E8%B4%B9%E8%80%85%E4%BF%A1%E6%81%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="收集、使用应经消费者同意" _mubu_text="%3Cspan%3E%E6%94%B6%E9%9B%86%E3%80%81%E4%BD%BF%E7%94%A8%E5%BA%94%E7%BB%8F%E6%B6%88%E8%B4%B9%E8%80%85%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="须严格保密，不得泄露、出售或非法提供" _mubu_text="%3Cspan%3E%E9%A1%BB%E4%B8%A5%E6%A0%BC%E4%BF%9D%E5%AF%86%EF%BC%8C%E4%B8%8D%E5%BE%97%E6%B3%84%E9%9C%B2%E3%80%81%E5%87%BA%E5%94%AE%E6%88%96%E9%9D%9E%E6%B3%95%E6%8F%90%E4%BE%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未经消费者同意或其明确表示拒绝的，不得向其发送商业性信息" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E6%B6%88%E8%B4%B9%E8%80%85%E5%90%8C%E6%84%8F%E6%88%96%E5%85%B6%E6%98%8E%E7%A1%AE%E8%A1%A8%E7%A4%BA%E6%8B%92%E7%BB%9D%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%90%91%E5%85%B6%E5%8F%91%E9%80%81%E5%95%86%E4%B8%9A%E6%80%A7%E4%BF%A1%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="争议的解决" _mubu_text="%3Cspan%3E%E4%BA%89%E8%AE%AE%E7%9A%84%E8%A7%A3%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="企业分立、合并后的赔偿责任" _mubu_text="%3Cspan%3E%E4%BC%81%E4%B8%9A%E5%88%86%E7%AB%8B%E3%80%81%E5%90%88%E5%B9%B6%E5%90%8E%E7%9A%84%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="向变更后承受其权利义务的企业要求赔偿" _mubu_text="%3Cspan%3E%E5%90%91%E5%8F%98%E6%9B%B4%E5%90%8E%E6%89%BF%E5%8F%97%E5%85%B6%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%9A%84%E4%BC%81%E4%B8%9A%E8%A6%81%E6%B1%82%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="借用营业执照经营的责任" _mubu_text="%3Cspan%3E%E5%80%9F%E7%94%A8%E8%90%A5%E4%B8%9A%E6%89%A7%E7%85%A7%E7%BB%8F%E8%90%A5%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="可向执照提供/持有人要求赔偿" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E6%89%A7%E7%85%A7%E6%8F%90%E4%BE%9B/%E6%8C%81%E6%9C%89%E4%BA%BA%E8%A6%81%E6%B1%82%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="展销会举办者、租赁柜台出租者的责任" _mubu_text="%3Cspan%3E%E5%B1%95%E9%94%80%E4%BC%9A%E4%B8%BE%E5%8A%9E%E8%80%85%E3%80%81%E7%A7%9F%E8%B5%81%E6%9F%9C%E5%8F%B0%E5%87%BA%E7%A7%9F%E8%80%85%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="展销会／柜台出租期间" _mubu_text="%3Cspan%3E%E5%B1%95%E9%94%80%E4%BC%9A%EF%BC%8F%E6%9F%9C%E5%8F%B0%E5%87%BA%E7%A7%9F%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="向销售者或服务者" _mubu_text="%3Cspan%3E%E5%90%91%E9%94%80%E5%94%AE%E8%80%85%E6%88%96%E6%9C%8D%E5%8A%A1%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="结束或期满后" _mubu_text="%3Cspan%3E%E7%BB%93%E6%9D%9F%E6%88%96%E6%9C%9F%E6%BB%A1%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="可向展销会的举办者、柜台的出租者" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E5%B1%95%E9%94%80%E4%BC%9A%E7%9A%84%E4%B8%BE%E5%8A%9E%E8%80%85%E3%80%81%E6%9F%9C%E5%8F%B0%E7%9A%84%E5%87%BA%E7%A7%9F%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="网络交易平台的责任" _mubu_text="%3Cspan%3E%E7%BD%91%E7%BB%9C%E4%BA%A4%E6%98%93%E5%B9%B3%E5%8F%B0%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="交易相对人" _mubu_text="%3Cspan%3E%E4%BA%A4%E6%98%93%E7%9B%B8%E5%AF%B9%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="平台责任" _mubu_text="%3Cspan%3E%E5%B9%B3%E5%8F%B0%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="不能提供销售者或者服务者的真实名称、地址和有效联系方式的" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%8F%90%E4%BE%9B%E9%94%80%E5%94%AE%E8%80%85%E6%88%96%E8%80%85%E6%9C%8D%E5%8A%A1%E8%80%85%E7%9A%84%E7%9C%9F%E5%AE%9E%E5%90%8D%E7%A7%B0%E3%80%81%E5%9C%B0%E5%9D%80%E5%92%8C%E6%9C%89%E6%95%88%E8%81%94%E7%B3%BB%E6%96%B9%E5%BC%8F%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="平台作出更有利于消费者的承诺的，应当履行承诺" _mubu_text="%3Cspan%3E%E5%B9%B3%E5%8F%B0%E4%BD%9C%E5%87%BA%E6%9B%B4%E6%9C%89%E5%88%A9%E4%BA%8E%E6%B6%88%E8%B4%B9%E8%80%85%E7%9A%84%E6%89%BF%E8%AF%BA%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%B1%A5%E8%A1%8C%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="平台连带责任" _mubu_text="%3Cspan%3E%E5%B9%B3%E5%8F%B0%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="(应)知侵害而未采取必要措施的" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E4%BE%B5%E5%AE%B3%E8%80%8C%E6%9C%AA%E9%87%87%E5%8F%96%E5%BF%85%E8%A6%81%E6%8E%AA%E6%96%BD%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="虚假广告制作者、发布者、参与者的责任" _mubu_text="%3Cspan%3E%E8%99%9A%E5%81%87%E5%B9%BF%E5%91%8A%E5%88%B6%E4%BD%9C%E8%80%85%E3%80%81%E5%8F%91%E5%B8%83%E8%80%85%E3%80%81%E5%8F%82%E4%B8%8E%E8%80%85%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="经营者、发布者责任" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E8%80%85%E3%80%81%E5%8F%91%E5%B8%83%E8%80%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="同前平台责任" _mubu_text="%3Cspan%3E%E5%90%8C%E5%89%8D%E5%B9%B3%E5%8F%B0%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="关系消费者生命健康的商品或服务" _mubu_text="%3Cspan%3E%E5%85%B3%E7%B3%BB%E6%B6%88%E8%B4%B9%E8%80%85%E7%94%9F%E5%91%BD%E5%81%A5%E5%BA%B7%E7%9A%84%E5%95%86%E5%93%81%E6%88%96%E6%9C%8D%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="应与提供该商品或服务的经营者承担连带责任" _mubu_text="%3Cspan%3E%E5%BA%94%E4%B8%8E%E6%8F%90%E4%BE%9B%E8%AF%A5%E5%95%86%E5%93%81%E6%88%96%E6%9C%8D%E5%8A%A1%E7%9A%84%E7%BB%8F%E8%90%A5%E8%80%85%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="社会团体或者其他组织、个人在其中向消费者推荐，造成损害的，应与经营者担连带责任" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E5%9B%A2%E4%BD%93%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%E3%80%81%E4%B8%AA%E4%BA%BA%E5%9C%A8%E5%85%B6%E4%B8%AD%E5%90%91%E6%B6%88%E8%B4%B9%E8%80%85%E6%8E%A8%E8%8D%90%EF%BC%8C%E9%80%A0%E6%88%90%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%E5%BA%94%E4%B8%8E%E7%BB%8F%E8%90%A5%E8%80%85%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="消费者协会" _mubu_text="%3Cspan%3E%E6%B6%88%E8%B4%B9%E8%80%85%E5%8D%8F%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得以收费或其他牟利方式向消费者推荐商品和服务" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E6%94%B6%E8%B4%B9%E6%88%96%E5%85%B6%E4%BB%96%E7%89%9F%E5%88%A9%E6%96%B9%E5%BC%8F%E5%90%91%E6%B6%88%E8%B4%B9%E8%80%85%E6%8E%A8%E8%8D%90%E5%95%86%E5%93%81%E5%92%8C%E6%9C%8D%E5%8A%A1%3C/span%3E" _note="并非完全不能推荐" _mubu_note="%3Cspan%3E%E5%B9%B6%E9%9D%9E%E5%AE%8C%E5%85%A8%E4%B8%8D%E8%83%BD%E6%8E%A8%E8%8D%90%3C/span%3E"/>
              <outline text="公益诉讼" _mubu_text="%3Cspan%3E%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="侵害众多消费者" _mubu_text="%3Cspan%3E%E4%BE%B5%E5%AE%B3%E4%BC%97%E5%A4%9A%E6%B6%88%E8%B4%B9%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="中国消协及省级以上的消协" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E6%B6%88%E5%8D%8F%E5%8F%8A%E7%9C%81%E7%BA%A7%E4%BB%A5%E4%B8%8A%E7%9A%84%E6%B6%88%E5%8D%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="惩罚性赔偿" _mubu_text="%3Cspan%3E%E6%83%A9%E7%BD%9A%E6%80%A7%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="明知存在缺陷仍然提供，造成消费者或其他受害人死亡或健康严重损害" _mubu_text="%3Cspan%3E%E6%98%8E%E7%9F%A5%E5%AD%98%E5%9C%A8%E7%BC%BA%E9%99%B7%E4%BB%8D%E7%84%B6%E6%8F%90%E4%BE%9B%EF%BC%8C%E9%80%A0%E6%88%90%E6%B6%88%E8%B4%B9%E8%80%85%E6%88%96%E5%85%B6%E4%BB%96%E5%8F%97%E5%AE%B3%E4%BA%BA%E6%AD%BB%E4%BA%A1%E6%88%96%E5%81%A5%E5%BA%B7%E4%B8%A5%E9%87%8D%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="受害人有权要求经营者赔偿损失" _mubu_text="%3Cspan%3E%E5%8F%97%E5%AE%B3%E4%BA%BA%E6%9C%89%E6%9D%83%E8%A6%81%E6%B1%82%E7%BB%8F%E8%90%A5%E8%80%85%E8%B5%94%E5%81%BF%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="并有权要求所受损失2倍以下的惩罚性赔偿" _mubu_text="%3Cspan%3E%E5%B9%B6%E6%9C%89%E6%9D%83%E8%A6%81%E6%B1%82%E6%89%80%E5%8F%97%E6%8D%9F%E5%A4%B12%E5%80%8D%E4%BB%A5%E4%B8%8B%E7%9A%84%E6%83%A9%E7%BD%9A%E6%80%A7%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="欺诈的法律责任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%AC%BA%E8%AF%88%E7%9A%84%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="退一" _mubu_text="%3Cspan%3E%E9%80%80%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="赔三" _mubu_text="%3Cspan%3E%E8%B5%94%E4%B8%89%3C/span%3E" _note="价款的三倍" _mubu_note="%3Cspan%3E%E4%BB%B7%E6%AC%BE%E7%9A%84%E4%B8%89%E5%80%8D%3C/span%3E"/>
              <outline text="500保底" _mubu_text="%3Cspan%3E500%E4%BF%9D%E5%BA%95%3C/span%3E" _note="增加赔偿＜500，为500" _mubu_note="%3Cspan%3E%E5%A2%9E%E5%8A%A0%E8%B5%94%E5%81%BF%EF%BC%9C500%EF%BC%8C%E4%B8%BA500%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
        <outline text="产品质量法" _mubu_text="%3Cspan%3E%E4%BA%A7%E5%93%81%E8%B4%A8%E9%87%8F%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="产品责任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BA%A7%E5%93%81%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="产品缺陷致人人身损害或者财产损害" _mubu_text="%3Cspan%3E%E4%BA%A7%E5%93%81%E7%BC%BA%E9%99%B7%E8%87%B4%E4%BA%BA%E4%BA%BA%E8%BA%AB%E6%8D%9F%E5%AE%B3%E6%88%96%E8%80%85%E8%B4%A2%E4%BA%A7%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="包括人身损害和缺陷产品以外的其他财产损害" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E4%BA%BA%E8%BA%AB%E6%8D%9F%E5%AE%B3%E5%92%8C%E7%BC%BA%E9%99%B7%E4%BA%A7%E5%93%81%E4%BB%A5%E5%A4%96%E7%9A%84%E5%85%B6%E4%BB%96%E8%B4%A2%E4%BA%A7%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="生产者与销售承担不真正连带责任" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E8%80%85%E4%B8%8E%E9%94%80%E5%94%AE%E6%89%BF%E6%8B%85%E4%B8%8D%E7%9C%9F%E6%AD%A3%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="对外" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="均承担无过错责任" _mubu_text="%3Cspan%3E%E5%9D%87%E6%89%BF%E6%8B%85%E6%97%A0%E8%BF%87%E9%94%99%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="对内追偿" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%86%85%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="销售者承担过错责任" _mubu_text="%3Cspan%3E%E9%94%80%E5%94%AE%E8%80%85%E6%89%BF%E6%8B%85%E8%BF%87%E9%94%99%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="生产者免责事由" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E8%80%85%E5%85%8D%E8%B4%A3%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
              <outline text="未将产品投入流通的" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%B0%86%E4%BA%A7%E5%93%81%E6%8A%95%E5%85%A5%E6%B5%81%E9%80%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="产品投入流通时，引起损害的缺陷尚不存在的" _mubu_text="%3Cspan%3E%E4%BA%A7%E5%93%81%E6%8A%95%E5%85%A5%E6%B5%81%E9%80%9A%E6%97%B6%EF%BC%8C%E5%BC%95%E8%B5%B7%E6%8D%9F%E5%AE%B3%E7%9A%84%E7%BC%BA%E9%99%B7%E5%B0%9A%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="将产品投入流通时的科学技术水平尚不能发现缺陷的存在的" _mubu_text="%3Cspan%3E%E5%B0%86%E4%BA%A7%E5%93%81%E6%8A%95%E5%85%A5%E6%B5%81%E9%80%9A%E6%97%B6%E7%9A%84%E7%A7%91%E5%AD%A6%E6%8A%80%E6%9C%AF%E6%B0%B4%E5%B9%B3%E5%B0%9A%E4%B8%8D%E8%83%BD%E5%8F%91%E7%8E%B0%E7%BC%BA%E9%99%B7%E7%9A%84%E5%AD%98%E5%9C%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="诉讼时效和请求权" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%E5%92%8C%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="诉讼时效" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="3年" _mubu_text="%3Cspan%3E3%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="请求权" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="缺陷产品交付最初消费者满10年丧失" _mubu_text="%3Cspan%3E%E7%BC%BA%E9%99%B7%E4%BA%A7%E5%93%81%E4%BA%A4%E4%BB%98%E6%9C%80%E5%88%9D%E6%B6%88%E8%B4%B9%E8%80%85%E6%BB%A110%E5%B9%B4%E4%B8%A7%E5%A4%B1%3C/span%3E" _note="尚未超过明示的安全使用期的除外" _mubu_note="%3Cspan%3E%E5%B0%9A%E6%9C%AA%E8%B6%85%E8%BF%87%E6%98%8E%E7%A4%BA%E7%9A%84%E5%AE%89%E5%85%A8%E4%BD%BF%E7%94%A8%E6%9C%9F%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="食品安全法" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E5%AE%89%E5%85%A8%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="食品安全标准" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E5%AE%89%E5%85%A8%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="强制执行的标准" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E7%9A%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="国家标准" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
