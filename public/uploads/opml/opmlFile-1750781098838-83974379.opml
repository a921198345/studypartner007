                <outline text="董事会成员中的职工代表可以成为审委会成员" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E6%88%90%E5%91%98%E4%B8%AD%E7%9A%84%E8%81%8C%E5%B7%A5%E4%BB%A3%E8%A1%A8%E5%8F%AF%E4%BB%A5%E6%88%90%E4%B8%BA%E5%AE%A1%E5%A7%94%E4%BC%9A%E6%88%90%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="成员过半数通过作出决议" _mubu_text="%3Cspan%3E%E6%88%90%E5%91%98%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%E4%BD%9C%E5%87%BA%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="一人一票表决" _mubu_text="%3Cspan%3E%E4%B8%80%E4%BA%BA%E4%B8%80%E7%A5%A8%E8%A1%A8%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
