                <outline text="有限公司应当置备股东名册" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8%E5%BA%94%E5%BD%93%E7%BD%AE%E5%A4%87%E8%82%A1%E4%B8%9C%E5%90%8D%E5%86%8C%3C/span%3E" _note="" _mubu_note="">
