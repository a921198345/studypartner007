          </outline>
          <outline text="基本养老保险基金" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%85%BB%E8%80%81%E4%BF%9D%E9%99%A9%E5%9F%BA%E9%87%91%3C/span%3E" _note="" _mubu_note="">
            <outline text="由用人单位和个人缴费以及政府补贴等组成" _mubu_text="%3Cspan%3E%E7%94%B1%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%92%8C%E4%B8%AA%E4%BA%BA%E7%BC%B4%E8%B4%B9%E4%BB%A5%E5%8F%8A%E6%94%BF%E5%BA%9C%E8%A1%A5%E8%B4%B4%E7%AD%89%E7%BB%84%E6%88%90%3C/span%3E" _note="单位＋个人＋政府" _mubu_note="%3Cspan%3E%E5%8D%95%E4%BD%8D%EF%BC%8B%E4%B8%AA%E4%BA%BA%EF%BC%8B%E6%94%BF%E5%BA%9C%3C/span%3E"/>
