            <outline text="赔偿范围" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="不能及于被保险人的人身或其财产" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E5%8F%8A%E4%BA%8E%E8%A2%AB%E4%BF%9D%E9%99%A9%E4%BA%BA%E7%9A%84%E4%BA%BA%E8%BA%AB%E6%88%96%E5%85%B6%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
