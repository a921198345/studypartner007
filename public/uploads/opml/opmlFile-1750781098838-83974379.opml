                  <outline text="作者不得许可第三人" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%80%85%E4%B8%8D%E5%BE%97%E8%AE%B8%E5%8F%AF%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="以与单位使用的相同方式使用该作品" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%B8%8E%E5%8D%95%E4%BD%8D%E4%BD%BF%E7%94%A8%E7%9A%84%E7%9B%B8%E5%90%8C%E6%96%B9%E5%BC%8F%E4%BD%BF%E7%94%A8%E8%AF%A5%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="经单位同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%8D%95%E4%BD%8D%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="相同方式使用作品所获报酬" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%90%8C%E6%96%B9%E5%BC%8F%E4%BD%BF%E7%94%A8%E4%BD%9C%E5%93%81%E6%89%80%E8%8E%B7%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="由作者与单位按约定的比例分配" _mubu_text="%3Cspan%3E%E7%94%B1%E4%BD%9C%E8%80%85%E4%B8%8E%E5%8D%95%E4%BD%8D%E6%8C%89%E7%BA%A6%E5%AE%9A%E7%9A%84%E6%AF%94%E4%BE%8B%E5%88%86%E9%85%8D%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="特殊职务作品" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E8%81%8C%E5%8A%A1%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="权利归属" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E5%BD%92%E5%B1%9E%3C/span%3E" _note="" _mubu_note="">
                <outline text="作者享有署名权" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%80%85%E4%BA%AB%E6%9C%89%E7%BD%B2%E5%90%8D%E6%9D%83%3C/span%3E" _note="单位可以给予作者奖励" _mubu_note="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%8F%AF%E4%BB%A5%E7%BB%99%E4%BA%88%E4%BD%9C%E8%80%85%E5%A5%96%E5%8A%B1%3C/span%3E"/>
                <outline text="著作权的其他权利由单位享有" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%E7%9A%84%E5%85%B6%E4%BB%96%E6%9D%83%E5%88%A9%E7%94%B1%E5%8D%95%E4%BD%8D%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
