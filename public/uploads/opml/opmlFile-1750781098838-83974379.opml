        <outline text="劳动合同的内容" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
          <outline text="必备条款、可备条款" _mubu_text="%3Cspan%3E%E5%BF%85%E5%A4%87%E6%9D%A1%E6%AC%BE%E3%80%81%E5%8F%AF%E5%A4%87%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="试用期条款" _mubu_text="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="时间限制" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="以完成一定任务为期限／＜3个月" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%AE%8C%E6%88%90%E4%B8%80%E5%AE%9A%E4%BB%BB%E5%8A%A1%E4%B8%BA%E6%9C%9F%E9%99%90%EF%BC%8F%EF%BC%9C3%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得约定试用期" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E7%BA%A6%E5%AE%9A%E8%AF%95%E7%94%A8%E6%9C%9F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="3个月≤＜1年" _mubu_text="%3Cspan%3E3%E4%B8%AA%E6%9C%88%E2%89%A4%EF%BC%9C1%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="最长1个月" _mubu_text="%3Cspan%3E%E6%9C%80%E9%95%BF1%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="1年≤＜3年" _mubu_text="%3Cspan%3E1%E5%B9%B4%E2%89%A4%EF%BC%9C3%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="最长2个月" _mubu_text="%3Cspan%3E%E6%9C%80%E9%95%BF2%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="＞3年" _mubu_text="%3Cspan%3E%EF%BC%9E3%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="最长6个月" _mubu_text="%3Cspan%3E%E6%9C%80%E9%95%BF6%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="试用期包含在劳动合同期限内" _mubu_text="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E5%8C%85%E5%90%AB%E5%9C%A8%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%9C%9F%E9%99%90%E5%86%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="合同仅约定试用期的" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E4%BB%85%E7%BA%A6%E5%AE%9A%E8%AF%95%E7%94%A8%E6%9C%9F%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="该期限为动合同期限" _mubu_text="%3Cspan%3E%E8%AF%A5%E6%9C%9F%E9%99%90%E4%B8%BA%E5%8A%A8%E5%90%88%E5%90%8C%E6%9C%9F%E9%99%90%3C/span%3E" _note="试用期不成立" _mubu_note="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E4%B8%8D%E6%88%90%E7%AB%8B%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="只能约定一次试用期" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E7%BA%A6%E5%AE%9A%E4%B8%80%E6%AC%A1%E8%AF%95%E7%94%A8%E6%9C%9F%3C/span%3E" _note="同一用人单位与同一劳动者" _mubu_note="%3Cspan%3E%E5%90%8C%E4%B8%80%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%8E%E5%90%8C%E4%B8%80%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E"/>
            <outline text="试用期的工资" _mubu_text="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E7%9A%84%E5%B7%A5%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="≥本位相同岗位最低档工资的80％/劳动合同约定工资的80%" _mubu_text="%3Cspan%3E%E2%89%A5%E6%9C%AC%E4%BD%8D%E7%9B%B8%E5%90%8C%E5%B2%97%E4%BD%8D%E6%9C%80%E4%BD%8E%E6%A1%A3%E5%B7%A5%E8%B5%84%E7%9A%8480%EF%BC%85/%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%E5%B7%A5%E8%B5%84%E7%9A%8480%25%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="≥用人单位所在地的量低工资标准" _mubu_text="%3Cspan%3E%E2%89%A5%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%89%80%E5%9C%A8%E5%9C%B0%E7%9A%84%E9%87%8F%E4%BD%8E%E5%B7%A5%E8%B5%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="解除合同" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动者解除" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="提前3天通知" _mubu_text="%3Cspan%3E%E6%8F%90%E5%89%8D3%E5%A4%A9%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="用人单位解除" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="劳动者有过错" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9C%89%E8%BF%87%E9%94%99%3C/span%3E" _note="直接解除合同的6种情形＋不能胜任＋伤病后无法工作的" _mubu_note="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%E7%9A%846%E7%A7%8D%E6%83%85%E5%BD%A2%EF%BC%8B%E4%B8%8D%E8%83%BD%E8%83%9C%E4%BB%BB%EF%BC%8B%E4%BC%A4%E7%97%85%E5%90%8E%E6%97%A0%E6%B3%95%E5%B7%A5%E4%BD%9C%E7%9A%84%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="服务期条款" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%9C%8D%E5%8A%A1%E6%9C%9F%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="前提" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
              <outline text="用人单位为劳动者提供专项培训费用" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%BA%E5%8A%B3%E5%8A%A8%E8%80%85%E6%8F%90%E4%BE%9B%E4%B8%93%E9%A1%B9%E5%9F%B9%E8%AE%AD%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="对其进行专业技术培训的" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%B6%E8%BF%9B%E8%A1%8C%E4%B8%93%E4%B8%9A%E6%8A%80%E6%9C%AF%E5%9F%B9%E8%AE%AD%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="违约金" _mubu_text="%3Cspan%3E%E8%BF%9D%E7%BA%A6%E9%87%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动者应支付" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%BA%94%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="劳动者有过错/单位无过错" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9C%89%E8%BF%87%E9%94%99/%E5%8D%95%E4%BD%8D%E6%97%A0%E8%BF%87%E9%94%99%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="数额" _mubu_text="%3Cspan%3E%E6%95%B0%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="≤用人单位提供的培训费用" _mubu_text="%3Cspan%3E%E2%89%A4%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%8F%90%E4%BE%9B%E7%9A%84%E5%9F%B9%E8%AE%AD%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="≤服务期尚未履行部分所应分摊的培训费用" _mubu_text="%3Cspan%3E%E2%89%A4%E6%9C%8D%E5%8A%A1%E6%9C%9F%E5%B0%9A%E6%9C%AA%E5%B1%A5%E8%A1%8C%E9%83%A8%E5%88%86%E6%89%80%E5%BA%94%E5%88%86%E6%91%8A%E7%9A%84%E5%9F%B9%E8%AE%AD%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="期间内劳动报酬" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E5%86%85%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note="">
              <outline text="不影响按照正常的工资调整机制提高劳动者在服务期期间的劳动报酬" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%8C%89%E7%85%A7%E6%AD%A3%E5%B8%B8%E7%9A%84%E5%B7%A5%E8%B5%84%E8%B0%83%E6%95%B4%E6%9C%BA%E5%88%B6%E6%8F%90%E9%AB%98%E5%8A%B3%E5%8A%A8%E8%80%85%E5%9C%A8%E6%9C%8D%E5%8A%A1%E6%9C%9F%E6%9C%9F%E9%97%B4%E7%9A%84%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="保密条款" _mubu_text="%3Cspan%3E%E4%BF%9D%E5%AF%86%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="员工的保密义务是约定的" _mubu_text="%3Cspan%3E%E5%91%98%E5%B7%A5%E7%9A%84%E4%BF%9D%E5%AF%86%E4%B9%89%E5%8A%A1%E6%98%AF%E7%BA%A6%E5%AE%9A%E7%9A%84%3C/span%3E" _note="既可有偿，也可无偿" _mubu_note="%3Cspan%3E%E6%97%A2%E5%8F%AF%E6%9C%89%E5%81%BF%EF%BC%8C%E4%B9%9F%E5%8F%AF%E6%97%A0%E5%81%BF%3C/span%3E"/>
            <outline text="董高的保密义务是法定、无偿的" _mubu_text="%3Cspan%3E%E8%91%A3%E9%AB%98%E7%9A%84%E4%BF%9D%E5%AF%86%E4%B9%89%E5%8A%A1%E6%98%AF%E6%B3%95%E5%AE%9A%E3%80%81%E6%97%A0%E5%81%BF%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="竞业限制条款" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E6%9D%A1%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="只针对负有保密义务的劳动者" _mubu_text="%3Cspan%3E%E5%8F%AA%E9%92%88%E5%AF%B9%E8%B4%9F%E6%9C%89%E4%BF%9D%E5%AF%86%E4%B9%89%E5%8A%A1%E7%9A%84%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="限于高级管理、技术人员和其他负有保密义务的" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E9%AB%98%E7%BA%A7%E7%AE%A1%E7%90%86%E3%80%81%E6%8A%80%E6%9C%AF%E4%BA%BA%E5%91%98%E5%92%8C%E5%85%B6%E4%BB%96%E8%B4%9F%E6%9C%89%E4%BF%9D%E5%AF%86%E4%B9%89%E5%8A%A1%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="劳动者离职后才承担该项义务" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E7%A6%BB%E8%81%8C%E5%90%8E%E6%89%8D%E6%89%BF%E6%8B%85%E8%AF%A5%E9%A1%B9%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="竞业限制期" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E6%9C%9F%3C/span%3E" _note="" _mubu_note="">
              <outline text="≤2年" _mubu_text="%3Cspan%3E%E2%89%A42%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="竞业期内，劳动者可以到非竞争单位就业" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%B8%9A%E6%9C%9F%E5%86%85%EF%BC%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8F%AF%E4%BB%A5%E5%88%B0%E9%9D%9E%E7%AB%9E%E4%BA%89%E5%8D%95%E4%BD%8D%E5%B0%B1%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="竞业限制内的经济补偿" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E5%86%85%E7%9A%84%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="未约定经济补偿的" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BA%A6%E5%AE%9A%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="按劳动者在劳动合同解除或者终止前12个月平均工资的30％按月支付" _mubu_text="%3Cspan%3E%E6%8C%89%E5%8A%B3%E5%8A%A8%E8%80%85%E5%9C%A8%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E8%A7%A3%E9%99%A4%E6%88%96%E8%80%85%E7%BB%88%E6%AD%A2%E5%89%8D12%E4%B8%AA%E6%9C%88%E5%B9%B3%E5%9D%87%E5%B7%A5%E8%B5%84%E7%9A%8430%EF%BC%85%E6%8C%89%E6%9C%88%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="月平均工资的30％＜劳动合同履行地最低工资标准=最低工资标准" _mubu_text="%3Cspan%3E%E6%9C%88%E5%B9%B3%E5%9D%87%E5%B7%A5%E8%B5%84%E7%9A%8430%EF%BC%85%EF%BC%9C%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E5%B1%A5%E8%A1%8C%E5%9C%B0%E6%9C%80%E4%BD%8E%E5%B7%A5%E8%B5%84%E6%A0%87%E5%87%86=%E6%9C%80%E4%BD%8E%E5%B7%A5%E8%B5%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="竞业限制解除" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="单位3个月未支付经济补偿" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D3%E4%B8%AA%E6%9C%88%E6%9C%AA%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="单位主动解除" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E4%B8%BB%E5%8A%A8%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="用人单位应额外支付劳动者3个月的竞业限制经济补偿" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%BA%94%E9%A2%9D%E5%A4%96%E6%94%AF%E4%BB%98%E5%8A%B3%E5%8A%A8%E8%80%853%E4%B8%AA%E6%9C%88%E7%9A%84%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="劳动者违约" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%BF%9D%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="向用人单位支付违约金" _mubu_text="%3Cspan%3E%E5%90%91%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%94%AF%E4%BB%98%E8%BF%9D%E7%BA%A6%E9%87%91%3C/span%3E" _note="" _mubu_note="">
                  <outline text="只有劳动者违反服务期或竞业限制才需支付违约金，其他都无效" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%9C%89%E5%8A%B3%E5%8A%A8%E8%80%85%E8%BF%9D%E5%8F%8D%E6%9C%8D%E5%8A%A1%E6%9C%9F%E6%88%96%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E6%89%8D%E9%9C%80%E6%94%AF%E4%BB%98%E8%BF%9D%E7%BA%A6%E9%87%91%EF%BC%8C%E5%85%B6%E4%BB%96%E9%83%BD%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="按约定继续履行竞业限制义务" _mubu_text="%3Cspan%3E%E6%8C%89%E7%BA%A6%E5%AE%9A%E7%BB%A7%E7%BB%AD%E5%B1%A5%E8%A1%8C%E7%AB%9E%E4%B8%9A%E9%99%90%E5%88%B6%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="单位可直接花钱解除，劳动者却不行" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%8F%AF%E7%9B%B4%E6%8E%A5%E8%8A%B1%E9%92%B1%E8%A7%A3%E9%99%A4%EF%BC%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8D%B4%E4%B8%8D%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="劳动合同无效" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="以欺诈、胁迫的手段或者乘人之危，订立或者变更劳动合同的" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%AC%BA%E8%AF%88%E3%80%81%E8%83%81%E8%BF%AB%E7%9A%84%E6%89%8B%E6%AE%B5%E6%88%96%E8%80%85%E4%B9%98%E4%BA%BA%E4%B9%8B%E5%8D%B1%EF%BC%8C%E8%AE%A2%E7%AB%8B%E6%88%96%E8%80%85%E5%8F%98%E6%9B%B4%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="用人单位免除自己的法定责任、排除劳动者权利的" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%85%8D%E9%99%A4%E8%87%AA%E5%B7%B1%E7%9A%84%E6%B3%95%E5%AE%9A%E8%B4%A3%E4%BB%BB%E3%80%81%E6%8E%92%E9%99%A4%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9D%83%E5%88%A9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="违反法律、行政法规强制性规定" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E3%80%81%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E5%BC%BA%E5%88%B6%E6%80%A7%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="无效或者部分无效有争议的" _mubu_text="%3Cspan%3E%E6%97%A0%E6%95%88%E6%88%96%E8%80%85%E9%83%A8%E5%88%86%E6%97%A0%E6%95%88%E6%9C%89%E4%BA%89%E8%AE%AE%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="由劳动争议仲裁机构或者法院确认" _mubu_text="%3Cspan%3E%E7%94%B1%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E4%BB%B2%E8%A3%81%E6%9C%BA%E6%9E%84%E6%88%96%E8%80%85%E6%B3%95%E9%99%A2%E7%A1%AE%E8%AE%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="后果" _mubu_text="%3Cspan%3E%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="部分无效，不影响其他部分效力" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E6%97%A0%E6%95%88%EF%BC%8C%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%85%B6%E4%BB%96%E9%83%A8%E5%88%86%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被确认无效后" _mubu_text="%3Cspan%3E%E8%A2%AB%E7%A1%AE%E8%AE%A4%E6%97%A0%E6%95%88%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动者已付出劳动的" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%B7%B2%E4%BB%98%E5%87%BA%E5%8A%B3%E5%8A%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="单位应支付报酬和经济补偿" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%BA%94%E6%94%AF%E4%BB%98%E6%8A%A5%E9%85%AC%E5%92%8C%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="报酬数额" _mubu_text="%3Cspan%3E%E6%8A%A5%E9%85%AC%E6%95%B0%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="参照本单位相同或者相近岗位劳动者的劳动报酬确定" _mubu_text="%3Cspan%3E%E5%8F%82%E7%85%A7%E6%9C%AC%E5%8D%95%E4%BD%8D%E7%9B%B8%E5%90%8C%E6%88%96%E8%80%85%E7%9B%B8%E8%BF%91%E5%B2%97%E4%BD%8D%E5%8A%B3%E5%8A%A8%E8%80%85%E7%9A%84%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="用人单位原因" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="造成损害的，单位应当赔偿" _mubu_text="%3Cspan%3E%E9%80%A0%E6%88%90%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%E5%8D%95%E4%BD%8D%E5%BA%94%E5%BD%93%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="劳动合同解除与终止" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E8%A7%A3%E9%99%A4%E4%B8%8E%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="协商解除" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%95%86%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动者提出" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="单位无须支付经济补偿金" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%97%A0%E9%A1%BB%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="单位提出" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="单位应当支付" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%BA%94%E5%BD%93%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="劳动者单方解除" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8D%95%E6%96%B9%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="预告解除" _mubu_text="%3Cspan%3E%E9%A2%84%E5%91%8A%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="劳动者提前30日以书面形式通知用人单位" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E6%8F%90%E5%89%8D30%E6%97%A5%E4%BB%A5%E4%B9%A6%E9%9D%A2%E5%BD%A2%E5%BC%8F%E9%80%9A%E7%9F%A5%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="试用期内" _mubu_text="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="提前3日通知" _mubu_text="%3Cspan%3E%E6%8F%90%E5%89%8D3%E6%97%A5%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="单位无须支付经济补偿金" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%97%A0%E9%A1%BB%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="任意解除" _mubu_text="%3Cspan%3E%E4%BB%BB%E6%84%8F%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="要提前通知" _mubu_text="%3Cspan%3E%E8%A6%81%E6%8F%90%E5%89%8D%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                  <outline text="未按约定提供劳动保护或条件" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%8C%89%E7%BA%A6%E5%AE%9A%E6%8F%90%E4%BE%9B%E5%8A%B3%E5%8A%A8%E4%BF%9D%E6%8A%A4%E6%88%96%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未及时足额支付报酬" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%8F%8A%E6%97%B6%E8%B6%B3%E9%A2%9D%E6%94%AF%E4%BB%98%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未依法为劳动者缴纳社保" _mubu_text="%3Cspan%3E%E6%9C%AA%E4%BE%9D%E6%B3%95%E4%B8%BA%E5%8A%B3%E5%8A%A8%E8%80%85%E7%BC%B4%E7%BA%B3%E7%A4%BE%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="单位规章制度违法、规，损害劳动者权益" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E8%A7%84%E7%AB%A0%E5%88%B6%E5%BA%A6%E8%BF%9D%E6%B3%95%E3%80%81%E8%A7%84%EF%BC%8C%E6%8D%9F%E5%AE%B3%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9D%83%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="劳动合同无效" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="无须提前通知" _mubu_text="%3Cspan%3E%E6%97%A0%E9%A1%BB%E6%8F%90%E5%89%8D%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                  <outline text="用人单位以暴力、威胁或非法限制人身自由的手段强迫劳动者劳动" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%BB%A5%E6%9A%B4%E5%8A%9B%E3%80%81%E5%A8%81%E8%83%81%E6%88%96%E9%9D%9E%E6%B3%95%E9%99%90%E5%88%B6%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%E7%9A%84%E6%89%8B%E6%AE%B5%E5%BC%BA%E8%BF%AB%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8A%B3%E5%8A%A8%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="用人单位违章指挥、强令冒险作业危及劳动者人身安全" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%BF%9D%E7%AB%A0%E6%8C%87%E6%8C%A5%E3%80%81%E5%BC%BA%E4%BB%A4%E5%86%92%E9%99%A9%E4%BD%9C%E4%B8%9A%E5%8D%B1%E5%8F%8A%E5%8A%B3%E5%8A%A8%E8%80%85%E4%BA%BA%E8%BA%AB%E5%AE%89%E5%85%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="单位应支付经济补偿金" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%BA%94%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="单位单方解除" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%8D%95%E6%96%B9%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="非过错性解除" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%BF%87%E9%94%99%E6%80%A7%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="单位提前30日以书面形式通知劳动者" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%8F%90%E5%89%8D30%E6%97%A5%E4%BB%A5%E4%B9%A6%E9%9D%A2%E5%BD%A2%E5%BC%8F%E9%80%9A%E7%9F%A5%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或额外支付劳动者一个月工资" _mubu_text="%3Cspan%3E%E6%88%96%E9%A2%9D%E5%A4%96%E6%94%AF%E4%BB%98%E5%8A%B3%E5%8A%A8%E8%80%85%E4%B8%80%E4%B8%AA%E6%9C%88%E5%B7%A5%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="劳动者患病或非因公负伤，医疗期满后不能从事任何工作" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E6%82%A3%E7%97%85%E6%88%96%E9%9D%9E%E5%9B%A0%E5%85%AC%E8%B4%9F%E4%BC%A4%EF%BC%8C%E5%8C%BB%E7%96%97%E6%9C%9F%E6%BB%A1%E5%90%8E%E4%B8%8D%E8%83%BD%E4%BB%8E%E4%BA%8B%E4%BB%BB%E4%BD%95%E5%B7%A5%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="劳动者不能胜任工作，经过培训或者调整工作岗位，仍不能胜任工作的" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E4%B8%8D%E8%83%BD%E8%83%9C%E4%BB%BB%E5%B7%A5%E4%BD%9C%EF%BC%8C%E7%BB%8F%E8%BF%87%E5%9F%B9%E8%AE%AD%E6%88%96%E8%80%85%E8%B0%83%E6%95%B4%E5%B7%A5%E4%BD%9C%E5%B2%97%E4%BD%8D%EF%BC%8C%E4%BB%8D%E4%B8%8D%E8%83%BD%E8%83%9C%E4%BB%BB%E5%B7%A5%E4%BD%9C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="劳动合同订立时所依据的客观情况发生重大变化，致使无法履行" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E8%AE%A2%E7%AB%8B%E6%97%B6%E6%89%80%E4%BE%9D%E6%8D%AE%E7%9A%84%E5%AE%A2%E8%A7%82%E6%83%85%E5%86%B5%E5%8F%91%E7%94%9F%E9%87%8D%E5%A4%A7%E5%8F%98%E5%8C%96%EF%BC%8C%E8%87%B4%E4%BD%BF%E6%97%A0%E6%B3%95%E5%B1%A5%E8%A1%8C%3C/span%3E" _note="协商不能就变更劳动合同达成协议的" _mubu_note="%3Cspan%3E%E5%8D%8F%E5%95%86%E4%B8%8D%E8%83%BD%E5%B0%B1%E5%8F%98%E6%9B%B4%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E8%BE%BE%E6%88%90%E5%8D%8F%E8%AE%AE%E7%9A%84%3C/span%3E"/>
                </outline>
                <outline text="应支付经济补偿金" _mubu_text="%3Cspan%3E%E5%BA%94%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在服务期内，劳动者无须支付违约金" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%8D%E5%8A%A1%E6%9C%9F%E5%86%85%EF%BC%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E6%97%A0%E9%A1%BB%E6%94%AF%E4%BB%98%E8%BF%9D%E7%BA%A6%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="过错性解除" _mubu_text="%3Cspan%3E%E8%BF%87%E9%94%99%E6%80%A7%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="试用期间被证明不符合录用条件的" _mubu_text="%3Cspan%3E%E8%AF%95%E7%94%A8%E6%9C%9F%E9%97%B4%E8%A2%AB%E8%AF%81%E6%98%8E%E4%B8%8D%E7%AC%A6%E5%90%88%E5%BD%95%E7%94%A8%E6%9D%A1%E4%BB%B6%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="严重违反用人单位规章制度的" _mubu_text="%3Cspan%3E%E4%B8%A5%E9%87%8D%E8%BF%9D%E5%8F%8D%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%A7%84%E7%AB%A0%E5%88%B6%E5%BA%A6%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="严重失职，营私舞弊，给用人单位造成重大损害的" _mubu_text="%3Cspan%3E%E4%B8%A5%E9%87%8D%E5%A4%B1%E8%81%8C%EF%BC%8C%E8%90%A5%E7%A7%81%E8%88%9E%E5%BC%8A%EF%BC%8C%E7%BB%99%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E9%80%A0%E6%88%90%E9%87%8D%E5%A4%A7%E6%8D%9F%E5%AE%B3%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="同时与其他单位建立劳动关系，对完成本单位的工作造成严重影响" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E4%B8%8E%E5%85%B6%E4%BB%96%E5%8D%95%E4%BD%8D%E5%BB%BA%E7%AB%8B%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%EF%BC%8C%E5%AF%B9%E5%AE%8C%E6%88%90%E6%9C%AC%E5%8D%95%E4%BD%8D%E7%9A%84%E5%B7%A5%E4%BD%9C%E9%80%A0%E6%88%90%E4%B8%A5%E9%87%8D%E5%BD%B1%E5%93%8D%3C/span%3E" _note="或者经用人单位提出，拒不改正的" _mubu_note="%3Cspan%3E%E6%88%96%E8%80%85%E7%BB%8F%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%8F%90%E5%87%BA%EF%BC%8C%E6%8B%92%E4%B8%8D%E6%94%B9%E6%AD%A3%E7%9A%84%3C/span%3E">
                    <outline text="给原单位造成损失的，其他单位和劳动者承担连带责任" _mubu_text="%3Cspan%3E%E7%BB%99%E5%8E%9F%E5%8D%95%E4%BD%8D%E9%80%A0%E6%88%90%E6%8D%9F%E5%A4%B1%E7%9A%84%EF%BC%8C%E5%85%B6%E4%BB%96%E5%8D%95%E4%BD%8D%E5%92%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="劳动者行为致使劳动合同无效的" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%A1%8C%E4%B8%BA%E8%87%B4%E4%BD%BF%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%97%A0%E6%95%88%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="被依法追究刑责" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BE%9D%E6%B3%95%E8%BF%BD%E7%A9%B6%E5%88%91%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="无须支付经济补偿金" _mubu_text="%3Cspan%3E%E6%97%A0%E9%A1%BB%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在服务期内，劳动者应当支付违约金" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%8D%E5%8A%A1%E6%9C%9F%E5%86%85%EF%BC%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E5%BA%94%E5%BD%93%E6%94%AF%E4%BB%98%E8%BF%9D%E7%BA%A6%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="经济性裁员" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%B5%8E%E6%80%A7%E8%A3%81%E5%91%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="依照企业破产法规定进行重整的" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%85%A7%E4%BC%81%E4%B8%9A%E7%A0%B4%E4%BA%A7%E6%B3%95%E8%A7%84%E5%AE%9A%E8%BF%9B%E8%A1%8C%E9%87%8D%E6%95%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="生产经营发生严重困难的" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E7%BB%8F%E8%90%A5%E5%8F%91%E7%94%9F%E4%B8%A5%E9%87%8D%E5%9B%B0%E9%9A%BE%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="企业转产、重大技术革新或者经营方式调整" _mubu_text="%3Cspan%3E%E4%BC%81%E4%B8%9A%E8%BD%AC%E4%BA%A7%E3%80%81%E9%87%8D%E5%A4%A7%E6%8A%80%E6%9C%AF%E9%9D%A9%E6%96%B0%E6%88%96%E8%80%85%E7%BB%8F%E8%90%A5%E6%96%B9%E5%BC%8F%E8%B0%83%E6%95%B4%3C/span%3E" _note="经变更劳动合同后，仍需裁减人员的" _mubu_note="%3Cspan%3E%E7%BB%8F%E5%8F%98%E6%9B%B4%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E5%90%8E%EF%BC%8C%E4%BB%8D%E9%9C%80%E8%A3%81%E5%87%8F%E4%BA%BA%E5%91%98%E7%9A%84%3C/span%3E"/>
                  <outline text="客观经济情况发生重大变化，致使劳动合同无法履行的" _mubu_text="%3Cspan%3E%E5%AE%A2%E8%A7%82%E7%BB%8F%E6%B5%8E%E6%83%85%E5%86%B5%E5%8F%91%E7%94%9F%E9%87%8D%E5%A4%A7%E5%8F%98%E5%8C%96%EF%BC%8C%E8%87%B4%E4%BD%BF%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%97%A0%E6%B3%95%E5%B1%A5%E8%A1%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="裁减人数众多" _mubu_text="%3Cspan%3E%E8%A3%81%E5%87%8F%E4%BA%BA%E6%95%B0%E4%BC%97%E5%A4%9A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="≥20人" _mubu_text="%3Cspan%3E%E2%89%A520%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="企业职工总数10％≤＜20人" _mubu_text="%3Cspan%3E%E4%BC%81%E4%B8%9A%E8%81%8C%E5%B7%A5%E6%80%BB%E6%95%B010%EF%BC%85%E2%89%A4%EF%BC%9C20%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="单位提前30日" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%8F%90%E5%89%8D30%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="向工会/全体职工说明" _mubu_text="%3Cspan%3E%E5%90%91%E5%B7%A5%E4%BC%9A/%E5%85%A8%E4%BD%93%E8%81%8C%E5%B7%A5%E8%AF%B4%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="听取其意见" _mubu_text="%3Cspan%3E%E5%90%AC%E5%8F%96%E5%85%B6%E6%84%8F%E8%A7%81%3C/span%3E" _note="非征得同意" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%BE%81%E5%BE%97%E5%90%8C%E6%84%8F%3C/span%3E"/>
                  <outline text="方案报劳动行政部门" _mubu_text="%3Cspan%3E%E6%96%B9%E6%A1%88%E6%8A%A5%E5%8A%B3%E5%8A%A8%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%3C/span%3E" _note="非需其批准" _mubu_note="%3Cspan%3E%E9%9D%9E%E9%9C%80%E5%85%B6%E6%89%B9%E5%87%86%3C/span%3E"/>
                </outline>
                <outline text="优先留用" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E7%95%99%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
                  <outline text="与本单位订立无固定期限/较长固定期限劳动合同的" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%9C%AC%E5%8D%95%E4%BD%8D%E8%AE%A2%E7%AB%8B%E6%97%A0%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90/%E8%BE%83%E9%95%BF%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="家庭无其他就业人员，有需要扶养的老人或者未成年人的" _mubu_text="%3Cspan%3E%E5%AE%B6%E5%BA%AD%E6%97%A0%E5%85%B6%E4%BB%96%E5%B0%B1%E4%B8%9A%E4%BA%BA%E5%91%98%EF%BC%8C%E6%9C%89%E9%9C%80%E8%A6%81%E6%89%B6%E5%85%BB%E7%9A%84%E8%80%81%E4%BA%BA%E6%88%96%E8%80%85%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="优先招用" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E6%8B%9B%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
                  <outline text="用人单位在6个月内重新招用人员的" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%9C%A86%E4%B8%AA%E6%9C%88%E5%86%85%E9%87%8D%E6%96%B0%E6%8B%9B%E7%94%A8%E4%BA%BA%E5%91%98%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="应当通知被裁减的人员" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%80%9A%E7%9F%A5%E8%A2%AB%E8%A3%81%E5%87%8F%E7%9A%84%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="在同等条件下优先招用" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E4%B8%8B%E4%BC%98%E5%85%88%E6%8B%9B%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="应支付经济补偿金" _mubu_text="%3Cspan%3E%E5%BA%94%E6%94%AF%E4%BB%98%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在服务期内，劳动者无须支付违约金" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%8D%E5%8A%A1%E6%9C%9F%E5%86%85%EF%BC%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E6%97%A0%E9%A1%BB%E6%94%AF%E4%BB%98%E8%BF%9D%E7%BA%A6%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="单位不得单方解除" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E4%B8%8D%E5%BE%97%E5%8D%95%E6%96%B9%E8%A7%A3%E9%99%A4%3C/span%3E" _note="过错性解除不受限制" _mubu_note="%3Cspan%3E%E8%BF%87%E9%94%99%E6%80%A7%E8%A7%A3%E9%99%A4%E4%B8%8D%E5%8F%97%E9%99%90%E5%88%B6%3C/span%3E">
              <outline text="有职业病危害的劳动者未进行离岗前职业健康检查" _mubu_text="%3Cspan%3E%E6%9C%89%E8%81%8C%E4%B8%9A%E7%97%85%E5%8D%B1%E5%AE%B3%E7%9A%84%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9C%AA%E8%BF%9B%E8%A1%8C%E7%A6%BB%E5%B2%97%E5%89%8D%E8%81%8C%E4%B8%9A%E5%81%A5%E5%BA%B7%E6%A3%80%E6%9F%A5%3C/span%3E" _note="或疑似职业病病人在诊断或医学观察期间的" _mubu_note="%3Cspan%3E%E6%88%96%E7%96%91%E4%BC%BC%E8%81%8C%E4%B8%9A%E7%97%85%E7%97%85%E4%BA%BA%E5%9C%A8%E8%AF%8A%E6%96%AD%E6%88%96%E5%8C%BB%E5%AD%A6%E8%A7%82%E5%AF%9F%E6%9C%9F%E9%97%B4%E7%9A%84%3C/span%3E"/>
              <outline text="在本单位患职业病或因工负伤并被确认(部分)丧失劳动能力的" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%AC%E5%8D%95%E4%BD%8D%E6%82%A3%E8%81%8C%E4%B8%9A%E7%97%85%E6%88%96%E5%9B%A0%E5%B7%A5%E8%B4%9F%E4%BC%A4%E5%B9%B6%E8%A2%AB%E7%A1%AE%E8%AE%A4(%E9%83%A8%E5%88%86)%E4%B8%A7%E5%A4%B1%E5%8A%B3%E5%8A%A8%E8%83%BD%E5%8A%9B%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="患病或非因工负伤，在规定的医疗期内的" _mubu_text="%3Cspan%3E%E6%82%A3%E7%97%85%E6%88%96%E9%9D%9E%E5%9B%A0%E5%B7%A5%E8%B4%9F%E4%BC%A4%EF%BC%8C%E5%9C%A8%E8%A7%84%E5%AE%9A%E7%9A%84%E5%8C%BB%E7%96%97%E6%9C%9F%E5%86%85%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="女职工在孕期、产期、哺乳期的" _mubu_text="%3Cspan%3E%E5%A5%B3%E8%81%8C%E5%B7%A5%E5%9C%A8%E5%AD%95%E6%9C%9F%E3%80%81%E4%BA%A7%E6%9C%9F%E3%80%81%E5%93%BA%E4%B9%B3%E6%9C%9F%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="在本单位连续工作满15年，且距法定退休年龄不足5年的" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%AC%E5%8D%95%E4%BD%8D%E8%BF%9E%E7%BB%AD%E5%B7%A5%E4%BD%9C%E6%BB%A115%E5%B9%B4%EF%BC%8C%E4%B8%94%E8%B7%9D%E6%B3%95%E5%AE%9A%E9%80%80%E4%BC%91%E5%B9%B4%E9%BE%84%E4%B8%8D%E8%B6%B35%E5%B9%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="单位迫使劳动者辞职" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E8%BF%AB%E4%BD%BF%E5%8A%B3%E5%8A%A8%E8%80%85%E8%BE%9E%E8%81%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="支付报酬+经济补偿+赔偿金" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E6%8A%A5%E9%85%AC+%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF+%E8%B5%94%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="单位解除的程序" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E8%A7%A3%E9%99%A4%E7%9A%84%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="单位单方解除，应事先将理由通知工会" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%8D%95%E6%96%B9%E8%A7%A3%E9%99%A4%EF%BC%8C%E5%BA%94%E4%BA%8B%E5%85%88%E5%B0%86%E7%90%86%E7%94%B1%E9%80%9A%E7%9F%A5%E5%B7%A5%E4%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="未事先通知" _mubu_text="%3Cspan%3E%E6%9C%AA%E4%BA%8B%E5%85%88%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="可请求赔偿金" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%AF%B7%E6%B1%82%E8%B5%94%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="起诉单位已经补正有关程序的除外" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E5%8D%95%E4%BD%8D%E5%B7%B2%E7%BB%8F%E8%A1%A5%E6%AD%A3%E6%9C%89%E5%85%B3%E7%A8%8B%E5%BA%8F%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="经济补偿金" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note="">
            <outline text="经济补偿金的标准" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%E7%9A%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="每满一年支付1个月工资" _mubu_text="%3Cspan%3E%E6%AF%8F%E6%BB%A1%E4%B8%80%E5%B9%B4%E6%94%AF%E4%BB%981%E4%B8%AA%E6%9C%88%E5%B7%A5%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="6个月≤＜1年" _mubu_text="%3Cspan%3E6%E4%B8%AA%E6%9C%88%E2%89%A4%EF%BC%9C1%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="按1年算，支付1个月工资的经济补偿" _mubu_text="%3Cspan%3E%E6%8C%891%E5%B9%B4%E7%AE%97%EF%BC%8C%E6%94%AF%E4%BB%981%E4%B8%AA%E6%9C%88%E5%B7%A5%E8%B5%84%E7%9A%84%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="＜6个月" _mubu_text="%3Cspan%3E%EF%BC%9C6%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="支付半个月工资的经济补偿" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E5%8D%8A%E4%B8%AA%E6%9C%88%E5%B7%A5%E8%B5%84%E7%9A%84%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="收入过高者经济补偿金" _mubu_text="%3Cspan%3E%E6%94%B6%E5%85%A5%E8%BF%87%E9%AB%98%E8%80%85%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="月工资＞所在地上年度职工月平均工资3倍" _mubu_text="%3Cspan%3E%E6%9C%88%E5%B7%A5%E8%B5%84%EF%BC%9E%E6%89%80%E5%9C%A8%E5%9C%B0%E4%B8%8A%E5%B9%B4%E5%BA%A6%E8%81%8C%E5%B7%A5%E6%9C%88%E5%B9%B3%E5%9D%87%E5%B7%A5%E8%B5%843%E5%80%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="按3倍支付" _mubu_text="%3Cspan%3E%E6%8C%893%E5%80%8D%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="且最高不超过12年" _mubu_text="%3Cspan%3E%E4%B8%94%E6%9C%80%E9%AB%98%E4%B8%8D%E8%B6%85%E8%BF%8712%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="违法解除/终止" _mubu_text="%3Cspan%3E%E8%BF%9D%E6%B3%95%E8%A7%A3%E9%99%A4/%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="要求单位继续履行合同" _mubu_text="%3Cspan%3E%E8%A6%81%E6%B1%82%E5%8D%95%E4%BD%8D%E7%BB%A7%E7%BB%AD%E5%B1%A5%E8%A1%8C%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="或按照经济补偿金的2倍要求赔偿金" _mubu_text="%3Cspan%3E%E6%88%96%E6%8C%89%E7%85%A7%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E9%87%91%E7%9A%842%E5%80%8D%E8%A6%81%E6%B1%82%E8%B5%94%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="单位支付了赔偿金的，不再支付补偿金" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%94%AF%E4%BB%98%E4%BA%86%E8%B5%94%E5%81%BF%E9%87%91%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%86%8D%E6%94%AF%E4%BB%98%E8%A1%A5%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="单位支付赔偿金的情形" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E6%94%AF%E4%BB%98%E8%B5%94%E5%81%BF%E9%87%91%E7%9A%84%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="用人单位违法解除或终止劳动合同" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%BF%9D%E6%B3%95%E8%A7%A3%E9%99%A4%E6%88%96%E7%BB%88%E6%AD%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="用人单位迫使劳动者提出解除合同" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%BF%AB%E4%BD%BF%E5%8A%B3%E5%8A%A8%E8%80%85%E6%8F%90%E5%87%BA%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="集体合同" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="由工会代表企业职工一方与用人单位订立" _mubu_text="%3Cspan%3E%E7%94%B1%E5%B7%A5%E4%BC%9A%E4%BB%A3%E8%A1%A8%E4%BC%81%E4%B8%9A%E8%81%8C%E5%B7%A5%E4%B8%80%E6%96%B9%E4%B8%8E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%AE%A2%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="劳动报酬和劳动条件" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E5%92%8C%E5%8A%B3%E5%8A%A8%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="不得低于当地人民政府规定的最低标准" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BD%8E%E4%BA%8E%E5%BD%93%E5%9C%B0%E4%BA%BA%E6%B0%91%E6%94%BF%E5%BA%9C%E8%A7%84%E5%AE%9A%E7%9A%84%E6%9C%80%E4%BD%8E%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="用人单位与劳动者订立的劳动合同中，不得低于集体合同规定的标准" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%AE%A2%E7%AB%8B%E7%9A%84%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E4%B8%AD%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BD%8E%E4%BA%8E%E9%9B%86%E4%BD%93%E5%90%88%E5%90%8C%E8%A7%84%E5%AE%9A%E7%9A%84%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="劳务派遣" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%3C/span%3E" _note="" _mubu_note="">
          <outline text="劳务派遣用工是补充形式" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E7%94%A8%E5%B7%A5%E6%98%AF%E8%A1%A5%E5%85%85%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="单位使用的被派遣劳动者数量不得超过其用工总量的10%" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E4%BD%BF%E7%94%A8%E7%9A%84%E8%A2%AB%E6%B4%BE%E9%81%A3%E5%8A%B3%E5%8A%A8%E8%80%85%E6%95%B0%E9%87%8F%E4%B8%8D%E5%BE%97%E8%B6%85%E8%BF%87%E5%85%B6%E7%94%A8%E5%B7%A5%E6%80%BB%E9%87%8F%E7%9A%8410%25%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律关系" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="劳务派遣单位称为用人单位" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E7%A7%B0%E4%B8%BA%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="接受劳务派遣的单位称为用工单位" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%8F%97%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E7%9A%84%E5%8D%95%E4%BD%8D%E7%A7%B0%E4%B8%BA%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="用人单位和用工单位订立劳务派遣协议" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E5%92%8C%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E8%AE%A2%E7%AB%8B%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="劳务派遣用工限制" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E7%94%A8%E5%B7%A5%E9%99%90%E5%88%B6%3C/span%3E" _note="择一即可" _mubu_note="%3Cspan%3E%E6%8B%A9%E4%B8%80%E5%8D%B3%E5%8F%AF%3C/span%3E">
            <outline text="临时性" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="岗位的存续时间不超过6个月" _mubu_text="%3Cspan%3E%E5%B2%97%E4%BD%8D%E7%9A%84%E5%AD%98%E7%BB%AD%E6%97%B6%E9%97%B4%E4%B8%8D%E8%B6%85%E8%BF%876%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="辅助性" _mubu_text="%3Cspan%3E%E8%BE%85%E5%8A%A9%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="非主营业务岗位" _mubu_text="%3Cspan%3E%E9%9D%9E%E4%B8%BB%E8%90%A5%E4%B8%9A%E5%8A%A1%E5%B2%97%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="替代性" _mubu_text="%3Cspan%3E%E6%9B%BF%E4%BB%A3%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="可由其他劳动者替代工作的岗位" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%B1%E5%85%B6%E4%BB%96%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9B%BF%E4%BB%A3%E5%B7%A5%E4%BD%9C%E7%9A%84%E5%B2%97%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="劳务派遣单位" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="经营劳务派遣业务，应向劳动行政部门依法申请行政许可" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E4%B8%9A%E5%8A%A1%EF%BC%8C%E5%BA%94%E5%90%91%E5%8A%B3%E5%8A%A8%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%E4%BE%9D%E6%B3%95%E7%94%B3%E8%AF%B7%E8%A1%8C%E6%94%BF%E8%AE%B8%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="派遣单位与被派遣者订立的劳动合同" _mubu_text="%3Cspan%3E%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E4%B8%8E%E8%A2%AB%E6%B4%BE%E9%81%A3%E8%80%85%E8%AE%A2%E7%AB%8B%E7%9A%84%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="应是2年以上的固定期限劳动合同" _mubu_text="%3Cspan%3E%E5%BA%94%E6%98%AF2%E5%B9%B4%E4%BB%A5%E4%B8%8A%E7%9A%84%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不得以非全日制用工形式招用" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E9%9D%9E%E5%85%A8%E6%97%A5%E5%88%B6%E7%94%A8%E5%B7%A5%E5%BD%A2%E5%BC%8F%E6%8B%9B%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="用人单位不得设立劳务派遣单位" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%8D%E5%BE%97%E8%AE%BE%E7%AB%8B%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%3C/span%3E" _note="向本单位或者所属单位派遣劳动者" _mubu_note="%3Cspan%3E%E5%90%91%E6%9C%AC%E5%8D%95%E4%BD%8D%E6%88%96%E8%80%85%E6%89%80%E5%B1%9E%E5%8D%95%E4%BD%8D%E6%B4%BE%E9%81%A3%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E"/>
            <outline text="社会保险" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="按用工单位所在地标准缴纳社会保险费" _mubu_text="%3Cspan%3E%E6%8C%89%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E6%89%80%E5%9C%A8%E5%9C%B0%E6%A0%87%E5%87%86%E7%BC%B4%E7%BA%B3%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%99%A9%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="用人单位承担工伤保险责任" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%89%BF%E6%8B%85%E5%B7%A5%E4%BC%A4%E4%BF%9D%E9%99%A9%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="用工单位" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="用工单位和劳动者之间不存在劳动合同关系" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E5%92%8C%E5%8A%B3%E5%8A%A8%E8%80%85%E4%B9%8B%E9%97%B4%E4%B8%8D%E5%AD%98%E5%9C%A8%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="但劳动者要服从用工单位的管理" _mubu_note="%3Cspan%3E%E4%BD%86%E5%8A%B3%E5%8A%A8%E8%80%85%E8%A6%81%E6%9C%8D%E4%BB%8E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E7%9A%84%E7%AE%A1%E7%90%86%3C/span%3E"/>
            <outline text="不得将连续用工期限分割订立数个短期劳务派遣协议" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%B0%86%E8%BF%9E%E7%BB%AD%E7%94%A8%E5%B7%A5%E6%9C%9F%E9%99%90%E5%88%86%E5%89%B2%E8%AE%A2%E7%AB%8B%E6%95%B0%E4%B8%AA%E7%9F%AD%E6%9C%9F%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不得将被派遣劳动者再派遣" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%B0%86%E8%A2%AB%E6%B4%BE%E9%81%A3%E5%8A%B3%E5%8A%A8%E8%80%85%E5%86%8D%E6%B4%BE%E9%81%A3%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="用工单位退回劳动者" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E9%80%80%E5%9B%9E%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E" _note="" _mubu_note="">
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="被派遣劳动者有过错（过失性辞退）" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%B4%BE%E9%81%A3%E5%8A%B3%E5%8A%A8%E8%80%85%E6%9C%89%E8%BF%87%E9%94%99%EF%BC%88%E8%BF%87%E5%A4%B1%E6%80%A7%E8%BE%9E%E9%80%80%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="《劳动合同法》第40条第1项、第2项" _mubu_text="%3Cspan%3E%E3%80%8A%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%B3%95%E3%80%8B%E7%AC%AC40%E6%9D%A1%E7%AC%AC1%E9%A1%B9%E3%80%81%E7%AC%AC2%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="用工单位可以将劳动者退回劳务派遣单位，劳务派遣单位可以与劳动者解除劳动合同" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E5%8F%AF%E4%BB%A5%E5%B0%86%E5%8A%B3%E5%8A%A8%E8%80%85%E9%80%80%E5%9B%9E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%EF%BC%8C%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E5%8F%AF%E4%BB%A5%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%A7%A3%E9%99%A4%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律责任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="行政责任" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="两方单位违反劳务派遣制度的" _mubu_text="%3Cspan%3E%E4%B8%A4%E6%96%B9%E5%8D%95%E4%BD%8D%E8%BF%9D%E5%8F%8D%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%88%B6%E5%BA%A6%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="连带责任" _mubu_text="%3Cspan%3E%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="用工单位给被派遣劳动者造成损害的" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E7%BB%99%E8%A2%AB%E6%B4%BE%E9%81%A3%E5%8A%B3%E5%8A%A8%E8%80%85%E9%80%A0%E6%88%90%E6%8D%9F%E5%AE%B3%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="用人单位造成" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E9%80%A0%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="用工单位无须承担连带责任" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E6%97%A0%E9%A1%BB%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="派遣工给他人造成损害" _mubu_text="%3Cspan%3E%E6%B4%BE%E9%81%A3%E5%B7%A5%E7%BB%99%E4%BB%96%E4%BA%BA%E9%80%A0%E6%88%90%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="用工单位承担侵权责任" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="劳务派遣单位有过错的，承担相应的责任" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E6%9C%89%E8%BF%87%E9%94%99%E7%9A%84%EF%BC%8C%E6%89%BF%E6%8B%85%E7%9B%B8%E5%BA%94%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="非全日制用工" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%85%A8%E6%97%A5%E5%88%B6%E7%94%A8%E5%B7%A5%3C/span%3E" _note="" _mubu_note="">
          <outline text="每日≤4h；每周≤24h" _mubu_text="%3Cspan%3E%E6%AF%8F%E6%97%A5%E2%89%A44h%EF%BC%9B%E6%AF%8F%E5%91%A8%E2%89%A424h%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="非全日制用工or一般形式用工(图)" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%85%A8%E6%97%A5%E5%88%B6%E7%94%A8%E5%B7%A5or%E4%B8%80%E8%88%AC%E5%BD%A2%E5%BC%8F%E7%94%A8%E5%B7%A5(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22XAyigYh6OO%22%2C%22oh%22%3A755%2C%22ow%22%3A1246%2C%22uri%22%3A%22document_image%2Fd29b30ab-60ac-4c17-b632-4945247b7594-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
      </outline>
      <outline text="劳动争议" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
        <outline text="认定" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="1、判断争议的主体是否是劳动者和单位" _mubu_text="%3Cspan%3E1%E3%80%81%E5%88%A4%E6%96%AD%E4%BA%89%E8%AE%AE%E7%9A%84%E4%B8%BB%E4%BD%93%E6%98%AF%E5%90%A6%E6%98%AF%E5%8A%B3%E5%8A%A8%E8%80%85%E5%92%8C%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="2、判断其争议的内容是否与劳动关系、劳动合同、职位等相关" _mubu_text="%3Cspan%3E2%E3%80%81%E5%88%A4%E6%96%AD%E5%85%B6%E4%BA%89%E8%AE%AE%E7%9A%84%E5%86%85%E5%AE%B9%E6%98%AF%E5%90%A6%E4%B8%8E%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E3%80%81%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E3%80%81%E8%81%8C%E4%BD%8D%E7%AD%89%E7%9B%B8%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="仲裁" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="一般规定" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="仲裁案件的当事人" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E6%A1%88%E4%BB%B6%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动者和用人单位为双方当事人" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%92%8C%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%BA%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="劳务派遣" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="派遣单位和用工单位为共同当事人" _mubu_text="%3Cspan%3E%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E5%92%8C%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E4%B8%BA%E5%85%B1%E5%90%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="合同履行地或用人单位所在地" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E5%B1%A5%E8%A1%8C%E5%9C%B0%E6%88%96%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%89%80%E5%9C%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="合同履行地优先" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E5%B1%A5%E8%A1%8C%E5%9C%B0%E4%BC%98%E5%85%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="时效" _mubu_text="%3Cspan%3E%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="1年" _mubu_text="%3Cspan%3E1%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="因拖欠劳动报酬发生争议的" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%8B%96%E6%AC%A0%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E5%8F%91%E7%94%9F%E4%BA%89%E8%AE%AE%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="劳动关系存续期间" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E5%AD%98%E7%BB%AD%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                    <outline text="不受时效期间限制" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%97%E6%97%B6%E6%95%88%E6%9C%9F%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="劳动关系终止" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                    <outline text="自劳动关系终止之日起一年内提出" _mubu_text="%3Cspan%3E%E8%87%AA%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E7%BB%88%E6%AD%A2%E4%B9%8B%E6%97%A5%E8%B5%B7%E4%B8%80%E5%B9%B4%E5%86%85%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="惩罚性赔偿部分" _mubu_text="%3Cspan%3E%E6%83%A9%E7%BD%9A%E6%80%A7%E8%B5%94%E5%81%BF%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不属于正常的劳动报酬，应当适用1年的仲裁时效" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%B1%9E%E4%BA%8E%E6%AD%A3%E5%B8%B8%E7%9A%84%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%EF%BC%8C%E5%BA%94%E5%BD%93%E9%80%82%E7%94%A81%E5%B9%B4%E7%9A%84%E4%BB%B2%E8%A3%81%E6%97%B6%E6%95%88%3C/span%3E" _note="如单位未按期订立书面劳动合同而应当支付&quot;二倍工资&quot;中增加的一倍工资" _mubu_note="%3Cspan%3E%E5%A6%82%E5%8D%95%E4%BD%8D%E6%9C%AA%E6%8C%89%E6%9C%9F%E8%AE%A2%E7%AB%8B%E4%B9%A6%E9%9D%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E8%80%8C%E5%BA%94%E5%BD%93%E6%94%AF%E4%BB%98%22%E4%BA%8C%E5%80%8D%E5%B7%A5%E8%B5%84%22%E4%B8%AD%E5%A2%9E%E5%8A%A0%E7%9A%84%E4%B8%80%E5%80%8D%E5%B7%A5%E8%B5%84%3C/span%3E"/>
