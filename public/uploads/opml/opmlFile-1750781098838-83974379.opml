            </outline>
            <outline text="离境清税" _mubu_text="%3Cspan%3E%E7%A6%BB%E5%A2%83%E6%B8%85%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
              <outline text="应在出境前向税务机关结清应纳税款、滞纳金或者提供担保" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E5%87%BA%E5%A2%83%E5%89%8D%E5%90%91%E7%A8%8E%E5%8A%A1%E6%9C%BA%E5%85%B3%E7%BB%93%E6%B8%85%E5%BA%94%E7%BA%B3%E7%A8%8E%E6%AC%BE%E3%80%81%E6%BB%9E%E7%BA%B3%E9%87%91%E6%88%96%E8%80%85%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="未，税务机关可以通知出境管理机关阻止其出境" _mubu_note="%3Cspan%3E%E6%9C%AA%EF%BC%8C%E7%A8%8E%E5%8A%A1%E6%9C%BA%E5%85%B3%E5%8F%AF%E4%BB%A5%E9%80%9A%E7%9F%A5%E5%87%BA%E5%A2%83%E7%AE%A1%E7%90%86%E6%9C%BA%E5%85%B3%E9%98%BB%E6%AD%A2%E5%85%B6%E5%87%BA%E5%A2%83%3C/span%3E"/>
            </outline>
            <outline text="税收优先权" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E4%BC%98%E5%85%88%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="税收优先于无担保债权" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E4%BC%98%E5%85%88%E4%BA%8E%E6%97%A0%E6%8B%85%E4%BF%9D%E5%80%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="与有担保债权" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%9C%89%E6%8B%85%E4%BF%9D%E5%80%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="先欠税，后设定抵押" _mubu_text="%3Cspan%3E%E5%85%88%E6%AC%A0%E7%A8%8E%EF%BC%8C%E5%90%8E%E8%AE%BE%E5%AE%9A%E6%8A%B5%E6%8A%BC%3C/span%3E" _note="" _mubu_note="">
                  <outline text="先还税，后还债" _mubu_text="%3Cspan%3E%E5%85%88%E8%BF%98%E7%A8%8E%EF%BC%8C%E5%90%8E%E8%BF%98%E5%80%BA%3C/span%3E" _note="有优先权" _mubu_note="%3Cspan%3E%E6%9C%89%E4%BC%98%E5%85%88%E6%9D%83%3C/span%3E"/>
                </outline>
                <outline text="先设定抵押，后欠税" _mubu_text="%3Cspan%3E%E5%85%88%E8%AE%BE%E5%AE%9A%E6%8A%B5%E6%8A%BC%EF%BC%8C%E5%90%8E%E6%AC%A0%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="先还债，后还税" _mubu_text="%3Cspan%3E%E5%85%88%E8%BF%98%E5%80%BA%EF%BC%8C%E5%90%8E%E8%BF%98%E7%A8%8E%3C/span%3E" _note="无优先权" _mubu_note="%3Cspan%3E%E6%97%A0%E4%BC%98%E5%85%88%E6%9D%83%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="税收优先于罚款、没收违法所得" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E4%BC%98%E5%85%88%E4%BA%8E%E7%BD%9A%E6%AC%BE%E3%80%81%E6%B2%A1%E6%94%B6%E8%BF%9D%E6%B3%95%E6%89%80%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="税收代位权、撤销权" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E4%BB%A3%E4%BD%8D%E6%9D%83%E3%80%81%E6%92%A4%E9%94%80%E6%9D%83%3C/span%3E" _note="与民法债的代位权、撤销权类似" _mubu_note="%3Cspan%3E%E4%B8%8E%E6%B0%91%E6%B3%95%E5%80%BA%E7%9A%84%E4%BB%A3%E4%BD%8D%E6%9D%83%E3%80%81%E6%92%A4%E9%94%80%E6%9D%83%E7%B1%BB%E4%BC%BC%3C/span%3E">
              <outline text="税务机关可以自己的名义向法院请求" _mubu_text="%3Cspan%3E%E7%A8%8E%E5%8A%A1%E6%9C%BA%E5%85%B3%E5%8F%AF%E4%BB%A5%E8%87%AA%E5%B7%B1%E7%9A%84%E5%90%8D%E4%B9%89%E5%90%91%E6%B3%95%E9%99%A2%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="行使范围以债权人的债权为限" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E8%8C%83%E5%9B%B4%E4%BB%A5%E5%80%BA%E6%9D%83%E4%BA%BA%E7%9A%84%E5%80%BA%E6%9D%83%E4%B8%BA%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="必要费用由欠税人负担" _mubu_text="%3Cspan%3E%E5%BF%85%E8%A6%81%E8%B4%B9%E7%94%A8%E7%94%B1%E6%AC%A0%E7%A8%8E%E4%BA%BA%E8%B4%9F%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="税收保全措施和强制执行措施，不能针对欠缴税款纳税人的债务人采用" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%94%B6%E4%BF%9D%E5%85%A8%E6%8E%AA%E6%96%BD%E5%92%8C%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E6%8E%AA%E6%96%BD%EF%BC%8C%E4%B8%8D%E8%83%BD%E9%92%88%E5%AF%B9%E6%AC%A0%E7%BC%B4%E7%A8%8E%E6%AC%BE%E7%BA%B3%E7%A8%8E%E4%BA%BA%E7%9A%84%E5%80%BA%E5%8A%A1%E4%BA%BA%E9%87%87%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="税款追征" _mubu_text="%3Cspan%3E%E7%A8%8E%E6%AC%BE%E8%BF%BD%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="税务机关原因" _mubu_text="%3Cspan%3E%E7%A8%8E%E5%8A%A1%E6%9C%BA%E5%85%B3%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="3年内，税务机关可以要求补缴税款" _mubu_text="%3Cspan%3E3%E5%B9%B4%E5%86%85%EF%BC%8C%E7%A8%8E%E5%8A%A1%E6%9C%BA%E5%85%B3%E5%8F%AF%E4%BB%A5%E8%A6%81%E6%B1%82%E8%A1%A5%E7%BC%B4%E7%A8%8E%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得加收滞纳金" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%8A%A0%E6%94%B6%E6%BB%9E%E7%BA%B3%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="纳税人原因" _mubu_text="%3Cspan%3E%E7%BA%B3%E7%A8%8E%E4%BA%BA%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="因失误导致未缴/少缴" _mubu_text="%3Cspan%3E%E5%9B%A0%E5%A4%B1%E8%AF%AF%E5%AF%BC%E8%87%B4%E6%9C%AA%E7%BC%B4/%E5%B0%91%E7%BC%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="累计欠税＜10万元" _mubu_text="%3Cspan%3E%E7%B4%AF%E8%AE%A1%E6%AC%A0%E7%A8%8E%EF%BC%9C10%E4%B8%87%E5%85%83%3C/span%3E" _note="" _mubu_note="">
                    <outline text="3年内追征" _mubu_text="%3Cspan%3E3%E5%B9%B4%E5%86%85%E8%BF%BD%E5%BE%81%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="加收滞纳金" _mubu_text="%3Cspan%3E%E5%8A%A0%E6%94%B6%E6%BB%9E%E7%BA%B3%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="累计欠税≥10万元" _mubu_text="%3Cspan%3E%E7%B4%AF%E8%AE%A1%E6%AC%A0%E7%A8%8E%E2%89%A510%E4%B8%87%E5%85%83%3C/span%3E" _note="" _mubu_note="">
                    <outline text="5年内追征" _mubu_text="%3Cspan%3E5%E5%B9%B4%E5%86%85%E8%BF%BD%E5%BE%81%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="加收滞纳金" _mubu_text="%3Cspan%3E%E5%8A%A0%E6%94%B6%E6%BB%9E%E7%BA%B3%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="偷税、抗税、骗税" _mubu_text="%3Cspan%3E%E5%81%B7%E7%A8%8E%E3%80%81%E6%8A%97%E7%A8%8E%E3%80%81%E9%AA%97%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="无限期追征" _mubu_text="%3Cspan%3E%E6%97%A0%E9%99%90%E6%9C%9F%E8%BF%BD%E5%BE%81%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="加收滞纳金" _mubu_text="%3Cspan%3E%E5%8A%A0%E6%94%B6%E6%BB%9E%E7%BA%B3%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="审计法" _mubu_text="%3Cspan%3E%E5%AE%A1%E8%AE%A1%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="土地法和房地产法" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E6%B3%95%E5%92%8C%E6%88%BF%E5%9C%B0%E4%BA%A7%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="土地管理法" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E7%AE%A1%E7%90%86%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="土地所有权和使用权" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E6%89%80%E6%9C%89%E6%9D%83%E5%92%8C%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="国家所有" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%89%80%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="集体所有" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E6%89%80%E6%9C%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="农用地承包" _mubu_text="%3Cspan%3E%E5%86%9C%E7%94%A8%E5%9C%B0%E6%89%BF%E5%8C%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="家庭承包为原则" _mubu_text="%3Cspan%3E%E5%AE%B6%E5%BA%AD%E6%89%BF%E5%8C%85%E4%B8%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="农村集体经济组织内部" _mubu_note="%3Cspan%3E%E5%86%9C%E6%9D%91%E9%9B%86%E4%BD%93%E7%BB%8F%E6%B5%8E%E7%BB%84%E7%BB%87%E5%86%85%E9%83%A8%3C/span%3E"/>
                <outline text="其他承包方式" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%89%BF%E5%8C%85%E6%96%B9%E5%BC%8F%3C/span%3E" _note="招标、拍卖、公开协商等" _mubu_note="%3Cspan%3E%E6%8B%9B%E6%A0%87%E3%80%81%E6%8B%8D%E5%8D%96%E3%80%81%E5%85%AC%E5%BC%80%E5%8D%8F%E5%95%86%E7%AD%89%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="耕地保护" _mubu_text="%3Cspan%3E%E8%80%95%E5%9C%B0%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="严格控制耕地转为非耕地" _mubu_text="%3Cspan%3E%E4%B8%A5%E6%A0%BC%E6%8E%A7%E5%88%B6%E8%80%95%E5%9C%B0%E8%BD%AC%E4%B8%BA%E9%9D%9E%E8%80%95%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="占用耕地补偿制度" _mubu_text="%3Cspan%3E%E5%8D%A0%E7%94%A8%E8%80%95%E5%9C%B0%E8%A1%A5%E5%81%BF%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="单位负责开垦与所占用耕地的数量和质量相当的耕地" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E8%B4%9F%E8%B4%A3%E5%BC%80%E5%9E%A6%E4%B8%8E%E6%89%80%E5%8D%A0%E7%94%A8%E8%80%95%E5%9C%B0%E7%9A%84%E6%95%B0%E9%87%8F%E5%92%8C%E8%B4%A8%E9%87%8F%E7%9B%B8%E5%BD%93%E7%9A%84%E8%80%95%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无，缴纳耕地开垦费，专款用于开垦新的耕地" _mubu_text="%3Cspan%3E%E6%97%A0%EF%BC%8C%E7%BC%B4%E7%BA%B3%E8%80%95%E5%9C%B0%E5%BC%80%E5%9E%A6%E8%B4%B9%EF%BC%8C%E4%B8%93%E6%AC%BE%E7%94%A8%E4%BA%8E%E5%BC%80%E5%9E%A6%E6%96%B0%E7%9A%84%E8%80%95%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="确保本行政区域内耕地总不减少、质量不降低" _mubu_text="%3Cspan%3E%E7%A1%AE%E4%BF%9D%E6%9C%AC%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%9F%9F%E5%86%85%E8%80%95%E5%9C%B0%E6%80%BB%E4%B8%8D%E5%87%8F%E5%B0%91%E3%80%81%E8%B4%A8%E9%87%8F%E4%B8%8D%E9%99%8D%E4%BD%8E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="永久基本农田保护制度" _mubu_text="%3Cspan%3E%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%E4%BF%9D%E6%8A%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="生产基地内耕地" _mubu_text="%3Cspan%3E%E7%94%9F%E4%BA%A7%E5%9F%BA%E5%9C%B0%E5%86%85%E8%80%95%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="高标准农田" _mubu_text="%3Cspan%3E%E9%AB%98%E6%A0%87%E5%87%86%E5%86%9C%E7%94%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="农业科教试验田等" _mubu_text="%3Cspan%3E%E5%86%9C%E4%B8%9A%E7%A7%91%E6%95%99%E8%AF%95%E9%AA%8C%E7%94%B0%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="严格保护" _mubu_text="%3Cspan%3E%E4%B8%A5%E6%A0%BC%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="保护应落实到地块" _mubu_text="%3Cspan%3E%E4%BF%9D%E6%8A%A4%E5%BA%94%E8%90%BD%E5%AE%9E%E5%88%B0%E5%9C%B0%E5%9D%97%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="划定后，任何单位和个人不得擅自占用或者改变其用途" _mubu_text="%3Cspan%3E%E5%88%92%E5%AE%9A%E5%90%8E%EF%BC%8C%E4%BB%BB%E4%BD%95%E5%8D%95%E4%BD%8D%E5%92%8C%E4%B8%AA%E4%BA%BA%E4%B8%8D%E5%BE%97%E6%93%85%E8%87%AA%E5%8D%A0%E7%94%A8%E6%88%96%E8%80%85%E6%94%B9%E5%8F%98%E5%85%B6%E7%94%A8%E9%80%94%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="涉及农用地转用或土地征收的，必须国院批准" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%86%9C%E7%94%A8%E5%9C%B0%E8%BD%AC%E7%94%A8%E6%88%96%E5%9C%9F%E5%9C%B0%E5%BE%81%E6%94%B6%E7%9A%84%EF%BC%8C%E5%BF%85%E9%A1%BB%E5%9B%BD%E9%99%A2%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="禁止占用永久基本农田发展林果业和挖塘养鱼" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E5%8D%A0%E7%94%A8%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%E5%8F%91%E5%B1%95%E6%9E%97%E6%9E%9C%E4%B8%9A%E5%92%8C%E6%8C%96%E5%A1%98%E5%85%BB%E9%B1%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="禁止闲置、荒芜耕地" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E9%97%B2%E7%BD%AE%E3%80%81%E8%8D%92%E8%8A%9C%E8%80%95%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="1年内不用而又可以耕种并收获的" _mubu_text="%3Cspan%3E1%E5%B9%B4%E5%86%85%E4%B8%8D%E7%94%A8%E8%80%8C%E5%8F%88%E5%8F%AF%E4%BB%A5%E8%80%95%E7%A7%8D%E5%B9%B6%E6%94%B6%E8%8E%B7%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应恢复耕种" _mubu_text="%3Cspan%3E%E5%BA%94%E6%81%A2%E5%A4%8D%E8%80%95%E7%A7%8D%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="1年以上未动工建设的" _mubu_text="%3Cspan%3E1%E5%B9%B4%E4%BB%A5%E4%B8%8A%E6%9C%AA%E5%8A%A8%E5%B7%A5%E5%BB%BA%E8%AE%BE%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="缴纳闲置费" _mubu_text="%3Cspan%3E%E7%BC%B4%E7%BA%B3%E9%97%B2%E7%BD%AE%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="连续2年未使用的" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD2%E5%B9%B4%E6%9C%AA%E4%BD%BF%E7%94%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="县级政府无偿收回" _mubu_text="%3Cspan%3E%E5%8E%BF%E7%BA%A7%E6%94%BF%E5%BA%9C%E6%97%A0%E5%81%BF%E6%94%B6%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="并组织恢复耕种" _mubu_text="%3Cspan%3E%E5%B9%B6%E7%BB%84%E7%BB%87%E6%81%A2%E5%A4%8D%E8%80%95%E7%A7%8D%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="建设用地" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="农用地转用审批" _mubu_text="%3Cspan%3E%E5%86%9C%E7%94%A8%E5%9C%B0%E8%BD%AC%E7%94%A8%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="国务院审批" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="永久基本农田转为建设用地的" _mubu_text="%3Cspan%3E%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%E8%BD%AC%E4%B8%BA%E5%BB%BA%E8%AE%BE%E7%94%A8%E5%9C%B0%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="其他" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="在土地利用总体规划确定范围内" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%9C%9F%E5%9C%B0%E5%88%A9%E7%94%A8%E6%80%BB%E4%BD%93%E8%A7%84%E5%88%92%E7%A1%AE%E5%AE%9A%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在土地利用总体规划确定范围外" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%9C%9F%E5%9C%B0%E5%88%A9%E7%94%A8%E6%80%BB%E4%BD%93%E8%A7%84%E5%88%92%E7%A1%AE%E5%AE%9A%E8%8C%83%E5%9B%B4%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国务院或省级政府" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E6%88%96%E7%9C%81%E7%BA%A7%E6%94%BF%E5%BA%9C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="土地征收" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E5%BE%81%E6%94%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="严格限制土地征收范围" _mubu_text="%3Cspan%3E%E4%B8%A5%E6%A0%BC%E9%99%90%E5%88%B6%E5%9C%9F%E5%9C%B0%E5%BE%81%E6%94%B6%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="土地征收的权限" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%9C%9F%E5%9C%B0%E5%BE%81%E6%94%B6%E7%9A%84%E6%9D%83%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="国务院批准" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="永久基本农田" _mubu_text="%3Cspan%3E%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="永久基本农田以外的耕地＞35公顷" _mubu_text="%3Cspan%3E%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%E4%BB%A5%E5%A4%96%E7%9A%84%E8%80%95%E5%9C%B0%EF%BC%9E35%E5%85%AC%E9%A1%B7%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其他土地＞70公顷" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%9C%9F%E5%9C%B0%EF%BC%9E70%E5%85%AC%E9%A1%B7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="省级政府批准" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E6%94%BF%E5%BA%9C%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="除国务院批准的其他" _mubu_text="%3Cspan%3E%E9%99%A4%E5%9B%BD%E5%8A%A1%E9%99%A2%E6%89%B9%E5%87%86%E7%9A%84%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="并报国务院备案" _mubu_text="%3Cspan%3E%E5%B9%B6%E6%8A%A5%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="征收农用地" _mubu_text="%3Cspan%3E%E5%BE%81%E6%94%B6%E5%86%9C%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="应先行办理农用地转用审批" _mubu_text="%3Cspan%3E%E5%BA%94%E5%85%88%E8%A1%8C%E5%8A%9E%E7%90%86%E5%86%9C%E7%94%A8%E5%9C%B0%E8%BD%AC%E7%94%A8%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="同时办理征地审批手续" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%8A%9E%E7%90%86%E5%BE%81%E5%9C%B0%E5%AE%A1%E6%89%B9%E6%89%8B%E7%BB%AD%3C/span%3E" _note="" _mubu_note="">
                  <outline text="省级批准，超过征地批准权限的，应当另行办理征地审批" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E6%89%B9%E5%87%86%EF%BC%8C%E8%B6%85%E8%BF%87%E5%BE%81%E5%9C%B0%E6%89%B9%E5%87%86%E6%9D%83%E9%99%90%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%8F%A6%E8%A1%8C%E5%8A%9E%E7%90%86%E5%BE%81%E5%9C%B0%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="补偿" _mubu_text="%3Cspan%3E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="同时应安排被征地农民的社会保障费用" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%BA%94%E5%AE%89%E6%8E%92%E8%A2%AB%E5%BE%81%E5%9C%B0%E5%86%9C%E6%B0%91%E7%9A%84%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%9A%9C%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="费用标准由省级制定" _mubu_text="%3Cspan%3E%E8%B4%B9%E7%94%A8%E6%A0%87%E5%87%86%E7%94%B1%E7%9C%81%E7%BA%A7%E5%88%B6%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="至少每3年调整或从新公布一次" _mubu_text="%3Cspan%3E%E8%87%B3%E5%B0%91%E6%AF%8F3%E5%B9%B4%E8%B0%83%E6%95%B4%E6%88%96%E4%BB%8E%E6%96%B0%E5%85%AC%E5%B8%83%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="临时用地" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="批准" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="报批前，先经有关城市规划行政主管部门同意" _mubu_text="%3Cspan%3E%E6%8A%A5%E6%89%B9%E5%89%8D%EF%BC%8C%E5%85%88%E7%BB%8F%E6%9C%89%E5%85%B3%E5%9F%8E%E5%B8%82%E8%A7%84%E5%88%92%E8%A1%8C%E6%94%BF%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="县级以上政府自然资源主管部门批准" _mubu_text="%3Cspan%3E%E5%8E%BF%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%94%BF%E5%BA%9C%E8%87%AA%E7%84%B6%E8%B5%84%E6%BA%90%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="用地" _mubu_text="%3Cspan%3E%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="应按合同约定用途使用" _mubu_text="%3Cspan%3E%E5%BA%94%E6%8C%89%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%E7%94%A8%E9%80%94%E4%BD%BF%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得修建永久性建筑物" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BF%AE%E5%BB%BA%E6%B0%B8%E4%B9%85%E6%80%A7%E5%BB%BA%E7%AD%91%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="一般不超过2年" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%B8%8D%E8%B6%85%E8%BF%872%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="宅基地" _mubu_text="%3Cspan%3E%E5%AE%85%E5%9F%BA%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="一户一宅" _mubu_text="%3Cspan%3E%E4%B8%80%E6%88%B7%E4%B8%80%E5%AE%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不得占用永久基本农田" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%8D%A0%E7%94%A8%E6%B0%B8%E4%B9%85%E5%9F%BA%E6%9C%AC%E5%86%9C%E7%94%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="审批" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="乡镇政府审核批准" _mubu_text="%3Cspan%3E%E4%B9%A1%E9%95%87%E6%94%BF%E5%BA%9C%E5%AE%A1%E6%A0%B8%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="出卖、出租、赠与住宅后，不得再申请" _mubu_text="%3Cspan%3E%E5%87%BA%E5%8D%96%E3%80%81%E5%87%BA%E7%A7%9F%E3%80%81%E8%B5%A0%E4%B8%8E%E4%BD%8F%E5%AE%85%E5%90%8E%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%86%8D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="宅基地的退出" _mubu_text="%3Cspan%3E%E5%AE%85%E5%9F%BA%E5%9C%B0%E7%9A%84%E9%80%80%E5%87%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="自愿有偿退出" _mubu_text="%3Cspan%3E%E8%87%AA%E6%84%BF%E6%9C%89%E5%81%BF%E9%80%80%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="鼓励农村集体经济组织及其成员盘活利用闲置宅基地和闲置住宅" _mubu_text="%3Cspan%3E%E9%BC%93%E5%8A%B1%E5%86%9C%E6%9D%91%E9%9B%86%E4%BD%93%E7%BB%8F%E6%B5%8E%E7%BB%84%E7%BB%87%E5%8F%8A%E5%85%B6%E6%88%90%E5%91%98%E7%9B%98%E6%B4%BB%E5%88%A9%E7%94%A8%E9%97%B2%E7%BD%AE%E5%AE%85%E5%9F%BA%E5%9C%B0%E5%92%8C%E9%97%B2%E7%BD%AE%E4%BD%8F%E5%AE%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="集体建设用地入市" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E5%BB%BA%E8%AE%BE%E7%94%A8%E5%9C%B0%E5%85%A5%E5%B8%82%3C/span%3E" _note="" _mubu_note="">
              <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="出让、出租等" _mubu_text="%3Cspan%3E%E5%87%BA%E8%AE%A9%E3%80%81%E5%87%BA%E7%A7%9F%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="村民会议2/3以上成员同意" _mubu_text="%3Cspan%3E%E6%9D%91%E6%B0%91%E4%BC%9A%E8%AE%AE2/3%E4%BB%A5%E4%B8%8A%E6%88%90%E5%91%98%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="或2/3以上村民代表的同意" _mubu_text="%3Cspan%3E%E6%88%962/3%E4%BB%A5%E4%B8%8A%E6%9D%91%E6%B0%91%E4%BB%A3%E8%A1%A8%E7%9A%84%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="集体经营性建设用地使用权可再流转" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E7%BB%8F%E8%90%A5%E6%80%A7%E5%BB%BA%E8%AE%BE%E7%94%A8%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%8F%AF%E5%86%8D%E6%B5%81%E8%BD%AC%3C/span%3E" _note="转让、互换、出资、赠与或抵押" _mubu_note="%3Cspan%3E%E8%BD%AC%E8%AE%A9%E3%80%81%E4%BA%92%E6%8D%A2%E3%80%81%E5%87%BA%E8%B5%84%E3%80%81%E8%B5%A0%E4%B8%8E%E6%88%96%E6%8A%B5%E6%8A%BC%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
        <outline text="城市房地产管理法" _mubu_text="%3Cspan%3E%E5%9F%8E%E5%B8%82%E6%88%BF%E5%9C%B0%E4%BA%A7%E7%AE%A1%E7%90%86%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="房地产开发用地" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%88%BF%E5%9C%B0%E4%BA%A7%E5%BC%80%E5%8F%91%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="出让" _mubu_text="%3Cspan%3E%E5%87%BA%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="出让国有土地使用权" _mubu_text="%3Cspan%3E%E5%87%BA%E8%AE%A9%E5%9B%BD%E6%9C%89%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有期限限制＋支付土地出让金" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9C%9F%E9%99%90%E9%99%90%E5%88%B6%EF%BC%8B%E6%94%AF%E4%BB%98%E5%9C%9F%E5%9C%B0%E5%87%BA%E8%AE%A9%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="拍卖、招标及协议" _mubu_text="%3Cspan%3E%E6%8B%8D%E5%8D%96%E3%80%81%E6%8B%9B%E6%A0%87%E5%8F%8A%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="协议方式受限" _mubu_text="%3Cspan%3E%E5%8D%8F%E8%AE%AE%E6%96%B9%E5%BC%8F%E5%8F%97%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="商业、旅游、娱乐和豪华住宅用地" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E3%80%81%E6%97%85%E6%B8%B8%E3%80%81%E5%A8%B1%E4%B9%90%E5%92%8C%E8%B1%AA%E5%8D%8E%E4%BD%8F%E5%AE%85%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="须尽可能采取拍卖、招标方式" _mubu_text="%3Cspan%3E%E9%A1%BB%E5%B0%BD%E5%8F%AF%E8%83%BD%E9%87%87%E5%8F%96%E6%8B%8D%E5%8D%96%E3%80%81%E6%8B%9B%E6%A0%87%E6%96%B9%E5%BC%8F%3C/span%3E" _note="实在没法可采取协议的方式" _mubu_note="%3Cspan%3E%E5%AE%9E%E5%9C%A8%E6%B2%A1%E6%B3%95%E5%8F%AF%E9%87%87%E5%8F%96%E5%8D%8F%E8%AE%AE%E7%9A%84%E6%96%B9%E5%BC%8F%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="续期" _mubu_text="%3Cspan%3E%E7%BB%AD%E6%9C%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="届满前1年申请" _mubu_text="%3Cspan%3E%E5%B1%8A%E6%BB%A1%E5%89%8D1%E5%B9%B4%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="重新签订出让合同，支付出让金" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E7%AD%BE%E8%AE%A2%E5%87%BA%E8%AE%A9%E5%90%88%E5%90%8C%EF%BC%8C%E6%94%AF%E4%BB%98%E5%87%BA%E8%AE%A9%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未申请或未获批，土地使用权由国家无偿收回" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%94%B3%E8%AF%B7%E6%88%96%E6%9C%AA%E8%8E%B7%E6%89%B9%EF%BC%8C%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E7%94%B1%E5%9B%BD%E5%AE%B6%E6%97%A0%E5%81%BF%E6%94%B6%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="合同签订" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E7%AD%BE%E8%AE%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="市、县人民政府土地管理部门与土地使用者签订" _mubu_text="%3Cspan%3E%E5%B8%82%E3%80%81%E5%8E%BF%E4%BA%BA%E6%B0%91%E6%94%BF%E5%BA%9C%E5%9C%9F%E5%9C%B0%E7%AE%A1%E7%90%86%E9%83%A8%E9%97%A8%E4%B8%8E%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E8%80%85%E7%AD%BE%E8%AE%A2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="划拨" _mubu_text="%3Cspan%3E%E5%88%92%E6%8B%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="无期限限制＋无出让金" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%9F%E9%99%90%E9%99%90%E5%88%B6%EF%BC%8B%E6%97%A0%E5%87%BA%E8%AE%A9%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="特殊用途" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E7%94%A8%E9%80%94%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="使用者缴纳补偿、安置等费用后交付" _mubu_text="%3Cspan%3E%E4%BD%BF%E7%94%A8%E8%80%85%E7%BC%B4%E7%BA%B3%E8%A1%A5%E5%81%BF%E3%80%81%E5%AE%89%E7%BD%AE%E7%AD%89%E8%B4%B9%E7%94%A8%E5%90%8E%E4%BA%A4%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
                  <outline text="集体所有的土地" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E6%89%80%E6%9C%89%E7%9A%84%E5%9C%9F%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其他单位正在使用的国有土地" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%8D%95%E4%BD%8D%E6%AD%A3%E5%9C%A8%E4%BD%BF%E7%94%A8%E7%9A%84%E5%9B%BD%E6%9C%89%E5%9C%9F%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="无偿交付" _mubu_text="%3Cspan%3E%E6%97%A0%E5%81%BF%E4%BA%A4%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国有的荒山、荒地和滩涂" _mubu_text="%3Cspan%3E%E5%9B%BD%E6%9C%89%E7%9A%84%E8%8D%92%E5%B1%B1%E3%80%81%E8%8D%92%E5%9C%B0%E5%92%8C%E6%BB%A9%E6%B6%82%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="市区内未有收益的空地" _mubu_text="%3Cspan%3E%E5%B8%82%E5%8C%BA%E5%86%85%E6%9C%AA%E6%9C%89%E6%94%B6%E7%9B%8A%E7%9A%84%E7%A9%BA%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="批准划拨" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%E5%88%92%E6%8B%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="县级以上政府" _mubu_text="%3Cspan%3E%E5%8E%BF%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%94%BF%E5%BA%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="房地产开发限期动工制度" _mubu_text="%3Cspan%3E%E6%88%BF%E5%9C%B0%E4%BA%A7%E5%BC%80%E5%8F%91%E9%99%90%E6%9C%9F%E5%8A%A8%E5%B7%A5%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="超过合同约定日期满1年未动工开发的" _mubu_text="%3Cspan%3E%E8%B6%85%E8%BF%87%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%E6%97%A5%E6%9C%9F%E6%BB%A11%E5%B9%B4%E6%9C%AA%E5%8A%A8%E5%B7%A5%E5%BC%80%E5%8F%91%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="征收出让金20％以下的土地闲置费" _mubu_text="%3Cspan%3E%E5%BE%81%E6%94%B6%E5%87%BA%E8%AE%A9%E9%87%9120%EF%BC%85%E4%BB%A5%E4%B8%8B%E7%9A%84%E5%9C%9F%E5%9C%B0%E9%97%B2%E7%BD%AE%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="满2年未动工开发的" _mubu_text="%3Cspan%3E%E6%BB%A12%E5%B9%B4%E6%9C%AA%E5%8A%A8%E5%B7%A5%E5%BC%80%E5%8F%91%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="无偿收回土地使用权" _mubu_text="%3Cspan%3E%E6%97%A0%E5%81%BF%E6%94%B6%E5%9B%9E%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="不可抗力" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E6%8A%97%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="政府及其有关部门的行为" _mubu_text="%3Cspan%3E%E6%94%BF%E5%BA%9C%E5%8F%8A%E5%85%B6%E6%9C%89%E5%85%B3%E9%83%A8%E9%97%A8%E7%9A%84%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="动工开发必需的前期工作" _mubu_text="%3Cspan%3E%E5%8A%A8%E5%B7%A5%E5%BC%80%E5%8F%91%E5%BF%85%E9%9C%80%E7%9A%84%E5%89%8D%E6%9C%9F%E5%B7%A5%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="房地产交易" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%88%BF%E5%9C%B0%E4%BA%A7%E4%BA%A4%E6%98%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="房地产转让" _mubu_text="%3Cspan%3E%E6%88%BF%E5%9C%B0%E4%BA%A7%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="出让地" _mubu_text="%3Cspan%3E%E5%87%BA%E8%AE%A9%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="按合同约定已支付全部出让金，并取得土地使用权证书" _mubu_text="%3Cspan%3E%E6%8C%89%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%E5%B7%B2%E6%94%AF%E4%BB%98%E5%85%A8%E9%83%A8%E5%87%BA%E8%AE%A9%E9%87%91%EF%BC%8C%E5%B9%B6%E5%8F%96%E5%BE%97%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E8%AF%81%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="完成开发投资总额的 25%以上" _mubu_text="%3Cspan%3E%E5%AE%8C%E6%88%90%E5%BC%80%E5%8F%91%E6%8A%95%E8%B5%84%E6%80%BB%E9%A2%9D%E7%9A%84%2025%25%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="房屋已经建成的，还应当持有房屋所有权证书" _mubu_text="%3Cspan%3E%E6%88%BF%E5%B1%8B%E5%B7%B2%E7%BB%8F%E5%BB%BA%E6%88%90%E7%9A%84%EF%BC%8C%E8%BF%98%E5%BA%94%E5%BD%93%E6%8C%81%E6%9C%89%E6%88%BF%E5%B1%8B%E6%89%80%E6%9C%89%E6%9D%83%E8%AF%81%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="划拔地" _mubu_text="%3Cspan%3E%E5%88%92%E6%8B%94%E5%9C%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="需要办理土地出让手续的" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E5%8A%9E%E7%90%86%E5%9C%9F%E5%9C%B0%E5%87%BA%E8%AE%A9%E6%89%8B%E7%BB%AD%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="由受让方办理土地使用权出让手续" _mubu_text="%3Cspan%3E%E7%94%B1%E5%8F%97%E8%AE%A9%E6%96%B9%E5%8A%9E%E7%90%86%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%AE%A9%E6%89%8B%E7%BB%AD%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="并依照国家有关规定缴纳土地使用权出让金" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%BE%9D%E7%85%A7%E5%9B%BD%E5%AE%B6%E6%9C%89%E5%85%B3%E8%A7%84%E5%AE%9A%E7%BC%B4%E7%BA%B3%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%AE%A9%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="不办理的" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8A%9E%E7%90%86%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="将转让房地产所获收益中的土地收益上缴国家或者作其他处理" _mubu_text="%3Cspan%3E%E5%B0%86%E8%BD%AC%E8%AE%A9%E6%88%BF%E5%9C%B0%E4%BA%A7%E6%89%80%E8%8E%B7%E6%94%B6%E7%9B%8A%E4%B8%AD%E7%9A%84%E5%9C%9F%E5%9C%B0%E6%94%B6%E7%9B%8A%E4%B8%8A%E7%BC%B4%E5%9B%BD%E5%AE%B6%E6%88%96%E8%80%85%E4%BD%9C%E5%85%B6%E4%BB%96%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="土地用途变更" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E7%94%A8%E9%80%94%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="两个同意" _mubu_text="%3Cspan%3E%E4%B8%A4%E4%B8%AA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="原出让方" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%87%BA%E8%AE%A9%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="市、县人民政府城市规划行政主管部门" _mubu_text="%3Cspan%3E%E5%B8%82%E3%80%81%E5%8E%BF%E4%BA%BA%E6%B0%91%E6%94%BF%E5%BA%9C%E5%9F%8E%E5%B8%82%E8%A7%84%E5%88%92%E8%A1%8C%E6%94%BF%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="变更协议" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="签订变更协议" _mubu_text="%3Cspan%3E%E7%AD%BE%E8%AE%A2%E5%8F%98%E6%9B%B4%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或重新签订合同" _mubu_text="%3Cspan%3E%E6%88%96%E9%87%8D%E6%96%B0%E7%AD%BE%E8%AE%A2%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="商品房预售制度" _mubu_text="%3Cspan%3E%E5%95%86%E5%93%81%E6%88%BF%E9%A2%84%E5%94%AE%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="已交付全部土地使用权出让金" _mubu_text="%3Cspan%3E%E5%B7%B2%E4%BA%A4%E4%BB%98%E5%85%A8%E9%83%A8%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E5%87%BA%E8%AE%A9%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="取得土地使用权证书" _mubu_text="%3Cspan%3E%E5%8F%96%E5%BE%97%E5%9C%9F%E5%9C%B0%E4%BD%BF%E7%94%A8%E6%9D%83%E8%AF%81%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="持有建设工程规划许可证" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E5%BB%BA%E8%AE%BE%E5%B7%A5%E7%A8%8B%E8%A7%84%E5%88%92%E8%AE%B8%E5%8F%AF%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="投入开发建设资金达工程建设总投资的25％以上" _mubu_text="%3Cspan%3E%E6%8A%95%E5%85%A5%E5%BC%80%E5%8F%91%E5%BB%BA%E8%AE%BE%E8%B5%84%E9%87%91%E8%BE%BE%E5%B7%A5%E7%A8%8B%E5%BB%BA%E8%AE%BE%E6%80%BB%E6%8A%95%E8%B5%84%E7%9A%8425%EF%BC%85%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="并已经确定施工进度和竣工交付日期" _mubu_note="%3Cspan%3E%E5%B9%B6%E5%B7%B2%E7%BB%8F%E7%A1%AE%E5%AE%9A%E6%96%BD%E5%B7%A5%E8%BF%9B%E5%BA%A6%E5%92%8C%E7%AB%A3%E5%B7%A5%E4%BA%A4%E4%BB%98%E6%97%A5%E6%9C%9F%3C/span%3E"/>
                <outline text="取得预售许可证明" _mubu_text="%3Cspan%3E%E5%8F%96%E5%BE%97%E9%A2%84%E5%94%AE%E8%AE%B8%E5%8F%AF%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="合同备案" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="报县级以上政府房产管理部门和土地管理部门登记备案" _mubu_text="%3Cspan%3E%E6%8A%A5%E5%8E%BF%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%94%BF%E5%BA%9C%E6%88%BF%E4%BA%A7%E7%AE%A1%E7%90%86%E9%83%A8%E9%97%A8%E5%92%8C%E5%9C%9F%E5%9C%B0%E7%AE%A1%E7%90%86%E9%83%A8%E9%97%A8%E7%99%BB%E8%AE%B0%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未备案，合同并不因此而无效" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%A4%87%E6%A1%88%EF%BC%8C%E5%90%88%E5%90%8C%E5%B9%B6%E4%B8%8D%E5%9B%A0%E6%AD%A4%E8%80%8C%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="劳动和社会保障法" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%92%8C%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%9A%9C%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="劳动法" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="劳动合同法" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="使用范围" _mubu_text="%3Cspan%3E%E4%BD%BF%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="劳动者" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="禁止招用童工" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%8B%9B%E7%94%A8%E7%AB%A5%E5%B7%A5%3C/span%3E" _note="＜16" _mubu_note="%3Cspan%3E%EF%BC%9C16%3C/span%3E">
                <outline text="但文艺、体育、特种工艺除外" _mubu_text="%3Cspan%3E%E4%BD%86%E6%96%87%E8%89%BA%E3%80%81%E4%BD%93%E8%82%B2%E3%80%81%E7%89%B9%E7%A7%8D%E5%B7%A5%E8%89%BA%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不适用劳动合同法的主体" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E6%B3%95%E7%9A%84%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="用人单位" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="劳动关系" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="兼具人身关系和财产关系" _mubu_text="%3Cspan%3E%E5%85%BC%E5%85%B7%E4%BA%BA%E8%BA%AB%E5%85%B3%E7%B3%BB%E5%92%8C%E8%B4%A2%E4%BA%A7%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="兼具平等关系和从属关系" _mubu_text="%3Cspan%3E%E5%85%BC%E5%85%B7%E5%B9%B3%E7%AD%89%E5%85%B3%E7%B3%BB%E5%92%8C%E4%BB%8E%E5%B1%9E%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="劳动合同的订立" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%E8%AE%A2%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="劳动合同的种类" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="以完成一定工作任务为期限的劳动合同" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%AE%8C%E6%88%90%E4%B8%80%E5%AE%9A%E5%B7%A5%E4%BD%9C%E4%BB%BB%E5%8A%A1%E4%B8%BA%E6%9C%9F%E9%99%90%E7%9A%84%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有固定期限的劳动合同" _mubu_text="%3Cspan%3E%E6%9C%89%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E7%9A%84%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无固定期限的劳动合同" _mubu_text="%3Cspan%3E%E6%97%A0%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E7%9A%84%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="订立情形" _mubu_text="%3Cspan%3E%E8%AE%A2%E7%AB%8B%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="用人单位与劳动者协商一致" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8D%8F%E5%95%86%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法定情形" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="应当订立无固定期限" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E8%AE%A2%E7%AB%8B%E6%97%A0%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%3C/span%3E" _note="除劳动者提出订立固定期限外" _mubu_note="%3Cspan%3E%E9%99%A4%E5%8A%B3%E5%8A%A8%E8%80%85%E6%8F%90%E5%87%BA%E8%AE%A2%E7%AB%8B%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E5%A4%96%3C/span%3E"/>
                  <outline text="劳动者在该用人单位连续工作满10年的" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%9C%A8%E8%AF%A5%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%BF%9E%E7%BB%AD%E5%B7%A5%E4%BD%9C%E6%BB%A110%E5%B9%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="”双十规则“" _mubu_text="%3Cspan%3E%E2%80%9D%E5%8F%8C%E5%8D%81%E8%A7%84%E5%88%99%E2%80%9C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="”2+1规则“" _mubu_text="%3Cspan%3E%E2%80%9D2+1%E8%A7%84%E5%88%99%E2%80%9C%3C/span%3E" _note="" _mubu_note="">
                    <outline text="连续订立2次固定期限劳动合同" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD%E8%AE%A2%E7%AB%8B2%E6%AC%A1%E5%9B%BA%E5%AE%9A%E6%9C%9F%E9%99%90%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="且劳动者无过错，能胜任，续订劳动合同的" _mubu_text="%3Cspan%3E%E4%B8%94%E5%8A%B3%E5%8A%A8%E8%80%85%E6%97%A0%E8%BF%87%E9%94%99%EF%BC%8C%E8%83%BD%E8%83%9C%E4%BB%BB%EF%BC%8C%E7%BB%AD%E8%AE%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="视为订立" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E8%AE%A2%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
                  <outline text="用人单位自用工之日起满1年不与劳动者订立书面劳动合同的" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E8%87%AA%E7%94%A8%E5%B7%A5%E4%B9%8B%E6%97%A5%E8%B5%B7%E6%BB%A11%E5%B9%B4%E4%B8%8D%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E8%AE%A2%E7%AB%8B%E4%B9%A6%E9%9D%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="违规后果" _mubu_text="%3Cspan%3E%E8%BF%9D%E8%A7%84%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="自应订立无固期之日起向劳动者每月支付2倍工资" _mubu_text="%3Cspan%3E%E8%87%AA%E5%BA%94%E8%AE%A2%E7%AB%8B%E6%97%A0%E5%9B%BA%E6%9C%9F%E4%B9%8B%E6%97%A5%E8%B5%B7%E5%90%91%E5%8A%B3%E5%8A%A8%E8%80%85%E6%AF%8F%E6%9C%88%E6%94%AF%E4%BB%982%E5%80%8D%E5%B7%A5%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="未订立书面劳动合同的处理" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%AE%A2%E7%AB%8B%E4%B9%A6%E9%9D%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="用工之日，为劳动关系建立之日" _mubu_text="%3Cspan%3E%E7%94%A8%E5%B7%A5%E4%B9%8B%E6%97%A5%EF%BC%8C%E4%B8%BA%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%E5%BB%BA%E7%AB%8B%E4%B9%8B%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="建立劳动关系，应当订立书面劳动合同" _mubu_text="%3Cspan%3E%E5%BB%BA%E7%AB%8B%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%EF%BC%8C%E5%BA%94%E5%BD%93%E8%AE%A2%E7%AB%8B%E4%B9%A6%E9%9D%A2%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="单位原因" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="1个月内" _mubu_text="%3Cspan%3E1%E4%B8%AA%E6%9C%88%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="应自用工之日起1个月内订立" _mubu_text="%3Cspan%3E%E5%BA%94%E8%87%AA%E7%94%A8%E5%B7%A5%E4%B9%8B%E6%97%A5%E8%B5%B71%E4%B8%AA%E6%9C%88%E5%86%85%E8%AE%A2%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="2~12个月" _mubu_text="%3Cspan%3E2~12%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="应向劳动者每月支付2倍工资" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E5%8A%B3%E5%8A%A8%E8%80%85%E6%AF%8F%E6%9C%88%E6%94%AF%E4%BB%982%E5%80%8D%E5%B7%A5%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="满1年" _mubu_text="%3Cspan%3E%E6%BB%A11%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="视为用人单位与劳动者已订立无固定期" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%B7%B2%E8%AE%A2%E7%AB%8B%E6%97%A0%E5%9B%BA%E5%AE%9A%E6%9C%9F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22q8JhLDm7Rw%22%2C%22oh%22%3A171%2C%22ow%22%3A851%2C%22uri%22%3A%22document_image%2F7cbdf676-b963-4a12-aaf1-4ca872aeddb6-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
            </outline>
            <outline text="劳动者原因" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="1个月内" _mubu_text="%3Cspan%3E1%E4%B8%AA%E6%9C%88%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="经书面通知后" _mubu_text="%3Cspan%3E%E7%BB%8F%E4%B9%A6%E9%9D%A2%E9%80%9A%E7%9F%A5%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="用人单位有权终止劳动关系" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%9C%89%E6%9D%83%E7%BB%88%E6%AD%A2%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="应支付劳动报酬，无经济补偿" _mubu_text="%3Cspan%3E%E5%BA%94%E6%94%AF%E4%BB%98%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%EF%BC%8C%E6%97%A0%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="2~12个月" _mubu_text="%3Cspan%3E2~12%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="用人单位有权终止劳动关系" _mubu_text="%3Cspan%3E%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%9C%89%E6%9D%83%E7%BB%88%E6%AD%A2%E5%8A%B3%E5%8A%A8%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="应支付报酬和经济补偿" _mubu_text="%3Cspan%3E%E5%BA%94%E6%94%AF%E4%BB%98%E6%8A%A5%E9%85%AC%E5%92%8C%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22UlC1BnaL8o%22%2C%22oh%22%3A164%2C%22ow%22%3A857%2C%22uri%22%3A%22document_image%2F800b5ee2-6938-4ea1-8964-77132851968b-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
            </outline>
          </outline>
