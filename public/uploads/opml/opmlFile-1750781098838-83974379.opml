      <outline text="军人保险法" _mubu_text="%3Cspan%3E%E5%86%9B%E4%BA%BA%E4%BF%9D%E9%99%A9%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
    </outline>
    <outline text="环境资源法" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E8%B5%84%E6%BA%90%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="环境保护法" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E4%BF%9D%E6%8A%A4%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="环境影响评价法" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="有关开发利用规划，建设对环境有影响的项目" _mubu_text="%3Cspan%3E%E6%9C%89%E5%85%B3%E5%BC%80%E5%8F%91%E5%88%A9%E7%94%A8%E8%A7%84%E5%88%92%EF%BC%8C%E5%BB%BA%E8%AE%BE%E5%AF%B9%E7%8E%AF%E5%A2%83%E6%9C%89%E5%BD%B1%E5%93%8D%E7%9A%84%E9%A1%B9%E7%9B%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="未依法进行环境影响评价，不得组织实施/开工建设" _mubu_text="%3Cspan%3E%E6%9C%AA%E4%BE%9D%E6%B3%95%E8%BF%9B%E8%A1%8C%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%EF%BC%8C%E4%B8%8D%E5%BE%97%E7%BB%84%E7%BB%87%E5%AE%9E%E6%96%BD/%E5%BC%80%E5%B7%A5%E5%BB%BA%E8%AE%BE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="规划的环境影响评价" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%92%E7%9A%84%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="土地利用的有关规划" _mubu_text="%3Cspan%3E%E5%9C%9F%E5%9C%B0%E5%88%A9%E7%94%A8%E7%9A%84%E6%9C%89%E5%85%B3%E8%A7%84%E5%88%92%3C/span%3E" _note="" _mubu_note="">
              <outline text="在规划编制过程中组织进行环境影响评价" _mubu_text="%3Cspan%3E%E5%9C%A8%E8%A7%84%E5%88%92%E7%BC%96%E5%88%B6%E8%BF%87%E7%A8%8B%E4%B8%AD%E7%BB%84%E7%BB%87%E8%BF%9B%E8%A1%8C%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%3C/span%3E" _note="评价文件不需单独提交" _mubu_note="%3Cspan%3E%E8%AF%84%E4%BB%B7%E6%96%87%E4%BB%B6%E4%B8%8D%E9%9C%80%E5%8D%95%E7%8B%AC%E6%8F%90%E4%BA%A4%3C/span%3E"/>
            </outline>
            <outline text="专项规划" _mubu_text="%3Cspan%3E%E4%B8%93%E9%A1%B9%E8%A7%84%E5%88%92%3C/span%3E" _note="" _mubu_note="">
              <outline text="应在该专项规划草案上报审批前组织进行评价" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E8%AF%A5%E4%B8%93%E9%A1%B9%E8%A7%84%E5%88%92%E8%8D%89%E6%A1%88%E4%B8%8A%E6%8A%A5%E5%AE%A1%E6%89%B9%E5%89%8D%E7%BB%84%E7%BB%87%E8%BF%9B%E8%A1%8C%E8%AF%84%E4%BB%B7%3C/span%3E" _note="评价文件需单独上报" _mubu_note="%3Cspan%3E%E8%AF%84%E4%BB%B7%E6%96%87%E4%BB%B6%E9%9C%80%E5%8D%95%E7%8B%AC%E4%B8%8A%E6%8A%A5%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="建设项目的环境影响评价" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E7%9A%84%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="分类管理" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="重大环境影响" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="环境影响报告书" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E6%8A%A5%E5%91%8A%E4%B9%A6%3C/span%3E" _note="全面评价" _mubu_note="%3Cspan%3E%E5%85%A8%E9%9D%A2%E8%AF%84%E4%BB%B7%3C/span%3E"/>
              </outline>
              <outline text="轻度环境影响" _mubu_text="%3Cspan%3E%E8%BD%BB%E5%BA%A6%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="环境影响报告表" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E6%8A%A5%E5%91%8A%E8%A1%A8%3C/span%3E" _note="分析或专项评价" _mubu_note="%3Cspan%3E%E5%88%86%E6%9E%90%E6%88%96%E4%B8%93%E9%A1%B9%E8%AF%84%E4%BB%B7%3C/span%3E"/>
              </outline>
              <outline text="影响很小，无须环评" _mubu_text="%3Cspan%3E%E5%BD%B1%E5%93%8D%E5%BE%88%E5%B0%8F%EF%BC%8C%E6%97%A0%E9%A1%BB%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="环境影响登记表" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E7%99%BB%E8%AE%B0%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="评价机构" _mubu_text="%3Cspan%3E%E8%AF%84%E4%BB%B7%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="委托技术单位" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E6%8A%80%E6%9C%AF%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="自行环评" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="环境影响评价文件的审批" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%E6%96%87%E4%BB%B6%E7%9A%84%E5%AE%A1%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="国务院" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="特殊性质" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E6%80%A7%E8%B4%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="跨省" _mubu_text="%3Cspan%3E%E8%B7%A8%E7%9C%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="国务院(授权)审批的" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2(%E6%8E%88%E6%9D%83)%E5%AE%A1%E6%89%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="省级政府" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E6%94%BF%E5%BA%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="上述以外" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%BF%B0%E4%BB%A5%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="可能造成跨行政区域的不良环境影响" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%83%BD%E9%80%A0%E6%88%90%E8%B7%A8%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%9F%9F%E7%9A%84%E4%B8%8D%E8%89%AF%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="共同的上一级生态环境主管部门" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%9A%84%E4%B8%8A%E4%B8%80%E7%BA%A7%E7%94%9F%E6%80%81%E7%8E%AF%E5%A2%83%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="规划环评or建设项目环评" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%92%E7%8E%AF%E8%AF%84or%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="应避免评价相重复" _mubu_text="%3Cspan%3E%E5%BA%94%E9%81%BF%E5%85%8D%E8%AF%84%E4%BB%B7%E7%9B%B8%E9%87%8D%E5%A4%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="大项目小规划" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%A1%B9%E7%9B%AE%E5%B0%8F%E8%A7%84%E5%88%92%3C/span%3E" _note="" _mubu_note="">
                <outline text="建设项目要进行环评" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E8%A6%81%E8%BF%9B%E8%A1%8C%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="对规划不用环评" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%A7%84%E5%88%92%E4%B8%8D%E7%94%A8%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="大规划小项目" _mubu_text="%3Cspan%3E%E5%A4%A7%E8%A7%84%E5%88%92%E5%B0%8F%E9%A1%B9%E7%9B%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="规划要进行环评" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%92%E8%A6%81%E8%BF%9B%E8%A1%8C%E7%8E%AF%E8%AF%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="建设项目要环评，应当简化" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E8%A6%81%E7%8E%AF%E8%AF%84%EF%BC%8C%E5%BA%94%E5%BD%93%E7%AE%80%E5%8C%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="作为整体建设项目的规划" _mubu_text="%3Cspan%3E%E4%BD%9C%E4%B8%BA%E6%95%B4%E4%BD%93%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E7%9A%84%E8%A7%84%E5%88%92%3C/span%3E" _note="" _mubu_note="">
                <outline text="按照建设项目进行环境影响评价" _mubu_text="%3Cspan%3E%E6%8C%89%E7%85%A7%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E8%BF%9B%E8%A1%8C%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="重新报批／审核环评文件" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E6%8A%A5%E6%89%B9%EF%BC%8F%E5%AE%A1%E6%A0%B8%E7%8E%AF%E8%AF%84%E6%96%87%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="重新报批" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E6%8A%A5%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="批准后发生重大变动的建设单位应重新报批" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%E5%90%8E%E5%8F%91%E7%94%9F%E9%87%8D%E5%A4%A7%E5%8F%98%E5%8A%A8%E7%9A%84%E5%BB%BA%E8%AE%BE%E5%8D%95%E4%BD%8D%E5%BA%94%E9%87%8D%E6%96%B0%E6%8A%A5%E6%89%B9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="重新审核" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E5%AE%A1%E6%A0%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="自批准之日起超5年方决定该项目开工建设的应报原审部门重新审核" _mubu_text="%3Cspan%3E%E8%87%AA%E6%89%B9%E5%87%86%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%B6%855%E5%B9%B4%E6%96%B9%E5%86%B3%E5%AE%9A%E8%AF%A5%E9%A1%B9%E7%9B%AE%E5%BC%80%E5%B7%A5%E5%BB%BA%E8%AE%BE%E7%9A%84%E5%BA%94%E6%8A%A5%E5%8E%9F%E5%AE%A1%E9%83%A8%E9%97%A8%E9%87%8D%E6%96%B0%E5%AE%A1%E6%A0%B8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="原审部门应自收到文件之日起10日内，将审核意见书面通知建设单位" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E9%83%A8%E9%97%A8%E5%BA%94%E8%87%AA%E6%94%B6%E5%88%B0%E6%96%87%E4%BB%B6%E4%B9%8B%E6%97%A5%E8%B5%B710%E6%97%A5%E5%86%85%EF%BC%8C%E5%B0%86%E5%AE%A1%E6%A0%B8%E6%84%8F%E8%A7%81%E4%B9%A6%E9%9D%A2%E9%80%9A%E7%9F%A5%E5%BB%BA%E8%AE%BE%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="后评价制度" _mubu_text="%3Cspan%3E%E5%90%8E%E8%AF%84%E4%BB%B7%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="主动后评价" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E5%90%8E%E8%AF%84%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="项目建设、运行中产生不符合文件情形的，建设单位应组织环境影响的后评价，采取改进措施，并报原审部门和建设项目审批部门备案" _mubu_text="%3Cspan%3E%E9%A1%B9%E7%9B%AE%E5%BB%BA%E8%AE%BE%E3%80%81%E8%BF%90%E8%A1%8C%E4%B8%AD%E4%BA%A7%E7%94%9F%E4%B8%8D%E7%AC%A6%E5%90%88%E6%96%87%E4%BB%B6%E6%83%85%E5%BD%A2%E7%9A%84%EF%BC%8C%E5%BB%BA%E8%AE%BE%E5%8D%95%E4%BD%8D%E5%BA%94%E7%BB%84%E7%BB%87%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E7%9A%84%E5%90%8E%E8%AF%84%E4%BB%B7%EF%BC%8C%E9%87%87%E5%8F%96%E6%94%B9%E8%BF%9B%E6%8E%AA%E6%96%BD%EF%BC%8C%E5%B9%B6%E6%8A%A5%E5%8E%9F%E5%AE%A1%E9%83%A8%E9%97%A8%E5%92%8C%E5%BB%BA%E8%AE%BE%E9%A1%B9%E7%9B%AE%E5%AE%A1%E6%89%B9%E9%83%A8%E9%97%A8%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="责成后评价" _mubu_text="%3Cspan%3E%E8%B4%A3%E6%88%90%E5%90%8E%E8%AF%84%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="原环境影响评价文件审批部门也可以责成建设单位进行环境影响的后评价，采取改进措施" _mubu_text="%3Cspan%3E%E5%8E%9F%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E8%AF%84%E4%BB%B7%E6%96%87%E4%BB%B6%E5%AE%A1%E6%89%B9%E9%83%A8%E9%97%A8%E4%B9%9F%E5%8F%AF%E4%BB%A5%E8%B4%A3%E6%88%90%E5%BB%BA%E8%AE%BE%E5%8D%95%E4%BD%8D%E8%BF%9B%E8%A1%8C%E7%8E%AF%E5%A2%83%E5%BD%B1%E5%93%8D%E7%9A%84%E5%90%8E%E8%AF%84%E4%BB%B7%EF%BC%8C%E9%87%87%E5%8F%96%E6%94%B9%E8%BF%9B%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="环境保护法" _mubu_text="%3Cspan%3E%E7%8E%AF%E5%A2%83%E4%BF%9D%E6%8A%A4%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
