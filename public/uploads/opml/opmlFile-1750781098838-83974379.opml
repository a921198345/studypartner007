              <outline text="构成" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="成员为3名以上" _mubu_text="%3Cspan%3E%E6%88%90%E5%91%98%E4%B8%BA3%E5%90%8D%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
