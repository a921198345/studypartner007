              </outline>
              <outline text="地方标准" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="地方特色食品" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E7%89%B9%E8%89%B2%E9%A3%9F%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="没有食品安全国家标准的" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%E9%A3%9F%E5%93%81%E5%AE%89%E5%85%A8%E5%9B%BD%E5%AE%B6%E6%A0%87%E5%87%86%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="省级政府卫生行政部门可以制定，报国务院卫生行政部门备案" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E6%94%BF%E5%BA%9C%E5%8D%AB%E7%94%9F%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%E5%8F%AF%E4%BB%A5%E5%88%B6%E5%AE%9A%EF%BC%8C%E6%8A%A5%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8D%AB%E7%94%9F%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="国标制定后，该地方标准即行废止" _mubu_text="%3Cspan%3E%E5%9B%BD%E6%A0%87%E5%88%B6%E5%AE%9A%E5%90%8E%EF%BC%8C%E8%AF%A5%E5%9C%B0%E6%96%B9%E6%A0%87%E5%87%86%E5%8D%B3%E8%A1%8C%E5%BA%9F%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="企业标准" _mubu_text="%3Cspan%3E%E4%BC%81%E4%B8%9A%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="制定严于国家或地方标准的企业标准" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%9A%E4%B8%A5%E4%BA%8E%E5%9B%BD%E5%AE%B6%E6%88%96%E5%9C%B0%E6%96%B9%E6%A0%87%E5%87%86%E7%9A%84%E4%BC%81%E4%B8%9A%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在本企业适用" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9C%AC%E4%BC%81%E4%B8%9A%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="并报省级地方备案" _mubu_text="%3Cspan%3E%E5%B9%B6%E6%8A%A5%E7%9C%81%E7%BA%A7%E5%9C%B0%E6%96%B9%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="食品生产经营的控制" _mubu_text="%3Cspan%3E%E9%A3%9F%E5%93%81%E7%94%9F%E4%BA%A7%E7%BB%8F%E8%90%A5%E7%9A%84%E6%8E%A7%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="对集中交易市场的管理" _mubu_text="%3Cspan%3E%E5%AF%B9%E9%9B%86%E4%B8%AD%E4%BA%A4%E6%98%93%E5%B8%82%E5%9C%BA%E7%9A%84%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
