              <outline text="行使职权所必须费用由公司承担" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E8%81%8C%E6%9D%83%E6%89%80%E5%BF%85%E9%A1%BB%E8%B4%B9%E7%94%A8%E7%94%B1%E5%85%AC%E5%8F%B8%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="股东会会议" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%BC%9A%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="定期会议和临时会议" _mubu_text="%3Cspan%3E%E5%AE%9A%E6%9C%9F%E4%BC%9A%E8%AE%AE%E5%92%8C%E4%B8%B4%E6%97%B6%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
