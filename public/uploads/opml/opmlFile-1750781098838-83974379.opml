        <outline text="国家出资公司" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%87%BA%E8%B5%84%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
          <outline text="包括国有独资公司、国有资本控股公司" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E5%9B%BD%E6%9C%89%E7%8B%AC%E8%B5%84%E5%85%AC%E5%8F%B8%E3%80%81%E5%9B%BD%E6%9C%89%E8%B5%84%E6%9C%AC%E6%8E%A7%E8%82%A1%E5%85%AC%E5%8F%B8%3C/span%3E" _note="国有资本参股公司不属于" _mubu_note="%3Cspan%3E%E5%9B%BD%E6%9C%89%E8%B5%84%E6%9C%AC%E5%8F%82%E8%82%A1%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%B1%9E%E4%BA%8E%3C/span%3E"/>
          <outline text="不设股东会" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AE%BE%E8%82%A1%E4%B8%9C%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="由履行出资人职责的机构行使股东会职权" _mubu_text="%3Cspan%3E%E7%94%B1%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%BA%BA%E8%81%8C%E8%B4%A3%E7%9A%84%E6%9C%BA%E6%9E%84%E8%A1%8C%E4%BD%BF%E8%82%A1%E4%B8%9C%E4%BC%9A%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="应设立董事会" _mubu_text="%3Cspan%3E%E5%BA%94%E8%AE%BE%E7%AB%8B%E8%91%A3%E4%BA%8B%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="应过半数为外部董事，并应当由公司职工代表" _mubu_text="%3Cspan%3E%E5%BA%94%E8%BF%87%E5%8D%8A%E6%95%B0%E4%B8%BA%E5%A4%96%E9%83%A8%E8%91%A3%E4%BA%8B%EF%BC%8C%E5%B9%B6%E5%BA%94%E5%BD%93%E7%94%B1%E5%85%AC%E5%8F%B8%E8%81%8C%E5%B7%A5%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="(副)董事长由履行出资人职权的机构从董事会成员中指定" _mubu_text="%3Cspan%3E(%E5%89%AF)%E8%91%A3%E4%BA%8B%E9%95%BF%E7%94%B1%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%BA%BA%E8%81%8C%E6%9D%83%E7%9A%84%E6%9C%BA%E6%9E%84%E4%BB%8E%E8%91%A3%E4%BA%8B%E4%BC%9A%E6%88%90%E5%91%98%E4%B8%AD%E6%8C%87%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="董事会由履行出资人职权的机构委派" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E7%94%B1%E5%B1%A5%E8%A1%8C%E5%87%BA%E8%B5%84%E4%BA%BA%E8%81%8C%E6%9D%83%E7%9A%84%E6%9C%BA%E6%9E%84%E5%A7%94%E6%B4%BE%3C/span%3E" _note="" _mubu_note="">
              <outline text="职工代表由公司职工代表大会选举产生" _mubu_text="%3Cspan%3E%E8%81%8C%E5%B7%A5%E4%BB%A3%E8%A1%A8%E7%94%B1%E5%85%AC%E5%8F%B8%E8%81%8C%E5%B7%A5%E4%BB%A3%E8%A1%A8%E5%A4%A7%E4%BC%9A%E9%80%89%E4%B8%BE%E4%BA%A7%E7%94%9F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="董事会成员可以兼任经理" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E6%88%90%E5%91%98%E5%8F%AF%E4%BB%A5%E5%85%BC%E4%BB%BB%E7%BB%8F%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="审计委员会与监事会" _mubu_text="%3Cspan%3E%E5%AE%A1%E8%AE%A1%E5%A7%94%E5%91%98%E4%BC%9A%E4%B8%8E%E7%9B%91%E4%BA%8B%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="设立审委会的可不设监事会或监事" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E5%AE%A1%E5%A7%94%E4%BC%9A%E7%9A%84%E5%8F%AF%E4%B8%8D%E8%AE%BE%E7%9B%91%E4%BA%8B%E4%BC%9A%E6%88%96%E7%9B%91%E4%BA%8B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="合伙企业法" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="普通合伙企业" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="设立" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="有符合要求的合伙人" _mubu_text="%3Cspan%3E%E6%9C%89%E7%AC%A6%E5%90%88%E8%A6%81%E6%B1%82%E7%9A%84%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="自然人、法人和其他组织均可成为合伙人" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E4%BA%BA%E3%80%81%E6%B3%95%E4%BA%BA%E5%92%8C%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%E5%9D%87%E5%8F%AF%E6%88%90%E4%B8%BA%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="≥2" _mubu_text="%3Cspan%3E%E2%89%A52%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="合伙人为自然人" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%BA%E8%87%AA%E7%84%B6%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="应具有完民行能" _mubu_text="%3Cspan%3E%E5%BA%94%E5%85%B7%E6%9C%89%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法律、行政法规禁止从事营利性活动的人不得" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E3%80%81%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E7%A6%81%E6%AD%A2%E4%BB%8E%E4%BA%8B%E8%90%A5%E5%88%A9%E6%80%A7%E6%B4%BB%E5%8A%A8%E7%9A%84%E4%BA%BA%E4%B8%8D%E5%BE%97%3C/span%3E" _note="国家公务员、法官、检察官" _mubu_note="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%85%AC%E5%8A%A1%E5%91%98%E3%80%81%E6%B3%95%E5%AE%98%E3%80%81%E6%A3%80%E5%AF%9F%E5%AE%98%3C/span%3E"/>
              </outline>
              <outline text="合伙人为法人、其他组织" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%BA%E6%B3%95%E4%BA%BA%E3%80%81%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得成为：“两国有、两非营利、一上市”" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%88%90%E4%B8%BA%EF%BC%9A%E2%80%9C%E4%B8%A4%E5%9B%BD%E6%9C%89%E3%80%81%E4%B8%A4%E9%9D%9E%E8%90%A5%E5%88%A9%E3%80%81%E4%B8%80%E4%B8%8A%E5%B8%82%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="有书面合伙协议" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B9%A6%E9%9D%A2%E5%90%88%E4%BC%99%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="全体合伙人签名、盖章后生效" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E7%AD%BE%E5%90%8D%E3%80%81%E7%9B%96%E7%AB%A0%E5%90%8E%E7%94%9F%E6%95%88%3C/span%3E" _note="登记非生效要件" _mubu_note="%3Cspan%3E%E7%99%BB%E8%AE%B0%E9%9D%9E%E7%94%9F%E6%95%88%E8%A6%81%E4%BB%B6%3C/span%3E"/>
              <outline text="修改或补充应经全体合伙人一致同意" _mubu_text="%3Cspan%3E%E4%BF%AE%E6%94%B9%E6%88%96%E8%A1%A5%E5%85%85%E5%BA%94%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="有合伙人缴付的出资" _mubu_text="%3Cspan%3E%E6%9C%89%E5%90%88%E4%BC%99%E4%BA%BA%E7%BC%B4%E4%BB%98%E7%9A%84%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="无最低金额限制" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%80%E4%BD%8E%E9%87%91%E9%A2%9D%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无形式要求" _mubu_text="%3Cspan%3E%E6%97%A0%E5%BD%A2%E5%BC%8F%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="认缴/实缴" _mubu_text="%3Cspan%3E%E8%AE%A4%E7%BC%B4/%E5%AE%9E%E7%BC%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="非货币出资" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%B4%A7%E5%B8%81%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="评估作价由当事人协商" _mubu_text="%3Cspan%3E%E8%AF%84%E4%BC%B0%E4%BD%9C%E4%BB%B7%E7%94%B1%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8D%8F%E5%95%86%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="可劳务出资" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%8A%B3%E5%8A%A1%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="无期限要求" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%9F%E9%99%90%E8%A6%81%E6%B1%82%3C/span%3E" _note="可分期缴付" _mubu_note="%3Cspan%3E%E5%8F%AF%E5%88%86%E6%9C%9F%E7%BC%B4%E4%BB%98%3C/span%3E"/>
            </outline>
            <outline text="有名称和经营场所" _mubu_text="%3Cspan%3E%E6%9C%89%E5%90%8D%E7%A7%B0%E5%92%8C%E7%BB%8F%E8%90%A5%E5%9C%BA%E6%89%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="名称应标明“普通合伙”" _mubu_text="%3Cspan%3E%E5%90%8D%E7%A7%B0%E5%BA%94%E6%A0%87%E6%98%8E%E2%80%9C%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="财产" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="财产" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="合伙企业财产属于全体合伙人共有，且属于共同共有" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E8%B4%A2%E4%BA%A7%E5%B1%9E%E4%BA%8E%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E5%85%B1%E6%9C%89%EF%BC%8C%E4%B8%94%E5%B1%9E%E4%BA%8E%E5%85%B1%E5%90%8C%E5%85%B1%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="清算前，合伙人不得请求分割" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E5%89%8D%EF%BC%8C%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%8D%E5%BE%97%E8%AF%B7%E6%B1%82%E5%88%86%E5%89%B2%3C/span%3E" _note="" _mubu_note="">
                <outline text="包括撤回原始投入" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E6%92%A4%E5%9B%9E%E5%8E%9F%E5%A7%8B%E6%8A%95%E5%85%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="也包括由原始投资收益转化的增量资产" _mubu_text="%3Cspan%3E%E4%B9%9F%E5%8C%85%E6%8B%AC%E7%94%B1%E5%8E%9F%E5%A7%8B%E6%8A%95%E8%B5%84%E6%94%B6%E7%9B%8A%E8%BD%AC%E5%8C%96%E7%9A%84%E5%A2%9E%E9%87%8F%E8%B5%84%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="清算前私自转移或者处分" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E5%89%8D%E7%A7%81%E8%87%AA%E8%BD%AC%E7%A7%BB%E6%88%96%E8%80%85%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得以此对抗善意第三人" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E6%AD%A4%E5%AF%B9%E6%8A%97%E5%96%84%E6%84%8F%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="合伙企业的税收" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E7%A8%8E%E6%94%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="由合伙人分别缴纳所得税" _mubu_text="%3Cspan%3E%E7%94%B1%E5%90%88%E4%BC%99%E4%BA%BA%E5%88%86%E5%88%AB%E7%BC%B4%E7%BA%B3%E6%89%80%E5%BE%97%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法人合伙人依法缴纳企业所得税" _mubu_text="%3Cspan%3E%E6%B3%95%E4%BA%BA%E5%90%88%E4%BC%99%E4%BA%BA%E4%BE%9D%E6%B3%95%E7%BC%B4%E7%BA%B3%E4%BC%81%E4%B8%9A%E6%89%80%E5%BE%97%E7%A8%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="自然人合伙人依法缴纳个人所得税" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E4%BA%BA%E5%90%88%E4%BC%99%E4%BA%BA%E4%BE%9D%E6%B3%95%E7%BC%B4%E7%BA%B3%E4%B8%AA%E4%BA%BA%E6%89%80%E5%BE%97%E7%A8%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="合伙企业不缴企业所得税" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%B8%8D%E7%BC%B4%E4%BC%81%E4%B8%9A%E6%89%80%E5%BE%97%E7%A8%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="财产份额" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="内部转让" _mubu_text="%3Cspan%3E%E5%86%85%E9%83%A8%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
                <outline text="应当通知其他合伙人" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%80%9A%E7%9F%A5%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="其他合伙人无优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="对外转让" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
                <outline text="须经其他合伙人一致同意" _mubu_text="%3Cspan%3E%E9%A1%BB%E7%BB%8F%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="其他合伙人有优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="份额的出质" _mubu_text="%3Cspan%3E%E4%BB%BD%E9%A2%9D%E7%9A%84%E5%87%BA%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="须经其他合伙人一致同意" _mubu_text="%3Cspan%3E%E9%A1%BB%E7%BB%8F%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="为法定，不能通过协议排除" _mubu_note="%3Cspan%3E%E4%B8%BA%E6%B3%95%E5%AE%9A%EF%BC%8C%E4%B8%8D%E8%83%BD%E9%80%9A%E8%BF%87%E5%8D%8F%E8%AE%AE%E6%8E%92%E9%99%A4%3C/span%3E"/>
                <outline text="未经一致同意的无效" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%E7%9A%84%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                  <outline text="即便质权人善意依然无效" _mubu_text="%3Cspan%3E%E5%8D%B3%E4%BE%BF%E8%B4%A8%E6%9D%83%E4%BA%BA%E5%96%84%E6%84%8F%E4%BE%9D%E7%84%B6%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="行为人依法承担赔偿" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E4%BA%BA%E4%BE%9D%E6%B3%95%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="事务" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="决议" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="一般事项" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="1人1票，过半通过" _mubu_text="%3Cspan%3E1%E4%BA%BA1%E7%A5%A8%EF%BC%8C%E8%BF%87%E5%8D%8A%E9%80%9A%E8%BF%87%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
              <outline text="重大事项" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="经全体一致同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%85%A8%E4%BD%93%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="事项(图)" _mubu_text="%3Cspan%3E%E4%BA%8B%E9%A1%B9(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
                  <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22TuOaLvbpIE%22%2C%22oh%22%3A481%2C%22ow%22%3A1356%2C%22uri%22%3A%22document_image%2F69b6b5d4-88ce-47b1-afc2-f580ae69404c-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
                <outline text="对重大事项擅自处理" _mubu_text="%3Cspan%3E%E5%AF%B9%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%E6%93%85%E8%87%AA%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不得对抗善三" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E4%B8%89%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="造成损失的，承担赔偿责任" _mubu_text="%3Cspan%3E%E9%80%A0%E6%88%90%E6%8D%9F%E5%A4%B1%E7%9A%84%EF%BC%8C%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="执行" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="共同执行" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="合伙人对执行合伙事务享有同等权利" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E5%AF%B9%E6%89%A7%E8%A1%8C%E5%90%88%E4%BC%99%E4%BA%8B%E5%8A%A1%E4%BA%AB%E6%9C%89%E5%90%8C%E7%AD%89%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="部分合伙人执行" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="委托一个或者数个合伙人对外代表合伙企业，执行合伙事务" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%B8%80%E4%B8%AA%E6%88%96%E8%80%85%E6%95%B0%E4%B8%AA%E5%90%88%E4%BC%99%E4%BA%BA%E5%AF%B9%E5%A4%96%E4%BB%A3%E8%A1%A8%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%EF%BC%8C%E6%89%A7%E8%A1%8C%E5%90%88%E4%BC%99%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="非自然人合伙人执行合伙事务的，由其委派的代表执行" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%87%AA%E7%84%B6%E4%BA%BA%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%A7%E8%A1%8C%E5%90%88%E4%BC%99%E4%BA%8B%E5%8A%A1%E7%9A%84%EF%BC%8C%E7%94%B1%E5%85%B6%E5%A7%94%E6%B4%BE%E7%9A%84%E4%BB%A3%E8%A1%A8%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="其他合伙人不再执行合伙事务" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%8D%E5%86%8D%E6%89%A7%E8%A1%8C%E5%90%88%E4%BC%99%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="其他合伙人有权监督执行事务" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E6%9D%83%E7%9B%91%E7%9D%A3%E6%89%A7%E8%A1%8C%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="异议权" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="提出异议时，应当暂停该项事务的执行" _mubu_text="%3Cspan%3E%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%E6%97%B6%EF%BC%8C%E5%BA%94%E5%BD%93%E6%9A%82%E5%81%9C%E8%AF%A5%E9%A1%B9%E4%BA%8B%E5%8A%A1%E7%9A%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="异议权or监督权(图)" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E6%9D%83or%E7%9B%91%E7%9D%A3%E6%9D%83(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
                    <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22W7da19K1eF%22%2C%22oh%22%3A141%2C%22ow%22%3A1078%2C%22uri%22%3A%22document_image%2F10e28d07-9c97-4d73-832e-6dab723aa77e-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
                  </outline>
                </outline>
                <outline text="内部约定不得对抗善意第三人" _mubu_text="%3Cspan%3E%E5%86%85%E9%83%A8%E7%BA%A6%E5%AE%9A%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E6%84%8F%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可外聘经营管理人员" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%A4%96%E8%81%98%E7%BB%8F%E8%90%A5%E7%AE%A1%E7%90%86%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
                  <outline text="经全体合伙人一致同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
                  <outline text="在合伙企业授权范围内履行" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E6%8E%88%E6%9D%83%E8%8C%83%E5%9B%B4%E5%86%85%E5%B1%A5%E8%A1%8C%3C/span%3E" _note="无专门授权责无权" _mubu_note="%3Cspan%3E%E6%97%A0%E4%B8%93%E9%97%A8%E6%8E%88%E6%9D%83%E8%B4%A3%E6%97%A0%E6%9D%83%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="合伙人" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="权利" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="知情权：有权查阅合伙企业会计账簿" _mubu_text="%3Cspan%3E%E7%9F%A5%E6%83%85%E6%9D%83%EF%BC%9A%E6%9C%89%E6%9D%83%E6%9F%A5%E9%98%85%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%BC%9A%E8%AE%A1%E8%B4%A6%E7%B0%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="竞业禁止" _mubu_text="%3Cspan%3E%E7%AB%9E%E4%B8%9A%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="绝对禁止" _mubu_text="%3Cspan%3E%E7%BB%9D%E5%AF%B9%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="自我交易" _mubu_text="%3Cspan%3E%E8%87%AA%E6%88%91%E4%BA%A4%E6%98%93%3C/span%3E" _note="" _mubu_note="">
                <outline text="相对禁止" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E7%A6%81%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="未经全体合伙人同意" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="不得从事伤害本合伙企业利益的活动" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%8E%E4%BA%8B%E4%BC%A4%E5%AE%B3%E6%9C%AC%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%88%A9%E7%9B%8A%E7%9A%84%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="退还+赔偿" _mubu_note="%3Cspan%3E%E9%80%80%E8%BF%98+%E8%B5%94%E5%81%BF%3C/span%3E"/>
          </outline>
          <outline text="利润分配与亏损分担" _mubu_text="%3Cspan%3E%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E4%B8%8E%E4%BA%8F%E6%8D%9F%E5%88%86%E6%8B%85%3C/span%3E" _note="" _mubu_note="">
            <outline text="顺序" _mubu_text="%3Cspan%3E%E9%A1%BA%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="约定优先→协商确定→按实缴出资比例→平均" _mubu_text="%3Cspan%3E%E7%BA%A6%E5%AE%9A%E4%BC%98%E5%85%88%E2%86%92%E5%8D%8F%E5%95%86%E7%A1%AE%E5%AE%9A%E2%86%92%E6%8C%89%E5%AE%9E%E7%BC%B4%E5%87%BA%E8%B5%84%E6%AF%94%E4%BE%8B%E2%86%92%E5%B9%B3%E5%9D%87%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="禁止规定" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得约定将全部利润分配给部分或由部分承担全部亏损" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E7%BA%A6%E5%AE%9A%E5%B0%86%E5%85%A8%E9%83%A8%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E7%BB%99%E9%83%A8%E5%88%86%E6%88%96%E7%94%B1%E9%83%A8%E5%88%86%E6%89%BF%E6%8B%85%E5%85%A8%E9%83%A8%E4%BA%8F%E6%8D%9F%3C/span%3E" _note="如有，该约定无效" _mubu_note="%3Cspan%3E%E5%A6%82%E6%9C%89%EF%BC%8C%E8%AF%A5%E7%BA%A6%E5%AE%9A%E6%97%A0%E6%95%88%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="与第三人的关系" _mubu_text="%3Cspan%3E%E4%B8%8E%E7%AC%AC%E4%B8%89%E4%BA%BA%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="与债务人的关系" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%80%BA%E5%8A%A1%E4%BA%BA%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="合伙企业先以其全部财产承担清偿责任" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%85%88%E4%BB%A5%E5%85%B6%E5%85%A8%E9%83%A8%E8%B4%A2%E4%BA%A7%E6%89%BF%E6%8B%85%E6%B8%85%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="普通合伙人承担补充责任" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%BF%E6%8B%85%E8%A1%A5%E5%85%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="无限连带" _mubu_note="%3Cspan%3E%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%3C/span%3E"/>
              <outline text="内部追偿" _mubu_text="%3Cspan%3E%E5%86%85%E9%83%A8%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合伙人个人债务" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="禁止抵销与禁止代位" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%8A%B5%E9%94%80%E4%B8%8E%E7%A6%81%E6%AD%A2%E4%BB%A3%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="合伙人发生与合伙企业无关的债务" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E5%8F%91%E7%94%9F%E4%B8%8E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E6%97%A0%E5%85%B3%E7%9A%84%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得以其债权抵销其对合伙企业的债务" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E5%85%B6%E5%80%BA%E6%9D%83%E6%8A%B5%E9%94%80%E5%85%B6%E5%AF%B9%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得代位行使合伙人在合伙企业中的权利" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A3%E4%BD%8D%E8%A1%8C%E4%BD%BF%E5%90%88%E4%BC%99%E4%BA%BA%E5%9C%A8%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%B8%AD%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="清偿规则" _mubu_text="%3Cspan%3E%E6%B8%85%E5%81%BF%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="以个人财产清偿" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%B8%AA%E4%BA%BA%E8%B4%A2%E4%BA%A7%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="收益清偿" _mubu_text="%3Cspan%3E%E6%94%B6%E7%9B%8A%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
                  <outline text="分取的收益用于清偿" _mubu_text="%3Cspan%3E%E5%88%86%E5%8F%96%E7%9A%84%E6%94%B6%E7%9B%8A%E7%94%A8%E4%BA%8E%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="已分配或即将分配的收益" _mubu_text="%3Cspan%3E%E5%B7%B2%E5%88%86%E9%85%8D%E6%88%96%E5%8D%B3%E5%B0%86%E5%88%86%E9%85%8D%E7%9A%84%E6%94%B6%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
                    <outline text="仅限于当期收益" _mubu_text="%3Cspan%3E%E4%BB%85%E9%99%90%E4%BA%8E%E5%BD%93%E6%9C%9F%E6%94%B6%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="强制执行财产份额" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="该合伙人在合伙企业中的财产份额" _mubu_text="%3Cspan%3E%E8%AF%A5%E5%90%88%E4%BC%99%E4%BA%BA%E5%9C%A8%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%B8%AD%E7%9A%84%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法院应通知全体合伙人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E9%80%9A%E7%9F%A5%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他合伙人有优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未购买，又不同意转让" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%B4%AD%E4%B9%B0%EF%BC%8C%E5%8F%88%E4%B8%8D%E5%90%8C%E6%84%8F%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
                    <outline text="为该合伙人办理退伙结算" _mubu_text="%3Cspan%3E%E4%B8%BA%E8%AF%A5%E5%90%88%E4%BC%99%E4%BA%BA%E5%8A%9E%E7%90%86%E9%80%80%E4%BC%99%E7%BB%93%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
            <outline text="双重优先原则" _mubu_text="%3Cspan%3E%E5%8F%8C%E9%87%8D%E4%BC%98%E5%85%88%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="同时存在合伙企业债务和合伙人个人债务" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%AD%98%E5%9C%A8%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E5%92%8C%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="合伙财产优先用于清偿合伙债务，个人财产优先用于清偿个人债务" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E8%B4%A2%E4%BA%A7%E4%BC%98%E5%85%88%E7%94%A8%E4%BA%8E%E6%B8%85%E5%81%BF%E5%90%88%E4%BC%99%E5%80%BA%E5%8A%A1%EF%BC%8C%E4%B8%AA%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%BC%98%E5%85%88%E7%94%A8%E4%BA%8E%E6%B8%85%E5%81%BF%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="入伙与退伙" _mubu_text="%3Cspan%3E%E5%85%A5%E4%BC%99%E4%B8%8E%E9%80%80%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="入伙" _mubu_text="%3Cspan%3E%E5%85%A5%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="经全体合伙人一致同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="依法订立书面入伙协议" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E8%AE%A2%E7%AB%8B%E4%B9%A6%E9%9D%A2%E5%85%A5%E4%BC%99%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="原合伙人应向其如实告知企业的经营状况和财务状况" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%90%88%E4%BC%99%E4%BA%BA%E5%BA%94%E5%90%91%E5%85%B6%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E4%BC%81%E4%B8%9A%E7%9A%84%E7%BB%8F%E8%90%A5%E7%8A%B6%E5%86%B5%E5%92%8C%E8%B4%A2%E5%8A%A1%E7%8A%B6%E5%86%B5%3C/span%3E" _note="新合伙人对入伙前的合伙企业债务要与原合伙人承担连带责任" _mubu_note="%3Cspan%3E%E6%96%B0%E5%90%88%E4%BC%99%E4%BA%BA%E5%AF%B9%E5%85%A5%E4%BC%99%E5%89%8D%E7%9A%84%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E8%A6%81%E4%B8%8E%E5%8E%9F%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E">
                <outline text="未履行如实告知义务" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%B1%A5%E8%A1%8C%E5%A6%82%E5%AE%9E%E5%91%8A%E7%9F%A5%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不影响新入伙合伙人的对外责任" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%96%B0%E5%85%A5%E4%BC%99%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E5%AF%B9%E5%A4%96%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="仅在合伙人之间产生赔偿责任" _mubu_text="%3Cspan%3E%E4%BB%85%E5%9C%A8%E5%90%88%E4%BC%99%E4%BA%BA%E4%B9%8B%E9%97%B4%E4%BA%A7%E7%94%9F%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="对入伙人债务承担另有约定" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%A5%E4%BC%99%E4%BA%BA%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E5%8F%A6%E6%9C%89%E7%BA%A6%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="该约定仅有内部效力，不得对抗善三" _mubu_text="%3Cspan%3E%E8%AF%A5%E7%BA%A6%E5%AE%9A%E4%BB%85%E6%9C%89%E5%86%85%E9%83%A8%E6%95%88%E5%8A%9B%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E5%96%84%E4%B8%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="退伙" _mubu_text="%3Cspan%3E%E9%80%80%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="自愿退伙" _mubu_text="%3Cspan%3E%E8%87%AA%E6%84%BF%E9%80%80%E4%BC%99%3C/span%3E" _note="声明退伙" _mubu_note="%3Cspan%3E%E5%A3%B0%E6%98%8E%E9%80%80%E4%BC%99%3C/span%3E">
                <outline text="约定合伙期限" _mubu_text="%3Cspan%3E%E7%BA%A6%E5%AE%9A%E5%90%88%E4%BC%99%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未约定合伙期限" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BA%A6%E5%AE%9A%E5%90%88%E4%BC%99%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="提前30日通知" _mubu_text="%3Cspan%3E%E6%8F%90%E5%89%8D30%E6%97%A5%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="法定退伙" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E9%80%80%E4%BC%99%3C/span%3E" _note="当然退伙" _mubu_note="%3Cspan%3E%E5%BD%93%E7%84%B6%E9%80%80%E4%BC%99%3C/span%3E">
                <outline text="主体资格丧失" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E8%B5%84%E6%A0%BC%E4%B8%A7%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="丧失完全行能" _mubu_text="%3Cspan%3E%E4%B8%A7%E5%A4%B1%E5%AE%8C%E5%85%A8%E8%A1%8C%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他合伙人一致同意，可转为有限合伙人、有限合伙企业" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%EF%BC%8C%E5%8F%AF%E8%BD%AC%E4%B8%BA%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E3%80%81%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未能一致同意，退伙" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%83%BD%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%EF%BC%8C%E9%80%80%E4%BC%99%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="事由实际发生之日为退伙之日" _mubu_text="%3Cspan%3E%E4%BA%8B%E7%94%B1%E5%AE%9E%E9%99%85%E5%8F%91%E7%94%9F%E4%B9%8B%E6%97%A5%E4%B8%BA%E9%80%80%E4%BC%99%E4%B9%8B%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="除名退伙" _mubu_text="%3Cspan%3E%E9%99%A4%E5%90%8D%E9%80%80%E4%BC%99%3C/span%3E" _note="开除退伙" _mubu_note="%3Cspan%3E%E5%BC%80%E9%99%A4%E9%80%80%E4%BC%99%3C/span%3E">
                <outline text="经其他合伙人一致同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="除名决议应当书面通知被除名人" _mubu_text="%3Cspan%3E%E9%99%A4%E5%90%8D%E5%86%B3%E8%AE%AE%E5%BA%94%E5%BD%93%E4%B9%A6%E9%9D%A2%E9%80%9A%E7%9F%A5%E8%A2%AB%E9%99%A4%E5%90%8D%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="接到通知之日生效，退伙" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E4%B9%8B%E6%97%A5%E7%94%9F%E6%95%88%EF%BC%8C%E9%80%80%E4%BC%99%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="有异议的，可自接到通知起30日内，向法院起诉" _mubu_text="%3Cspan%3E%E6%9C%89%E5%BC%82%E8%AE%AE%E7%9A%84%EF%BC%8C%E5%8F%AF%E8%87%AA%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E8%B5%B730%E6%97%A5%E5%86%85%EF%BC%8C%E5%90%91%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="合伙人死亡时的继承" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E6%AD%BB%E4%BA%A1%E6%97%B6%E7%9A%84%E7%BB%A7%E6%89%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="取得" _mubu_text="%3Cspan%3E%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note="">
                  <outline text="按合伙协议的约定或经全体合伙人一致同意" _mubu_text="%3Cspan%3E%E6%8C%89%E5%90%88%E4%BC%99%E5%8D%8F%E8%AE%AE%E7%9A%84%E7%BA%A6%E5%AE%9A%E6%88%96%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="继承开始之日取得合伙人资格" _mubu_text="%3Cspan%3E%E7%BB%A7%E6%89%BF%E5%BC%80%E5%A7%8B%E4%B9%8B%E6%97%A5%E5%8F%96%E5%BE%97%E5%90%88%E4%BC%99%E4%BA%BA%E8%B5%84%E6%A0%BC%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="不能继承的" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E7%BB%A7%E6%89%BF%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="应向继承人退还被继承合伙人的财产份额" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E7%BB%A7%E6%89%BF%E4%BA%BA%E9%80%80%E8%BF%98%E8%A2%AB%E7%BB%A7%E6%89%BF%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="继承人非完民行能人" _mubu_text="%3Cspan%3E%E7%BB%A7%E6%89%BF%E4%BA%BA%E9%9D%9E%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他合伙人一致同意，可转为有限合伙人、有限合伙企业" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%EF%BC%8C%E5%8F%AF%E8%BD%AC%E4%B8%BA%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E3%80%81%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未能一致同意，应向继承人退还被继承合伙人的财产份额" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%83%BD%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%EF%BC%8C%E5%BA%94%E5%90%91%E7%BB%A7%E6%89%BF%E4%BA%BA%E9%80%80%E8%BF%98%E8%A2%AB%E7%BB%A7%E6%89%BF%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="结算" _mubu_text="%3Cspan%3E%E7%BB%93%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
                <outline text="有未了结事务，了结后结算" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9C%AA%E4%BA%86%E7%BB%93%E4%BA%8B%E5%8A%A1%EF%BC%8C%E4%BA%86%E7%BB%93%E5%90%8E%E7%BB%93%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="退还办法" _mubu_text="%3Cspan%3E%E9%80%80%E8%BF%98%E5%8A%9E%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
                  <outline text="协议约定/全体决定" _mubu_text="%3Cspan%3E%E5%8D%8F%E8%AE%AE%E7%BA%A6%E5%AE%9A/%E5%85%A8%E4%BD%93%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="退还货币/实物" _mubu_text="%3Cspan%3E%E9%80%80%E8%BF%98%E8%B4%A7%E5%B8%81/%E5%AE%9E%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="分担亏损" _mubu_text="%3Cspan%3E%E5%88%86%E6%8B%85%E4%BA%8F%E6%8D%9F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="退伙时合伙企业的财产少于合伙企业债务" _mubu_text="%3Cspan%3E%E9%80%80%E4%BC%99%E6%97%B6%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E8%B4%A2%E4%BA%A7%E5%B0%91%E4%BA%8E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="退伙人应分担亏损" _mubu_text="%3Cspan%3E%E9%80%80%E4%BC%99%E4%BA%BA%E5%BA%94%E5%88%86%E6%8B%85%E4%BA%8F%E6%8D%9F%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="承担无限连带责任" _mubu_text="%3Cspan%3E%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                  <outline text="基于其退伙前的原因发生的合伙企业债务" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E5%85%B6%E9%80%80%E4%BC%99%E5%89%8D%E7%9A%84%E5%8E%9F%E5%9B%A0%E5%8F%91%E7%94%9F%E7%9A%84%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="仍应与其他合伙人一起承担无限连带责任" _mubu_text="%3Cspan%3E%E4%BB%8D%E5%BA%94%E4%B8%8E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%B5%B7%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="特殊的普通合伙企业" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E7%9A%84%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="以专业知识和专门技能为客户提供有偿服务的专业服务机构" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%B8%93%E4%B8%9A%E7%9F%A5%E8%AF%86%E5%92%8C%E4%B8%93%E9%97%A8%E6%8A%80%E8%83%BD%E4%B8%BA%E5%AE%A2%E6%88%B7%E6%8F%90%E4%BE%9B%E6%9C%89%E5%81%BF%E6%9C%8D%E5%8A%A1%E7%9A%84%E4%B8%93%E4%B8%9A%E6%9C%8D%E5%8A%A1%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="债务承担的特殊规则" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E7%9A%84%E7%89%B9%E6%AE%8A%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="合伙人在执业中因故意或重大过失造成合伙企业债务" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E5%9C%A8%E6%89%A7%E4%B8%9A%E4%B8%AD%E5%9B%A0%E6%95%85%E6%84%8F%E6%88%96%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%E9%80%A0%E6%88%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="先以合伙企业财产承担" _mubu_text="%3Cspan%3E%E5%85%88%E4%BB%A5%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E8%B4%A2%E4%BA%A7%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="过错合伙人无限连带责任，其他合伙人有限责任" _mubu_text="%3Cspan%3E%E8%BF%87%E9%94%99%E5%90%88%E4%BC%99%E4%BA%BA%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%EF%BC%8C%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="过错合伙人赔偿企业损失" _mubu_text="%3Cspan%3E%E8%BF%87%E9%94%99%E5%90%88%E4%BC%99%E4%BA%BA%E8%B5%94%E5%81%BF%E4%BC%81%E4%B8%9A%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="其他债务" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="与普通合伙企业债务承担规则完全相同" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%A7%84%E5%88%99%E5%AE%8C%E5%85%A8%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="名称中应标明“特殊普通合伙”字样" _mubu_text="%3Cspan%3E%E5%90%8D%E7%A7%B0%E4%B8%AD%E5%BA%94%E6%A0%87%E6%98%8E%E2%80%9C%E7%89%B9%E6%AE%8A%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E2%80%9D%E5%AD%97%E6%A0%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="本质上仍是普通合伙企业" _mubu_text="%3Cspan%3E%E6%9C%AC%E8%B4%A8%E4%B8%8A%E4%BB%8D%E6%98%AF%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="只在责任承担方面有特殊的规定，其他都与普通合伙企业的规定相同" _mubu_text="%3Cspan%3E%E5%8F%AA%E5%9C%A8%E8%B4%A3%E4%BB%BB%E6%89%BF%E6%8B%85%E6%96%B9%E9%9D%A2%E6%9C%89%E7%89%B9%E6%AE%8A%E7%9A%84%E8%A7%84%E5%AE%9A%EF%BC%8C%E5%85%B6%E4%BB%96%E9%83%BD%E4%B8%8E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E8%A7%84%E5%AE%9A%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="普通合伙企业or有限公司(图)" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9Aor%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22uDOVPvN9vM%22%2C%22oh%22%3A813%2C%22ow%22%3A1226%2C%22uri%22%3A%22document_image%2Fa8f74d8e-3f15-48f6-b37b-31189ef71b7e-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="有限合伙企业" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="无特殊规定则参照适用普通合伙规定" _mubu_text="%3Cspan%3E%E6%97%A0%E7%89%B9%E6%AE%8A%E8%A7%84%E5%AE%9A%E5%88%99%E5%8F%82%E7%85%A7%E9%80%82%E7%94%A8%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="设立" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="合伙人" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="2~50合伙人" _mubu_text="%3Cspan%3E2~50%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="至少一个普通/有限合伙人" _mubu_text="%3Cspan%3E%E8%87%B3%E5%B0%91%E4%B8%80%E4%B8%AA%E6%99%AE%E9%80%9A/%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有限合伙人" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="不要求有完民行能" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E6%B1%82%E6%9C%89%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="无禁止成为的限制" _mubu_text="%3Cspan%3E%E6%97%A0%E7%A6%81%E6%AD%A2%E6%88%90%E4%B8%BA%E7%9A%84%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="出资" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="有限合伙人不得以劳务出资" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%8D%E5%BE%97%E4%BB%A5%E5%8A%B3%E5%8A%A1%E5%87%BA%E8%B5%84%3C/span%3E" _note="普通合伙人则可以" _mubu_note="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E5%88%99%E5%8F%AF%E4%BB%A5%3C/span%3E"/>
            </outline>
            <outline text="名称中应标明“有限合伙”字样" _mubu_text="%3Cspan%3E%E5%90%8D%E7%A7%B0%E4%B8%AD%E5%BA%94%E6%A0%87%E6%98%8E%E2%80%9C%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E2%80%9D%E5%AD%97%E6%A0%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="份额的流转" _mubu_text="%3Cspan%3E%E4%BB%BD%E9%A2%9D%E7%9A%84%E6%B5%81%E8%BD%AC%3C/span%3E" _note="" _mubu_note="">
            <outline text="有限合伙人财产份额的流转" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%E7%9A%84%E6%B5%81%E8%BD%AC%3C/span%3E" _note="" _mubu_note="">
              <outline text="提前30日通知即可转让" _mubu_text="%3Cspan%3E%E6%8F%90%E5%89%8D30%E6%97%A5%E9%80%9A%E7%9F%A5%E5%8D%B3%E5%8F%AF%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="其他合伙人无优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="出质" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="可自由出质" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%87%AA%E7%94%B1%E5%87%BA%E8%B4%A8%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="事务执行" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%8A%A1%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="普通合伙人执行事务" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%A7%E8%A1%8C%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="可要求报酬" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%A6%81%E6%B1%82%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="有限合伙人不执行事务" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%8D%E6%89%A7%E8%A1%8C%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得对外代表有限合伙企业" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%AF%B9%E5%A4%96%E4%BB%A3%E8%A1%A8%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="对关乎自己利益事务可有限参与，无需担责" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%B3%E4%B9%8E%E8%87%AA%E5%B7%B1%E5%88%A9%E7%9B%8A%E4%BA%8B%E5%8A%A1%E5%8F%AF%E6%9C%89%E9%99%90%E5%8F%82%E4%B8%8E%EF%BC%8C%E6%97%A0%E9%9C%80%E6%8B%85%E8%B4%A3%3C/span%3E" _note="不视为执行事务" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%A7%86%E4%B8%BA%E6%89%A7%E8%A1%8C%E4%BA%8B%E5%8A%A1%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="表见普通合伙" _mubu_text="%3Cspan%3E%E8%A1%A8%E8%A7%81%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="第三人有理由相信有限合伙人为普通合伙人并与其交易" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%9C%89%E7%90%86%E7%94%B1%E7%9B%B8%E4%BF%A1%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%BA%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E5%B9%B6%E4%B8%8E%E5%85%B6%E4%BA%A4%E6%98%93%3C/span%3E" _note="具有普通合伙人的外观" _mubu_note="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E5%A4%96%E8%A7%82%3C/span%3E"/>
            <outline text="该笔交易，该有限合伙人承担与普通合伙人同样的责任" _mubu_text="%3Cspan%3E%E8%AF%A5%E7%AC%94%E4%BA%A4%E6%98%93%EF%BC%8C%E8%AF%A5%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%BF%E6%8B%85%E4%B8%8E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E5%90%8C%E6%A0%B7%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="无限连带责任" _mubu_note="%3Cspan%3E%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E">
              <outline text="其他有限合伙人仍担有限责任" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%BB%8D%E6%8B%85%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="内部可追偿" _mubu_text="%3Cspan%3E%E5%86%85%E9%83%A8%E5%8F%AF%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="与第三人关系" _mubu_text="%3Cspan%3E%E4%B8%8E%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="企业债务的清偿" _mubu_text="%3Cspan%3E%E4%BC%81%E4%B8%9A%E5%80%BA%E5%8A%A1%E7%9A%84%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="先以合伙企业的全部财产进行清偿" _mubu_text="%3Cspan%3E%E5%85%88%E4%BB%A5%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E5%85%A8%E9%83%A8%E8%B4%A2%E4%BA%A7%E8%BF%9B%E8%A1%8C%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="普通合伙人无限连带，有限合伙人有限" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%EF%BC%8C%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="有限合伙人个人债务" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%AA%E4%BA%BA%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="以自有财产清偿" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%87%AA%E6%9C%89%E8%B4%A2%E4%BA%A7%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不足以清偿" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%B6%B3%E4%BB%A5%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="收益清偿" _mubu_text="%3Cspan%3E%E6%94%B6%E7%9B%8A%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="强执财产份额" _mubu_text="%3Cspan%3E%E5%BC%BA%E6%89%A7%E8%B4%A2%E4%BA%A7%E4%BB%BD%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法院应通知全体合伙人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E9%80%9A%E7%9F%A5%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其他合伙人有优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%90%88%E4%BC%99%E4%BA%BA%E6%9C%89%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="有限合伙人主动转让时，其他无优先购买权" _mubu_note="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%BB%E5%8A%A8%E8%BD%AC%E8%AE%A9%E6%97%B6%EF%BC%8C%E5%85%B6%E4%BB%96%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="有限合伙人" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="权利：知情权" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%EF%BC%9A%E7%9F%A5%E6%83%85%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="无当然的竞业禁止义务" _mubu_text="%3Cspan%3E%E6%97%A0%E5%BD%93%E7%84%B6%E7%9A%84%E7%AB%9E%E4%B8%9A%E7%A6%81%E6%AD%A2%E4%B9%89%E5%8A%A1%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              <outline text="无当然的自我交易禁止义务" _mubu_text="%3Cspan%3E%E6%97%A0%E5%BD%93%E7%84%B6%E7%9A%84%E8%87%AA%E6%88%91%E4%BA%A4%E6%98%93%E7%A6%81%E6%AD%A2%E4%B9%89%E5%8A%A1%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="利润分配与亏损分担" _mubu_text="%3Cspan%3E%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E4%B8%8E%E4%BA%8F%E6%8D%9F%E5%88%86%E6%8B%85%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则上不得将全部利润分配给部分合伙人，另约除外" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E4%B8%8D%E5%BE%97%E5%B0%86%E5%85%A8%E9%83%A8%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E7%BB%99%E9%83%A8%E5%88%86%E5%90%88%E4%BC%99%E4%BA%BA%EF%BC%8C%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E" _note="“有福可不同享”" _mubu_note="%3Cspan%3E%E2%80%9C%E6%9C%89%E7%A6%8F%E5%8F%AF%E4%B8%8D%E5%90%8C%E4%BA%AB%E2%80%9D%3C/span%3E"/>
            <outline text="协议不得约定有部分合伙人承担全部亏损" _mubu_text="%3Cspan%3E%E5%8D%8F%E8%AE%AE%E4%B8%8D%E5%BE%97%E7%BA%A6%E5%AE%9A%E6%9C%89%E9%83%A8%E5%88%86%E5%90%88%E4%BC%99%E4%BA%BA%E6%89%BF%E6%8B%85%E5%85%A8%E9%83%A8%E4%BA%8F%E6%8D%9F%3C/span%3E" _note="“有难须同当”" _mubu_note="%3Cspan%3E%E2%80%9C%E6%9C%89%E9%9A%BE%E9%A1%BB%E5%90%8C%E5%BD%93%E2%80%9D%3C/span%3E"/>
          </outline>
          <outline text="有限合伙人入伙与退伙" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E5%85%A5%E4%BC%99%E4%B8%8E%E9%80%80%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="入伙" _mubu_text="%3Cspan%3E%E5%85%A5%E4%BC%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="按合伙协议约定" _mubu_text="%3Cspan%3E%E6%8C%89%E5%90%88%E4%BC%99%E5%8D%8F%E8%AE%AE%E7%BA%A6%E5%AE%9A%3C/span%3E" _note="无约适用普通合伙" _mubu_note="%3Cspan%3E%E6%97%A0%E7%BA%A6%E9%80%82%E7%94%A8%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%3C/span%3E"/>
              <outline text="对入伙前合伙企业的债务" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%A5%E4%BC%99%E5%89%8D%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E7%9A%84%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="以其认缴的出资额为限承担责任" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%85%B6%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%E4%B8%BA%E9%99%90%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="有限合伙or普通合伙退伙(图)" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99or%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E9%80%80%E4%BC%99(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
              <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22pDHhjXGjz5%22%2C%22oh%22%3A2148%2C%22ow%22%3A2588%2C%22uri%22%3A%22document_image%2F1eba666b-1b80-415e-982c-750074d76252-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
          </outline>
          <outline text="有限合伙人与普通合伙人的转化" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%8E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E8%BD%AC%E5%8C%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="应经全体合伙人一致同意" _mubu_text="%3Cspan%3E%E5%BA%94%E7%BB%8F%E5%85%A8%E4%BD%93%E5%90%88%E4%BC%99%E4%BA%BA%E4%B8%80%E8%87%B4%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="转换期间债务承担无限连带责任" _mubu_text="%3Cspan%3E%E8%BD%AC%E6%8D%A2%E6%9C%9F%E9%97%B4%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="无另约免责" _mubu_note="%3Cspan%3E%E6%97%A0%E5%8F%A6%E7%BA%A6%E5%85%8D%E8%B4%A3%3C/span%3E">
              <outline text="相当于退伙再入伙" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%BD%93%E4%BA%8E%E9%80%80%E4%BC%99%E5%86%8D%E5%85%A5%E4%BC%99%3C/span%3E" _note="对原身份期间债务" _mubu_note="%3Cspan%3E%E5%AF%B9%E5%8E%9F%E8%BA%AB%E4%BB%BD%E6%9C%9F%E9%97%B4%E5%80%BA%E5%8A%A1%3C/span%3E"/>
            </outline>
            <outline text="有限合伙企业仅剩有限合伙人的，应当解散" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%BB%85%E5%89%A9%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E8%A7%A3%E6%95%A3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有限合伙企业仅剩普通合伙人的，转为普通合伙企业" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E4%BB%85%E5%89%A9%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%EF%BC%8C%E8%BD%AC%E4%B8%BA%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="有限合伙or普通合伙(图)" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99or%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22zitN1nnpnS%22%2C%22oh%22%3A955%2C%22ow%22%3A1004%2C%22uri%22%3A%22document_image%2F97e042f1-45bb-43a6-ae1f-fe66aa24a80a-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="合伙人责任承担总结" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BA%BA%E8%B4%A3%E4%BB%BB%E6%89%BF%E6%8B%85%E6%80%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="普通合伙人的无限连带责任" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="新入伙的" _mubu_text="%3Cspan%3E%E6%96%B0%E5%85%A5%E4%BC%99%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="对入伙前的债务，承担无限连带责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%A5%E4%BC%99%E5%89%8D%E7%9A%84%E5%80%BA%E5%8A%A1%EF%BC%8C%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合伙期间" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="对企业不能清偿的部分，承担无限连带责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BC%81%E4%B8%9A%E4%B8%8D%E8%83%BD%E6%B8%85%E5%81%BF%E7%9A%84%E9%83%A8%E5%88%86%EF%BC%8C%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="退伙的" _mubu_text="%3Cspan%3E%E9%80%80%E4%BC%99%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="对基于其退伙前的原因发生的债务，承担无限连带责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%9F%BA%E4%BA%8E%E5%85%B6%E9%80%80%E4%BC%99%E5%89%8D%E7%9A%84%E5%8E%9F%E5%9B%A0%E5%8F%91%E7%94%9F%E7%9A%84%E5%80%BA%E5%8A%A1%EF%BC%8C%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合伙企业注销、破产后" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E4%BC%81%E4%B8%9A%E6%B3%A8%E9%94%80%E3%80%81%E7%A0%B4%E4%BA%A7%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
              <outline text="依然承担无限连带责任" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%84%B6%E6%89%BF%E6%8B%85%E6%97%A0%E9%99%90%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="有限合伙人的有限责任" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%90%88%E4%BC%99%E4%BA%BA%E7%9A%84%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="新入伙的" _mubu_text="%3Cspan%3E%E6%96%B0%E5%85%A5%E4%BC%99%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="对入伙前的债务，以认缴的出资额为限承担有限责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%A5%E4%BC%99%E5%89%8D%E7%9A%84%E5%80%BA%E5%8A%A1%EF%BC%8C%E4%BB%A5%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%E4%B8%BA%E9%99%90%E6%89%BF%E6%8B%85%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合伙期间" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="对企业不能清偿的部分，以认缴的出资额为限承担有限责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BC%81%E4%B8%9A%E4%B8%8D%E8%83%BD%E6%B8%85%E5%81%BF%E7%9A%84%E9%83%A8%E5%88%86%EF%BC%8C%E4%BB%A5%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%E4%B8%BA%E9%99%90%E6%89%BF%E6%8B%85%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="退伙的" _mubu_text="%3Cspan%3E%E9%80%80%E4%BC%99%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="基于退伙前原因发生的债务，以退伙时取回的财产为限，承担有限责任" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E9%80%80%E4%BC%99%E5%89%8D%E5%8E%9F%E5%9B%A0%E5%8F%91%E7%94%9F%E7%9A%84%E5%80%BA%E5%8A%A1%EF%BC%8C%E4%BB%A5%E9%80%80%E4%BC%99%E6%97%B6%E5%8F%96%E5%9B%9E%E7%9A%84%E8%B4%A2%E4%BA%A7%E4%B8%BA%E9%99%90%EF%BC%8C%E6%89%BF%E6%8B%85%E6%9C%89%E9%99%90%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
