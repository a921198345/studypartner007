            <outline text="发起人" _mubu_text="%3Cspan%3E%E5%8F%91%E8%B5%B7%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="人数" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="1~50人" _mubu_text="%3Cspan%3E1~50%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="无民行能、国籍和住所地要求" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B0%91%E8%A1%8C%E8%83%BD%E3%80%81%E5%9B%BD%E7%B1%8D%E5%92%8C%E4%BD%8F%E6%89%80%E5%9C%B0%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="出资方式" _mubu_text="%3Cspan%3E%E5%87%BA%E8%B5%84%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="一般为认缴资本制" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%B8%BA%E8%AE%A4%E7%BC%B4%E8%B5%84%E6%9C%AC%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无最低注册资本、出资期限、各期出资金额和验资要求" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%80%E4%BD%8E%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%E3%80%81%E5%87%BA%E8%B5%84%E6%9C%9F%E9%99%90%E3%80%81%E5%90%84%E6%9C%9F%E5%87%BA%E8%B5%84%E9%87%91%E9%A2%9D%E5%92%8C%E9%AA%8C%E8%B5%84%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="股权转让" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="对内转让" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%86%85%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="股东之间相互部分/全部转让" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%B9%8B%E9%97%B4%E7%9B%B8%E4%BA%92%E9%83%A8%E5%88%86/%E5%85%A8%E9%83%A8%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无须向其他通知、同意" _mubu_text="%3Cspan%3E%E6%97%A0%E9%A1%BB%E5%90%91%E5%85%B6%E4%BB%96%E9%80%9A%E7%9F%A5%E3%80%81%E5%90%8C%E6%84%8F%3C/span%3E" _note="完全自由" _mubu_note="%3Cspan%3E%E5%AE%8C%E5%85%A8%E8%87%AA%E7%94%B1%3C/span%3E"/>
              <outline text="其他股东无优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对外转让" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="通知" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="书面通知其他股东" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E9%80%9A%E7%9F%A5%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="其他股东的优先购买权" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E7%9A%84%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="同等条件下，优先购买权" _mubu_text="%3Cspan%3E%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E4%B8%8B%EF%BC%8C%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="行使期间" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="接书面通知起满30日未予答复视为放弃" _mubu_text="%3Cspan%3E%E6%8E%A5%E4%B9%A6%E9%9D%A2%E9%80%9A%E7%9F%A5%E8%B5%B7%E6%BB%A130%E6%97%A5%E6%9C%AA%E4%BA%88%E7%AD%94%E5%A4%8D%E8%A7%86%E4%B8%BA%E6%94%BE%E5%BC%83%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="两个以上的股东主张" _mubu_text="%3Cspan%3E%E4%B8%A4%E4%B8%AA%E4%BB%A5%E4%B8%8A%E7%9A%84%E8%82%A1%E4%B8%9C%E4%B8%BB%E5%BC%A0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="先协商确定" _mubu_text="%3Cspan%3E%E5%85%88%E5%8D%8F%E5%95%86%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="协商不成，按出资比例行使" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%95%86%E4%B8%8D%E6%88%90%EF%BC%8C%E6%8C%89%E5%87%BA%E8%B5%84%E6%AF%94%E4%BE%8B%E8%A1%8C%E4%BD%BF%3C/span%3E" _note="认缴" _mubu_note="%3Cspan%3E%E8%AE%A4%E7%BC%B4%3C/span%3E"/>
                </outline>
                <outline text="转让方反悔" _mubu_text="%3Cspan%3E%E8%BD%AC%E8%AE%A9%E6%96%B9%E5%8F%8D%E6%82%94%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他股东则无相应优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E5%88%99%E6%97%A0%E7%9B%B8%E5%BA%94%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
                  <outline text="可主张转让股东赔偿其合理损失" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%BB%E5%BC%A0%E8%BD%AC%E8%AE%A9%E8%82%A1%E4%B8%9C%E8%B5%94%E5%81%BF%E5%85%B6%E5%90%88%E7%90%86%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="缔约过失责任" _mubu_note="%3Cspan%3E%E7%BC%94%E7%BA%A6%E8%BF%87%E5%A4%B1%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
                </outline>
                <outline text="损害其他股东优先购买权" _mubu_text="%3Cspan%3E%E6%8D%9F%E5%AE%B3%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                  <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                    <outline text="未就其股权转让事项征求其他股东意见" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%B0%B1%E5%85%B6%E8%82%A1%E6%9D%83%E8%BD%AC%E8%AE%A9%E4%BA%8B%E9%A1%B9%E5%BE%81%E6%B1%82%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="或者以欺诈、恶意串通等手段加以损害" _mubu_text="%3Cspan%3E%E6%88%96%E8%80%85%E4%BB%A5%E6%AC%BA%E8%AF%88%E3%80%81%E6%81%B6%E6%84%8F%E4%B8%B2%E9%80%9A%E7%AD%89%E6%89%8B%E6%AE%B5%E5%8A%A0%E4%BB%A5%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                    <outline text="(应)知道行使优先购买权的同等条件之日起30日内" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E8%A1%8C%E4%BD%BF%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%E7%9A%84%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="或自股权变更登记之日起1年" _mubu_text="%3Cspan%3E%E6%88%96%E8%87%AA%E8%82%A1%E6%9D%83%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%E4%B9%8B%E6%97%A5%E8%B5%B71%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="其他股东异议" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                    <outline text="仅提出异议，但不同等条件购买的，不支持" _mubu_text="%3Cspan%3E%E4%BB%85%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%EF%BC%8C%E4%BD%86%E4%B8%8D%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E8%B4%AD%E4%B9%B0%E7%9A%84%EF%BC%8C%E4%B8%8D%E6%94%AF%E6%8C%81%3C/span%3E" _note="" _mubu_note="">
                      <outline text="非因自身原因导致无法购买，请求损害赔偿的除外" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%9B%A0%E8%87%AA%E8%BA%AB%E5%8E%9F%E5%9B%A0%E5%AF%BC%E8%87%B4%E6%97%A0%E6%B3%95%E8%B4%AD%E4%B9%B0%EF%BC%8C%E8%AF%B7%E6%B1%82%E6%8D%9F%E5%AE%B3%E8%B5%94%E5%81%BF%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="因股东行使优先购买权而不能实现合同目的，可请求转让股东承担民责" _mubu_text="%3Cspan%3E%E5%9B%A0%E8%82%A1%E4%B8%9C%E8%A1%8C%E4%BD%BF%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%E8%80%8C%E4%B8%8D%E8%83%BD%E5%AE%9E%E7%8E%B0%E5%90%88%E5%90%8C%E7%9B%AE%E7%9A%84%EF%BC%8C%E5%8F%AF%E8%AF%B7%E6%B1%82%E8%BD%AC%E8%AE%A9%E8%82%A1%E4%B8%9C%E6%89%BF%E6%8B%85%E6%B0%91%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
            <outline text="股权的强制执行" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E7%9A%84%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="法院依强制执行程序转让股东的股权" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BE%9D%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E7%A8%8B%E5%BA%8F%E8%BD%AC%E8%AE%A9%E8%82%A1%E4%B8%9C%E7%9A%84%E8%82%A1%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应通知公司及全体股东" _mubu_text="%3Cspan%3E%E5%BA%94%E9%80%9A%E7%9F%A5%E5%85%AC%E5%8F%B8%E5%8F%8A%E5%85%A8%E4%BD%93%E8%82%A1%E4%B8%9C%3C/span%3E" _note="无需征得股东同意" _mubu_note="%3Cspan%3E%E6%97%A0%E9%9C%80%E5%BE%81%E5%BE%97%E8%82%A1%E4%B8%9C%E5%90%8C%E6%84%8F%3C/span%3E"/>
              <outline text="其他股东有优先购买权" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%9C%89%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="除斥期间：通知之日起20日" _mubu_text="%3Cspan%3E%E9%99%A4%E6%96%A5%E6%9C%9F%E9%97%B4%EF%BC%9A%E9%80%9A%E7%9F%A5%E4%B9%8B%E6%97%A5%E8%B5%B720%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="转让后处理" _mubu_text="%3Cspan%3E%E8%BD%AC%E8%AE%A9%E5%90%8E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="主动转让/强制执行后" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E8%BD%AC%E8%AE%A9/%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公司应注销原股东出资证明书，向新股东签发出资证明书" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%BA%94%E6%B3%A8%E9%94%80%E5%8E%9F%E8%82%A1%E4%B8%9C%E5%87%BA%E8%B5%84%E8%AF%81%E6%98%8E%E4%B9%A6%EF%BC%8C%E5%90%91%E6%96%B0%E8%82%A1%E4%B8%9C%E7%AD%BE%E5%8F%91%E5%87%BA%E8%B5%84%E8%AF%81%E6%98%8E%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="相应修改公司章程和股东名册" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%BA%94%E4%BF%AE%E6%94%B9%E5%85%AC%E5%8F%B8%E7%AB%A0%E7%A8%8B%E5%92%8C%E8%82%A1%E4%B8%9C%E5%90%8D%E5%86%8C%3C/span%3E" _note="有关股东及其出资额的记载" _mubu_note="%3Cspan%3E%E6%9C%89%E5%85%B3%E8%82%A1%E4%B8%9C%E5%8F%8A%E5%85%B6%E5%87%BA%E8%B5%84%E9%A2%9D%E7%9A%84%E8%AE%B0%E8%BD%BD%3C/span%3E">
                <outline text="该项修改无须再由股东会表决" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AF%A5%E9%A1%B9%E4%BF%AE%E6%94%B9%E6%97%A0%E9%A1%BB%E5%86%8D%E7%94%B1%E8%82%A1%E4%B8%9C%E4%BC%9A%E8%A1%A8%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="股权的继承" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E7%9A%84%E7%BB%A7%E6%89%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则上当然继承" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E5%BD%93%E7%84%B6%E7%BB%A7%E6%89%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="其合法继承人可以继承股东资格" _mubu_text="%3Cspan%3E%E5%85%B6%E5%90%88%E6%B3%95%E7%BB%A7%E6%89%BF%E4%BA%BA%E5%8F%AF%E4%BB%A5%E7%BB%A7%E6%89%BF%E8%82%A1%E4%B8%9C%E8%B5%84%E6%A0%BC%3C/span%3E" _note="章程另规除外" _mubu_note="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E5%8F%A6%E8%A7%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
              <outline text="无优先购买权" _mubu_text="%3Cspan%3E%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="股权继承时，其他股东无优先购买权" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E7%BB%A7%E6%89%BF%E6%97%B6%EF%BC%8C%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%97%A0%E4%BC%98%E5%85%88%E8%B4%AD%E4%B9%B0%E6%9D%83%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="异议股东的回购请求权" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%BC%82%E8%AE%AE%E8%82%A1%E4%B8%9C%E7%9A%84%E5%9B%9E%E8%B4%AD%E8%AF%B7%E6%B1%82%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="对股东会决议投反对票的股东可以请求公司按照合理的价格收购其股权" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E6%8A%95%E5%8F%8D%E5%AF%B9%E7%A5%A8%E7%9A%84%E8%82%A1%E4%B8%9C%E5%8F%AF%E4%BB%A5%E8%AF%B7%E6%B1%82%E5%85%AC%E5%8F%B8%E6%8C%89%E7%85%A7%E5%90%88%E7%90%86%E7%9A%84%E4%BB%B7%E6%A0%BC%E6%94%B6%E8%B4%AD%E5%85%B6%E8%82%A1%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司5年连续盈利且符合分配利润条件但不向股东分配利润" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B85%E5%B9%B4%E8%BF%9E%E7%BB%AD%E7%9B%88%E5%88%A9%E4%B8%94%E7%AC%A6%E5%90%88%E5%88%86%E9%85%8D%E5%88%A9%E6%B6%A6%E6%9D%A1%E4%BB%B6%E4%BD%86%E4%B8%8D%E5%90%91%E8%82%A1%E4%B8%9C%E5%88%86%E9%85%8D%E5%88%A9%E6%B6%A6%3C/span%3E" _note="“过去”" _mubu_note="%3Cspan%3E%E2%80%9C%E8%BF%87%E5%8E%BB%E2%80%9D%3C/span%3E"/>
                <outline text="公司合并、分立、转让主要财产的" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%90%88%E5%B9%B6%E3%80%81%E5%88%86%E7%AB%8B%E3%80%81%E8%BD%AC%E8%AE%A9%E4%B8%BB%E8%A6%81%E8%B4%A2%E4%BA%A7%E7%9A%84%3C/span%3E" _note="“现在”" _mubu_note="%3Cspan%3E%E2%80%9C%E7%8E%B0%E5%9C%A8%E2%80%9D%3C/span%3E"/>
                <outline text="章程规定的解散事由出现，股东会通过决议修改章程使公司存续的" _mubu_text="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%E7%9A%84%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%E5%87%BA%E7%8E%B0%EF%BC%8C%E8%82%A1%E4%B8%9C%E4%BC%9A%E9%80%9A%E8%BF%87%E5%86%B3%E8%AE%AE%E4%BF%AE%E6%94%B9%E7%AB%A0%E7%A8%8B%E4%BD%BF%E5%85%AC%E5%8F%B8%E5%AD%98%E7%BB%AD%E7%9A%84%3C/span%3E" _note="“将来”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%B0%86%E6%9D%A5%E2%80%9D%3C/span%3E"/>
                <outline text="公司的控股股东滥用股东权利严重损害公司或其他股东利益的其他股东有权请求公司按合理价格收购其股权" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E7%9A%84%E6%8E%A7%E8%82%A1%E8%82%A1%E4%B8%9C%E6%BB%A5%E7%94%A8%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%E4%B8%A5%E9%87%8D%E6%8D%9F%E5%AE%B3%E5%85%AC%E5%8F%B8%E6%88%96%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E5%88%A9%E7%9B%8A%E7%9A%84%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%9C%89%E6%9D%83%E8%AF%B7%E6%B1%82%E5%85%AC%E5%8F%B8%E6%8C%89%E5%90%88%E7%90%86%E4%BB%B7%E6%A0%BC%E6%94%B6%E8%B4%AD%E5%85%B6%E8%82%A1%E6%9D%83%3C/span%3E" _note="该项无需股东持有异议" _mubu_note="%3Cspan%3E%E8%AF%A5%E9%A1%B9%E6%97%A0%E9%9C%80%E8%82%A1%E4%B8%9C%E6%8C%81%E6%9C%89%E5%BC%82%E8%AE%AE%3C/span%3E"/>
              </outline>
              <outline text="先协商，再诉讼；60日＋30日" _mubu_text="%3Cspan%3E%E5%85%88%E5%8D%8F%E5%95%86%EF%BC%8C%E5%86%8D%E8%AF%89%E8%AE%BC%EF%BC%9B60%E6%97%A5%EF%BC%8B30%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="股权让与担保" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E8%AE%A9%E4%B8%8E%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="不适用股权转让规则" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%E8%82%A1%E6%9D%83%E8%BD%AC%E8%AE%A9%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="债权人不担责和义务" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E4%B8%8D%E6%8B%85%E8%B4%A3%E5%92%8C%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="“一股二买”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%80%E8%82%A1%E4%BA%8C%E4%B9%B0%E2%80%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="登记给收就给谁" _mubu_text="%3Cspan%3E%E7%99%BB%E8%AE%B0%E7%BB%99%E6%94%B6%E5%B0%B1%E7%BB%99%E8%B0%81%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="股权归属争议" _mubu_text="%3Cspan%3E%E8%82%A1%E6%9D%83%E5%BD%92%E5%B1%9E%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="请求确认享有股权应证明" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E7%A1%AE%E8%AE%A4%E4%BA%AB%E6%9C%89%E8%82%A1%E6%9D%83%E5%BA%94%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="已依法向公司出资或者认缴出资，且不违反法律法规的强制性规定" _mubu_text="%3Cspan%3E%E5%B7%B2%E4%BE%9D%E6%B3%95%E5%90%91%E5%85%AC%E5%8F%B8%E5%87%BA%E8%B5%84%E6%88%96%E8%80%85%E8%AE%A4%E7%BC%B4%E5%87%BA%E8%B5%84%EF%BC%8C%E4%B8%94%E4%B8%8D%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E6%B3%95%E8%A7%84%E7%9A%84%E5%BC%BA%E5%88%B6%E6%80%A7%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="已受让或以其他形式继受公司股权，且不违反法律法规的强制性规定" _mubu_text="%3Cspan%3E%E5%B7%B2%E5%8F%97%E8%AE%A9%E6%88%96%E4%BB%A5%E5%85%B6%E4%BB%96%E5%BD%A2%E5%BC%8F%E7%BB%A7%E5%8F%97%E5%85%AC%E5%8F%B8%E8%82%A1%E6%9D%83%EF%BC%8C%E4%B8%94%E4%B8%8D%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E6%B3%95%E8%A7%84%E7%9A%84%E5%BC%BA%E5%88%B6%E6%80%A7%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
