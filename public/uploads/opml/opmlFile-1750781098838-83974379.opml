            <outline text="优先认购权" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E8%AE%A4%E8%B4%AD%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="有限" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="按实缴出资比例优先认购" _mubu_text="%3Cspan%3E%E6%8C%89%E5%AE%9E%E7%BC%B4%E5%87%BA%E8%B5%84%E6%AF%94%E4%BE%8B%E4%BC%98%E5%85%88%E8%AE%A4%E8%B4%AD%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
                <outline text="其他入股，应取得股东放弃优先认购权的声明" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%85%A5%E8%82%A1%EF%BC%8C%E5%BA%94%E5%8F%96%E5%BE%97%E8%82%A1%E4%B8%9C%E6%94%BE%E5%BC%83%E4%BC%98%E5%85%88%E8%AE%A4%E8%B4%AD%E6%9D%83%E7%9A%84%E5%A3%B0%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="股份" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
                <outline text="无该项权利" _mubu_text="%3Cspan%3E%E6%97%A0%E8%AF%A5%E9%A1%B9%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="其他表述" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%A1%A8%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="股份公司以增发股份方式收购其他公司股份/资产" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%E4%BB%A5%E5%A2%9E%E5%8F%91%E8%82%A1%E4%BB%BD%E6%96%B9%E5%BC%8F%E6%94%B6%E8%B4%AD%E5%85%B6%E4%BB%96%E5%85%AC%E5%8F%B8%E8%82%A1%E4%BB%BD/%E8%B5%84%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="投资者出资入股公司" _mubu_text="%3Cspan%3E%E6%8A%95%E8%B5%84%E8%80%85%E5%87%BA%E8%B5%84%E5%85%A5%E8%82%A1%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="平价入股" _mubu_text="%3Cspan%3E%E5%B9%B3%E4%BB%B7%E5%85%A5%E8%82%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="溢价入股" _mubu_text="%3Cspan%3E%E6%BA%A2%E4%BB%B7%E5%85%A5%E8%82%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="减少注册资本" _mubu_text="%3Cspan%3E%E5%87%8F%E5%B0%91%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%3C/span%3E" _note="" _mubu_note="">
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="股东被除名后减资" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E8%A2%AB%E9%99%A4%E5%90%8D%E5%90%8E%E5%87%8F%E8%B5%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="资金闲置" _mubu_text="%3Cspan%3E%E8%B5%84%E9%87%91%E9%97%B2%E7%BD%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="亏损严重，名实不符" _mubu_text="%3Cspan%3E%E4%BA%8F%E6%8D%9F%E4%B8%A5%E9%87%8D%EF%BC%8C%E5%90%8D%E5%AE%9E%E4%B8%8D%E7%AC%A6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="必须编制资产负债表及财产清单" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E7%BC%96%E5%88%B6%E8%B5%84%E4%BA%A7%E8%B4%9F%E5%80%BA%E8%A1%A8%E5%8F%8A%E8%B4%A2%E4%BA%A7%E6%B8%85%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="决议" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%3C/span%3E" _note="属于重大事项" _mubu_note="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%3C/span%3E"/>
            </outline>
            <outline text="债权人保护制度" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E4%BF%9D%E6%8A%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="公司" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                <outline text="决议之日起10日内通知债权人，并于30日内在报纸上或国家企业信用信息公示系统公告" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%E4%B9%8B%E6%97%A5%E8%B5%B710%E6%97%A5%E5%86%85%E9%80%9A%E7%9F%A5%E5%80%BA%E6%9D%83%E4%BA%BA%EF%BC%8C%E5%B9%B6%E4%BA%8E30%E6%97%A5%E5%86%85%E5%9C%A8%E6%8A%A5%E7%BA%B8%E4%B8%8A%E6%88%96%E5%9B%BD%E5%AE%B6%E4%BC%81%E4%B8%9A%E4%BF%A1%E7%94%A8%E4%BF%A1%E6%81%AF%E5%85%AC%E7%A4%BA%E7%B3%BB%E7%BB%9F%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未通知债权人，损害债权人利益的" _mubu_text="%3Cspan%3E%E6%9C%AA%E9%80%9A%E7%9F%A5%E5%80%BA%E6%9D%83%E4%BA%BA%EF%BC%8C%E6%8D%9F%E5%AE%B3%E5%80%BA%E6%9D%83%E4%BA%BA%E5%88%A9%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不影响减少注册资本的效力" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%87%8F%E5%B0%91%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="但不得对抗该债权人" _mubu_text="%3Cspan%3E%E4%BD%86%E4%B8%8D%E5%BE%97%E5%AF%B9%E6%8A%97%E8%AF%A5%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                    <outline text="债权人依然有权要求以减资前同等力度保护" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E4%BE%9D%E7%84%B6%E6%9C%89%E6%9D%83%E8%A6%81%E6%B1%82%E4%BB%A5%E5%87%8F%E8%B5%84%E5%89%8D%E5%90%8C%E7%AD%89%E5%8A%9B%E5%BA%A6%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="可让股东在退回的范围内担责" _mubu_note="%3Cspan%3E%E5%8F%AF%E8%AE%A9%E8%82%A1%E4%B8%9C%E5%9C%A8%E9%80%80%E5%9B%9E%E7%9A%84%E8%8C%83%E5%9B%B4%E5%86%85%E6%8B%85%E8%B4%A3%3C/span%3E"/>
                  </outline>
                </outline>
              </outline>
              <outline text="债权人" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="接到通知书之日起30日内，未接到通知书的自公告之日起45日内" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E4%B9%A6%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%EF%BC%8C%E6%9C%AA%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E4%B9%A6%E7%9A%84%E8%87%AA%E5%85%AC%E5%91%8A%E4%B9%8B%E6%97%A5%E8%B5%B745%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="有权要求公司清偿债务" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9D%83%E8%A6%81%E6%B1%82%E5%85%AC%E5%8F%B8%E6%B8%85%E5%81%BF%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="或提供相应的担保" _mubu_text="%3Cspan%3E%E6%88%96%E6%8F%90%E4%BE%9B%E7%9B%B8%E5%BA%94%E7%9A%84%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="禁止非等比例减资" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E9%9D%9E%E7%AD%89%E6%AF%94%E4%BE%8B%E5%87%8F%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="应按照股东出资或持有股份的比例相应减少出资额或股份" _mubu_text="%3Cspan%3E%E5%BA%94%E6%8C%89%E7%85%A7%E8%82%A1%E4%B8%9C%E5%87%BA%E8%B5%84%E6%88%96%E6%8C%81%E6%9C%89%E8%82%A1%E4%BB%BD%E7%9A%84%E6%AF%94%E4%BE%8B%E7%9B%B8%E5%BA%94%E5%87%8F%E5%B0%91%E5%87%BA%E8%B5%84%E9%A2%9D%E6%88%96%E8%82%A1%E4%BB%BD%3C/span%3E" _note="另规、另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E8%A7%84%E3%80%81%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="减资的路径" _mubu_text="%3Cspan%3E%E5%87%8F%E8%B5%84%E7%9A%84%E8%B7%AF%E5%BE%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="向股东退还减资对应的资金" _mubu_text="%3Cspan%3E%E5%90%91%E8%82%A1%E4%B8%9C%E9%80%80%E8%BF%98%E5%87%8F%E8%B5%84%E5%AF%B9%E5%BA%94%E7%9A%84%E8%B5%84%E9%87%91%3C/span%3E" _note="" _mubu_note="">
                <outline text="直接返还" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%BF%94%E8%BF%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="降低股东认缴的出资额" _mubu_text="%3Cspan%3E%E9%99%8D%E4%BD%8E%E8%82%A1%E4%B8%9C%E8%AE%A4%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="无返还" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%94%E8%BF%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="减免未来出资金额" _mubu_text="%3Cspan%3E%E5%87%8F%E5%85%8D%E6%9C%AA%E6%9D%A5%E5%87%BA%E8%B5%84%E9%87%91%E9%A2%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="减少注册资本以弥补亏损" _mubu_text="%3Cspan%3E%E5%87%8F%E5%B0%91%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC%E4%BB%A5%E5%BC%A5%E8%A1%A5%E4%BA%8F%E6%8D%9F%3C/span%3E" _note="" _mubu_note="">
                <outline text="无返还、无减免" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%94%E8%BF%98%E3%80%81%E6%97%A0%E5%87%8F%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="前提" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="使用公积金弥补后仍有亏损的" _mubu_text="%3Cspan%3E%E4%BD%BF%E7%94%A8%E5%85%AC%E7%A7%AF%E9%87%91%E5%BC%A5%E8%A1%A5%E5%90%8E%E4%BB%8D%E6%9C%89%E4%BA%8F%E6%8D%9F%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="对股东" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不得向股东分配，也不得免除股东缴纳出资或股款义务" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%90%91%E8%82%A1%E4%B8%9C%E5%88%86%E9%85%8D%EF%BC%8C%E4%B9%9F%E4%B8%8D%E5%BE%97%E5%85%8D%E9%99%A4%E8%82%A1%E4%B8%9C%E7%BC%B4%E7%BA%B3%E5%87%BA%E8%B5%84%E6%88%96%E8%82%A1%E6%AC%BE%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="对债权人" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="无需通知" _mubu_text="%3Cspan%3E%E6%97%A0%E9%9C%80%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                    <outline text="自作出减资决议起30日内在报纸或国家企业信用信息公示系统公告" _mubu_text="%3Cspan%3E%E8%87%AA%E4%BD%9C%E5%87%BA%E5%87%8F%E8%B5%84%E5%86%B3%E8%AE%AE%E8%B5%B730%E6%97%A5%E5%86%85%E5%9C%A8%E6%8A%A5%E7%BA%B8%E6%88%96%E5%9B%BD%E5%AE%B6%E4%BC%81%E4%B8%9A%E4%BF%A1%E7%94%A8%E4%BF%A1%E6%81%AF%E5%85%AC%E7%A4%BA%E7%B3%BB%E7%BB%9F%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="无债权人保护制度" _mubu_text="%3Cspan%3E%E6%97%A0%E5%80%BA%E6%9D%83%E4%BA%BA%E4%BF%9D%E6%8A%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="公司偿债能力没有下降" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%81%BF%E5%80%BA%E8%83%BD%E5%8A%9B%E6%B2%A1%E6%9C%89%E4%B8%8B%E9%99%8D%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="限制分红" _mubu_text="%3Cspan%3E%E9%99%90%E5%88%B6%E5%88%86%E7%BA%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="在法定和任意公积金累计额达到公司注册资本50%前，不得分配利润" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%B3%95%E5%AE%9A%E5%92%8C%E4%BB%BB%E6%84%8F%E5%85%AC%E7%A7%AF%E9%87%91%E7%B4%AF%E8%AE%A1%E9%A2%9D%E8%BE%BE%E5%88%B0%E5%85%AC%E5%8F%B8%E6%B3%A8%E5%86%8C%E8%B5%84%E6%9C%AC50%25%E5%89%8D%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%88%86%E9%85%8D%E5%88%A9%E6%B6%A6%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="违法减资" _mubu_text="%3Cspan%3E%E8%BF%9D%E6%B3%95%E5%87%8F%E8%B5%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="股东应退还其收到的资金，减免出资的应恢复原状" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E5%BA%94%E9%80%80%E8%BF%98%E5%85%B6%E6%94%B6%E5%88%B0%E7%9A%84%E8%B5%84%E9%87%91%EF%BC%8C%E5%87%8F%E5%85%8D%E5%87%BA%E8%B5%84%E7%9A%84%E5%BA%94%E6%81%A2%E5%A4%8D%E5%8E%9F%E7%8A%B6%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="给公司造成损失，股东及负有责任的董监高应赔偿" _mubu_text="%3Cspan%3E%E7%BB%99%E5%85%AC%E5%8F%B8%E9%80%A0%E6%88%90%E6%8D%9F%E5%A4%B1%EF%BC%8C%E8%82%A1%E4%B8%9C%E5%8F%8A%E8%B4%9F%E6%9C%89%E8%B4%A3%E4%BB%BB%E7%9A%84%E8%91%A3%E7%9B%91%E9%AB%98%E5%BA%94%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="应依法办理变更登记" _mubu_text="%3Cspan%3E%E5%BA%94%E4%BE%9D%E6%B3%95%E5%8A%9E%E7%90%86%E5%8F%98%E6%9B%B4%E7%99%BB%E8%AE%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="合并、分立" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%E3%80%81%E5%88%86%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="形式" _mubu_text="%3Cspan%3E%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="合并" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="吸收：A+B=A" _mubu_text="%3Cspan%3E%E5%90%B8%E6%94%B6%EF%BC%9AA+B=A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="新设：A+B=C" _mubu_text="%3Cspan%3E%E6%96%B0%E8%AE%BE%EF%BC%9AA+B=C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="分立" _mubu_text="%3Cspan%3E%E5%88%86%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="派生：A=A+B" _mubu_text="%3Cspan%3E%E6%B4%BE%E7%94%9F%EF%BC%9AA=A+B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="新设：A=B+C" _mubu_text="%3Cspan%3E%E6%96%B0%E8%AE%BE%EF%BC%9AA=B+C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="决议" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%3C/span%3E" _note="属于重大事项" _mubu_note="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%3C/span%3E">
              <outline text="被合并方无需股东会决议情形" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%90%88%E5%B9%B6%E6%96%B9%E6%97%A0%E9%9C%80%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司与其持股90%以上的公司合并" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E4%B8%8E%E5%85%B6%E6%8C%81%E8%82%A190%25%E4%BB%A5%E4%B8%8A%E7%9A%84%E5%85%AC%E5%8F%B8%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="但应通知其他股东" _mubu_text="%3Cspan%3E%E4%BD%86%E5%BA%94%E9%80%9A%E7%9F%A5%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他股东有权请求公司按合理价格收购其股权或股份" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E6%9C%89%E6%9D%83%E8%AF%B7%E6%B1%82%E5%85%AC%E5%8F%B8%E6%8C%89%E5%90%88%E7%90%86%E4%BB%B7%E6%A0%BC%E6%94%B6%E8%B4%AD%E5%85%B6%E8%82%A1%E6%9D%83%E6%88%96%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="合并主导方无需股东会决议情形" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%E4%B8%BB%E5%AF%BC%E6%96%B9%E6%97%A0%E9%9C%80%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司合并支付的价款不超过本公司净资产10%" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%90%88%E5%B9%B6%E6%94%AF%E4%BB%98%E7%9A%84%E4%BB%B7%E6%AC%BE%E4%B8%8D%E8%B6%85%E8%BF%87%E6%9C%AC%E5%85%AC%E5%8F%B8%E5%87%80%E8%B5%84%E4%BA%A710%25%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
              <outline text="合并不经股东会决议的，应经董事会决议" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%E4%B8%8D%E7%BB%8F%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E7%9A%84%EF%BC%8C%E5%BA%94%E7%BB%8F%E8%91%A3%E4%BA%8B%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="流程" _mubu_text="%3Cspan%3E%E6%B5%81%E7%A8%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="必须编制资产负债表及财产清单" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E7%BC%96%E5%88%B6%E8%B5%84%E4%BA%A7%E8%B4%9F%E5%80%BA%E8%A1%A8%E5%8F%8A%E8%B4%A2%E4%BA%A7%E6%B8%85%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="决议之日起10日内通知债权人，并于30日内在报纸上或国家企业信用信息公示系统公告" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%E4%B9%8B%E6%97%A5%E8%B5%B710%E6%97%A5%E5%86%85%E9%80%9A%E7%9F%A5%E5%80%BA%E6%9D%83%E4%BA%BA%EF%BC%8C%E5%B9%B6%E4%BA%8E30%E6%97%A5%E5%86%85%E5%9C%A8%E6%8A%A5%E7%BA%B8%E4%B8%8A%E6%88%96%E5%9B%BD%E5%AE%B6%E4%BC%81%E4%B8%9A%E4%BF%A1%E7%94%A8%E4%BF%A1%E6%81%AF%E5%85%AC%E7%A4%BA%E7%B3%BB%E7%BB%9F%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="债务人保护制度" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E4%BF%9D%E6%8A%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="合并" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="债权人" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="接到通知书之日起30日内，未接到通知书的自公告之日起45日内" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E4%B9%A6%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%EF%BC%8C%E6%9C%AA%E6%8E%A5%E5%88%B0%E9%80%9A%E7%9F%A5%E4%B9%A6%E7%9A%84%E8%87%AA%E5%85%AC%E5%91%8A%E4%B9%8B%E6%97%A5%E8%B5%B745%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="有权要求公司清偿债务" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9D%83%E8%A6%81%E6%B1%82%E5%85%AC%E5%8F%B8%E6%B8%85%E5%81%BF%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或提供相应的担保" _mubu_text="%3Cspan%3E%E6%88%96%E6%8F%90%E4%BE%9B%E7%9B%B8%E5%BA%94%E7%9A%84%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="各方债权债务由合并后公司继承" _mubu_text="%3Cspan%3E%E5%90%84%E6%96%B9%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E7%94%B1%E5%90%88%E5%B9%B6%E5%90%8E%E5%85%AC%E5%8F%B8%E7%BB%A7%E6%89%BF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公司变更中，只有合并、减资才有债务人保护制度" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%8F%98%E6%9B%B4%E4%B8%AD%EF%BC%8C%E5%8F%AA%E6%9C%89%E5%90%88%E5%B9%B6%E3%80%81%E5%87%8F%E8%B5%84%E6%89%8D%E6%9C%89%E5%80%BA%E5%8A%A1%E4%BA%BA%E4%BF%9D%E6%8A%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="分立" _mubu_text="%3Cspan%3E%E5%88%86%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="没有" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="分立后公司对分立前债务承担连带责任" _mubu_text="%3Cspan%3E%E5%88%86%E7%AB%8B%E5%90%8E%E5%85%AC%E5%8F%B8%E5%AF%B9%E5%88%86%E7%AB%8B%E5%89%8D%E5%80%BA%E5%8A%A1%E6%89%BF%E6%8B%85%E8%BF%9E%E5%B8%A6%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="分立前与债权人有书面另约除外" _mubu_note="%3Cspan%3E%E5%88%86%E7%AB%8B%E5%89%8D%E4%B8%8E%E5%80%BA%E6%9D%83%E4%BA%BA%E6%9C%89%E4%B9%A6%E9%9D%A2%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="不需清算" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%9C%80%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="股东有异议回购申请权" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%9C%89%E5%BC%82%E8%AE%AE%E5%9B%9E%E8%B4%AD%E7%94%B3%E8%AF%B7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="变更形式" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="有限←→股份" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E2%86%90%E2%86%92%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="决议" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%3C/span%3E" _note="属于重大事项" _mubu_note="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%3C/span%3E"/>
            <outline text="有限变股份" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%8F%98%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="符合股份设立条件" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E8%82%A1%E4%BB%BD%E8%AE%BE%E7%AB%8B%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="折合实收股本总额≤公司净资产额" _mubu_text="%3Cspan%3E%E6%8A%98%E5%90%88%E5%AE%9E%E6%94%B6%E8%82%A1%E6%9C%AC%E6%80%BB%E9%A2%9D%E2%89%A4%E5%85%AC%E5%8F%B8%E5%87%80%E8%B5%84%E4%BA%A7%E9%A2%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="相当于非货币出资，估价不得高于原价值" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%BD%93%E4%BA%8E%E9%9D%9E%E8%B4%A7%E5%B8%81%E5%87%BA%E8%B5%84%EF%BC%8C%E4%BC%B0%E4%BB%B7%E4%B8%8D%E5%BE%97%E9%AB%98%E4%BA%8E%E5%8E%9F%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可自行选择低于" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%87%AA%E8%A1%8C%E9%80%89%E6%8B%A9%E4%BD%8E%E4%BA%8E%3C/span%3E" _note="放资本公积金" _mubu_note="%3Cspan%3E%E6%94%BE%E8%B5%84%E6%9C%AC%E5%85%AC%E7%A7%AF%E9%87%91%3C/span%3E"/>
              </outline>
              <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22p0stl2MQdC%22%2C%22oh%22%3A361%2C%22ow%22%3A1000%2C%22uri%22%3A%22document_image%2Fdb180b22-3bfe-44f1-a67e-edc2a60706ce-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="公司解散、清算、注销" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E8%A7%A3%E6%95%A3%E3%80%81%E6%B8%85%E7%AE%97%E3%80%81%E6%B3%A8%E9%94%80%3C/span%3E" _note="" _mubu_note="">
          <outline text="解散" _mubu_text="%3Cspan%3E%E8%A7%A3%E6%95%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="一般解散" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%A7%A3%E6%95%A3%3C/span%3E" _note="" _mubu_note="">
              <outline text="章程规定解散事由" _mubu_text="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="股东(大)会决议" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公司合并或分立" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%90%88%E5%B9%B6%E6%88%96%E5%88%86%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="强制解散" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E8%A7%A3%E6%95%A3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="股东请求法院解散" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E8%AF%B7%E6%B1%82%E6%B3%95%E9%99%A2%E8%A7%A3%E6%95%A3%3C/span%3E" _note="司法解散" _mubu_note="%3Cspan%3E%E5%8F%B8%E6%B3%95%E8%A7%A3%E6%95%A3%3C/span%3E">
              <outline text="经营管理发生严重困难，股东利益将受重大损失" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%90%A5%E7%AE%A1%E7%90%86%E5%8F%91%E7%94%9F%E4%B8%A5%E9%87%8D%E5%9B%B0%E9%9A%BE%EF%BC%8C%E8%82%A1%E4%B8%9C%E5%88%A9%E7%9B%8A%E5%B0%86%E5%8F%97%E9%87%8D%E5%A4%A7%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="持有全部表决权10%以上的股东请求" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E5%85%A8%E9%83%A8%E8%A1%A8%E5%86%B3%E6%9D%8310%25%E4%BB%A5%E4%B8%8A%E7%9A%84%E8%82%A1%E4%B8%9C%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="事由" _mubu_text="%3Cspan%3E%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司持续2年以上无法召开股东(大)会" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%8C%81%E7%BB%AD2%E5%B9%B4%E4%BB%A5%E4%B8%8A%E6%97%A0%E6%B3%95%E5%8F%AC%E5%BC%80%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="持续2年以上不能做出有效股东(大)会决议" _mubu_text="%3Cspan%3E%E6%8C%81%E7%BB%AD2%E5%B9%B4%E4%BB%A5%E4%B8%8A%E4%B8%8D%E8%83%BD%E5%81%9A%E5%87%BA%E6%9C%89%E6%95%88%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公司董事长期冲突，且无法通过股东(大)会解决" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E8%91%A3%E4%BA%8B%E9%95%BF%E6%9C%9F%E5%86%B2%E7%AA%81%EF%BC%8C%E4%B8%94%E6%97%A0%E6%B3%95%E9%80%9A%E8%BF%87%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E8%A7%A3%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不受理事由" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%97%E7%90%86%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="股东以知情权、利润分配请求权等权益受到损害为由" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%BB%A5%E7%9F%A5%E6%83%85%E6%9D%83%E3%80%81%E5%88%A9%E6%B6%A6%E5%88%86%E9%85%8D%E8%AF%B7%E6%B1%82%E6%9D%83%E7%AD%89%E6%9D%83%E7%9B%8A%E5%8F%97%E5%88%B0%E6%8D%9F%E5%AE%B3%E4%B8%BA%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="以公司亏损、财产不足以偿还全部债务为由" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%85%AC%E5%8F%B8%E4%BA%8F%E6%8D%9F%E3%80%81%E8%B4%A2%E4%BA%A7%E4%B8%8D%E8%B6%B3%E4%BB%A5%E5%81%BF%E8%BF%98%E5%85%A8%E9%83%A8%E5%80%BA%E5%8A%A1%E4%B8%BA%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="以公司被吊销企业法人营业执照未进行清算为由" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%85%AC%E5%8F%B8%E8%A2%AB%E5%90%8A%E9%94%80%E4%BC%81%E4%B8%9A%E6%B3%95%E4%BA%BA%E8%90%A5%E4%B8%9A%E6%89%A7%E7%85%A7%E6%9C%AA%E8%BF%9B%E8%A1%8C%E6%B8%85%E7%AE%97%E4%B8%BA%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="诉讼" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="原告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="单独或合计占公司全部表决权10%以上" _mubu_text="%3Cspan%3E%E5%8D%95%E7%8B%AC%E6%88%96%E5%90%88%E8%AE%A1%E5%8D%A0%E5%85%AC%E5%8F%B8%E5%85%A8%E9%83%A8%E8%A1%A8%E5%86%B3%E6%9D%8310%25%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="公司" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="原告以其他股东为被告一并提起诉讼的" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E4%BB%A5%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E4%B8%BA%E8%A2%AB%E5%91%8A%E4%B8%80%E5%B9%B6%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应告知原告将其变更为第三人" _mubu_text="%3Cspan%3E%E5%BA%94%E5%91%8A%E7%9F%A5%E5%8E%9F%E5%91%8A%E5%B0%86%E5%85%B6%E5%8F%98%E6%9B%B4%E4%B8%BA%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="坚持不予变更的，法院应驳回对其他股东的起诉" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%B8%8D%E4%BA%88%E5%8F%98%E6%9B%B4%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E9%A9%B3%E5%9B%9E%E5%AF%B9%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E7%9A%84%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="解散诉讼同时又申请清算的" _mubu_text="%3Cspan%3E%E8%A7%A3%E6%95%A3%E8%AF%89%E8%AE%BC%E5%90%8C%E6%97%B6%E5%8F%88%E7%94%B3%E8%AF%B7%E6%B8%85%E7%AE%97%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法院清算不予受理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%B8%85%E7%AE%97%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="解散诉讼同时又申请财产、证据保全" _mubu_text="%3Cspan%3E%E8%A7%A3%E6%95%A3%E8%AF%89%E8%AE%BC%E5%90%8C%E6%97%B6%E5%8F%88%E7%94%B3%E8%AF%B7%E8%B4%A2%E4%BA%A7%E3%80%81%E8%AF%81%E6%8D%AE%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
                  <outline text="股东提供担保" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="不影响正常经营下，法院可保全" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%AD%A3%E5%B8%B8%E7%BB%8F%E8%90%A5%E4%B8%8B%EF%BC%8C%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="审理解散诉讼应注重调解" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E8%A7%A3%E6%95%A3%E8%AF%89%E8%AE%BC%E5%BA%94%E6%B3%A8%E9%87%8D%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="判决对全体股东有约束力" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E5%AF%B9%E5%85%A8%E4%BD%93%E8%82%A1%E4%B8%9C%E6%9C%89%E7%BA%A6%E6%9D%9F%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="出现上述解散事由，应在10日内将解散事由通过国家企业信用信息公示系统予以公示" _mubu_text="%3Cspan%3E%E5%87%BA%E7%8E%B0%E4%B8%8A%E8%BF%B0%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%EF%BC%8C%E5%BA%94%E5%9C%A810%E6%97%A5%E5%86%85%E5%B0%86%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%E9%80%9A%E8%BF%87%E5%9B%BD%E5%AE%B6%E4%BC%81%E4%B8%9A%E4%BF%A1%E7%94%A8%E4%BF%A1%E6%81%AF%E5%85%AC%E7%A4%BA%E7%B3%BB%E7%BB%9F%E4%BA%88%E4%BB%A5%E5%85%AC%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可挽回情形" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%8C%BD%E5%9B%9E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="因章程规定期限届满或其他解散事由出现" _mubu_text="%3Cspan%3E%E5%9B%A0%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%E6%88%96%E5%85%B6%E4%BB%96%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%E5%87%BA%E7%8E%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="且尚未向股东分配财产" _mubu_text="%3Cspan%3E%E4%B8%94%E5%B0%9A%E6%9C%AA%E5%90%91%E8%82%A1%E4%B8%9C%E5%88%86%E9%85%8D%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="通过修改章程或经股东会决议而存续" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E4%BF%AE%E6%94%B9%E7%AB%A0%E7%A8%8B%E6%88%96%E7%BB%8F%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E8%80%8C%E5%AD%98%E7%BB%AD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="通过比例为2/3以上表决" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E6%AF%94%E4%BE%8B%E4%B8%BA2/3%E4%BB%A5%E4%B8%8A%E8%A1%A8%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="清算" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
            <outline text="概述" _mubu_text="%3Cspan%3E%E6%A6%82%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="概念" _mubu_text="%3Cspan%3E%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
                <outline text="算是公司已经出现解散事由，了结其全部债权债务、业务，终结公司的一切法律关系，处理公司剩余财产的程序" _mubu_text="%3Cspan%3E%E7%AE%97%E6%98%AF%E5%85%AC%E5%8F%B8%E5%B7%B2%E7%BB%8F%E5%87%BA%E7%8E%B0%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%EF%BC%8C%E4%BA%86%E7%BB%93%E5%85%B6%E5%85%A8%E9%83%A8%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E3%80%81%E4%B8%9A%E5%8A%A1%EF%BC%8C%E7%BB%88%E7%BB%93%E5%85%AC%E5%8F%B8%E7%9A%84%E4%B8%80%E5%88%87%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%EF%BC%8C%E5%A4%84%E7%90%86%E5%85%AC%E5%8F%B8%E5%89%A9%E4%BD%99%E8%B4%A2%E4%BA%A7%E7%9A%84%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="除公司因合并或分立导致的解散，以及因破产适用破产清算程序外，因其他原因解散的公司都应当进行清算" _mubu_text="%3Cspan%3E%E9%99%A4%E5%85%AC%E5%8F%B8%E5%9B%A0%E5%90%88%E5%B9%B6%E6%88%96%E5%88%86%E7%AB%8B%E5%AF%BC%E8%87%B4%E7%9A%84%E8%A7%A3%E6%95%A3%EF%BC%8C%E4%BB%A5%E5%8F%8A%E5%9B%A0%E7%A0%B4%E4%BA%A7%E9%80%82%E7%94%A8%E7%A0%B4%E4%BA%A7%E6%B8%85%E7%AE%97%E7%A8%8B%E5%BA%8F%E5%A4%96%EF%BC%8C%E5%9B%A0%E5%85%B6%E4%BB%96%E5%8E%9F%E5%9B%A0%E8%A7%A3%E6%95%A3%E7%9A%84%E5%85%AC%E5%8F%B8%E9%83%BD%E5%BA%94%E5%BD%93%E8%BF%9B%E8%A1%8C%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="清算期间" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="清算期间，公司仍然存续，但不得开展与清算无关的经营活动" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E6%9C%9F%E9%97%B4%EF%BC%8C%E5%85%AC%E5%8F%B8%E4%BB%8D%E7%84%B6%E5%AD%98%E7%BB%AD%EF%BC%8C%E4%BD%86%E4%B8%8D%E5%BE%97%E5%BC%80%E5%B1%95%E4%B8%8E%E6%B8%85%E7%AE%97%E6%97%A0%E5%85%B3%E7%9A%84%E7%BB%8F%E8%90%A5%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="类别" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
              <outline text="自行清算" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
                <outline text="解散事由出现之日起15日内" _mubu_text="%3Cspan%3E%E8%A7%A3%E6%95%A3%E4%BA%8B%E7%94%B1%E5%87%BA%E7%8E%B0%E4%B9%8B%E6%97%A5%E8%B5%B715%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="清算组的组成" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E7%BB%84%E7%9A%84%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="由董事组成，董事即清算义务人" _mubu_text="%3Cspan%3E%E7%94%B1%E8%91%A3%E4%BA%8B%E7%BB%84%E6%88%90%EF%BC%8C%E8%91%A3%E4%BA%8B%E5%8D%B3%E6%B8%85%E7%AE%97%E4%B9%89%E5%8A%A1%E4%BA%BA%3C/span%3E" _note="章程另规或股东会决议另选他人的除外" _mubu_note="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E5%8F%A6%E8%A7%84%E6%88%96%E8%82%A1%E4%B8%9C%E4%BC%9A%E5%86%B3%E8%AE%AE%E5%8F%A6%E9%80%89%E4%BB%96%E4%BA%BA%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                </outline>
                <outline text="责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                  <outline text="清算义务人未及时履行清算义务，给公司或债权人造成损失，应赔偿" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E4%B9%89%E5%8A%A1%E4%BA%BA%E6%9C%AA%E5%8F%8A%E6%97%B6%E5%B1%A5%E8%A1%8C%E6%B8%85%E7%AE%97%E4%B9%89%E5%8A%A1%EF%BC%8C%E7%BB%99%E5%85%AC%E5%8F%B8%E6%88%96%E5%80%BA%E6%9D%83%E4%BA%BA%E9%80%A0%E6%88%90%E6%8D%9F%E5%A4%B1%EF%BC%8C%E5%BA%94%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22NVeueTkEtP%22%2C%22oh%22%3A266%2C%22ow%22%3A669%2C%22uri%22%3A%22document_image%2F3c6f1410-0d64-4c91-a96b-79aefab17311-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
              </outline>
              <outline text="法院指定清算" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%8C%87%E5%AE%9A%E6%B8%85%E7%AE%97%3C/span%3E" _note="强制清算" _mubu_note="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%B8%85%E7%AE%97%3C/span%3E">
                <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="利害关系人申请" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
                    <outline text="逾期不成立清算组或成立后不清算" _mubu_text="%3Cspan%3E%E9%80%BE%E6%9C%9F%E4%B8%8D%E6%88%90%E7%AB%8B%E6%B8%85%E7%AE%97%E7%BB%84%E6%88%96%E6%88%90%E7%AB%8B%E5%90%8E%E4%B8%8D%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="清算组故意拖延清算" _mubu_text="%3Cspan%3E%E6%B8%85%E7%AE%97%E7%BB%84%E6%95%85%E6%84%8F%E6%8B%96%E5%BB%B6%E6%B8%85%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
