                <outline text="提议主体" _mubu_text="%3Cspan%3E%E6%8F%90%E8%AE%AE%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
                  <outline text="无顺序" _mubu_text="%3Cspan%3E%E6%97%A0%E9%A1%BA%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
