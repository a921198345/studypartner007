              <outline text="在约定的使用范围内" _mubu_text="%3Cspan%3E%E5%9C%A8%E7%BA%A6%E5%AE%9A%E7%9A%84%E4%BD%BF%E7%94%A8%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无约" _mubu_text="%3Cspan%3E%E6%97%A0%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="在委托创作的特定目的范围内免费使用" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%A7%94%E6%89%98%E5%88%9B%E4%BD%9C%E7%9A%84%E7%89%B9%E5%AE%9A%E7%9B%AE%E7%9A%84%E8%8C%83%E5%9B%B4%E5%86%85%E5%85%8D%E8%B4%B9%E4%BD%BF%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="视听作品" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%A7%86%E5%90%AC%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="电影、电视剧作品" _mubu_text="%3Cspan%3E%E7%94%B5%E5%BD%B1%E3%80%81%E7%94%B5%E8%A7%86%E5%89%A7%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="著作权" _mubu_text="%3Cspan%3E%E8%91%97%E4%BD%9C%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="制作者享有" _mubu_text="%3Cspan%3E%E5%88%B6%E4%BD%9C%E8%80%85%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="享有署名权" _mubu_text="%3Cspan%3E%E4%BA%AB%E6%9C%89%E7%BD%B2%E5%90%8D%E6%9D%83%3C/span%3E" _note="和获酬权" _mubu_note="%3Cspan%3E%E5%92%8C%E8%8E%B7%E9%85%AC%E6%9D%83%3C/span%3E">
                <outline text="编剧、导演、摄影、作词、作曲等作者" _mubu_text="%3Cspan%3E%E7%BC%96%E5%89%A7%E3%80%81%E5%AF%BC%E6%BC%94%E3%80%81%E6%91%84%E5%BD%B1%E3%80%81%E4%BD%9C%E8%AF%8D%E3%80%81%E4%BD%9C%E6%9B%B2%E7%AD%89%E4%BD%9C%E8%80%85%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="其他视听作品" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%A7%86%E5%90%AC%E4%BD%9C%E5%93%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="有约从约" _mubu_text="%3Cspan%3E%E6%9C%89%E7%BA%A6%E4%BB%8E%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无约" _mubu_text="%3Cspan%3E%E6%97%A0%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="制作者享有" _mubu_text="%3Cspan%3E%E5%88%B6%E4%BD%9C%E8%80%85%E4%BA%AB%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="作者享有署名权和获酬权" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%80%85%E4%BA%AB%E6%9C%89%E7%BD%B2%E5%90%8D%E6%9D%83%E5%92%8C%E8%8E%B7%E9%85%AC%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="视听作品中的剧本、音乐等" _mubu_text="%3Cspan%3E%E8%A7%86%E5%90%AC%E4%BD%9C%E5%93%81%E4%B8%AD%E7%9A%84%E5%89%A7%E6%9C%AC%E3%80%81%E9%9F%B3%E4%B9%90%E7%AD%89%3C/span%3E" _note="" _mubu_note="">
