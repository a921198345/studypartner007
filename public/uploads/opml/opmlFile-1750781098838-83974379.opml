            <outline text="内部关系" _mubu_text="%3Cspan%3E%E5%86%85%E9%83%A8%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="股东权利归实际不归名义" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%E5%BD%92%E5%AE%9E%E9%99%85%E4%B8%8D%E5%BD%92%E5%90%8D%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="实际出资人不能直接向公司主张权利" _mubu_text="%3Cspan%3E%E5%AE%9E%E9%99%85%E5%87%BA%E8%B5%84%E4%BA%BA%E4%B8%8D%E8%83%BD%E7%9B%B4%E6%8E%A5%E5%90%91%E5%85%AC%E5%8F%B8%E4%B8%BB%E5%BC%A0%E6%9D%83%E5%88%A9%3C/span%3E" _note="通过名义股东间接主张" _mubu_note="%3Cspan%3E%E9%80%9A%E8%BF%87%E5%90%8D%E4%B9%89%E8%82%A1%E4%B8%9C%E9%97%B4%E6%8E%A5%E4%B8%BB%E5%BC%A0%3C/span%3E"/>
            </outline>
            <outline text="外部关系" _mubu_text="%3Cspan%3E%E5%A4%96%E9%83%A8%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="名义股东擅自处分" _mubu_text="%3Cspan%3E%E5%90%8D%E4%B9%89%E8%82%A1%E4%B8%9C%E6%93%85%E8%87%AA%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="参照适用善意取得制度" _mubu_text="%3Cspan%3E%E5%8F%82%E7%85%A7%E9%80%82%E7%94%A8%E5%96%84%E6%84%8F%E5%8F%96%E5%BE%97%E5%88%B6%E5%BA%A6%3C/span%3E" _note="但命题人认为名义股东为有权处分" _mubu_note="%3Cspan%3E%E4%BD%86%E5%91%BD%E9%A2%98%E4%BA%BA%E8%AE%A4%E4%B8%BA%E5%90%8D%E4%B9%89%E8%82%A1%E4%B8%9C%E4%B8%BA%E6%9C%89%E6%9D%83%E5%A4%84%E5%88%86%3C/span%3E"/>
                <outline text="造成实际出资人损失的，承担赔偿责任" _mubu_text="%3Cspan%3E%E9%80%A0%E6%88%90%E5%AE%9E%E9%99%85%E5%87%BA%E8%B5%84%E4%BA%BA%E6%8D%9F%E5%A4%B1%E7%9A%84%EF%BC%8C%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="对外责任" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="名义股东对公司债权人承担补充赔偿责任" _mubu_text="%3Cspan%3E%E5%90%8D%E4%B9%89%E8%82%A1%E4%B8%9C%E5%AF%B9%E5%85%AC%E5%8F%B8%E5%80%BA%E6%9D%83%E4%BA%BA%E6%89%BF%E6%8B%85%E8%A1%A5%E5%85%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="不能以其为名义股东抗辩" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E4%BB%A5%E5%85%B6%E4%B8%BA%E5%90%8D%E4%B9%89%E8%82%A1%E4%B8%9C%E6%8A%97%E8%BE%A9%3C/span%3E"/>
                <outline text="有权向实际出资人追偿" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9D%83%E5%90%91%E5%AE%9E%E9%99%85%E5%87%BA%E8%B5%84%E4%BA%BA%E8%BF%BD%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="冒名股东" _mubu_text="%3Cspan%3E%E5%86%92%E5%90%8D%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="一切由冒名人承担" _mubu_text="%3Cspan%3E%E4%B8%80%E5%88%87%E7%94%B1%E5%86%92%E5%90%8D%E4%BA%BA%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被冒名者不担责也无权" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%86%92%E5%90%8D%E8%80%85%E4%B8%8D%E6%8B%85%E8%B4%A3%E4%B9%9F%E6%97%A0%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="股东权利" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="收益分配权" _mubu_text="%3Cspan%3E%E6%94%B6%E7%9B%8A%E5%88%86%E9%85%8D%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="分配依据" _mubu_text="%3Cspan%3E%E5%88%86%E9%85%8D%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="有限" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="按实缴的出资比例分红，另约除外" _mubu_text="%3Cspan%3E%E6%8C%89%E5%AE%9E%E7%BC%B4%E7%9A%84%E5%87%BA%E8%B5%84%E6%AF%94%E4%BE%8B%E5%88%86%E7%BA%A2%EF%BC%8C%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="股份" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
                  <outline text="按股东持有的股份比例分配，另约除外" _mubu_text="%3Cspan%3E%E6%8C%89%E8%82%A1%E4%B8%9C%E6%8C%81%E6%9C%89%E7%9A%84%E8%82%A1%E4%BB%BD%E6%AF%94%E4%BE%8B%E5%88%86%E9%85%8D%EF%BC%8C%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E" _note="认缴比例" _mubu_note="%3Cspan%3E%E8%AE%A4%E7%BC%B4%E6%AF%94%E4%BE%8B%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="诉讼" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="列公司为被告" _mubu_text="%3Cspan%3E%E5%88%97%E5%85%AC%E5%8F%B8%E4%B8%BA%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="载明具体分配方案的决议" _mubu_text="%3Cspan%3E%E8%BD%BD%E6%98%8E%E5%85%B7%E4%BD%93%E5%88%86%E9%85%8D%E6%96%B9%E6%A1%88%E7%9A%84%E5%86%B3%E8%AE%AE%3C/span%3E" _note="股东(大)会决议" _mubu_note="%3Cspan%3E%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E5%86%B3%E8%AE%AE%3C/span%3E">
                  <outline text="提交的支持" _mubu_text="%3Cspan%3E%E6%8F%90%E4%BA%A4%E7%9A%84%E6%94%AF%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="未提交的不支持" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%8F%90%E4%BA%A4%E7%9A%84%E4%B8%8D%E6%94%AF%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="强制分红制度" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E5%88%86%E7%BA%A2%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="违反法规滥用股东权利致公司不分配利润，给其他股东造成损失的" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E6%B3%95%E8%A7%84%E6%BB%A5%E7%94%A8%E8%82%A1%E4%B8%9C%E6%9D%83%E5%88%A9%E8%87%B4%E5%85%AC%E5%8F%B8%E4%B8%8D%E5%88%86%E9%85%8D%E5%88%A9%E6%B6%A6%EF%BC%8C%E7%BB%99%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%E9%80%A0%E6%88%90%E6%8D%9F%E5%A4%B1%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="分配时间" _mubu_text="%3Cspan%3E%E5%88%86%E9%85%8D%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="股东会作出分配利润的决议" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%BC%9A%E4%BD%9C%E5%87%BA%E5%88%86%E9%85%8D%E5%88%A9%E6%B6%A6%E7%9A%84%E5%86%B3%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="董事会应在决议作出之日起6个月内进行分配" _mubu_text="%3Cspan%3E%E8%91%A3%E4%BA%8B%E4%BC%9A%E5%BA%94%E5%9C%A8%E5%86%B3%E8%AE%AE%E4%BD%9C%E5%87%BA%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%E5%86%85%E8%BF%9B%E8%A1%8C%E5%88%86%E9%85%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="优先认购权" _mubu_text="%3Cspan%3E%E4%BC%98%E5%85%88%E8%AE%A4%E8%B4%AD%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="相比于外人有限" _mubu_text="%3Cspan%3E%E7%9B%B8%E6%AF%94%E4%BA%8E%E5%A4%96%E4%BA%BA%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有限" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="新增资本时" _mubu_text="%3Cspan%3E%E6%96%B0%E5%A2%9E%E8%B5%84%E6%9C%AC%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="在同等条件下" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%90%8C%E7%AD%89%E6%9D%A1%E4%BB%B6%E4%B8%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="按实缴出资比例认缴出资" _mubu_text="%3Cspan%3E%E6%8C%89%E5%AE%9E%E7%BC%B4%E5%87%BA%E8%B5%84%E6%AF%94%E4%BE%8B%E8%AE%A4%E7%BC%B4%E5%87%BA%E8%B5%84%3C/span%3E" _note="维护原有人合性" _mubu_note="%3Cspan%3E%E7%BB%B4%E6%8A%A4%E5%8E%9F%E6%9C%89%E4%BA%BA%E5%90%88%E6%80%A7%3C/span%3E">
                  <outline text="另约除外" _mubu_text="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="股份" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
                <outline text="无优先认购权" _mubu_text="%3Cspan%3E%E6%97%A0%E4%BC%98%E5%85%88%E8%AE%A4%E8%B4%AD%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="知情权" _mubu_text="%3Cspan%3E%E7%9F%A5%E6%83%85%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="基本规定" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="对一般文件的查阅与复制" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%B8%80%E8%88%AC%E6%96%87%E4%BB%B6%E7%9A%84%E6%9F%A5%E9%98%85%E4%B8%8E%E5%A4%8D%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="本公司及全资子公司的" _mubu_text="%3Cspan%3E%E6%9C%AC%E5%85%AC%E5%8F%B8%E5%8F%8A%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="章程、股东名册、股东会会议记录、董事会会议决议、监事会会议决议、财务会计报告" _mubu_text="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E3%80%81%E8%82%A1%E4%B8%9C%E5%90%8D%E5%86%8C%E3%80%81%E8%82%A1%E4%B8%9C%E4%BC%9A%E4%BC%9A%E8%AE%AE%E8%AE%B0%E5%BD%95%E3%80%81%E8%91%A3%E4%BA%8B%E4%BC%9A%E4%BC%9A%E8%AE%AE%E5%86%B3%E8%AE%AE%E3%80%81%E7%9B%91%E4%BA%8B%E4%BC%9A%E4%BC%9A%E8%AE%AE%E5%86%B3%E8%AE%AE%E3%80%81%E8%B4%A2%E5%8A%A1%E4%BC%9A%E8%AE%A1%E6%8A%A5%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="对会计账簿和凭证的查阅" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BC%9A%E8%AE%A1%E8%B4%A6%E7%B0%BF%E5%92%8C%E5%87%AD%E8%AF%81%E7%9A%84%E6%9F%A5%E9%98%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="有限公司" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                    <outline text="任意股东" _mubu_text="%3Cspan%3E%E4%BB%BB%E6%84%8F%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="股份公司" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note="">
                    <outline text="连续180日持股" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD180%E6%97%A5%E6%8C%81%E8%82%A1%3C/span%3E" _note="" _mubu_note="">
                      <outline text="章程对持股比例有较低规定的从其规定" _mubu_text="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E5%AF%B9%E6%8C%81%E8%82%A1%E6%AF%94%E4%BE%8B%E6%9C%89%E8%BE%83%E4%BD%8E%E8%A7%84%E5%AE%9A%E7%9A%84%E4%BB%8E%E5%85%B6%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="且单独或合计持股比例3％以上的" _mubu_text="%3Cspan%3E%E4%B8%94%E5%8D%95%E7%8B%AC%E6%88%96%E5%90%88%E8%AE%A1%E6%8C%81%E8%82%A1%E6%AF%94%E4%BE%8B3%EF%BC%85%E4%BB%A5%E4%B8%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="书面请求" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不正当理由" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="查阅会计账簿、凭证" _mubu_text="%3Cspan%3E%E6%9F%A5%E9%98%85%E4%BC%9A%E8%AE%A1%E8%B4%A6%E7%B0%BF%E3%80%81%E5%87%AD%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="公司可拒绝，15日内说明" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E5%8F%AF%E6%8B%92%E7%BB%9D%EF%BC%8C15%E6%97%A5%E5%86%85%E8%AF%B4%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="股东有实质性竞争关系的业务的" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E6%9C%89%E5%AE%9E%E8%B4%A8%E6%80%A7%E7%AB%9E%E4%BA%89%E5%85%B3%E7%B3%BB%E7%9A%84%E4%B8%9A%E5%8A%A1%E7%9A%84%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
                  <outline text="为了向他人通报有关信息" _mubu_text="%3Cspan%3E%E4%B8%BA%E4%BA%86%E5%90%91%E4%BB%96%E4%BA%BA%E9%80%9A%E6%8A%A5%E6%9C%89%E5%85%B3%E4%BF%A1%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="前三年内，曾通过查阅会计账簿，向他人通报有关信息损害公司" _mubu_text="%3Cspan%3E%E5%89%8D%E4%B8%89%E5%B9%B4%E5%86%85%EF%BC%8C%E6%9B%BE%E9%80%9A%E8%BF%87%E6%9F%A5%E9%98%85%E4%BC%9A%E8%AE%A1%E8%B4%A6%E7%B0%BF%EF%BC%8C%E5%90%91%E4%BB%96%E4%BA%BA%E9%80%9A%E6%8A%A5%E6%9C%89%E5%85%B3%E4%BF%A1%E6%81%AF%E6%8D%9F%E5%AE%B3%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="不得约定、变相实质性剥夺此权利" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E7%BA%A6%E5%AE%9A%E3%80%81%E5%8F%98%E7%9B%B8%E5%AE%9E%E8%B4%A8%E6%80%A7%E5%89%A5%E5%A4%BA%E6%AD%A4%E6%9D%83%E5%88%A9%3C/span%3E" _note="自我放弃后可追回" _mubu_note="%3Cspan%3E%E8%87%AA%E6%88%91%E6%94%BE%E5%BC%83%E5%90%8E%E5%8F%AF%E8%BF%BD%E5%9B%9E%3C/span%3E"/>
              <outline text="诉讼" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：起诉时有股东资格" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E8%B5%B7%E8%AF%89%E6%97%B6%E6%9C%89%E8%82%A1%E4%B8%9C%E8%B5%84%E6%A0%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="有初步证明" _mubu_text="%3Cspan%3E%E6%9C%89%E5%88%9D%E6%AD%A5%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="持股期间合法权益受损" _mubu_text="%3Cspan%3E%E6%8C%81%E8%82%A1%E6%9C%9F%E9%97%B4%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%E5%8F%97%E6%8D%9F%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="请求查阅复制其持股期间文件材料" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E6%9F%A5%E9%98%85%E5%A4%8D%E5%88%B6%E5%85%B6%E6%8C%81%E8%82%A1%E6%9C%9F%E9%97%B4%E6%96%87%E4%BB%B6%E6%9D%90%E6%96%99%3C/span%3E" _note="只能翻旧账" _mubu_note="%3Cspan%3E%E5%8F%AA%E8%83%BD%E7%BF%BB%E6%97%A7%E8%B4%A6%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="委托中介机构" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%B8%AD%E4%BB%8B%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="由会计师、律师等负有保密义务的执业人员依规进行" _mubu_text="%3Cspan%3E%E7%94%B1%E4%BC%9A%E8%AE%A1%E5%B8%88%E3%80%81%E5%BE%8B%E5%B8%88%E7%AD%89%E8%B4%9F%E6%9C%89%E4%BF%9D%E5%AF%86%E4%B9%89%E5%8A%A1%E7%9A%84%E6%89%A7%E4%B8%9A%E4%BA%BA%E5%91%98%E4%BE%9D%E8%A7%84%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不得泄露公司商业秘密" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%B3%84%E9%9C%B2%E5%85%AC%E5%8F%B8%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="谁泄露谁赔偿" _mubu_note="%3Cspan%3E%E8%B0%81%E6%B3%84%E9%9C%B2%E8%B0%81%E8%B5%94%E5%81%BF%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="股东代表诉讼制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%82%A1%E4%B8%9C%E4%BB%A3%E8%A1%A8%E8%AF%89%E8%AE%BC%E5%88%B6%E5%BA%A6%3C/span%3E" _note="派生诉讼、股东代位诉讼、间接诉讼" _mubu_note="%3Cspan%3E%E6%B4%BE%E7%94%9F%E8%AF%89%E8%AE%BC%E3%80%81%E8%82%A1%E4%B8%9C%E4%BB%A3%E4%BD%8D%E8%AF%89%E8%AE%BC%E3%80%81%E9%97%B4%E6%8E%A5%E8%AF%89%E8%AE%BC%3C/span%3E">
            <outline text="起因" _mubu_text="%3Cspan%3E%E8%B5%B7%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="本公司或全资子公司利益受损" _mubu_text="%3Cspan%3E%E6%9C%AC%E5%85%AC%E5%8F%B8%E6%88%96%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%E5%88%A9%E7%9B%8A%E5%8F%97%E6%8D%9F%3C/span%3E" _note="而非股东直接利益受损" _mubu_note="%3Cspan%3E%E8%80%8C%E9%9D%9E%E8%82%A1%E4%B8%9C%E7%9B%B4%E6%8E%A5%E5%88%A9%E7%9B%8A%E5%8F%97%E6%8D%9F%3C/span%3E"/>
            </outline>
            <outline text="原告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="有限" _mubu_text="%3Cspan%3E%E6%9C%89%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="任意股东" _mubu_text="%3Cspan%3E%E4%BB%BB%E6%84%8F%E8%82%A1%E4%B8%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="股份" _mubu_text="%3Cspan%3E%E8%82%A1%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
                <outline text="连续180日持股" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD180%E6%97%A5%E6%8C%81%E8%82%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="且单独或合计持股比例1％以上的" _mubu_text="%3Cspan%3E%E4%B8%94%E5%8D%95%E7%8B%AC%E6%88%96%E5%90%88%E8%AE%A1%E6%8C%81%E8%82%A1%E6%AF%94%E4%BE%8B1%EF%BC%85%E4%BB%A5%E4%B8%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="起诉时具有股东资格" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E6%97%B6%E5%85%B7%E6%9C%89%E8%82%A1%E4%B8%9C%E8%B5%84%E6%A0%BC%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以股东自己名义" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%82%A1%E4%B8%9C%E8%87%AA%E5%B7%B1%E5%90%8D%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="致损方，包括公司和全资子公司的董监高及他人" _mubu_text="%3Cspan%3E%E8%87%B4%E6%8D%9F%E6%96%B9%EF%BC%8C%E5%8C%85%E6%8B%AC%E5%85%AC%E5%8F%B8%E5%92%8C%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%E7%9A%84%E8%91%A3%E7%9B%91%E9%AB%98%E5%8F%8A%E4%BB%96%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="第三人" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="公司或全资子公司" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%88%96%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="反诉" _mubu_text="%3Cspan%3E%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="以他人为被告的代表诉讼中" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%BB%96%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%E7%9A%84%E4%BB%A3%E8%A1%A8%E8%AF%89%E8%AE%BC%E4%B8%AD%3C/span%3E" _note="董监高以外" _mubu_note="%3Cspan%3E%E8%91%A3%E7%9B%91%E9%AB%98%E4%BB%A5%E5%A4%96%3C/span%3E"/>
              <outline text="应受理" _mubu_text="%3Cspan%3E%E5%BA%94%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="以原告股东恶意起诉为由提起" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%8E%9F%E5%91%8A%E8%82%A1%E4%B8%9C%E6%81%B6%E6%84%8F%E8%B5%B7%E8%AF%89%E4%B8%BA%E7%94%B1%E6%8F%90%E8%B5%B7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不受理" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="以公司在案涉纠纷中应当承担侵权或者违约等责任为由对公司提出" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%85%AC%E5%8F%B8%E5%9C%A8%E6%A1%88%E6%B6%89%E7%BA%A0%E7%BA%B7%E4%B8%AD%E5%BA%94%E5%BD%93%E6%89%BF%E6%8B%85%E4%BE%B5%E6%9D%83%E6%88%96%E8%80%85%E8%BF%9D%E7%BA%A6%E7%AD%89%E8%B4%A3%E4%BB%BB%E4%B8%BA%E7%94%B1%E5%AF%B9%E5%85%AC%E5%8F%B8%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="前置程序" _mubu_text="%3Cspan%3E%E5%89%8D%E7%BD%AE%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="股东书面请求公司内部相应机构" _mubu_text="%3Cspan%3E%E8%82%A1%E4%B8%9C%E4%B9%A6%E9%9D%A2%E8%AF%B7%E6%B1%82%E5%85%AC%E5%8F%B8%E5%86%85%E9%83%A8%E7%9B%B8%E5%BA%94%E6%9C%BA%E6%9E%84%3C/span%3E" _note="内部救济途径" _mubu_note="%3Cspan%3E%E5%86%85%E9%83%A8%E6%95%91%E6%B5%8E%E9%80%94%E5%BE%84%3C/span%3E">
                <outline text="交叉请求" _mubu_text="%3Cspan%3E%E4%BA%A4%E5%8F%89%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                  <outline text="董、高→监事(会)" _mubu_text="%3Cspan%3E%E8%91%A3%E3%80%81%E9%AB%98%E2%86%92%E7%9B%91%E4%BA%8B(%E4%BC%9A)%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="监事、他人→(执行)董事(会)" _mubu_text="%3Cspan%3E%E7%9B%91%E4%BA%8B%E3%80%81%E4%BB%96%E4%BA%BA%E2%86%92(%E6%89%A7%E8%A1%8C)%E8%91%A3%E4%BA%8B(%E4%BC%9A)%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="机构收到书面请求后" _mubu_text="%3Cspan%3E%E6%9C%BA%E6%9E%84%E6%94%B6%E5%88%B0%E4%B9%A6%E9%9D%A2%E8%AF%B7%E6%B1%82%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="遇三种情况" _mubu_text="%3Cspan%3E%E9%81%87%E4%B8%89%E7%A7%8D%E6%83%85%E5%86%B5%3C/span%3E" _note="" _mubu_note="">
                <outline text="拒绝提起诉讼" _mubu_text="%3Cspan%3E%E6%8B%92%E7%BB%9D%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="30日内未提起诉讼" _mubu_text="%3Cspan%3E30%E6%97%A5%E5%86%85%E6%9C%AA%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="情况紧急，不立即提起将使公司利益受难以弥补损害" _mubu_text="%3Cspan%3E%E6%83%85%E5%86%B5%E7%B4%A7%E6%80%A5%EF%BC%8C%E4%B8%8D%E7%AB%8B%E5%8D%B3%E6%8F%90%E8%B5%B7%E5%B0%86%E4%BD%BF%E5%85%AC%E5%8F%B8%E5%88%A9%E7%9B%8A%E5%8F%97%E9%9A%BE%E4%BB%A5%E5%BC%A5%E8%A1%A5%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="公司有关机关不存在起诉可能性时，豁免前置程序" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%9C%89%E5%85%B3%E6%9C%BA%E5%85%B3%E4%B8%8D%E5%AD%98%E5%9C%A8%E8%B5%B7%E8%AF%89%E5%8F%AF%E8%83%BD%E6%80%A7%E6%97%B6%EF%BC%8C%E8%B1%81%E5%85%8D%E5%89%8D%E7%BD%AE%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="如两方都侵害时" _mubu_note="%3Cspan%3E%E5%A6%82%E4%B8%A4%E6%96%B9%E9%83%BD%E4%BE%B5%E5%AE%B3%E6%97%B6%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="利益归属" _mubu_text="%3Cspan%3E%E5%88%A9%E7%9B%8A%E5%BD%92%E5%B1%9E%3C/span%3E" _note="" _mubu_note="">
              <outline text="所得利益归公司或全资子公司" _mubu_text="%3Cspan%3E%E6%89%80%E5%BE%97%E5%88%A9%E7%9B%8A%E5%BD%92%E5%85%AC%E5%8F%B8%E6%88%96%E5%85%A8%E8%B5%84%E5%AD%90%E5%85%AC%E5%8F%B8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="合理费用" _mubu_text="%3Cspan%3E%E5%90%88%E7%90%86%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="部分/全部得到法院支持，公司应当承担" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86/%E5%85%A8%E9%83%A8%E5%BE%97%E5%88%B0%E6%B3%95%E9%99%A2%E6%94%AF%E6%8C%81%EF%BC%8C%E5%85%AC%E5%8F%B8%E5%BA%94%E5%BD%93%E6%89%BF%E6%8B%85%3C/span%3E" _note="败诉则不承担" _mubu_note="%3Cspan%3E%E8%B4%A5%E8%AF%89%E5%88%99%E4%B8%8D%E6%89%BF%E6%8B%85%3C/span%3E"/>
            </outline>
            <outline text="调解" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
              <outline text="调解协议经公司股东(大)会、董事会决议通过后" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E7%BB%8F%E5%85%AC%E5%8F%B8%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E3%80%81%E8%91%A3%E4%BA%8B%E4%BC%9A%E5%86%B3%E8%AE%AE%E9%80%9A%E8%BF%87%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="决议机关，章程规定" _mubu_text="%3Cspan%3E%E5%86%B3%E8%AE%AE%E6%9C%BA%E5%85%B3%EF%BC%8C%E7%AB%A0%E7%A8%8B%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="章程没有规定，以股东(大)会为决议机关" _mubu_text="%3Cspan%3E%E7%AB%A0%E7%A8%8B%E6%B2%A1%E6%9C%89%E8%A7%84%E5%AE%9A%EF%BC%8C%E4%BB%A5%E8%82%A1%E4%B8%9C(%E5%A4%A7)%E4%BC%9A%E4%B8%BA%E5%86%B3%E8%AE%AE%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
