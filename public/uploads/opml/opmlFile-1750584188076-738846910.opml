            <outline text="同级找同级，是幹旋受贿；" _mubu_text="%3Cspan%3E%E5%90%8C%E7%BA%A7%E6%89%BE%E5%90%8C%E7%BA%A7%EF%BC%8C%E6%98%AF%E5%B9%B9%E6%97%8B%E5%8F%97%E8%B4%BF%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="下级找上级，是斡旋受贿。" _mubu_text="%3Cspan%3E%E4%B8%8B%E7%BA%A7%E6%89%BE%E4%B8%8A%E7%BA%A7%EF%BC%8C%E6%98%AF%E6%96%A1%E6%97%8B%E5%8F%97%E8%B4%BF%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
