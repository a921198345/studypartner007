        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22rPVyTuZGuJ%22%2C%22oh%22%3A1000%2C%22ow%22%3A2292%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F917612A56C3E45871717544845.jpg%22%2C%22w%22%3A400%7D%5D"/>
