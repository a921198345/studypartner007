      <outline text="05 回避" _mubu_text="%3Cspan%3E05%20%E5%9B%9E%E9%81%BF%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="理由与种类" _mubu_text="%3Cspan%3E%E7%90%86%E7%94%B1%E4%B8%8E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="适用人员" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
            <outline text="司法警察回避：检察院适用，法院不适用" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E8%AD%A6%E5%AF%9F%E5%9B%9E%E9%81%BF%EF%BC%9A%E6%A3%80%E5%AF%9F%E9%99%A2%E9%80%82%E7%94%A8%EF%BC%8C%E6%B3%95%E9%99%A2%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有专门知识的人回避：公诉人聘请的适用" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%93%E9%97%A8%E7%9F%A5%E8%AF%86%E7%9A%84%E4%BA%BA%E5%9B%9E%E9%81%BF%EF%BC%9A%E5%85%AC%E8%AF%89%E4%BA%BA%E8%81%98%E8%AF%B7%E7%9A%84%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="证人不适用" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="辩护人、诉讼代理人不适用" _mubu_text="%3Cspan%3E%E8%BE%A9%E6%8A%A4%E4%BA%BA%E3%80%81%E8%AF%89%E8%AE%BC%E4%BB%A3%E7%90%86%E4%BA%BA%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="我国无整体回避" _mubu_text="%3Cspan%3E%E6%88%91%E5%9B%BD%E6%97%A0%E6%95%B4%E4%BD%93%E5%9B%9E%E9%81%BF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="回避理由" _mubu_text="%3Cspan%3E%E5%9B%9E%E9%81%BF%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="身份不当" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%BA%AB%E4%BB%BD%E4%B8%8D%E5%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="当事人、辩护人近亲属" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E3%80%81%E8%BE%A9%E6%8A%A4%E4%BA%BA%E8%BF%91%E4%BA%B2%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
