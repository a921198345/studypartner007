              <outline text="减刑建议向中院以上提，并抄送检察院" _mubu_text="%3Cspan%3E%E5%87%8F%E5%88%91%E5%BB%BA%E8%AE%AE%E5%90%91%E4%B8%AD%E9%99%A2%E4%BB%A5%E4%B8%8A%E6%8F%90%EF%BC%8C%E5%B9%B6%E6%8A%84%E9%80%81%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="解除终止" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
