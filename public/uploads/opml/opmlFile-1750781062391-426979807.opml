              <outline text="无期（怀孕、哺乳）、有期、拘役" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9C%9F%EF%BC%88%E6%80%80%E5%AD%95%E3%80%81%E5%93%BA%E4%B9%B3%EF%BC%89%E3%80%81%E6%9C%89%E6%9C%9F%E3%80%81%E6%8B%98%E5%BD%B9%3C/span%3E" _note="" _mubu_note=""/>
