            <outline text="申请回避：当事人、法定代理人、诉讼代理人、辩护人（口头或书面形式均可）" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E5%9B%9E%E9%81%BF%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BD%93%3C/span%3E%3Cspan%3E%E4%BA%8B%E4%BA%BA%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B3%95%3C/span%3E%3Cspan%3E%E5%AE%9A%E4%BB%A3%E7%90%86%E4%BA%BA%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%AF%89%3C/span%3E%3Cspan%3E%E8%AE%BC%E4%BB%A3%E7%90%86%E4%BA%BA%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%BE%A9%3C/span%3E%3Cspan%3E%E6%8A%A4%E4%BA%BA%EF%BC%88%E5%8F%A3%E5%A4%B4%E6%88%96%E4%B9%A6%E9%9D%A2%E5%BD%A2%E5%BC%8F%E5%9D%87%E5%8F%AF%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22RCz8U3q7rY%22%2C%22oh%22%3A1000%2C%22ow%22%3A3092%2C%22uri%22%3A%22document_image%2FDBD4691D54C14CDD1715677149.jpg%22%2C%22w%22%3A400%7D%5D"/>
            <outline text="指令回避：公、检、法负责人命令退出" _mubu_text="%3Cspan%3E%E6%8C%87%E4%BB%A4%E5%9B%9E%E9%81%BF%EF%BC%9A%E5%85%AC%E3%80%81%E6%A3%80%E3%80%81%E6%B3%95%E8%B4%9F%E8%B4%A3%E4%BA%BA%E5%91%BD%E4%BB%A4%E9%80%80%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
