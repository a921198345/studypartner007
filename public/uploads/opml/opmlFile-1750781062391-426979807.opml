                  </outline>
                </outline>
                <outline text="3 年以下" _mubu_text="%3Cspan%20class=%22bold%22%3E3%20%E5%B9%B4%3C/span%3E%3Cspan%3E%E4%BB%A5%E4%B8%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="侵犯人身权利（第四章）、侵犯财产权利（第五章）" _mubu_text="%3Cspan%3E%E4%BE%B5%E7%8A%AF%E4%BA%BA%E8%BA%AB%E6%9D%83%E5%88%A9%EF%BC%88%E7%AC%AC%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%9B%3C/span%3E%3Cspan%3E%E7%AB%A0%EF%BC%89%E3%80%81%E4%BE%B5%E7%8A%AF%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%88%A9%EF%BC%88%E7%AC%AC%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BA%94%3C/span%3E%3Cspan%3E%E7%AB%A0%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="过失犯罪：非渎七以下" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%BF%87%E5%A4%B1%E7%8A%AF%E7%BD%AA%EF%BC%9A%E9%9D%9E%E6%B8%8E%E4%B8%83%E4%BB%A5%E4%B8%8B%3C/span%3E" _note="" _mubu_note="">
