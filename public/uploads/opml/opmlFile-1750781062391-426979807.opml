            </outline>
            <outline text="违法违规" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%BF%9D%E6%B3%95%E8%BF%9D%E8%A7%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="需举证" _mubu_text="%3Cspan%3E%E9%9C%80%E4%B8%BE%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="跨越阶段" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%B7%A8%E8%B6%8A%E9%98%B6%E6%AE%B5%3C/span%3E" _note="" _mubu_note="">
              <outline text="曾参前，不参后（如参与一审，不得二审）" _mubu_text="%3Cspan%3E%E6%9B%BE%E5%8F%82%E5%89%8D%EF%BC%8C%E4%B8%8D%E5%8F%82%E5%90%8E%EF%BC%88%E5%A6%82%E5%8F%82%E4%B8%8E%E4%B8%80%E5%AE%A1%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BA%8C%E5%AE%A1%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="若发回，人要换（如参与二审发回重审，还可以进行二审）" _mubu_text="%3Cspan%3E%E8%8B%A5%E5%8F%91%E5%9B%9E%EF%BC%8C%E4%BA%BA%E8%A6%81%E6%8D%A2%EF%BC%88%E5%A6%82%E5%8F%82%E4%B8%8E%E4%BA%8C%E5%AE%A1%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%EF%BC%8C%E8%BF%98%E5%8F%AF%E4%BB%A5%E8%BF%9B%E8%A1%8C%E4%BA%8C%E5%AE%A1%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="再上诉，人不变" _mubu_text="%3Cspan%3E%E5%86%8D%E4%B8%8A%E8%AF%89%EF%BC%8C%E4%BA%BA%E4%B8%8D%E5%8F%98%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="回避种类" _mubu_text="%3Cspan%3E%E5%9B%9E%E9%81%BF%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
