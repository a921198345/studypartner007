          </outline>
          <outline text="取保候审" _mubu_text="%3Cspan%3E%E5%8F%96%E4%BF%9D%E5%80%99%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】管制拘役附加刑，有期病孕无危险，期限届满未办结，可以取保公安管" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E7%AE%A1%E5%88%B6%E6%8B%98%E5%BD%B9%E9%99%84%E5%8A%A0%E5%88%91%EF%BC%8C%E6%9C%89%E6%9C%9F%E7%97%85%E5%AD%95%3C/span%3E%3Cspan%20class=%22bold%20text-color-blue%22%3E%E6%97%A0%E5%8D%B1%E9%99%A9%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%E6%9C%AA%E5%8A%9E%E7%BB%93%EF%BC%8C%E5%8F%AF%E4%BB%A5%E5%8F%96%E4%BF%9D%E5%85%AC%E5%AE%89%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="保证人、保证金择一选择" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E4%BA%BA%E3%80%81%E4%BF%9D%E8%AF%81%E9%87%91%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8B%A9%E4%B8%80%3C/span%3E%3Cspan%3E%E9%80%89%E6%8B%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="保证人" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="处罚：罚款或追究刑事责任，由公安机关决定" _mubu_text="%3Cspan%3E%E5%A4%84%E7%BD%9A%EF%BC%9A%E7%BD%9A%E6%AC%BE%E6%88%96%E8%BF%BD%E7%A9%B6%E5%88%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%EF%BC%8C%E7%94%B1%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="未成年人优先适用保证人保证" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%E4%BC%98%E5%85%88%E9%80%82%E7%94%A8%E4%BF%9D%E8%AF%81%E4%BA%BA%E4%BF%9D%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="保证金" _mubu_text="%3Cspan%3E%E4%BF%9D%E8%AF%81%E9%87%91%3C/span%3E" _note="" _mubu_note="">
                <outline text="暂扣/没收" _mubu_text="%3Cspan%3E%E6%9A%82%E6%89%A3/%E6%B2%A1%E6%94%B6%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22UFzMrBrqZ7%22%2C%22oh%22%3A800%2C%22ow%22%3A4000%2C%22uri%22%3A%22document_image%2F5B59E28A969444291716346515.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="应当遵守" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%BA%94%E5%BD%93%E9%81%B5%E5%AE%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="离开居住市县，执行机关批准，决定机关同意" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%A6%BB%3C/span%3E%3Cspan%3E%E5%BC%80%E5%B1%85%E4%BD%8F%E5%B8%82%E5%8E%BF%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%A7%E8%A1%8C%E6%9C%BA%E5%85%B3%3C/span%3E%3Cspan%3E%E6%89%B9%E5%87%86%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%86%B3%E5%AE%9A%E6%9C%BA%E5%85%B3%3C/span%3E%3Cspan%3E%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="联系方式变动 24 小时内报告" _mubu_text="%3Cspan%3E%E8%81%94%E7%B3%BB%E6%96%B9%E5%BC%8F%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%98%3C/span%3E%3Cspan%3E%E5%8A%A8%2024%20%E5%B0%8F%E6%97%B6%E5%86%85%E6%8A%A5%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="传讯及时到案" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BC%A0%3C/span%3E%3Cspan%3E%E8%AE%AF%E5%8F%8A%E6%97%B6%E5%88%B0%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得干扰证人" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%B9%B2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%B0%3C/span%3E%3Cspan%3E%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得毁灭证据" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%AF%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%81%AD%3C/span%3E%3Cspan%3E%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="可以遵守" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E9%81%B5%E5%AE%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="特定场所、特定人员、特定活动" _mubu_text="%3Cspan%3E%E7%89%B9%E5%AE%9A%E5%9C%BA%E6%89%80%E3%80%81%E7%89%B9%E5%AE%9A%E4%BA%BA%E5%91%98%E3%80%81%E7%89%B9%E5%AE%9A%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="交证件" _mubu_text="%3Cspan%3E%E4%BA%A4%E8%AF%81%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="【口诀】取保禁足变要告，不扰证据传即到；上交证件禁止令，办案机关酌情考" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%8F%96%E4%BF%9D%E7%A6%81%E8%B6%B3%E5%8F%98%E8%A6%81%E5%91%8A%EF%BC%8C%E4%B8%8D%E6%89%B0%E8%AF%81%E6%8D%AE%E4%BC%A0%E5%8D%B3%E5%88%B0%EF%BC%9B%E4%B8%8A%E4%BA%A4%E8%AF%81%E4%BB%B6%E7%A6%81%E6%AD%A2%E4%BB%A4%EF%BC%8C%E5%8A%9E%E6%A1%88%E6%9C%BA%E5%85%B3%E9%85%8C%E6%83%85%E8%80%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="自动解除" _mubu_text="%3Cspan%3E%E8%87%AA%E5%8A%A8%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="后面生效执行，取保自动取消" _mubu_text="%3Cspan%3E%E5%90%8E%E9%9D%A2%E7%94%9F%E6%95%88%E6%89%A7%E8%A1%8C%EF%BC%8C%E5%8F%96%E4%BF%9D%E8%87%AA%E5%8A%A8%E5%8F%96%E6%B6%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="期限、计算" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%E3%80%81%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
              <outline text="同一阶段，累计算；跨越阶段，重新算" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E9%98%B6%E6%AE%B5%EF%BC%8C%E7%B4%AF%E8%AE%A1%E7%AE%97%EF%BC%9B%E8%B7%A8%E8%B6%8A%E9%98%B6%E6%AE%B5%EF%BC%8C%E9%87%8D%E6%96%B0%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="监视居住" _mubu_text="%3Cspan%3E%E7%9B%91%E8%A7%86%E5%B1%85%E4%BD%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】本该逮捕却病孕，特情届满唯一人；本可取保却发现，没人没钱可监居" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E6%9C%AC%E8%AF%A5%E9%80%AE%E6%8D%95%E5%8D%B4%E7%97%85%E5%AD%95%EF%BC%8C%E7%89%B9%E6%83%85%E5%B1%8A%E6%BB%A1%E5%94%AF%E4%B8%80%E4%BA%BA%EF%BC%9B%E6%9C%AC%E5%8F%AF%E5%8F%96%E4%BF%9D%E5%8D%B4%E5%8F%91%E7%8E%B0%EF%BC%8C%E6%B2%A1%E4%BA%BA%E6%B2%A1%E9%92%B1%E5%8F%AF%E7%9B%91%E5%B1%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="监视居住是逮捕和取保的替代性措施" _mubu_text="%3Cspan%3E%E7%9B%91%E8%A7%86%E5%B1%85%E4%BD%8F%E6%98%AF%E9%80%AE%E6%8D%95%E5%92%8C%E5%8F%96%E4%BF%9D%E7%9A%84%E6%9B%BF%E4%BB%A3%E6%80%A7%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="地点" _mubu_text="%3Cspan%3E%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：住处" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%BD%8F%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外：指定住所" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E6%8C%87%E5%AE%9A%E4%BD%8F%E6%89%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="无固定住处" _mubu_text="%3Cspan%3E%E6%97%A0%E5%9B%BA%E5%AE%9A%E4%BD%8F%E5%A4%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="涉嫌国恐，有碍侦查，上级公安批准" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B6%89%E5%AB%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%9B%BD%E6%81%90%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%EF%BC%8C%E6%9C%89%E7%A2%8D%E4%BE%A6%E6%9F%A5%EF%BC%8C%E4%B8%8A%E7%BA%A7%E5%85%AC%E5%AE%89%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不含黑毒" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%90%AB%3C/span%3E%3Cspan%3E%E9%BB%91%E6%AF%92%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="不是检察院批准" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%98%AF%3C/span%3E%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="【口诀】有家在家，没家指家；国恐侦批，有家指家" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E6%9C%89%E5%AE%B6%E5%9C%A8%E5%AE%B6%EF%BC%8C%E6%B2%A1%E5%AE%B6%E6%8C%87%E5%AE%B6%EF%BC%9B%E5%9B%BD%E6%81%90%E4%BE%A6%E6%89%B9%EF%BC%8C%E6%9C%89%E5%AE%B6%E6%8C%87%E5%AE%B6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="取保候审 VS 监视居住" _mubu_text="%3Cspan%3E%E5%8F%96%E4%BF%9D%E5%80%99%E5%AE%A1%20VS%20%E7%9B%91%E8%A7%86%E5%B1%85%E4%BD%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="共同点口诀：不扰证据传即到" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E5%85%B1%E5%90%8C%E7%82%B9%E5%8F%A3%E8%AF%80%EF%BC%9A%E4%B8%8D%E6%89%B0%E8%AF%81%E6%8D%AE%E4%BC%A0%E5%8D%B3%E5%88%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="期限、变更" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%E3%80%81%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="只有在指定居所监视居住的才会折抵刑期" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%9C%89%E5%9C%A8%E6%8C%87%E5%AE%9A%E5%B1%85%E6%89%80%E7%9B%91%E8%A7%86%E5%B1%85%E4%BD%8F%E7%9A%84%E6%89%8D%E4%BC%9A%E6%8A%98%E6%8A%B5%E5%88%91%E6%9C%9F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="拘留" _mubu_text="%3Cspan%3E%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="先行拘留条件" _mubu_text="%3Cspan%3E%E5%85%88%E8%A1%8C%E6%8B%98%E7%95%99%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="【口诀】现行现场有证据，要死要逃毁证据，不知是谁流多结，公安无证可先拘" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E7%8E%B0%E8%A1%8C%E7%8E%B0%E5%9C%BA%E6%9C%89%E8%AF%81%E6%8D%AE%EF%BC%8C%3C/span%3E%3Cspan%20class=%22underline%20text-color-blue%22%3E%E8%A6%81%E6%AD%BB%E8%A6%81%E9%80%83%E6%AF%81%E8%AF%81%E6%8D%AE%EF%BC%8C%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E4%B8%8D%E7%9F%A5%E6%98%AF%E8%B0%81%E6%B5%81%E5%A4%9A%E7%BB%93%EF%BC%8C%E5%85%AC%E5%AE%89%E6%97%A0%E8%AF%81%E5%8F%AF%E5%85%88%E6%8B%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="拘留后 24 小时内" _mubu_text="%3Cspan%3E%E6%8B%98%E7%95%99%E5%90%8E%2024%20%E5%B0%8F%E6%97%B6%E5%86%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="送看" _mubu_text="%3Cspan%3E%E9%80%81%E7%9C%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="讯问：可在送看前讯问" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AE%AF%E9%97%AE%EF%BC%9A%E5%8F%AF%E5%9C%A8%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E9%80%81%E7%9C%8B%E5%89%8D%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E8%AE%AF%E9%97%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="通知家属" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%E5%AE%B6%E5%B1%9E%3C/span%3E" _note="" _mubu_note="">
                <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="无法通知" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="涉嫌国恐，有碍侦查" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B6%89%E5%AB%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%9B%BD%E6%81%90%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%EF%BC%8C%E6%9C%89%E7%A2%8D%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="公安机关" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="一般 3 日" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%203%20%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="特殊 3 + 4 日" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%203%20+%204%20%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="流多结 30 日" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E6%B5%81%E5%A4%9A%E7%BB%93%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%2030%20%E6%97%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="流窜、多次、结伙" _mubu_text="%3Cspan%3E%E6%B5%81%E7%AA%9C%E3%80%81%E5%A4%9A%E6%AC%A1%E3%80%81%E7%BB%93%E4%BC%99%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="检察院作出决定 + 7 日" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E4%BD%9C%E5%87%BA%E5%86%B3%E5%AE%9A%20+%207%20%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="检查机关" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%A3%80%E6%9F%A5%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="监察委员会已采取留置措施" _mubu_text="%3Cspan%3E%E7%9B%91%E5%AF%9F%E5%A7%94%E5%91%98%E4%BC%9A%E5%B7%B2%E9%87%87%E5%8F%96%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%95%99%E7%BD%AE%3C/span%3E%3Cspan%3E%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检察院先行拘留" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%85%88%E8%A1%8C%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="作出决定 10 + 4 日" _mubu_text="%3Cspan%3E%E4%BD%9C%E5%87%BA%E5%86%B3%E5%AE%9A%2010%20+%204%20%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="逮捕" _mubu_text="%3Cspan%3E%E9%80%AE%E6%8D%95%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="应当逮捕条件" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%80%AE%E6%8D%95%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="有证据 + 徒刑以上 + 取保候审尚不足以防止发生社会危险性" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%20+%20%E5%BE%92%E5%88%91%E4%BB%A5%E4%B8%8A%20+%20%E5%8F%96%E4%BF%9D%E5%80%99%E5%AE%A1%E5%B0%9A%E4%B8%8D%E8%B6%B3%E4%BB%A5%E9%98%B2%E6%AD%A2%E5%8F%91%E7%94%9F%E7%A4%BE%E4%BC%9A%E5%8D%B1%E9%99%A9%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有证据 + 徒刑以上 + 10 年以上" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%20+%20%E5%BE%92%E5%88%91%E4%BB%A5%E4%B8%8A%20+%2010%20%E5%B9%B4%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="有证据 + 徒刑以上 + 曾经故意犯罪或身份不明" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%20+%20%E5%BE%92%E5%88%91%E4%BB%A5%E4%B8%8A%20+%20%E6%9B%BE%E7%BB%8F%E6%95%85%E6%84%8F%E7%8A%AF%E7%BD%AA%E6%88%96%E8%BA%AB%E4%BB%BD%E4%B8%8D%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="【记忆逻辑】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E8%AE%B0%E5%BF%86%E9%80%BB%E8%BE%91%E3%80%91%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22B7WeKemTBl%22%2C%22oh%22%3A1000%2C%22ow%22%3A3743%2C%22uri%22%3A%22document_image%2FE45552D9BCC84B6C1716348184.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="审查批准 VS 审查决定" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E6%89%B9%E5%87%86%20VS%20%E5%AE%A1%E6%9F%A5%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="审查批准逮捕（检察院批准公安）" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E6%89%B9%E5%87%86%E9%80%AE%E6%8D%95%EF%BC%88%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86%E5%85%AC%E5%AE%89%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="讯问" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：可以讯问" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%8F%AF%E4%BB%A5%E8%AE%AF%E9%97%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例外：应当讯问" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E8%AE%AF%E9%97%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="逮捕条件、嫌犯要求、重大违法、重大疑难、未成年、盲聋哑、精神病" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%AE%E6%8D%95%E6%9D%A1%E4%BB%B6%E3%80%81%E5%AB%8C%E7%8A%AF%E8%A6%81%E6%B1%82%E3%80%81%E9%87%8D%E5%A4%A7%E8%BF%9D%E6%B3%95%E3%80%81%E9%87%8D%E5%A4%A7%E7%96%91%E9%9A%BE%E3%80%81%E6%9C%AA%E6%88%90%E5%B9%B4%E3%80%81%E7%9B%B2%E8%81%8B%E5%93%91%E3%80%81%E7%B2%BE%E7%A5%9E%E7%97%85%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="【口诀】逮捕疑问向检话，重大疑难又违法，认罪认罚盲精未，审查批捕应问他" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E9%80%AE%E6%8D%95%E7%96%91%E9%97%AE%E5%90%91%E6%A3%80%E8%AF%9D%EF%BC%8C%E9%87%8D%E5%A4%A7%E7%96%91%E9%9A%BE%E5%8F%88%E8%BF%9D%E6%B3%95%EF%BC%8C%E8%AE%A4%E7%BD%AA%E8%AE%A4%E7%BD%9A%E7%9B%B2%E7%B2%BE%E6%9C%AA%EF%BC%8C%E5%AE%A1%E6%9F%A5%E6%89%B9%E6%8D%95%E5%BA%94%E9%97%AE%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="审查批捕时需要补充侦查的，只能退回公安机关补充侦查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E6%89%B9%E6%8D%95%E6%97%B6%E9%9C%80%E8%A6%81%E8%A1%A5%E5%85%85%E4%BE%A6%E6%9F%A5%E7%9A%84%EF%BC%8C%E5%8F%AA%E8%83%BD%E9%80%80%E5%9B%9E%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%E8%A1%A5%E5%85%85%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="vs审查起诉阶段" _mubu_text="%3Cspan%3Evs%E5%AE%A1%E6%9F%A5%E8%B5%B7%E8%AF%89%E9%98%B6%E6%AE%B5%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22a9xowHFbB8%22%2C%22oh%22%3A587%2C%22ow%22%3A4000%2C%22uri%22%3A%22document_image%2F6BEE9D29E7F24D6C1716348613.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
              <outline text="检察院不批准逮捕的，公安可以复议、复核，但应当先将犯罪嫌疑人立即释放（先放人再救济）" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E4%B8%8D%E6%89%B9%E5%87%86%E9%80%AE%E6%8D%95%E7%9A%84%EF%BC%8C%E5%85%AC%E5%AE%89%E5%8F%AF%E4%BB%A5%E5%A4%8D%E8%AE%AE%E3%80%81%E5%A4%8D%E6%A0%B8%EF%BC%8C%E4%BD%86%E5%BA%94%E5%BD%93%E5%85%88%E5%B0%86%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E7%AB%8B%E5%8D%B3%E9%87%8A%E6%94%BE%EF%BC%88%E5%85%88%E6%94%BE%E4%BA%BA%E5%86%8D%E6%95%91%E6%B5%8E%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="审查决定逮捕（检察院自己决定）" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E5%86%B3%E5%AE%9A%E9%80%AE%E6%8D%95%EF%BC%88%E6%A3%80%E5%AF%9F%E9%99%A2%E8%87%AA%E5%B7%B1%E5%86%B3%E5%AE%9A%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="逮捕执行程序" _mubu_text="%3Cspan%3E%E9%80%AE%E6%8D%95%E6%89%A7%E8%A1%8C%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="可以先行拘留但不可以先行逮捕，逮捕必须持有逮捕证" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E5%85%88%E8%A1%8C%E6%8B%98%E7%95%99%E4%BD%86%E4%B8%8D%E5%8F%AF%E4%BB%A5%E5%85%88%E8%A1%8C%E9%80%AE%E6%8D%95%EF%BC%8C%E9%80%AE%E6%8D%95%E5%BF%85%E9%A1%BB%E6%8C%81%E6%9C%89%E9%80%AE%E6%8D%95%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="逮捕后，24小时内" _mubu_text="%3Cspan%3E%E9%80%AE%E6%8D%95%E5%90%8E%EF%BC%8C24%E5%B0%8F%E6%97%B6%E5%86%85%3C/span%3E" _note="" _mubu_note="">
              <outline text="送看" _mubu_text="%3Cspan%3E%E9%80%81%E7%9C%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="讯问" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="通知" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="例外：无法通知" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E6%97%A0%E6%B3%95%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="区别：涉嫌国恐，拘留可不通知，逮捕必须通知" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8C%BA%E5%88%AB%EF%BC%9A%E6%B6%89%E5%AB%8C%E5%9B%BD%E6%81%90%EF%BC%8C%E6%8B%98%E7%95%99%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%8F%AF%E4%B8%8D%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%9A%E7%9F%A5%EF%BC%8C%E9%80%AE%E6%8D%95%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%BF%85%E9%A1%BB%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="审判阶段逮捕的变更" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E9%98%B6%E6%AE%B5%E9%80%AE%E6%8D%95%E7%9A%84%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】免处罚、非监禁、超期限，法院逮捕放或变" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%85%8D%E5%A4%84%E7%BD%9A%E3%80%81%E9%9D%9E%E7%9B%91%E7%A6%81%E3%80%81%E8%B6%85%E6%9C%9F%E9%99%90%EF%BC%8C%E6%B3%95%E9%99%A2%E9%80%AE%E6%8D%95%E6%94%BE%E6%88%96%E5%8F%98%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="羁押必要性检查（检察院对逮捕的监督）" _mubu_text="%3Cspan%3E%E7%BE%81%E6%8A%BC%E5%BF%85%E8%A6%81%E6%80%A7%E6%A3%80%E6%9F%A5%EF%BC%88%E6%A3%80%E5%AF%9F%E9%99%A2%E5%AF%B9%E9%80%AE%E6%8D%95%E7%9A%84%E7%9B%91%E7%9D%A3%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="概念" _mubu_text="%3Cspan%3E%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
              <outline text="被逮捕后的犯罪嫌疑人、被告人" _mubu_text="%3Cspan%3E%E8%A2%AB%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%AE%E6%8D%95%E5%90%8E%3C/span%3E%3Cspan%3E%E7%9A%84%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E3%80%81%E8%A2%AB%E5%91%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="启动方式" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="依职权：①未经羁押必要性审查、②可能判处三年有期徒刑以下，在提起公诉前应当开展一次" _mubu_text="%3Cspan%3E%E4%BE%9D%E8%81%8C%E6%9D%83%EF%BC%9A%E2%91%A0%E6%9C%AA%E7%BB%8F%E7%BE%81%E6%8A%BC%E5%BF%85%E8%A6%81%E6%80%A7%E5%AE%A1%E6%9F%A5%E3%80%81%E2%91%A1%E5%8F%AF%E8%83%BD%E5%88%A4%E5%A4%84%E4%B8%89%E5%B9%B4%E6%9C%89%E6%9C%9F%E5%BE%92%E5%88%91%E4%BB%A5%E4%B8%8B%EF%BC%8C%E5%9C%A8%E6%8F%90%E8%B5%B7%E5%85%AC%E8%AF%89%E5%89%8D%E5%BA%94%E5%BD%93%E5%BC%80%E5%B1%95%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="依申请" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="依建议：看守所依书面提出，检察院收到后应当立即开展" _mubu_text="%3Cspan%3E%E4%BE%9D%E5%BB%BA%E8%AE%AE%EF%BC%9A%E7%9C%8B%E5%AE%88%E6%89%80%E4%BE%9D%E4%B9%A6%E9%9D%A2%E6%8F%90%E5%87%BA%EF%BC%8C%E6%A3%80%E5%AF%9F%E9%99%A2%E6%94%B6%E5%88%B0%E5%90%8E%E5%BA%94%E5%BD%93%E7%AB%8B%E5%8D%B3%E5%BC%80%E5%B1%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="负责部门" _mubu_text="%3Cspan%3E%E8%B4%9F%E8%B4%A3%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="捕诉部门" _mubu_text="%3Cspan%3E%E6%8D%95%E8%AF%89%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="应当建议变更" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%BB%BA%E8%AE%AE%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】清白轻微，超期取监，建议放人" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E6%B8%85%E7%99%BD%E8%BD%BB%E5%BE%AE%EF%BC%8C%E8%B6%85%E6%9C%9F%E5%8F%96%E7%9B%91%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-blue%22%3E%E5%BB%BA%E8%AE%AE%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E6%94%BE%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="强制措施是为了防止犯罪嫌疑人、被告人跑了、死了或者毁灭证据，因此有必要限制、剥夺其自由，保证处于我们的掌控之下。" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%8E%AA%E6%96%BD%E6%98%AF%E4%B8%BA%E4%BA%86%E9%98%B2%E6%AD%A2%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E3%80%81%E8%A2%AB%E5%91%8A%E4%BA%BA%E8%B7%91%E4%BA%86%E3%80%81%E6%AD%BB%E4%BA%86%E6%88%96%E8%80%85%E6%AF%81%E7%81%AD%E8%AF%81%E6%8D%AE%EF%BC%8C%E5%9B%A0%E6%AD%A4%E6%9C%89%E5%BF%85%E8%A6%81%E9%99%90%E5%88%B6%E3%80%81%E5%89%A5%E5%A4%BA%E5%85%B6%E8%87%AA%E7%94%B1%EF%BC%8C%E4%BF%9D%E8%AF%81%E5%A4%84%E4%BA%8E%E6%88%91%E4%BB%AC%E7%9A%84%E6%8E%8C%E6%8E%A7%E4%B9%8B%E4%B8%8B%E3%80%82%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22lNIeL1xeNh%22%2C%22oh%22%3A1000%2C%22ow%22%3A2859%2C%22uri%22%3A%22document_image%2FF7A1391129B844A91716349410.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
      </outline>
      <outline text="09 附带民事诉讼" _mubu_text="%3Cspan%3E09%20%E9%99%84%E5%B8%A6%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="附民当事人" _mubu_text="%3Cspan%3E%E9%99%84%E6%B0%91%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="原告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="被害人、近亲属" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%AE%B3%E4%BA%BA%E3%80%81%E8%BF%91%E4%BA%B2%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="国家财产：检察院，自己的名义" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E8%B4%A2%E4%BA%A7%EF%BC%9A%E6%A3%80%E5%AF%9F%E9%99%A2%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%87%AA%E5%B7%B1%3C/span%3E%3Cspan%3E%E7%9A%84%E5%90%8D%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="被告人、共同侵害人、监护人、遗产继承人" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%A2%AB%3C/span%3E%3Cspan%3E%E5%91%8A%E4%BA%BA%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%85%B1%3C/span%3E%3Cspan%3E%E5%90%8C%E4%BE%B5%E5%AE%B3%E4%BA%BA%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%9B%91%3C/span%3E%3Cspan%3E%E6%8A%A4%E4%BA%BA%E3%80%81%E9%81%97%E4%BA%A7%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BB%A7%3C/span%3E%3Cspan%3E%E6%89%BF%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】共侵被告人、监护继承人、附民责任人" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%85%B1%E4%BE%B5%E8%A2%AB%E5%91%8A%E4%BA%BA%E3%80%81%E7%9B%91%E6%8A%A4%E7%BB%A7%E6%89%BF%E4%BA%BA%E3%80%81%E9%99%84%E6%B0%91%E8%B4%A3%E4%BB%BB%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="注意" _mubu_text="%3Cspan%3E%E6%B3%A8%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="亲属、朋友自愿代为赔偿，可以准许" _mubu_text="%3Cspan%3E%E4%BA%B2%E5%B1%9E%E3%80%81%E6%9C%8B%E5%8F%8B%E8%87%AA%E6%84%BF%E4%BB%A3%E4%B8%BA%E8%B5%94%E5%81%BF%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E5%87%86%E8%AE%B8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="同案犯在逃，不列为被告" _mubu_text="%3Cspan%3E%E5%90%8C%E6%A1%88%E7%8A%AF%E5%9C%A8%E9%80%83%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%88%97%E4%B8%BA%3C/span%3E%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="附民的提起" _mubu_text="%3Cspan%3E%E9%99%84%E6%B0%91%E7%9A%84%E6%8F%90%E8%B5%B7%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="赔偿：损失" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="只赔物质损失，不赔精神损失" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%B5%94%E7%89%A9%E8%B4%A8%E6%8D%9F%E5%A4%B1%EF%BC%8C%E4%B8%8D%E8%B5%94%E7%B2%BE%E7%A5%9E%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="只赔直接损失，不赔间接损失" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%B5%94%E7%9B%B4%E6%8E%A5%E6%8D%9F%E5%A4%B1%EF%BC%8C%E4%B8%8D%E8%B5%94%E9%97%B4%E6%8E%A5%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无死亡赔偿金" _mubu_text="%3Cspan%3E%E6%97%A0%E6%AD%BB%E4%BA%A1%E8%B5%94%E5%81%BF%E9%87%91%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="非法占有、处置财产：追缴或退赔，不提附民" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%9D%9E%E6%B3%95%E5%8D%A0%E6%9C%89%E3%80%81%E5%A4%84%E7%BD%AE%E8%B4%A2%E4%BA%A7%EF%BC%9A%E8%BF%BD%E7%BC%B4%E6%88%96%E9%80%80%E8%B5%94%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%B8%8D%E6%8F%90%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E9%99%84%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="国家机关工作人员职权犯罪：申请国家赔偿，不提附民" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E5%B7%A5%E4%BD%9C%E4%BA%BA%E5%91%98%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E8%81%8C%E6%9D%83%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E7%8A%AF%E7%BD%AA%EF%BC%9A%E7%94%B3%E8%AF%B7%E5%9B%BD%E5%AE%B6%E8%B5%94%E5%81%BF%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%B8%8D%E6%8F%90%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E9%99%84%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="财产保全" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉中财产保全" _mubu_text="%3Cspan%3E%E8%AF%89%E4%B8%AD%E8%B4%A2%E4%BA%A7%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="可以责令提供担保" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E8%B4%A3%E4%BB%A4%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="情况紧急 48 小时作出裁定" _mubu_text="%3Cspan%3E%E6%83%85%E5%86%B5%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%B4%A7%E6%80%A5%3C/span%3E%3Cspan%3E%2048%20%E5%B0%8F%E6%97%B6%E4%BD%9C%E5%87%BA%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="诉前财产保全" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8D%E8%B4%A2%E4%BA%A7%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="附带民事诉讼前" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%99%84%E5%B8%A6%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E6%B0%91%E4%BA%8B%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E8%AF%89%E8%AE%BC%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应当责令提供担保" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E8%B4%A3%E4%BB%A4%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="必须 48 小时作出裁定" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BF%85%E9%A1%BB%3C/span%3E%3Cspan%3E%2048%20%E5%B0%8F%E6%97%B6%E4%BD%9C%E5%87%BA%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="附民相关程序" _mubu_text="%3Cspan%3E%E9%99%84%E6%B0%91%E7%9B%B8%E5%85%B3%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="与刑事审判的关系" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%88%91%E4%BA%8B%E5%AE%A1%E5%88%A4%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="不可以先民后刑" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E4%BB%A5%E5%85%88%E6%B0%91%E5%90%8E%E5%88%91%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="不到庭处理" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%88%B0%E5%BA%AD%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="原告不来，应按撤诉；被告不来，可以缺席。下落不明，告知另诉。" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E4%B8%8D%E6%9D%A5%EF%BC%8C%E5%BA%94%E6%8C%89%E6%92%A4%E8%AF%89%EF%BC%9B%E8%A2%AB%E5%91%8A%E4%B8%8D%E6%9D%A5%EF%BC%8C%E5%8F%AF%E4%BB%A5%E7%BC%BA%E5%B8%AD%E3%80%82%E4%B8%8B%E8%90%BD%E4%B8%8D%E6%98%8E%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%E3%80%82%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22oMVnoWnBLZ%22%2C%22oh%22%3A1000%2C%22ow%22%3A3661%2C%22uri%22%3A%22document_image%2F5572350DF8124EA91716350121.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="赔偿数额与调解" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E6%95%B0%E9%A2%9D%E4%B8%8E%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="可以不制作调解书" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E5%88%B6%E4%BD%9C%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="应当制作笔录" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E5%88%B6%E4%BD%9C%E7%AC%94%E5%BD%95%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="刑事被告人无罪或撤诉处理" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E8%A2%AB%E5%91%8A%E4%BA%BA%E6%97%A0%E7%BD%AA%E6%88%96%E6%92%A4%E8%AF%89%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="附民处理" _mubu_text="%3Cspan%3E%E9%99%84%E6%B0%91%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22G1gMJl8RgU%22%2C%22oh%22%3A1000%2C%22ow%22%3A3347%2C%22uri%22%3A%22document_image%2FC4BD008279C744981716350282.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="二审期间提起附民的处理" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E6%9C%9F%E9%97%B4%E6%8F%90%E8%B5%B7%E9%99%84%E6%B0%91%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="注意区分在公诉与自诉案件中，二审中提出反诉的不同处理" _mubu_text="%3Cspan%3E%E6%B3%A8%E6%84%8F%E5%8C%BA%E5%88%86%E5%9C%A8%E5%85%AC%E8%AF%89%E4%B8%8E%E8%87%AA%E8%AF%89%E6%A1%88%E4%BB%B6%E4%B8%AD%EF%BC%8C%E4%BA%8C%E5%AE%A1%E4%B8%AD%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%E7%9A%84%E4%B8%8D%E5%90%8C%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22TsHTeHmz2U%22%2C%22oh%22%3A1000%2C%22ow%22%3A1797%2C%22uri%22%3A%22document_image%2F3CE60B24341148911716359647.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="有人要求走赔偿" _mubu_text="%3Cspan%3E%E6%9C%89%E4%BA%BA%E8%A6%81%E6%B1%82%E8%B5%B0%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22z0r5LUZTeu%22%2C%22oh%22%3A1154%2C%22ow%22%3A1000%2C%22uri%22%3A%22document_image%2F9A98C18334D8492A1716359764.jpg%22%2C%22w%22%3A346%7D%5D"/>
        </outline>
      </outline>
      <outline text="10 期间与送达" _mubu_text="%3Cspan%3E10%20%E6%9C%9F%E9%97%B4%E4%B8%8E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="期间" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="期间与期日的区别" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E4%B8%8E%E6%9C%9F%E6%97%A5%E7%9A%84%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="" _heading="3">
            <outline text="只有公安司法机关的期间可以重新计算，诉讼参与人的期间不会重新计算" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%9C%89%E5%85%AC%E5%AE%89%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E7%9A%84%E6%9C%9F%E9%97%B4%E5%8F%AF%E4%BB%A5%E9%87%8D%E6%96%B0%E8%AE%A1%E7%AE%97%EF%BC%8C%E8%AF%89%E8%AE%BC%E5%8F%82%E4%B8%8E%E4%BA%BA%E7%9A%84%E6%9C%9F%E9%97%B4%E4%B8%8D%E4%BC%9A%E9%87%8D%E6%96%B0%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note="" _heading="3" _mubu_images="%5B%7B%22id%22%3A%22TboS87Xm7H%22%2C%22oh%22%3A999%2C%22ow%22%3A2879%2C%22uri%22%3A%22document_image%2F638ABF180B674F3B1716359829.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="期间的计算" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E7%9A%84%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note="" _heading="3">
            <outline text="1. 期间的最后 1日问题：" _mubu_text="%3Cspan%3E1.%20%E6%9C%9F%E9%97%B4%E7%9A%84%E6%9C%80%E5%90%8E%201%E6%97%A5%E9%97%AE%E9%A2%98%EF%BC%9A%3C/span%3E" _note="" _mubu_note="" _heading="3">
              <outline text="期间的最后 1日为节假日的，以节假日后的第1日为期间届满日期。对于犯罪嫌疑人、被告人或者罪犯在押期间，应当至期间屆满之县为止，不得因节假日而延长在押期限至节假日后的第 1日。" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E7%9A%84%E6%9C%80%E5%90%8E%201%E6%97%A5%E4%B8%BA%E8%8A%82%E5%81%87%E6%97%A5%E7%9A%84%EF%BC%8C%E4%BB%A5%E8%8A%82%E5%81%87%E6%97%A5%E5%90%8E%E7%9A%84%E7%AC%AC1%E6%97%A5%E4%B8%BA%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E6%97%A5%E6%9C%9F%E3%80%82%E5%AF%B9%E4%BA%8E%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E3%80%81%E8%A2%AB%E5%91%8A%E4%BA%BA%E6%88%96%E8%80%85%E7%BD%AA%E7%8A%AF%E5%9C%A8%E6%8A%BC%E6%9C%9F%E9%97%B4%EF%BC%8C%E5%BA%94%E5%BD%93%E8%87%B3%E6%9C%9F%E9%97%B4%E5%B1%86%E6%BB%A1%E4%B9%8B%E5%8E%BF%E4%B8%BA%E6%AD%A2%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%9B%A0%E8%8A%82%E5%81%87%E6%97%A5%E8%80%8C%E5%BB%B6%E9%95%BF%E5%9C%A8%E6%8A%BC%E6%9C%9F%E9%99%90%E8%87%B3%E8%8A%82%E5%81%87%E6%97%A5%E5%90%8E%E7%9A%84%E7%AC%AC%201%E6%97%A5%E3%80%82%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            </outline>
            <outline text="2. 交邮日期问题：" _mubu_text="%3Cspan%3E2.%20%E4%BA%A4%E9%82%AE%E6%97%A5%E6%9C%9F%E9%97%AE%E9%A2%98%EF%BC%9A%3C/span%3E" _note="" _mubu_note="" _heading="3">
              <outline text="上诉状或者其他文件在期满前已经交邮的，不算过期。交邮时间以当地邮局所盖邮戳为准。" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%AF%89%E7%8A%B6%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E6%96%87%E4%BB%B6%E5%9C%A8%E6%9C%9F%E6%BB%A1%E5%89%8D%E5%B7%B2%E7%BB%8F%E4%BA%A4%E9%82%AE%E7%9A%84%EF%BC%8C%E4%B8%8D%E7%AE%97%E8%BF%87%E6%9C%9F%E3%80%82%E4%BA%A4%E9%82%AE%E6%97%B6%E9%97%B4%E4%BB%A5%E5%BD%93%E5%9C%B0%E9%82%AE%E5%B1%80%E6%89%80%E7%9B%96%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%82%AE%E6%88%B3%3C/span%3E%3Cspan%3E%E4%B8%BA%E5%87%86%E3%80%82%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            </outline>
            <outline text="3. 路途上的时间问题：" _mubu_text="%3Cspan%3E3.%20%E8%B7%AF%E9%80%94%E4%B8%8A%E7%9A%84%E6%97%B6%E9%97%B4%E9%97%AE%E9%A2%98%EF%BC%9A%3C/span%3E" _note="" _mubu_note="" _heading="3">
              <outline text="法定期间不包括路途上的时间。有关诉讼文书材料在公交司法机关之国传递过程中的时间，也应当在法定期间内予以扣除。" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E6%9C%9F%E9%97%B4%3C/span%3E%3Cspan%20class=%22underline%22%3E%E4%B8%8D%E5%8C%85%E6%8B%AC%E8%B7%AF%E9%80%94%E4%B8%8A%E7%9A%84%E6%97%B6%E9%97%B4%3C/span%3E%3Cspan%3E%E3%80%82%E6%9C%89%E5%85%B3%E8%AF%89%E8%AE%BC%E6%96%87%E4%B9%A6%E6%9D%90%E6%96%99%E5%9C%A8%E5%85%AC%E4%BA%A4%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E4%B9%8B%E5%9B%BD%E4%BC%A0%E9%80%92%E8%BF%87%E7%A8%8B%E4%B8%AD%E7%9A%84%E6%97%B6%E9%97%B4%EF%BC%8C%E4%B9%9F%E5%BA%94%E5%BD%93%E5%9C%A8%E6%B3%95%E5%AE%9A%E6%9C%9F%E9%97%B4%E5%86%85%E4%BA%88%E4%BB%A5%E6%89%A3%E9%99%A4%E3%80%82%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            </outline>
          </outline>
          <outline text="期间的重新计算" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E7%9A%84%E9%87%8D%E6%96%B0%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note="" _heading="3">
            <outline text="【口诀】另有重罪，补侦发回，改变管辖，期限重推" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%8F%A6%E6%9C%89%E9%87%8D%E7%BD%AA%EF%BC%8C%E8%A1%A5%E4%BE%A6%E5%8F%91%E5%9B%9E%EF%BC%8C%E6%94%B9%E5%8F%98%E7%AE%A1%E8%BE%96%EF%BC%8C%E6%9C%9F%E9%99%90%E9%87%8D%E6%8E%A8%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="自查清其身份之日起计算，但是不得停止对其犯罪行为的侦查取证" _mubu_text="%3Cspan%3E%E8%87%AA%E6%9F%A5%E6%B8%85%E5%85%B6%E8%BA%AB%E4%BB%BD%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%AE%A1%E7%AE%97%EF%BC%8C%E4%BD%86%E6%98%AF%E4%B8%8D%E5%BE%97%E5%81%9C%E6%AD%A2%E5%AF%B9%E5%85%B6%E7%8A%AF%E7%BD%AA%E8%A1%8C%E4%B8%BA%E7%9A%84%E4%BE%A6%E6%9F%A5%E5%8F%96%E8%AF%81%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="鉴定时间不计入办案期限" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E6%97%B6%E9%97%B4%E4%B8%8D%E8%AE%A1%E5%85%A5%E5%8A%9E%E6%A1%88%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
          </outline>
        </outline>
        <outline text="送达" _mubu_text="%3Cspan%3E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="" _heading="3">
            <outline text="直接送达" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3">
              <outline text="本人不在，可交成年家属或所在单位的负责人员代收" _mubu_text="%3Cspan%3E%E6%9C%AC%E4%BA%BA%E4%B8%8D%E5%9C%A8%EF%BC%8C%E5%8F%AF%E4%BA%A4%E6%88%90%E5%B9%B4%E5%AE%B6%E5%B1%9E%E6%88%96%E6%89%80%E5%9C%A8%E5%8D%95%E4%BD%8D%E7%9A%84%E8%B4%9F%E8%B4%A3%E4%BA%BA%E5%91%98%E4%BB%A3%E6%94%B6%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
              <outline text="签收的日期为送达日期" _mubu_text="%3Cspan%3E%E7%AD%BE%E6%94%B6%E7%9A%84%E6%97%A5%E6%9C%9F%E4%B8%BA%E9%80%81%E8%BE%BE%E6%97%A5%E6%9C%9F%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            </outline>
            <outline text="留置送达" _mubu_text="%3Cspan%3E%E7%95%99%E7%BD%AE%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="委托送达" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="邮寄送达" _mubu_text="%3Cspan%3E%E9%82%AE%E5%AF%84%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="转交送达" _mubu_text="%3Cspan%3E%E8%BD%AC%E4%BA%A4%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
          </outline>
          <outline text="命题角度" _mubu_text="%3Cspan%3E%E5%91%BD%E9%A2%98%E8%A7%92%E5%BA%A6%3C/span%3E" _note="" _mubu_note="" _heading="3">
            <outline text="不论何种送达，必须有送达回证" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AE%BA%E4%BD%95%E7%A7%8D%E9%80%81%E8%BE%BE%EF%BC%8C%E5%BF%85%E9%A1%BB%E6%9C%89%E9%80%81%E8%BE%BE%E5%9B%9E%E8%AF%81%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="在送达回证上写明，不能在送达的文书上写明" _mubu_text="%3Cspan%3E%E5%9C%A8%E9%80%81%E8%BE%BE%E5%9B%9E%E8%AF%81%E4%B8%8A%E5%86%99%E6%98%8E%EF%BC%8C%E4%B8%8D%E8%83%BD%E5%9C%A8%E9%80%81%E8%BE%BE%E7%9A%84%E6%96%87%E4%B9%A6%E4%B8%8A%E5%86%99%E6%98%8E%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
            <outline text="五种送达，可视情况进行转换" _mubu_text="%3Cspan%3E%E4%BA%94%E7%A7%8D%E9%80%81%E8%BE%BE%EF%BC%8C%E5%8F%AF%E8%A7%86%E6%83%85%E5%86%B5%E8%BF%9B%E8%A1%8C%E8%BD%AC%E6%8D%A2%3C/span%3E" _note="" _mubu_note="" _heading="3"/>
          </outline>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="即时，必达！" _mubu_text="%3Cspan%3E%E5%8D%B3%E6%97%B6%EF%BC%8C%E5%BF%85%E8%BE%BE%EF%BC%81%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22Hwl9xRI2It%22%2C%22oh%22%3A1000%2C%22ow%22%3A3026%2C%22uri%22%3A%22document_image%2F544EB665C3E246CE1716361441.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
      </outline>
      <outline text="【编体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%BC%96%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
        <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22wZhWGIZpWM%22%2C%22oh%22%3A1000%2C%22ow%22%3A1455%2C%22uri%22%3A%22document_image%2F4D06E2F70B0F41501716361498.jpg%22%2C%22w%22%3A400%7D%5D"/>
        <outline text=" “通用制度编”，这些制度其穿于立案、侦查、起诉、审判、执行各个诉讼阶段。" _mubu_text="%3Cspan%3E%20%E2%80%9C%E9%80%9A%E7%94%A8%E5%88%B6%E5%BA%A6%E7%BC%96%E2%80%9D%EF%BC%8C%E8%BF%99%E4%BA%9B%E5%88%B6%E5%BA%A6%E5%85%B6%E7%A9%BF%E4%BA%8E%E7%AB%8B%E6%A1%88%E3%80%81%E4%BE%A6%E6%9F%A5%E3%80%81%E8%B5%B7%E8%AF%89%E3%80%81%E5%AE%A1%E5%88%A4%E3%80%81%E6%89%A7%E8%A1%8C%E5%90%84%E4%B8%AA%E8%AF%89%E8%AE%BC%E9%98%B6%E6%AE%B5%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="刑事诉讼，以人为本。至于人，无非国家专门机关和诉讼参与人，他们在各个诉讼阶段均需遵守一定的制度。" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E8%AF%89%E8%AE%BC%EF%BC%8C%E4%BB%A5%E4%BA%BA%E4%B8%BA%E6%9C%AC%E3%80%82%E8%87%B3%E4%BA%8E%E4%BA%BA%EF%BC%8C%E6%97%A0%E9%9D%9E%E5%9B%BD%E5%AE%B6%E4%B8%93%E9%97%A8%E6%9C%BA%E5%85%B3%E5%92%8C%E8%AF%89%E8%AE%BC%E5%8F%82%E4%B8%8E%E4%BA%BA%EF%BC%8C%E4%BB%96%E4%BB%AC%E5%9C%A8%E5%90%84%E4%B8%AA%E8%AF%89%E8%AE%BC%E9%98%B6%E6%AE%B5%E5%9D%87%E9%9C%80%E9%81%B5%E5%AE%88%E4%B8%80%E5%AE%9A%E7%9A%84%E5%88%B6%E5%BA%A6%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
    </outline>
    <outline text="第三编 办案流程" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E7%BC%96%20%E5%8A%9E%E6%A1%88%E6%B5%81%E7%A8%8B%3C/span%3E" _note="" _mubu_note="" _heading="1">
      <outline text="11 立案" _mubu_text="%3Cspan%3E11%20%E7%AB%8B%E6%A1%88%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="立案的材料来源和条件" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E7%9A%84%E6%9D%90%E6%96%99%E6%9D%A5%E6%BA%90%E5%92%8C%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="材料来源" _mubu_text="%3Cspan%3E%E6%9D%90%E6%96%99%E6%9D%A5%E6%BA%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="区分报案、举报和控告" _mubu_text="%3Cspan%3E%E5%8C%BA%E5%88%86%E6%8A%A5%E6%A1%88%E3%80%81%E4%B8%BE%E6%8A%A5%E5%92%8C%E6%8E%A7%E5%91%8A%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22XuGpRs7LIT%22%2C%22oh%22%3A672%2C%22ow%22%3A4000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F9429EF7CBFD444C01716884450.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
          <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="不要求犯罪事实已经查清" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E6%B1%82%E7%8A%AF%E7%BD%AA%E4%BA%8B%E5%AE%9E%E5%B7%B2%E7%BB%8F%E6%9F%A5%E6%B8%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不要求知道犯罪嫌疑人身份" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E6%B1%82%E7%9F%A5%E9%81%93%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E8%BA%AB%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不要求犯罪嫌疑人已经到案" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E6%B1%82%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E5%B7%B2%E7%BB%8F%E5%88%B0%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="立案程序" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="对立案材料的接受" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%AB%8B%E6%A1%88%E6%9D%90%E6%96%99%E7%9A%84%E6%8E%A5%E5%8F%97%3C/span%3E" _note="" _mubu_note="">
            <outline text="对报案、控告、举报和自首都应当接受" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%8A%A5%E6%A1%88%E3%80%81%E6%8E%A7%E5%91%8A%E3%80%81%E4%B8%BE%E6%8A%A5%E5%92%8C%E8%87%AA%E9%A6%96%E9%83%BD%E5%BA%94%E5%BD%93%E6%8E%A5%E5%8F%97%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可书面可口头" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B9%A6%E9%9D%A2%E5%8F%AF%E5%8F%A3%E5%A4%B4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="公安机关初查" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%E5%88%9D%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="仅允许任意性措施" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%BB%85%E5%85%81%E8%AE%B8%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E4%BB%BB%E6%84%8F%E6%80%A7%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="不限制被查对象人身、财产权利的措施" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%99%90%E5%88%B6%E8%A2%AB%E6%9F%A5%E5%AF%B9%E8%B1%A1%E4%BA%BA%E8%BA%AB%E3%80%81%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%88%A9%E7%9A%84%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="勘验、检查、询问等" _mubu_text="%3Cspan%3E%E5%8B%98%E9%AA%8C%E3%80%81%E6%A3%80%E6%9F%A5%E3%80%81%E8%AF%A2%E9%97%AE%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="不包含强制性措施" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8C%85%E5%90%AB%E5%BC%BA%E5%88%B6%E6%80%A7%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="强制措施、查封、扣押、冻结、技术侦查等" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%8E%AA%E6%96%BD%E3%80%81%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%E3%80%81%E5%86%BB%E7%BB%93%E3%80%81%E6%8A%80%E6%9C%AF%E4%BE%A6%E6%9F%A5%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="对不立案决定的救济" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%B8%8D%E7%AB%8B%E6%A1%88%E5%86%B3%E5%AE%9A%E7%9A%84%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="决定不予立案，将原因通知控告人" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E4%B8%8D%E4%BA%88%E7%AB%8B%E6%A1%88%EF%BC%8C%E5%B0%86%E5%8E%9F%E5%9B%A0%E9%80%9A%E7%9F%A5%E6%8E%A7%E5%91%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="控告人：被害人" _mubu_text="%3Cspan%3E%E6%8E%A7%E5%91%8A%E4%BA%BA%EF%BC%9A%E8%A2%AB%E5%AE%B3%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对不立案不服" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%B8%8D%E7%AB%8B%E6%A1%88%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="找公安：申请复议 + 向上一级公安申请复核" _mubu_text="%3Cspan%3E%E6%89%BE%E5%85%AC%E5%AE%89%EF%BC%9A%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%20+%20%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E5%85%AC%E5%AE%89%E7%94%B3%E8%AF%B7%E5%A4%8D%E6%A0%B8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="找检察院：申请检察院立案监督" _mubu_text="%3Cspan%3E%E6%89%BE%E6%A3%80%E5%AF%9F%E9%99%A2%EF%BC%9A%E7%94%B3%E8%AF%B7%E6%A3%80%E5%AF%9F%E9%99%A2%E7%AB%8B%E6%A1%88%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="找法院：提起自诉" _mubu_text="%3Cspan%3E%E6%89%BE%E6%B3%95%E9%99%A2%EF%BC%9A%E6%8F%90%E8%B5%B7%E8%87%AA%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对撤销案件不服" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%92%A4%E9%94%80%E6%A1%88%E4%BB%B6%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="提起自诉" _mubu_text="%3Cspan%3E%E6%8F%90%E8%B5%B7%E8%87%AA%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对不起诉不服" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%B8%8D%E8%B5%B7%E8%AF%89%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="向上一级检察院申诉" _mubu_text="%3Cspan%3E%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%A3%80%E5%AF%9F%E9%99%A2%E7%94%B3%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="提起自诉" _mubu_text="%3Cspan%3E%E6%8F%90%E8%B5%B7%E8%87%AA%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对宣告无罪不服" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%AE%A3%E5%91%8A%E6%97%A0%E7%BD%AA%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="判决未生效：申请检察院抗诉" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E6%9C%AA%E7%94%9F%E6%95%88%EF%BC%9A%E7%94%B3%E8%AF%B7%E6%A3%80%E5%AF%9F%E9%99%A2%E6%8A%97%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="判决已生效：申诉" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E5%B7%B2%E7%94%9F%E6%95%88%EF%BC%9A%E7%94%B3%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="立案监督" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="主体：捕诉部门" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E6%8D%95%E8%AF%89%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="对象：应当立案不立案or不应立案而立案" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B1%A1%EF%BC%9A%E5%BA%94%E5%BD%93%E7%AB%8B%E6%A1%88%E4%B8%8D%E7%AB%8B%E6%A1%88or%E4%B8%8D%E5%BA%94%E7%AB%8B%E6%A1%88%E8%80%8C%E7%AB%8B%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="程序：检察院监督公安" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%EF%BC%9A%E6%A3%80%E5%AF%9F%E9%99%A2%E7%9B%91%E7%9D%A3%E5%85%AC%E5%AE%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22syuPtaDfRP%22%2C%22oh%22%3A1000%2C%22ow%22%3A1608%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F818676A0A83749C11716884995.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22TwZi23TxKQ%22%2C%22oh%22%3A1000%2C%22ow%22%3A1701%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F7456ACC2BC014DC61716884394.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
      </outline>
      <outline text="12 侦查" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E12%20%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="概述" _mubu_text="%3Cspan%3E%E6%A6%82%E8%BF%B0%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="侦查原则" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="程序法定原则" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E6%B3%95%E5%AE%9A%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="比例原则" _mubu_text="%3Cspan%3E%E6%AF%94%E4%BE%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="侦查的法律控制" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E7%9A%84%E6%B3%95%E5%BE%8B%E6%8E%A7%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="强制性，事前审查" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%80%A7%EF%BC%8C%E4%BA%8B%E5%89%8D%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="任意性，事后审查" _mubu_text="%3Cspan%3E%E4%BB%BB%E6%84%8F%E6%80%A7%EF%BC%8C%E4%BA%8B%E5%90%8E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="侦查行为" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="讯问犯罪嫌疑人" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="讯问人员" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="侦查人员 2 名" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E4%BA%BA%E5%91%98%202%20%E5%90%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="讯问同案嫌疑人，个别进行" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AE%AF%E9%97%AE%E5%90%8C%E6%A1%88%E5%AB%8C%E7%96%91%E4%BA%BA%EF%BC%8C%E4%B8%AA%E5%88%AB%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="区别：讯问同案被告人，无需个别" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8C%BA%E5%88%AB%EF%BC%9A%E8%AE%AF%E9%97%AE%E5%90%8C%E6%A1%88%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E8%A2%AB%E5%91%8A%E4%BA%BA%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E6%97%A0%E9%9C%80%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%AA%E5%88%AB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="连续讯问，不超过12小时" _mubu_text="%3Cspan%3E%E8%BF%9E%E7%BB%AD%E8%AE%AF%E9%97%AE%EF%BC%8C%E4%B8%8D%E8%B6%85%E8%BF%8712%E5%B0%8F%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="讯问地点" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】一般公安讯问室，特定情况就地问。已送看的看守所，未送看的可在家" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E4%B8%80%E8%88%AC%E5%85%AC%E5%AE%89%E8%AE%AF%E9%97%AE%E5%AE%A4%EF%BC%8C%E7%89%B9%E5%AE%9A%E6%83%85%E5%86%B5%E5%B0%B1%E5%9C%B0%E9%97%AE%E3%80%82%E5%B7%B2%E9%80%81%E7%9C%8B%E7%9A%84%E7%9C%8B%E5%AE%88%E6%89%80%EF%BC%8C%E6%9C%AA%E9%80%81%E7%9C%8B%E7%9A%84%E5%8F%AF%E5%9C%A8%E5%AE%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="已羁押：看守所" _mubu_text="%3Cspan%3E%E5%B7%B2%E7%BE%81%E6%8A%BC%EF%BC%9A%E7%9C%8B%E5%AE%88%E6%89%80%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未羁押：哪都行" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BE%81%E6%8A%BC%EF%BC%9A%E5%93%AA%E9%83%BD%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="讯问笔录" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E7%AC%94%E5%BD%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：应当有签名、捺印" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%BA%94%E5%BD%93%E6%9C%89%E7%AD%BE%E5%90%8D%E3%80%81%E6%8D%BA%E5%8D%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外：在特定情况下，犯罪嫌疑人拒绝签名，侦查人员应当在笔录上注明，经查证属实的，仍可作为定案依据" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E5%9C%A8%E7%89%B9%E5%AE%9A%E6%83%85%E5%86%B5%E4%B8%8B%EF%BC%8C%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E6%8B%92%E7%BB%9D%E7%AD%BE%E5%90%8D%EF%BC%8C%E4%BE%A6%E6%9F%A5%E4%BA%BA%E5%91%98%E5%BA%94%E5%BD%93%E5%9C%A8%E7%AC%94%E5%BD%95%E4%B8%8A%E6%B3%A8%E6%98%8E%EF%BC%8C%E7%BB%8F%E6%9F%A5%E8%AF%81%E5%B1%9E%E5%AE%9E%E7%9A%84%EF%BC%8C%E4%BB%8D%E5%8F%AF%E4%BD%9C%E4%B8%BA%E5%AE%9A%E6%A1%88%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="录音录像" _mubu_text="%3Cspan%3E%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="公安：无期、死刑、重大，应当录音录像" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%EF%BC%9A%E6%97%A0%E6%9C%9F%E3%80%81%E6%AD%BB%E5%88%91%E3%80%81%E9%87%8D%E5%A4%A7%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检察、监察：应当录音录像" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E3%80%81%E7%9B%91%E5%AF%9F%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="讯问时律师不能在场" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E6%97%B6%E5%BE%8B%E5%B8%88%E4%B8%8D%E8%83%BD%E5%9C%A8%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="【口诀】公安问可以录，无死重应当录，检察监察都要录" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%85%AC%E5%AE%89%E9%97%AE%E5%8F%AF%E4%BB%A5%E5%BD%95%EF%BC%8C%E6%97%A0%E6%AD%BB%E9%87%8D%E5%BA%94%E5%BD%93%E5%BD%95%EF%BC%8C%E6%A3%80%E5%AF%9F%E7%9B%91%E5%AF%9F%E9%83%BD%E8%A6%81%E5%BD%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="询问证人、被害人" _mubu_text="%3Cspan%3E%E8%AF%A2%E9%97%AE%E8%AF%81%E4%BA%BA%E3%80%81%E8%A2%AB%E5%AE%B3%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="现场、证人提出的地点" _mubu_text="%3Cspan%3E%E7%8E%B0%E5%9C%BA%E3%80%81%E8%AF%81%E4%BA%BA%E6%8F%90%E5%87%BA%E7%9A%84%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="并非侦查人员指定的地点" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%B9%B6%E9%9D%9E%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E4%BE%A6%E6%9F%A5%E4%BA%BA%E5%91%98%E6%8C%87%E5%AE%9A%E7%9A%84%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="勘验、检查" _mubu_text="%3Cspan%3E%E5%8B%98%E9%AA%8C%E3%80%81%E6%A3%80%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="必须持证" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E6%8C%81%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="检查必须持证，搜查有可能不用持证" _mubu_text="%3Cspan%3E%E6%A3%80%E6%9F%A5%E5%BF%85%E9%A1%BB%E6%8C%81%E8%AF%81%EF%BC%8C%E6%90%9C%E6%9F%A5%E6%9C%89%E5%8F%AF%E8%83%BD%E4%B8%8D%E7%94%A8%E6%8C%81%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="应当邀请无关见证人" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%82%80%E8%AF%B7%E6%97%A0%E5%85%B3%E8%A7%81%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="(1） 见证人与证人不同，见证人不得与案件存在利害关系。" _mubu_text="%3Cspan%3E(1%EF%BC%89%20%E8%A7%81%E8%AF%81%E4%BA%BA%E4%B8%8E%E8%AF%81%E4%BA%BA%E4%B8%8D%E5%90%8C%EF%BC%8C%E8%A7%81%E8%AF%81%E4%BA%BA%E4%B8%8D%E5%BE%97%E4%B8%8E%E6%A1%88%E4%BB%B6%E5%AD%98%E5%9C%A8%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="(2） 由于客观原因无法由符合条件的人员担任见证人的，应当在笔录材料中注明情况，并对相关活动进行全程录音录像" _mubu_text="%3Cspan%3E(2%EF%BC%89%20%E7%94%B1%E4%BA%8E%E5%AE%A2%E8%A7%82%E5%8E%9F%E5%9B%A0%E6%97%A0%E6%B3%95%E7%94%B1%E7%AC%A6%E5%90%88%E6%9D%A1%E4%BB%B6%E7%9A%84%E4%BA%BA%E5%91%98%E6%8B%85%E4%BB%BB%E8%A7%81%E8%AF%81%E4%BA%BA%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%9C%A8%E7%AC%94%E5%BD%95%E6%9D%90%E6%96%99%E4%B8%AD%E6%B3%A8%E6%98%8E%E6%83%85%E5%86%B5%EF%BC%8C%E5%B9%B6%E5%AF%B9%E7%9B%B8%E5%85%B3%E6%B4%BB%E5%8A%A8%E8%BF%9B%E8%A1%8C%E5%85%A8%E7%A8%8B%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="专家辅助人勘查" _mubu_text="%3Cspan%3E%E4%B8%93%E5%AE%B6%E8%BE%85%E5%8A%A9%E4%BA%BA%E5%8B%98%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="不只有侦查人员才有权进行勘验、检查" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AA%E6%9C%89%E4%BE%A6%E6%9F%A5%E4%BA%BA%E5%91%98%E6%89%8D%E6%9C%89%E6%9D%83%E8%BF%9B%E8%A1%8C%E5%8B%98%E9%AA%8C%E3%80%81%E6%A3%80%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="解剖尸体" _mubu_text="%3Cspan%3E%E8%A7%A3%E5%89%96%E5%B0%B8%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="公安机关决定，无需家属同意" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%E5%86%B3%E5%AE%9A%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%97%A0%E9%9C%80%3C/span%3E%3Cspan%3E%E5%AE%B6%E5%B1%9E%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="通知家属到场" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%E5%AE%B6%E5%B1%9E%E5%88%B0%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="强制检查" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%A3%80%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="仅对犯罪嫌疑人强制" _mubu_text="%3Cspan%3E%E4%BB%85%E5%AF%B9%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E5%BC%BA%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="检查妇女" _mubu_text="%3Cspan%3E%E6%A3%80%E6%9F%A5%E5%A6%87%E5%A5%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="女工作人员" _mubu_text="%3Cspan%3E%E5%A5%B3%E5%B7%A5%E4%BD%9C%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="医师男女不限" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8C%BB%E5%B8%88%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E7%94%B7%E5%A5%B3%E4%B8%8D%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="复验复查" _mubu_text="%3Cspan%3E%E5%A4%8D%E9%AA%8C%E5%A4%8D%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="检察院发现公安勘查不合格：可要求公安或自行复验复查" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E5%8F%91%E7%8E%B0%E5%85%AC%E5%AE%89%E5%8B%98%E6%9F%A5%E4%B8%8D%E5%90%88%E6%A0%BC%EF%BC%9A%E5%8F%AF%E8%A6%81%E6%B1%82%E5%85%AC%E5%AE%89%E6%88%96%E8%87%AA%E8%A1%8C%E5%A4%8D%E9%AA%8C%E5%A4%8D%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="搜查" _mubu_text="%3Cspan%3E%E6%90%9C%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="搜查证" _mubu_text="%3Cspan%3E%E6%90%9C%E6%9F%A5%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="逮捕拘留，情况紧急，可以无证" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%80%AE%E6%8D%95%E6%8B%98%E7%95%99%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%83%85%E5%86%B5%E7%B4%A7%E6%80%A5%EF%BC%8C%E5%8F%AF%E4%BB%A5%E6%97%A0%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="仅为搜查，必须出示，即上门搜查，必须有证" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BB%85%3C/span%3E%3Cspan%3E%E4%B8%BA%E6%90%9C%E6%9F%A5%EF%BC%8C%E5%BF%85%E9%A1%BB%E5%87%BA%E7%A4%BA%EF%BC%8C%E5%8D%B3%E4%B8%8A%E9%97%A8%E6%90%9C%E6%9F%A5%EF%BC%8C%E5%BF%85%E9%A1%BB%E6%9C%89%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="【总结】只有传唤、拘留、搜查，有可能无证实施，其他侦查行为必须有证" _mubu_text="%3Cspan%3E%E3%80%90%E6%80%BB%E7%BB%93%E3%80%91%E5%8F%AA%E6%9C%89%E4%BC%A0%E5%94%A4%E3%80%81%E6%8B%98%E7%95%99%E3%80%81%E6%90%9C%E6%9F%A5%EF%BC%8C%E6%9C%89%E5%8F%AF%E8%83%BD%E6%97%A0%E8%AF%81%E5%AE%9E%E6%96%BD%EF%BC%8C%E5%85%B6%E4%BB%96%E4%BE%A6%E6%9F%A5%E8%A1%8C%E4%B8%BA%E5%BF%85%E9%A1%BB%E6%9C%89%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="在场" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%9C%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="被搜查人或家属" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%90%9C%E6%9F%A5%E4%BA%BA%E6%88%96%E5%AE%B6%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="和邻居或见证人" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%92%8C%3C/span%3E%3Cspan%3E%E9%82%BB%E5%B1%85%E6%88%96%E8%A7%81%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="查封、扣押、查询、冻结" _mubu_text="%3Cspan%3E%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%E3%80%81%E6%9F%A5%E8%AF%A2%E3%80%81%E5%86%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="查封、扣押 VS 查询、冻结" _mubu_text="%3Cspan%3E%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%20VS%20%E6%9F%A5%E8%AF%A2%E3%80%81%E5%86%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="查封、扣押：有形物（物证、书证）" _mubu_text="%3Cspan%3E%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%EF%BC%9A%E6%9C%89%E5%BD%A2%E7%89%A9%EF%BC%88%E7%89%A9%E8%AF%81%E3%80%81%E4%B9%A6%E8%AF%81%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="查询、冻结：无形物（财产）" _mubu_text="%3Cspan%3E%E6%9F%A5%E8%AF%A2%E3%80%81%E5%86%BB%E7%BB%93%EF%BC%9A%E6%97%A0%E5%BD%A2%E7%89%A9%EF%BC%88%E8%B4%A2%E4%BA%A7%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="查封、扣押" _mubu_text="%3Cspan%3E%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%3C/span%3E" _note="" _mubu_note="">
              <outline text="应扣尽扣" _mubu_text="%3Cspan%3E%E5%BA%94%E6%89%A3%E5%B0%BD%E6%89%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="邮件、电报需公安或检察院批准" _mubu_text="%3Cspan%3E%E9%82%AE%E4%BB%B6%E3%80%81%E7%94%B5%E6%8A%A5%E9%9C%80%E5%85%AC%E5%AE%89%E6%88%96%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="与案件无关，不能查封、扣押" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%A1%88%E4%BB%B6%E6%97%A0%E5%85%B3%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%83%BD%3C/span%3E%3Cspan%3E%E6%9F%A5%E5%B0%81%E3%80%81%E6%89%A3%E6%8A%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="3 日内退还" _mubu_text="%3Cspan%3E3%20%E6%97%A5%E5%86%85%E9%80%80%E8%BF%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="随案移送，法院裁处" _mubu_text="%3Cspan%3E%E9%9A%8F%E6%A1%88%E7%A7%BB%E9%80%81%EF%BC%8C%E6%B3%95%E9%99%A2%E8%A3%81%E5%A4%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院在判决中未对涉案财物作出处理的，可出决定书" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%9C%A8%E5%88%A4%E5%86%B3%E4%B8%AD%E6%9C%AA%E5%AF%B9%E6%B6%89%E6%A1%88%E8%B4%A2%E7%89%A9%E4%BD%9C%E5%87%BA%E5%A4%84%E7%90%86%E7%9A%84%EF%BC%8C%E5%8F%AF%E5%87%BA%E5%86%B3%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="查扣电子数据原始存储介质" _mubu_text="%3Cspan%3E%E6%9F%A5%E6%89%A3%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%E5%8E%9F%E5%A7%8B%E5%AD%98%E5%82%A8%E4%BB%8B%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="应当以封存状态随案移送" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E4%BB%A5%E5%B0%81%E5%AD%98%E7%8A%B6%E6%80%81%E9%9A%8F%E6%A1%88%E7%A7%BB%E9%80%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="查询、冻结" _mubu_text="%3Cspan%3E%E6%9F%A5%E8%AF%A2%E3%80%81%E5%86%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得扣划" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%89%A3%E5%88%92%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不得重复冻结，可以轮候冻结" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E9%87%8D%E5%A4%8D%E5%86%BB%E7%BB%93%EF%BC%8C%E5%8F%AF%E4%BB%A5%E8%BD%AE%E5%80%99%E5%86%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="股票基金：书面申请出售，所得继续冻结" _mubu_text="%3Cspan%3E%E8%82%A1%E7%A5%A8%E5%9F%BA%E9%87%91%EF%BC%9A%E4%B9%A6%E9%9D%A2%E7%94%B3%E8%AF%B7%E5%87%BA%E5%94%AE%EF%BC%8C%E6%89%80%E5%BE%97%E7%BB%A7%E7%BB%AD%E5%86%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="鉴定" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="鉴定人签名，可无鉴定部门盖章" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E7%AD%BE%E5%90%8D%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E6%97%A0%3C/span%3E%3Cspan%3E%E9%89%B4%E5%AE%9A%E9%83%A8%E9%97%A8%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="区别：行政法，鉴定人员签名和鉴定部门盖章" _mubu_text="%3Cspan%3E%E5%8C%BA%E5%88%AB%EF%BC%9A%E8%A1%8C%E6%94%BF%E6%B3%95%EF%BC%8C%E9%89%B4%E5%AE%9A%E4%BA%BA%E5%91%98%E7%AD%BE%E5%90%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%92%8C%3C/span%3E%3Cspan%3E%E9%89%B4%E5%AE%9A%E9%83%A8%E9%97%A8%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="不同意见，应当注明" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%E6%84%8F%E8%A7%81%EF%BC%8C%E5%BA%94%E5%BD%93%E6%B3%A8%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="精神病鉴定期间不计入办案期限，其他计入" _mubu_text="%3Cspan%3E%E7%B2%BE%E7%A5%9E%E7%97%85%E9%89%B4%E5%AE%9A%E6%9C%9F%E9%97%B4%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%AE%A1%E5%85%A5%3C/span%3E%3Cspan%3E%E5%8A%9E%E6%A1%88%E6%9C%9F%E9%99%90%EF%BC%8C%E5%85%B6%E4%BB%96%E8%AE%A1%E5%85%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="技术侦查" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E6%8A%80%E6%9C%AF%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="可以适用" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="公安" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="立案后" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%AB%8B%E6%A1%88%3C/span%3E%3Cspan%3E%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="国恐黑毒或七年以上" _mubu_text="%3Cspan%3E%E5%9B%BD%E6%81%90%E9%BB%91%E6%AF%92%E6%88%96%E4%B8%83%E5%B9%B4%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="检察院" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="立案后" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="交由有关机关，检察院不能实施" _mubu_text="%3Cspan%3E%E4%BA%A4%E7%94%B1%E6%9C%89%E5%85%B3%E6%9C%BA%E5%85%B3%EF%BC%8C%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%83%BD%3C/span%3E%3Cspan%3E%E5%AE%9E%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="在逃犯" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%9C%A8%E9%80%83%3C/span%3E%3Cspan%3E%E7%8A%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="批准机关" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="市公安以上批准、执行" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B8%82%E5%85%AC%E5%AE%89%3C/span%3E%3Cspan%3E%E4%BB%A5%E4%B8%8A%E6%89%B9%E5%87%86%E3%80%81%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="有效期限" _mubu_text="%3Cspan%3E%E6%9C%89%E6%95%88%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
              <outline text="每次3 个月，可延长" _mubu_text="%3Cspan%3E%E6%AF%8F%E6%AC%A13%20%E4%B8%AA%E6%9C%88%EF%BC%8C%E5%8F%AF%E5%BB%B6%E9%95%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="重新报批" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E6%8A%A5%E6%89%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="变更种类、对象" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E7%A7%8D%E7%B1%BB%E3%80%81%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="技侦对象" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E6%8A%80%E4%BE%A6%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="犯罪嫌疑人、被告人" _mubu_text="%3Cspan%3E%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E3%80%81%E8%A2%AB%E5%91%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="与犯罪活动直接关联人员，可对非嫌疑人适用" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%8E%E7%8A%AF%E7%BD%AA%E6%B4%BB%E5%8A%A8%E7%9B%B4%E6%8E%A5%E5%85%B3%E8%81%94%E4%BA%BA%E5%91%98%EF%BC%8C%E5%8F%AF%E5%AF%B9%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E9%9D%9E%E5%AB%8C%E7%96%91%E4%BA%BA%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="秘密侦查" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E7%A7%98%E5%AF%86%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="不诱使犯罪" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AF%B1%E4%BD%BF%E7%8A%AF%E7%BD%AA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="毒品：控制下交付" _mubu_text="%3Cspan%3E%E6%AF%92%E5%93%81%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8E%A7%E5%88%B6%3C/span%3E%3Cspan%3E%E4%B8%8B%E4%BA%A4%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="县级以上批准即可" _mubu_text="%3Cspan%3E%E5%8E%BF%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%89%B9%E5%87%86%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="证据调查与适用" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E8%AF%81%E6%8D%AE%E8%B0%83%E6%9F%A5%E4%B8%8E%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="具备证据资格，无须转化" _mubu_text="%3Cspan%3E%E5%85%B7%E5%A4%87%E8%AF%81%E6%8D%AE%E8%B5%84%E6%A0%BC%EF%BC%8C%E6%97%A0%E9%A1%BB%E8%BD%AC%E5%8C%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可在庭外核实" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%9C%A8%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%AD%E5%A4%96%3C/span%3E%3Cspan%3E%E6%A0%B8%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="裁判文书中不得表述身份、设备、方法" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%E6%96%87%E4%B9%A6%E4%B8%AD%E4%B8%8D%E5%BE%97%E8%A1%A8%E8%BF%B0%E8%BA%AB%E4%BB%BD%E3%80%81%E8%AE%BE%E5%A4%87%E3%80%81%E6%96%B9%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未移送的不得直接认定" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%A7%BB%E9%80%81%E7%9A%84%E4%B8%8D%E5%BE%97%E7%9B%B4%E6%8E%A5%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="通缉" _mubu_text="%3Cspan%3E%E9%80%9A%E7%BC%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="只能由公安发布" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E7%94%B1%E5%85%AC%E5%AE%89%E5%8F%91%E5%B8%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="辨认" _mubu_text="%3Cspan%3E%E8%BE%A8%E8%AE%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="数量" _mubu_text="%3Cspan%3E%E6%95%B0%E9%87%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="人数 7" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%95%B0%207%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="照片 10" _mubu_text="%3Cspan%3E%E7%85%A7%E7%89%87%2010%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="物品 5" _mubu_text="%3Cspan%3E%E7%89%A9%E5%93%81%205%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="尸体不受数量限制" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B0%B8%E4%BD%93%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%8F%97%E6%95%B0%E9%87%8F%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="【口诀】辨认混杂七十五，检察物照少五张" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E8%BE%A8%E8%AE%A4%E6%B7%B7%E6%9D%82%E4%B8%83%E5%8D%81%E4%BA%94%EF%BC%8C%E6%A3%80%E5%AF%9F%E7%89%A9%E7%85%A7%E5%B0%91%E4%BA%94%E5%BC%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="辨认对象仅嫌疑人，无被害人、证人" _mubu_text="%3Cspan%3E%E8%BE%A8%E8%AE%A4%E5%AF%B9%E8%B1%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BB%85%E5%AB%8C%E7%96%91%E4%BA%BA%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%97%A0%E8%A2%AB%E5%AE%B3%E4%BA%BA%E3%80%81%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="侦查终结" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="对案件的处理" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%A1%88%E4%BB%B6%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="移送告知" _mubu_text="%3Cspan%3E%E7%A7%BB%E9%80%81%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="公安机关告知" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%E6%9C%BA%E5%85%B3%E5%91%8A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只告知犯罪嫌疑人及其辩护律师" _mubu_text="%3Cspan%3E%E5%8F%AA%E5%91%8A%E7%9F%A5%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E5%8F%8A%E5%85%B6%E8%BE%A9%E6%8A%A4%E5%BE%8B%E5%B8%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="提前介入" _mubu_text="%3Cspan%3E%E6%8F%90%E5%89%8D%E4%BB%8B%E5%85%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="检察院对监委会：客客气气；检察院对公安局：毫不客气" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E5%AF%B9%E7%9B%91%E5%A7%94%E4%BC%9A%EF%BC%9A%E5%AE%A2%E5%AE%A2%E6%B0%94%E6%B0%94%EF%BC%9B%E6%A3%80%E5%AF%9F%E9%99%A2%E5%AF%B9%E5%85%AC%E5%AE%89%E5%B1%80%EF%BC%9A%E6%AF%AB%E4%B8%8D%E5%AE%A2%E6%B0%94%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="撤销案件" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="抓人需批准，放人只通知" _mubu_text="%3Cspan%3E%E6%8A%93%E4%BA%BA%E9%9C%80%E6%89%B9%E5%87%86%EF%BC%8C%E6%94%BE%E4%BA%BA%E5%8F%AA%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="意见附卷" _mubu_text="%3Cspan%3E%E6%84%8F%E8%A7%81%E9%99%84%E5%8D%B7%3C/span%3E" _note="" _mubu_note="">
              <outline text="律师意见应当附卷" _mubu_text="%3Cspan%3E%E5%BE%8B%E5%B8%88%E6%84%8F%E8%A7%81%E5%BA%94%E5%BD%93%E9%99%84%E5%8D%B7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="侦查的羁押期限：逮捕后到侦查终结" _mubu_text="%3Cspan%3E%E4%BE%A6%E6%9F%A5%E7%9A%84%E7%BE%81%E6%8A%BC%E6%9C%9F%E9%99%90%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%AE%E6%8D%95%3C/span%3E%3Cspan%3E%E5%90%8E%E5%88%B0%E4%BE%A6%E6%9F%A5%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
              <outline text="2 个月：一般情形" _mubu_text="%3Cspan%3E2%20%E4%B8%AA%E6%9C%88%EF%BC%9A%E4%B8%80%E8%88%AC%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="2 + 1 个月：上一级检察院批准，案情复杂" _mubu_text="%3Cspan%3E2%20+%201%20%E4%B8%AA%E6%9C%88%EF%BC%9A%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86%EF%BC%8C%E6%A1%88%E6%83%85%E5%A4%8D%E6%9D%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="2 + 1 + 2 个月：省级检察院批准，交集流广" _mubu_text="%3Cspan%3E2%20+%201%20+%202%20%E4%B8%AA%E6%9C%88%EF%BC%9A%E7%9C%81%E7%BA%A7%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86%EF%BC%8C%E4%BA%A4%E9%9B%86%E6%B5%81%E5%B9%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="交通、集团、流窜、面广" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%A4%3C/span%3E%3Cspan%3E%E9%80%9A%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%9B%86%3C/span%3E%3Cspan%3E%E5%9B%A2%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B5%81%3C/span%3E%3Cspan%3E%E7%AA%9C%E3%80%81%E9%9D%A2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%B9%BF%3C/span%3E" _note="" _mubu_note=""/>
