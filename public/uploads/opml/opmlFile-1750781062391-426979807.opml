              <outline text="【口诀】期满赦免解矫正，收监死亡即终止" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E6%9C%9F%E6%BB%A1%E8%B5%A6%E5%85%8D%E8%A7%A3%E7%9F%AB%E6%AD%A3%EF%BC%8C%E6%94%B6%E7%9B%91%E6%AD%BB%E4%BA%A1%E5%8D%B3%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
