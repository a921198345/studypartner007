            </outline>
            <outline text="应当开庭审理" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E5%BC%80%E5%BA%AD%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="【口诀】重大立功影响宽，破格减刑有意见，职务黑金开庭判。" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E9%87%8D%E5%A4%A7%E7%AB%8B%E5%8A%9F%E5%BD%B1%E5%93%8D%E5%AE%BD%EF%BC%8C%E7%A0%B4%E6%A0%BC%E5%87%8F%E5%88%91%E6%9C%89%E6%84%8F%E8%A7%81%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%20text-color-blue%22%3E%E8%81%8C%E5%8A%A1%E9%BB%91%E9%87%91%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E5%BC%80%E5%BA%AD%E5%88%A4%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="在罪犯刑罚执行场所或人民法院确定场所进行" _mubu_text="%3Cspan%3E%E5%9C%A8%E7%BD%AA%E7%8A%AF%E5%88%91%E7%BD%9A%E6%89%A7%E8%A1%8C%E5%9C%BA%E6%89%80%E6%88%96%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E7%A1%AE%E5%AE%9A%E5%9C%BA%E6%89%80%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="应当通知参加庭审" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E9%80%9A%E7%9F%A5%E5%8F%82%E5%8A%A0%E5%BA%AD%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="检察院、执行机关、罪犯" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E3%80%81%E6%89%A7%E8%A1%8C%E6%9C%BA%E5%85%B3%E3%80%81%E7%BD%AA%E7%8A%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="执行无辩护" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E6%97%A0%E8%BE%A9%E6%8A%A4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="书面审理提讯" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E5%AE%A1%E7%90%86%E6%8F%90%E8%AE%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="减刑：可以提讯" _mubu_text="%3Cspan%3E%E5%87%8F%E5%88%91%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E6%8F%90%E8%AE%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="假释：应当提讯" _mubu_text="%3Cspan%3E%E5%81%87%E9%87%8A%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E6%8F%90%E8%AE%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法院减刑假释裁定不当，检察院提出书面纠正意见" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%87%8F%E5%88%91%E5%81%87%E9%87%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%A3%81%E5%AE%9A%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BD%93%EF%BC%8C%E6%A3%80%E5%AF%9F%E9%99%A2%E6%8F%90%E5%87%BA%E4%B9%A6%E9%9D%A2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BA%A0%E6%AD%A3%3C/span%3E%3Cspan%3E%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="无上诉、抗诉" _mubu_text="%3Cspan%3E%E6%97%A0%E4%B8%8A%E8%AF%89%E3%80%81%E6%8A%97%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="对新罪、申诉的处理" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B0%E7%BD%AA%E3%80%81%E7%94%B3%E8%AF%89%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="对新罪、漏罪的处理：移送检察院" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B0%E7%BD%AA%E3%80%81%E6%BC%8F%E7%BD%AA%E7%9A%84%E5%A4%84%E7%90%86%EF%BC%9A%E7%A7%BB%E9%80%81%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="发现错判、申诉的处理：移送检察院或法院" _mubu_text="%3Cspan%3E%E5%8F%91%E7%8E%B0%E9%94%99%E5%88%A4%E3%80%81%E7%94%B3%E8%AF%89%E7%9A%84%E5%A4%84%E7%90%86%EF%BC%9A%E7%A7%BB%E9%80%81%E6%A3%80%E5%AF%9F%E9%99%A2%E6%88%96%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22tOLExqH5Sn%22%2C%22oh%22%3A1118%2C%22ow%22%3A1000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F5F6C00CA77FD4C191717573166.jpg%22%2C%22w%22%3A357%7D%5D"/>
        </outline>
      </outline>
      <outline text="【编体系逻辑】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%BC%96%E4%BD%93%E7%B3%BB%E9%80%BB%E8%BE%91%E3%80%91%3C/span%3E" _note="" _mubu_note="">
        <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22vrQNrhV3x8%22%2C%22oh%22%3A1097%2C%22ow%22%3A1000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F4E122BA2198A4FE21717572522.jpg%22%2C%22w%22%3A364%7D%5D"/>
      </outline>
    </outline>
    <outline text="第四编 特别程序" _mubu_text="%3Cspan%3E%E7%AC%AC%E5%9B%9B%E7%BC%96%20%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="" _heading="1">
      <outline text="20 未成年人" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E20%20%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="一般规定" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="应当讯问、听取" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E8%AE%AF%E9%97%AE%E3%80%81%E5%90%AC%E5%8F%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="询问未成年被害人、证人，原则上只询问1次；犯罪嫌疑人没有次数限制" _mubu_text="%3Cspan%3E%E8%AF%A2%E9%97%AE%E6%9C%AA%E6%88%90%E5%B9%B4%E8%A2%AB%E5%AE%B3%E4%BA%BA%E3%80%81%E8%AF%81%E4%BA%BA%EF%BC%8C%E5%8E%9F%E5%88%99%E4%B8%8A%E5%8F%AA%E8%AF%A2%E9%97%AE1%E6%AC%A1%EF%BC%9B%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E6%B2%A1%E6%9C%89%E6%AC%A1%E6%95%B0%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="“性侵”“暴力”案件应当录音录像" _mubu_text="%3Cspan%3E%E2%80%9C%E6%80%A7%E4%BE%B5%E2%80%9D%E2%80%9C%E6%9A%B4%E5%8A%9B%E2%80%9D%E6%A1%88%E4%BB%B6%E5%BA%94%E5%BD%93%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="合适成年人制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%90%88%E9%80%82%E6%88%90%E5%B9%B4%E4%BA%BA%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="应当通知法定代理人到场" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%80%9A%E7%9F%A5%E6%B3%95%E5%AE%9A%E4%BB%A3%E7%90%86%E4%BA%BA%E5%88%B0%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法定代理人可以代为行使诉讼权利，合适成年人不行" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E4%BB%A3%E7%90%86%E4%BA%BA%E5%8F%AF%E4%BB%A5%E4%BB%A3%E4%B8%BA%E8%A1%8C%E4%BD%BF%E8%AF%89%E8%AE%BC%E6%9D%83%E5%88%A9%EF%BC%8C%E5%90%88%E9%80%82%E6%88%90%E5%B9%B4%E4%BA%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="明确拒绝合适成年人，应当另行通知" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A1%AE%E6%8B%92%E7%BB%9D%E5%90%88%E9%80%82%E6%88%90%E5%B9%B4%E4%BA%BA%EF%BC%8C%E5%BA%94%E5%BD%93%E5%8F%A6%E8%A1%8C%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="提出意见，签字盖章" _mubu_text="%3Cspan%3E%E6%8F%90%E5%87%BA%E6%84%8F%E8%A7%81%EF%BC%8C%E7%AD%BE%E5%AD%97%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可以会见" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%BC%9A%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="可制作社会调查报告" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%88%B6%E4%BD%9C%E7%A4%BE%E4%BC%9A%E8%B0%83%E6%9F%A5%E6%8A%A5%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="控辩双方可以制作" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%8E%A7%E8%BE%A9%3C/span%3E%3Cspan%3E%E5%8F%8C%E6%96%B9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E5%88%B6%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="办案教育参考，不是证据" _mubu_text="%3Cspan%3E%E5%8A%9E%E6%A1%88%E6%95%99%E8%82%B2%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%82%E8%80%83%3C/span%3E%3Cspan%3E%EF%BC%8C%E4%B8%8D%E6%98%AF%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="女性未成年人，女工作人员在场" _mubu_text="%3Cspan%3E%E5%A5%B3%E6%80%A7%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9C%AA%E6%88%90%E5%B9%B4%3C/span%3E%3Cspan%3E%E4%BA%BA%EF%BC%8C%E5%A5%B3%E5%B7%A5%E4%BD%9C%E4%BA%BA%E5%91%98%E5%9C%A8%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="少年法庭审理范围" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%B0%91%E5%B9%B4%E6%B3%95%E5%BA%AD%E5%AE%A1%E7%90%86%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="独自犯罪" _mubu_text="%3Cspan%3E%E7%8B%AC%E8%87%AA%E7%8A%AF%E7%BD%AA%3C/span%3E" _note="" _mubu_note="">
              <outline text="应当：犯罪时不满 18，立案时不满 20 适用" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%EF%BC%9A%E7%8A%AF%E7%BD%AA%E6%97%B6%E4%B8%8D%E6%BB%A1%2018%EF%BC%8C%E7%AB%8B%E6%A1%88%E6%97%B6%E4%B8%8D%E6%BB%A1%2020%20%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可以：不满22的在校学生；强奸、猥亵、虐待、遗弃未成年等侵害未成年人身权利的案件" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%EF%BC%9A%E4%B8%8D%E6%BB%A122%E7%9A%84%E5%9C%A8%E6%A0%A1%E5%AD%A6%E7%94%9F%EF%BC%9B%E5%BC%BA%E5%A5%B8%E3%80%81%E7%8C%A5%E4%BA%B5%E3%80%81%E8%99%90%E5%BE%85%E3%80%81%E9%81%97%E5%BC%83%E6%9C%AA%E6%88%90%E5%B9%B4%E7%AD%89%E4%BE%B5%E5%AE%B3%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%E8%BA%AB%E6%9D%83%E5%88%A9%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="共同犯罪" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%8A%AF%E7%BD%AA%3C/span%3E" _note="" _mubu_note="">
              <outline text="是否适用，由院长根据实际情况决定" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%98%AF%E5%90%A6%3C/span%3E%3Cspan%3E%E9%80%82%E7%94%A8%EF%BC%8C%E7%94%B1%E9%99%A2%E9%95%BF%E6%A0%B9%E6%8D%AE%E5%AE%9E%E9%99%85%E6%83%85%E5%86%B5%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="不公开审理" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%AC%E5%BC%80%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="不满18周岁一律不公开，但学校和未成年人保护组织可以派代表到场" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%BB%A118%E5%91%A8%E5%B2%81%E4%B8%80%E5%BE%8B%E4%B8%8D%E5%85%AC%E5%BC%80%EF%BC%8C%E4%BD%86%E5%AD%A6%E6%A0%A1%E5%92%8C%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%E4%BF%9D%E6%8A%A4%E7%BB%84%E7%BB%87%E5%8F%AF%E4%BB%A5%E6%B4%BE%E4%BB%A3%E8%A1%A8%E5%88%B0%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="补充陈述" _mubu_text="%3Cspan%3E%E8%A1%A5%E5%85%85%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="法定代理人，不是辩护人、合适成年人" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%B3%95%E5%AE%9A%3C/span%3E%3Cspan%3E%E4%BB%A3%E7%90%86%E4%BA%BA%EF%BC%8C%E4%B8%8D%E6%98%AF%E8%BE%A9%E6%8A%A4%E4%BA%BA%E3%80%81%E5%90%88%E9%80%82%E6%88%90%E5%B9%B4%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="补充陈述，不是代替陈述" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%A1%A5%E5%85%85%3C/span%3E%3Cspan%3E%E9%99%88%E8%BF%B0%EF%BC%8C%E4%B8%8D%E6%98%AF%E4%BB%A3%E6%9B%BF%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="附条件不起诉（暂缓起诉）" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%99%84%E6%9D%A1%E4%BB%B6%E4%B8%8D%E8%B5%B7%E8%AF%89%EF%BC%88%E6%9A%82%E7%BC%93%E8%B5%B7%E8%AF%89%EF%BC%89%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="适用条件：【口诀】未成年、四五六、一起悔，可缓诉" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%EF%BC%9A%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E6%9C%AA%E6%88%90%E5%B9%B4%E3%80%81%E5%9B%9B%E4%BA%94%E5%85%AD%E3%80%81%E4%B8%80%E8%B5%B7%E6%82%94%EF%BC%8C%E5%8F%AF%E7%BC%93%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
