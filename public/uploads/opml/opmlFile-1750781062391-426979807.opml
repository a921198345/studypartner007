            <outline text="客观性" _mubu_text="%3Cspan%3E%E5%AE%A2%E8%A7%82%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="关联性：证明力的原因" _mubu_text="%3Cspan%3E%E5%85%B3%E8%81%94%E6%80%A7%EF%BC%9A%E8%AF%81%E6%98%8E%E5%8A%9B%E7%9A%84%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="合法性" _mubu_text="%3Cspan%3E%E5%90%88%E6%B3%95%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="【理论逻辑】" _mubu_text="%3Cspan%3E%E3%80%90%E7%90%86%E8%AE%BA%E9%80%BB%E8%BE%91%E3%80%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22bKQWCh87d2%22%2C%22oh%22%3A572%2C%22ow%22%3A4000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F3DEF4CF62DC341B71716280278.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
          </outline>
          <outline text="种类与分类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%E4%B8%8E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="物证" _mubu_text="%3Cspan%3E%E7%89%A9%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="书证" _mubu_text="%3Cspan%3E%E4%B9%A6%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="以记载的内容和反应的思想来证明" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%AE%B0%E8%BD%BD%E7%9A%84%E5%86%85%E5%AE%B9%E5%92%8C%E5%8F%8D%E5%BA%94%E7%9A%84%E6%80%9D%E6%83%B3%E6%9D%A5%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="必须产生在案发中，不能发生在办案过程中" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E4%BA%A7%E7%94%9F%E5%9C%A8%E6%A1%88%E5%8F%91%E4%B8%AD%EF%BC%8C%E4%B8%8D%E8%83%BD%E5%8F%91%E7%94%9F%E5%9C%A8%E5%8A%9E%E6%A1%88%E8%BF%87%E7%A8%8B%E4%B8%AD%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="证人证言" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="证人证言是案外人陈述" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%AF%81%E8%A8%80%E6%98%AF%E6%A1%88%E5%A4%96%E4%BA%BA%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="证人身份优先" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%BA%AB%E4%BB%BD%E4%BC%98%E5%85%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="证人证言的载体" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%AF%81%E8%A8%80%E7%9A%84%E8%BD%BD%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不影响证言性质" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E8%AF%81%E8%A8%80%E6%80%A7%E8%B4%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="证人保护" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国控黑毒应当保护，其他可请求保护" _mubu_text="%3Cspan%3E%E5%9B%BD%E6%8E%A7%E9%BB%91%E6%AF%92%E5%BA%94%E5%BD%93%E4%BF%9D%E6%8A%A4%EF%BC%8C%E5%85%B6%E4%BB%96%E5%8F%AF%E8%AF%B7%E6%B1%82%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="证人补助" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%A1%A5%E5%8A%A9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="谁通知，谁为证人补助" _mubu_text="%3Cspan%3E%E8%B0%81%E9%80%9A%E7%9F%A5%EF%BC%8C%E8%B0%81%E4%B8%BA%E8%AF%81%E4%BA%BA%E8%A1%A5%E5%8A%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="不得担任见证人" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%8B%85%E4%BB%BB%E8%A7%81%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="身体不行，身份不行" _mubu_text="%3Cspan%3E%E8%BA%AB%E4%BD%93%E4%B8%8D%E8%A1%8C%EF%BC%8C%E8%BA%AB%E4%BB%BD%E4%B8%8D%E8%A1%8C%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22xxNoBf3Awt%22%2C%22oh%22%3A999%2C%22ow%22%3A3170%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F15EE9D778E4C4F531716298370.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
                <outline text="证言采信规则" _mubu_text="%3Cspan%3E%E8%AF%81%E8%A8%80%E9%87%87%E4%BF%A1%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="前后矛盾，需要印证" _mubu_text="%3Cspan%3E%E5%89%8D%E5%90%8E%E7%9F%9B%E7%9B%BE%EF%BC%8C%E9%9C%80%E8%A6%81%E5%8D%B0%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="被害人陈述" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%AE%B3%E4%BA%BA%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="犯罪嫌疑人、被告人供述和辩解" _mubu_text="%3Cspan%3E%E7%8A%AF%E7%BD%AA%E5%AB%8C%E7%96%91%E4%BA%BA%E3%80%81%E8%A2%AB%E5%91%8A%E4%BA%BA%E4%BE%9B%E8%BF%B0%E5%92%8C%E8%BE%A9%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="被告人翻供：前后矛盾，需要印证" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%E4%BA%BA%E7%BF%BB%E4%BE%9B%EF%BC%9A%E5%89%8D%E5%90%8E%E7%9F%9B%E7%9B%BE%EF%BC%8C%E9%9C%80%E8%A6%81%E5%8D%B0%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="鉴定意见" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="必须有鉴定人签字，公章不能代替" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E6%9C%89%E9%89%B4%E5%AE%9A%E4%BA%BA%E7%AD%BE%E5%AD%97%EF%BC%8C%E5%85%AC%E7%AB%A0%E4%B8%8D%E8%83%BD%E4%BB%A3%E6%9B%BF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="倾向性意见不是鉴定意见，只能参考" _mubu_text="%3Cspan%3E%E5%80%BE%E5%90%91%E6%80%A7%E6%84%8F%E8%A7%81%E4%B8%8D%E6%98%AF%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%EF%BC%8C%E5%8F%AA%E8%83%BD%E5%8F%82%E8%80%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="意见不一致应列明，不能少数服从多数" _mubu_text="%3Cspan%3E%E6%84%8F%E8%A7%81%E4%B8%8D%E4%B8%80%E8%87%B4%E5%BA%94%E5%88%97%E6%98%8E%EF%BC%8C%E4%B8%8D%E8%83%BD%E5%B0%91%E6%95%B0%E6%9C%8D%E4%BB%8E%E5%A4%9A%E6%95%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="有专门知识的人提出的意见" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%93%E9%97%A8%E7%9F%A5%E8%AF%86%E7%9A%84%E4%BA%BA%E6%8F%90%E5%87%BA%E7%9A%84%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
                  <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22TJuER7abp6%22%2C%22oh%22%3A1000%2C%22ow%22%3A3477%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2F1B0F261452B14E721716298799.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
              </outline>
              <outline text="勘验、检查、辨认、侦查笔录" _mubu_text="%3Cspan%3E%E5%8B%98%E9%AA%8C%E3%80%81%E6%A3%80%E6%9F%A5%E3%80%81%E8%BE%A8%E8%AE%A4%E3%80%81%E4%BE%A6%E6%9F%A5%E7%AC%94%E5%BD%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="视听资料、电子数据" _mubu_text="%3Cspan%3E%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%E3%80%81%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="需要与案件相关，能反映案件起因、预备行为的材料" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E4%B8%8E%E6%A1%88%E4%BB%B6%E7%9B%B8%E5%85%B3%EF%BC%8C%E8%83%BD%E5%8F%8D%E6%98%A0%E6%A1%88%E4%BB%B6%E8%B5%B7%E5%9B%A0%E3%80%81%E9%A2%84%E5%A4%87%E8%A1%8C%E4%B8%BA%E7%9A%84%E6%9D%90%E6%96%99%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="取证有条件的，应当录音录像；拆分过程，应当录音录像" _mubu_text="%3Cspan%3E%E5%8F%96%E8%AF%81%E6%9C%89%E6%9D%A1%E4%BB%B6%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%EF%BC%9B%E6%8B%86%E5%88%86%E8%BF%87%E7%A8%8B%EF%BC%8C%E5%BA%94%E5%BD%93%E5%BD%95%E9%9F%B3%E5%BD%95%E5%83%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="【识别证据】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E8%AF%86%E5%88%AB%E8%AF%81%E6%8D%AE%E3%80%91%3C/span%3E" _note="" _mubu_note="">
                <outline text="1、认定证据种类，要看取证时的状态来确定（捏捏）" _mubu_text="%3Cspan%3E1%E3%80%81%E8%AE%A4%E5%AE%9A%E8%AF%81%E6%8D%AE%E7%A7%8D%E7%B1%BB%EF%BC%8C%E8%A6%81%E7%9C%8B%E5%8F%96%E8%AF%81%E6%97%B6%E7%9A%84%E7%8A%B6%E6%80%81%E6%9D%A5%E7%A1%AE%E5%AE%9A%EF%BC%88%E6%8D%8F%E6%8D%8F%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="2、取证时的证据状态确定后，对该证据的复制、打印、拍照、录制等传来形式，均不影响证据的原始属性" _mubu_text="%3Cspan%3E2%E3%80%81%E5%8F%96%E8%AF%81%E6%97%B6%E7%9A%84%E8%AF%81%E6%8D%AE%E7%8A%B6%E6%80%81%E7%A1%AE%E5%AE%9A%E5%90%8E%EF%BC%8C%E5%AF%B9%E8%AF%A5%E8%AF%81%E6%8D%AE%E7%9A%84%E5%A4%8D%E5%88%B6%E3%80%81%E6%89%93%E5%8D%B0%E3%80%81%E6%8B%8D%E7%85%A7%E3%80%81%E5%BD%95%E5%88%B6%E7%AD%89%E4%BC%A0%E6%9D%A5%E5%BD%A2%E5%BC%8F%EF%BC%8C%E5%9D%87%E4%B8%8D%E5%BD%B1%E5%93%8D%E8%AF%81%E6%8D%AE%E7%9A%84%E5%8E%9F%E5%A7%8B%E5%B1%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="3、一项证据可以多种类合体，关键看用证据的哪一方面来证明什么案情" _mubu_text="%3Cspan%3E3%E3%80%81%E4%B8%80%E9%A1%B9%E8%AF%81%E6%8D%AE%E5%8F%AF%E4%BB%A5%E5%A4%9A%E7%A7%8D%E7%B1%BB%E5%90%88%E4%BD%93%EF%BC%8C%E5%85%B3%E9%94%AE%E7%9C%8B%E7%94%A8%E8%AF%81%E6%8D%AE%E7%9A%84%E5%93%AA%E4%B8%80%E6%96%B9%E9%9D%A2%E6%9D%A5%E8%AF%81%E6%98%8E%E4%BB%80%E4%B9%88%E6%A1%88%E6%83%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="产生时间：" _mubu_text="%3Cspan%3E%E4%BA%A7%E7%94%9F%E6%97%B6%E9%97%B4%EF%BC%9A%3C/span%3E" _note="" _mubu_note="">
                  <outline text="案发中：物证、书证、视听资料电子数据" _mubu_text="%3Cspan%3E%E6%A1%88%E5%8F%91%E4%B8%AD%EF%BC%9A%E7%89%A9%E8%AF%81%E3%80%81%E4%B9%A6%E8%AF%81%E3%80%81%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="案发后：案发后的办案中" _mubu_text="%3Cspan%3E%E6%A1%88%E5%8F%91%E5%90%8E%EF%BC%9A%E6%A1%88%E5%8F%91%E5%90%8E%E7%9A%84%E5%8A%9E%E6%A1%88%E4%B8%AD%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="原始证据 VS 传来证据" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%A7%8B%E8%AF%81%E6%8D%AE%20VS%20%E4%BC%A0%E6%9D%A5%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="(1）通过技术手段提取的指纹，属于原始证据。" _mubu_text="%3Cspan%3E(1%EF%BC%89%E9%80%9A%E8%BF%87%E6%8A%80%E6%9C%AF%E6%89%8B%E6%AE%B5%E6%8F%90%E5%8F%96%E7%9A%84%E6%8C%87%E7%BA%B9%EF%BC%8C%E5%B1%9E%E4%BA%8E%E5%8E%9F%E5%A7%8B%E8%AF%81%E6%8D%AE%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="(2） 通过技术手段提取的音、视频等电子数据，属于传来证据。" _mubu_text="%3Cspan%3E(2%EF%BC%89%20%E9%80%9A%E8%BF%87%E6%8A%80%E6%9C%AF%E6%89%8B%E6%AE%B5%E6%8F%90%E5%8F%96%E7%9A%84%E9%9F%B3%E3%80%81%E8%A7%86%E9%A2%91%E7%AD%89%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%EF%BC%8C%E5%B1%9E%E4%BA%8E%E4%BC%A0%E6%9D%A5%E8%AF%81%E6%8D%AE%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="有罪证据 VS 无罪证据" _mubu_text="%3Cspan%3E%E6%9C%89%E7%BD%AA%E8%AF%81%E6%8D%AE%20VS%20%E6%97%A0%E7%BD%AA%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="言词证据 VS 实物证据" _mubu_text="%3Cspan%3E%E8%A8%80%E8%AF%8D%E8%AF%81%E6%8D%AE%20VS%20%E5%AE%9E%E7%89%A9%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
