              <outline text="暂予监外执行的——移送报到" _mubu_text="%3Cspan%3E%E6%9A%82%E4%BA%88%E7%9B%91%E5%A4%96%E6%89%A7%E8%A1%8C%E7%9A%84%E2%80%94%E2%80%94%E7%A7%BB%E9%80%81%E6%8A%A5%E5%88%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="监督管理" _mubu_text="%3Cspan%3E%E7%9B%91%E7%9D%A3%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="迁居无需法院批准" _mubu_text="%3Cspan%3E%E8%BF%81%E5%B1%85%E6%97%A0%E9%9C%80%E6%B3%95%E9%99%A2%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
