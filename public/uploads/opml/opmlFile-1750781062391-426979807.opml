              <outline text="公安负责人，检委会决定回避" _mubu_text="%3Cspan%3E%E5%85%AC%E5%AE%89%E8%B4%9F%E8%B4%A3%E4%BA%BA%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%A3%80%E5%A7%94%E4%BC%9A%3C/span%3E%3Cspan%3E%E5%86%B3%E5%AE%9A%E5%9B%9E%E9%81%BF%3C/span%3E" _note="" _mubu_note=""/>
