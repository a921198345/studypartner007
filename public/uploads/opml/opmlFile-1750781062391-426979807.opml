              <outline text="【口诀】陪审不可以，七c五二一" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E9%99%AA%E5%AE%A1%E4%B8%8D%E5%8F%AF%E4%BB%A5%EF%BC%8C%E4%B8%83c%E4%BA%94%E4%BA%8C%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="审判委员会" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E5%A7%94%E5%91%98%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="应当上审委会" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E4%B8%8A%E5%AE%A1%E5%A7%94%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="死再上会" _mubu_text="%3Cspan%3E%E6%AD%BB%E5%86%8D%E4%B8%8A%E4%BC%9A%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22npl8TVPf22%22%2C%22oh%22%3A1000%2C%22ow%22%3A3728%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2FB3F7F0A66D754BB31717043283.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
            <outline text="院长最后发言" _mubu_text="%3Cspan%3E%E9%99%A2%E9%95%BF%E6%9C%80%E5%90%8E%E5%8F%91%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="【章节体系】" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E7%AB%A0%E8%8A%82%E4%BD%93%E7%B3%BB%E3%80%91%3C/span%3E" _note="" _mubu_note="">
          <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%229Lc0YoNrDB%22%2C%22oh%22%3A851%2C%22ow%22%3A4000%2C%22uri%22%3A%22https%3A%2F%2Fdocument-image.mubu.com%2Fdocument_image%2FA1D295F06F764C161717041565.jpg%22%2C%22w%22%3A400%7D%5D"/>
        </outline>
      </outline>
      <outline text="15 一审程序" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E15%20%E4%B8%80%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="" _heading="2">
        <outline text="公诉案件" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%89%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="庭前审查" _mubu_text="%3Cspan%3E%E5%BA%AD%E5%89%8D%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="审查方式：形式性审查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E6%96%B9%E5%BC%8F%EF%BC%9A%E5%BD%A2%E5%BC%8F%E6%80%A7%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审查处理" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="决定开庭" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E5%BC%80%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应当退检：【口诀】亲告不在不应管，死亡时效已赦免，无事又来应退检" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E9%80%80%E6%A3%80%EF%BC%9A%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E4%BA%B2%E5%91%8A%E4%B8%8D%E5%9C%A8%E4%B8%8D%E5%BA%94%E7%AE%A1%EF%BC%8C%E6%AD%BB%E4%BA%A1%E6%97%B6%E6%95%88%E5%B7%B2%E8%B5%A6%E5%85%8D%EF%BC%8C%E6%97%A0%E4%BA%8B%E5%8F%88%E6%9D%A5%E5%BA%94%E9%80%80%E6%A3%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="不属于本院管辖" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%B1%9E%E4%BA%8E%E6%9C%AC%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="被告人不在案" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%E4%BA%BA%E4%B8%8D%E5%9C%A8%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法院裁定撤诉，无新重新起诉" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%A3%81%E5%AE%9A%E6%92%A4%E8%AF%89%EF%BC%8C%E6%97%A0%E6%96%B0%E9%87%8D%E6%96%B0%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="显时赦，告死他，虽是坏人不追责（无显）" _mubu_text="%3Cspan%3E%E6%98%BE%E6%97%B6%E8%B5%A6%EF%BC%8C%E5%91%8A%E6%AD%BB%E4%BB%96%EF%BC%8C%E8%99%BD%E6%98%AF%E5%9D%8F%E4%BA%BA%E4%B8%8D%E8%BF%BD%E8%B4%A3%EF%BC%88%E6%97%A0%E6%98%BE%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="补充材料" _mubu_text="%3Cspan%3E%E8%A1%A5%E5%85%85%E6%9D%90%E6%96%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="依法受理" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="身份不明，但事实清楚证据充分" _mubu_text="%3Cspan%3E%E8%BA%AB%E4%BB%BD%E4%B8%8D%E6%98%8E%EF%BC%8C%E4%BD%86%E4%BA%8B%E5%AE%9E%E6%B8%85%E6%A5%9A%E8%AF%81%E6%8D%AE%E5%85%85%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="重新起诉：证据不足无罪，发现新证据，原判无需推翻，只需重新起诉" _mubu_text="%3Cspan%3E%E9%87%8D%E6%96%B0%E8%B5%B7%E8%AF%89%EF%BC%9A%E8%AF%81%E6%8D%AE%E4%B8%8D%E8%B6%B3%E6%97%A0%E7%BD%AA%EF%BC%8C%E5%8F%91%E7%8E%B0%E6%96%B0%E8%AF%81%E6%8D%AE%EF%BC%8C%E5%8E%9F%E5%88%A4%E6%97%A0%E9%9C%80%E6%8E%A8%E7%BF%BB%EF%BC%8C%E5%8F%AA%E9%9C%80%E9%87%8D%E6%96%B0%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="开庭审判前的准备" _mubu_text="%3Cspan%3E%E5%BC%80%E5%BA%AD%E5%AE%A1%E5%88%A4%E5%89%8D%E7%9A%84%E5%87%86%E5%A4%87%3C/span%3E" _note="" _mubu_note="">
            <outline text="通知与送达" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%E4%B8%8E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
              <outline text="简易速裁无需按时送达" _mubu_text="%3Cspan%3E%E7%AE%80%E6%98%93%E9%80%9F%E8%A3%81%E6%97%A0%E9%9C%80%E6%8C%89%E6%97%B6%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="庭前会议" _mubu_text="%3Cspan%3E%E5%BA%AD%E5%89%8D%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="召开条件：【口诀】庭前会，多杂大，可召开" _mubu_text="%3Cspan%3E%E5%8F%AC%E5%BC%80%E6%9D%A1%E4%BB%B6%EF%BC%9A%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%BA%AD%E5%89%8D%E4%BC%9A%EF%BC%8C%E5%A4%9A%E6%9D%82%E5%A4%A7%EF%BC%8C%E5%8F%AF%E5%8F%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="可以召开" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E5%8F%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="控辩审" _mubu_text="%3Cspan%3E%E6%8E%A7%E8%BE%A9%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可以通知被告，参加并非权利" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E9%80%9A%E7%9F%A5%E8%A2%AB%E5%91%8A%EF%BC%8C%E5%8F%82%E5%8A%A0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%B9%B6%E9%9D%9E%3C/span%3E%3Cspan%3E%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="会议内容" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="只处理程序" _mubu_text="%3Cspan%3E%E5%8F%AA%E5%A4%84%E7%90%86%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不审查事实、证据" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AE%A1%E6%9F%A5%E4%BA%8B%E5%AE%9E%E3%80%81%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="证据展示" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E5%B1%95%E7%A4%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="庭审时调查证据" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%BA%AD%E5%AE%A1%E6%97%B6%3C/span%3E%3Cspan%3E%E8%B0%83%E6%9F%A5%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="庭前会议不调查证据" _mubu_text="%3Cspan%3E%E5%BA%AD%E5%89%8D%E4%BC%9A%E8%AE%AE%E4%B8%8D%E8%B0%83%E6%9F%A5%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="法庭审判" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E5%AE%A1%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="开庭" _mubu_text="%3Cspan%3E%E5%BC%80%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
              <outline text="被害人不来，接着审；" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%AE%B3%E4%BA%BA%E4%B8%8D%E6%9D%A5%EF%BC%8C%E6%8E%A5%E7%9D%80%E5%AE%A1%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="辩护人不来，被告人同意才能接着审；" _mubu_text="%3Cspan%3E%E8%BE%A9%E6%8A%A4%E4%BA%BA%E4%B8%8D%E6%9D%A5%EF%BC%8C%E8%A2%AB%E5%91%8A%E4%BA%BA%E5%90%8C%E6%84%8F%E6%89%8D%E8%83%BD%E6%8E%A5%E7%9D%80%E5%AE%A1%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="“盲精未，无死缺，应法援”，同意了也不能接着审" _mubu_text="%3Cspan%3E%E2%80%9C%E7%9B%B2%E7%B2%BE%E6%9C%AA%EF%BC%8C%E6%97%A0%E6%AD%BB%E7%BC%BA%EF%BC%8C%E5%BA%94%E6%B3%95%E6%8F%B4%E2%80%9D%EF%BC%8C%E5%90%8C%E6%84%8F%E4%BA%86%E4%B9%9F%E4%B8%8D%E8%83%BD%E6%8E%A5%E7%9D%80%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法庭调查" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E8%B0%83%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="一般发问规则" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E5%8F%91%E9%97%AE%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="先控后辩" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%88%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E6%8E%A7%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E5%90%8E%E8%BE%A9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="控方：公诉人、被害人、附民原告人" _mubu_text="%3Cspan%3E%E6%8E%A7%E6%96%B9%EF%BC%9A%E5%85%AC%E8%AF%89%E4%BA%BA%E3%80%81%E8%A2%AB%E5%AE%B3%E4%BA%BA%E3%80%81%E9%99%84%E6%B0%91%E5%8E%9F%E5%91%8A%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="一般出庭规则" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E5%87%BA%E5%BA%AD%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院审查证人出庭必要性" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%AE%A1%E6%9F%A5%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%E5%BF%85%E8%A6%81%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="证人出庭谁通知谁补助" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%E8%B0%81%E9%80%9A%E7%9F%A5%E8%B0%81%E8%A1%A5%E5%8A%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="证人调查规则" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%B0%83%E6%9F%A5%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="可以不出庭：【口诀】重病、远离、在国外，作证可以不出庭" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E5%87%BA%E5%BA%AD%EF%BC%9A%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E9%87%8D%E7%97%85%E3%80%81%E8%BF%9C%E7%A6%BB%E3%80%81%E5%9C%A8%E5%9B%BD%E5%A4%96%EF%BC%8C%E4%BD%9C%E8%AF%81%E5%8F%AF%E4%BB%A5%E4%B8%8D%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="证人拒不出庭后果" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E6%8B%92%E4%B8%8D%E5%87%BA%E5%BA%AD%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="强制证人出庭令，不是拘传、传唤" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%E4%BB%A4%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%98%AF%3C/span%3E%3Cspan%3E%E6%8B%98%E4%BC%A0%E3%80%81%E4%BC%A0%E5%94%A4%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="配偶、父母、子女不强制" _mubu_text="%3Cspan%3E%E9%85%8D%E5%81%B6%E3%80%81%E7%88%B6%E6%AF%8D%E3%80%81%E5%AD%90%E5%A5%B3%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%BC%BA%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="不豁免作证义务" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%B1%81%E5%85%8D%E4%BD%9C%E8%AF%81%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="不含兄弟姐妹" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%B8%8D%E5%90%AB%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%84%E5%BC%9F%E5%A7%90%E5%A6%B9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="司法拘留" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="讯问证人规则" _mubu_text="%3Cspan%3E%E8%AE%AF%E9%97%AE%E8%AF%81%E4%BA%BA%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="证人先说——提请方发文——对方发问" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E5%85%88%E8%AF%B4%E2%80%94%E2%80%94%E6%8F%90%E8%AF%B7%E6%96%B9%E5%8F%91%E6%96%87%E2%80%94%E2%80%94%E5%AF%B9%E6%96%B9%E5%8F%91%E9%97%AE%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="鉴定人调查规则" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E8%B0%83%E6%9F%A5%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="鉴定人拒不到庭后果" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E6%8B%92%E4%B8%8D%E5%88%B0%E5%BA%AD%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不到庭，鉴定意见不作为定案根据" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%88%B0%E5%BA%AD%EF%BC%8C%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%E4%B8%8D%E4%BD%9C%E4%B8%BA%E5%AE%9A%E6%A1%88%E6%A0%B9%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="正当理由，延期审理" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%EF%BC%8C%E5%BB%B6%E6%9C%9F%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="有专门知识的人出庭" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%93%E9%97%A8%E7%9F%A5%E8%AF%86%E7%9A%84%E4%BA%BA%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
                  <outline text="依申请" _mubu_text="%3Cspan%3E%E4%BE%9D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="其他调查规则" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E8%B0%83%E6%9F%A5%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="法庭庭外调查证据" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E5%BA%AD%E5%A4%96%E8%B0%83%E6%9F%A5%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不能搜查" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%83%BD%3C/span%3E%3Cspan%3E%E6%90%9C%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="法庭调取的证据应质证" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B3%95%E5%BA%AD%E8%B0%83%E5%8F%96%E7%9A%84%E8%AF%81%E6%8D%AE%E5%BA%94%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                  <outline text="双方无异议除外" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E6%97%A0%E5%BC%82%E8%AE%AE%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="补充侦查" _mubu_text="%3Cspan%3E%E8%A1%A5%E5%85%85%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                  <outline text="公诉人建议，应当同意" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%89%E4%BA%BA%E5%BB%BA%E8%AE%AE%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BA%94%E5%BD%93%3C/span%3E%3Cspan%3E%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="发现新量刑证据" _mubu_text="%3Cspan%3E%E5%8F%91%E7%8E%B0%E6%96%B0%E9%87%8F%E5%88%91%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="合议庭发现：移送" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E%3Cspan%3E%E5%8F%91%E7%8E%B0%EF%BC%9A%E7%A7%BB%E9%80%81%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="被告人提出：侦查" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%A2%AB%E5%91%8A%E4%BA%BA%3C/span%3E%3Cspan%3E%E6%8F%90%E5%87%BA%EF%BC%9A%E4%BE%A6%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="法庭辩论" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="量刑建议与审查" _mubu_text="%3Cspan%3E%E9%87%8F%E5%88%91%E5%BB%BA%E8%AE%AE%E4%B8%8E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="怎么提" _mubu_text="%3Cspan%3E%E6%80%8E%E4%B9%88%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="一般幅度刑，例外确定刑" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E5%B9%85%E5%BA%A6%E5%88%91%EF%BC%8C%E4%BE%8B%E5%A4%96%E7%A1%AE%E5%AE%9A%E5%88%91%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="一般量刑建议书，速裁例外可在起诉书" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E9%87%8F%E5%88%91%E5%BB%BA%E8%AE%AE%E4%B9%A6%EF%BC%8C%E9%80%9F%E8%A3%81%E4%BE%8B%E5%A4%96%E5%8F%AF%E5%9C%A8%E8%B5%B7%E8%AF%89%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="认罪认罚不从宽【口诀】国恐黑头，三特累惯，性侵小孩，虽然不宽" _mubu_text="%3Cspan%3E%E8%AE%A4%E7%BD%AA%E8%AE%A4%E7%BD%9A%E4%B8%8D%E4%BB%8E%E5%AE%BD%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%9B%BD%E6%81%90%E9%BB%91%E5%A4%B4%EF%BC%8C%E4%B8%89%E7%89%B9%E7%B4%AF%E6%83%AF%EF%BC%8C%E6%80%A7%E4%BE%B5%E5%B0%8F%E5%AD%A9%EF%BC%8C%E8%99%BD%E7%84%B6%E4%B8%8D%E5%AE%BD%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="仅以量刑过重上诉，应当抗诉" _mubu_text="%3Cspan%3E%E4%BB%85%E4%BB%A5%E9%87%8F%E5%88%91%E8%BF%87%E9%87%8D%E4%B8%8A%E8%AF%89%EF%BC%8C%E5%BA%94%E5%BD%93%E6%8A%97%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="怎么判" _mubu_text="%3Cspan%3E%E6%80%8E%E4%B9%88%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="是否采纳法院定" _mubu_text="%3Cspan%3E%E6%98%AF%E5%90%A6%E9%87%87%E7%BA%B3%E6%B3%95%E9%99%A2%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="量刑说理是必须" _mubu_text="%3Cspan%3E%E9%87%8F%E5%88%91%E8%AF%B4%E7%90%86%E6%98%AF%E5%BF%85%E9%A1%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="宣判" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="宣判前撤诉：法院审查，宣告判决前" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%88%A4%E5%89%8D%E6%92%A4%E8%AF%89%EF%BC%9A%E6%B3%95%E9%99%A2%E5%AE%A1%E6%9F%A5%EF%BC%8C%E5%AE%A3%E5%91%8A%E5%88%A4%E5%86%B3%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宣判前更换审判人员：评议前，换人重判；评议后，不换写名" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%88%A4%E5%89%8D%E6%9B%B4%E6%8D%A2%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%EF%BC%9A%E8%AF%84%E8%AE%AE%E5%89%8D%EF%BC%8C%E6%8D%A2%E4%BA%BA%E9%87%8D%E5%88%A4%EF%BC%9B%E8%AF%84%E8%AE%AE%E5%90%8E%EF%BC%8C%E4%B8%8D%E6%8D%A2%E5%86%99%E5%90%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检察院变更、追加、补充起诉" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E5%8F%98%E6%9B%B4%E3%80%81%E8%BF%BD%E5%8A%A0%E3%80%81%E8%A1%A5%E5%85%85%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22BmCjW5NwQu%22%2C%22oh%22%3A1000%2C%22ow%22%3A3623%2C%22uri%22%3A%22document_image%2F27EDBEA61C814D891717125932.jpg%22%2C%22w%22%3A400%7D%5D"/>
              </outline>
              <outline text="法院发现新事实：不告不理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%91%E7%8E%B0%E6%96%B0%E4%BA%8B%E5%AE%9E%EF%BC%9A%E4%B8%8D%E5%91%8A%E4%B8%8D%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宣判时，被告人必须到庭" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%AE%A3%E5%88%A4%E6%97%B6%EF%BC%8C%E8%A2%AB%E5%91%8A%E4%BA%BA%3C/span%3E%3Cspan%3E%E5%BF%85%E9%A1%BB%E5%88%B0%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宣判情形" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%88%A4%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="不满 16或精神病，不负刑事责任" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%BB%A1%2016%E6%88%96%E7%B2%BE%E7%A5%9E%E7%97%85%EF%BC%8C%E4%B8%8D%E8%B4%9F%E5%88%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="公开宣判" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%85%AC%E5%BC%80%3C/span%3E%3Cspan%3E%E5%AE%A3%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="送达" _mubu_text="%3Cspan%3E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="当庭宣判：5 日内" _mubu_text="%3Cspan%3E%E5%BD%93%E5%BA%AD%E5%AE%A3%E5%88%A4%EF%BC%9A5%20%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="区别民诉：10 日内" _mubu_text="%3Cspan%3E%E5%8C%BA%E5%88%AB%E6%B0%91%E8%AF%89%EF%BC%9A10%20%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="定期宣判：当庭" _mubu_text="%3Cspan%3E%E5%AE%9A%E6%9C%9F%E5%AE%A3%E5%88%A4%EF%BC%9A%E5%BD%93%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="其他程序" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="延期、中止、终止、休庭" _mubu_text="%3Cspan%3E%E5%BB%B6%E6%9C%9F%E3%80%81%E4%B8%AD%E6%AD%A2%E3%80%81%E7%BB%88%E6%AD%A2%E3%80%81%E4%BC%91%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
              <outline text="延期vs中止【口诀】等人等证可延期，不可抗力需中止" _mubu_text="%3Cspan%3E%E5%BB%B6%E6%9C%9Fvs%E4%B8%AD%E6%AD%A2%E3%80%90%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E5%8F%A3%E8%AF%80%E3%80%91%E7%AD%89%E4%BA%BA%E7%AD%89%E8%AF%81%E5%8F%AF%E5%BB%B6%E6%9C%9F%EF%BC%8C%E4%B8%8D%E5%8F%AF%E6%8A%97%E5%8A%9B%E9%9C%80%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="休庭【口诀】回避审完缺律师，应当休庭作处置；等人等证做准备，可以休庭等一会" _mubu_text="%3Cspan%3E%E4%BC%91%E5%BA%AD%3C/span%3E%3Cspan%20class=%22%20text-color-blue%22%3E%E3%80%90%E5%8F%A3%E8%AF%80%E3%80%91%E5%9B%9E%E9%81%BF%E5%AE%A1%E5%AE%8C%E7%BC%BA%E5%BE%8B%E5%B8%88%EF%BC%8C%E5%BA%94%E5%BD%93%E4%BC%91%E5%BA%AD%E4%BD%9C%E5%A4%84%E7%BD%AE%EF%BC%9B%E7%AD%89%E4%BA%BA%E7%AD%89%E8%AF%81%E5%81%9A%E5%87%86%E5%A4%87%EF%BC%8C%E5%8F%AF%E4%BB%A5%E4%BC%91%E5%BA%AD%E7%AD%89%E4%B8%80%E4%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法庭秩序" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E7%A7%A9%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="涉案财物处置" _mubu_text="%3Cspan%3E%E6%B6%89%E6%A1%88%E8%B4%A2%E7%89%A9%E5%A4%84%E7%BD%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="自诉案件" _mubu_text="%3Cspan%3E%E8%87%AA%E8%AF%89%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="" _heading="3">
          <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
            <outline text="五个可以：调和撤反简" _mubu_text="%3Cspan%3E%E4%BA%94%E4%B8%AA%E5%8F%AF%E4%BB%A5%EF%BC%9A%E8%B0%83%E5%92%8C%E6%92%A4%E5%8F%8D%E7%AE%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="调解" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%B0%83%3C/span%3E%3Cspan%3E%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="和解" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%92%8C%3C/span%3E%3Cspan%3E%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="撤回自诉" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%92%A4%3C/span%3E%3Cspan%3E%E5%9B%9E%E8%87%AA%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="反诉" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%8D%3C/span%3E%3Cspan%3E%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="简易程序" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%AE%80%3C/span%3E%3Cspan%3E%E6%98%93%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="公转自，两个不可以：调反" _mubu_text="%3Cspan%3E%E5%85%AC%E8%BD%AC%E8%87%AA%EF%BC%8C%E4%B8%A4%E4%B8%AA%E4%B8%8D%E5%8F%AF%E4%BB%A5%EF%BC%9A%E8%B0%83%E5%8F%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="调解" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%B0%83%3C/span%3E%3Cspan%3E%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="反诉" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%8D%3C/span%3E%3Cspan%3E%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
