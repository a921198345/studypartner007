              <outline text="刑诉程序正在进行：可以拒绝" _mubu_text="%3Cspan%3E%E5%88%91%E8%AF%89%E7%A8%8B%E5%BA%8F%E6%AD%A3%E5%9C%A8%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%BF%9B%E8%A1%8C%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E6%8B%92%E7%BB%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="酷刑程序缺席判" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%85%B7%E5%88%91%E7%A8%8B%E5%BA%8F%E7%BC%BA%E5%B8%AD%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="引渡程序" _mubu_text="%3Cspan%3E%E5%BC%95%E6%B8%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="联系机关：外交部" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%81%94%E7%B3%BB%3C/span%3E%3Cspan%3E%E6%9C%BA%E5%85%B3%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%A4%96%E4%BA%A4%E9%83%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="决策机关" _mubu_text="%3Cspan%3E%E5%86%B3%E7%AD%96%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="引出：高院裁定，最高院复核" _mubu_text="%3Cspan%3E%E5%BC%95%E5%87%BA%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%AB%98%E9%99%A2%E8%A3%81%3C/span%3E%3Cspan%3E%E5%AE%9A%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9C%80%E9%AB%98%3C/span%3E%3Cspan%3E%E9%99%A2%E5%A4%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%A0%B8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="引入承诺" _mubu_text="%3Cspan%3E%E5%BC%95%E5%85%A5%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="量刑：最高院" _mubu_text="%3Cspan%3E%E9%87%8F%E5%88%91%EF%BC%9A%E6%9C%80%E9%AB%98%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="定罪（限制追诉）：最高检" _mubu_text="%3Cspan%3E%E5%AE%9A%E7%BD%AA%EF%BC%88%E9%99%90%E5%88%B6%E8%BF%BD%E8%AF%89%EF%BC%89%EF%BC%9A%E6%9C%80%E9%AB%98%E6%A3%80%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="引渡效果" _mubu_text="%3Cspan%3E%E5%BC%95%E6%B8%A1%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="罪名特定" _mubu_text="%3Cspan%3E%E7%BD%AA%E5%90%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%89%B9%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="非经引出国同意不得转引渡" _mubu_text="%3Cspan%3E%E9%9D%9E%E7%BB%8F%E5%BC%95%E5%87%BA%E5%9B%BD%E5%90%8C%E6%84%8F%E4%B8%8D%E5%BE%97%E8%BD%AC%E5%BC%95%E6%B8%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="庇护" _mubu_text="%3Cspan%3E%E5%BA%87%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
          <outline text="对象：境内外国人" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B1%A1%EF%BC%9A%E5%A2%83%E5%86%85%E5%A4%96%E5%9B%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="构成：允许居留 + 拒绝引渡" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%EF%BC%9A%E5%85%81%E8%AE%B8%E5%B1%85%E7%95%99%20+%20%E6%8B%92%E7%BB%9D%E5%BC%95%E6%B8%A1%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="无权庇护：战争罪、危害人类罪、劫机罪、侵害外交代表罪" _mubu_text="%3Cspan%3E%E6%97%A0%E6%9D%83%E5%BA%87%E6%8A%A4%EF%BC%9A%E6%88%98%E4%BA%89%E7%BD%AA%E3%80%81%E5%8D%B1%E5%AE%B3%E4%BA%BA%E7%B1%BB%E7%BD%AA%E3%80%81%E5%8A%AB%E6%9C%BA%E7%BD%AA%E3%80%81%E4%BE%B5%E5%AE%B3%E5%A4%96%E4%BA%A4%E4%BB%A3%E8%A1%A8%E7%BD%AA%3C/span%3E" _note="" _mubu_note="">
            <outline text="或引渡或起诉：劫机罪" _mubu_text="%3Cspan%3E%E6%88%96%E5%BC%95%E6%B8%A1%E6%88%96%E8%B5%B7%E8%AF%89%EF%BC%9A%E5%8A%AB%E6%9C%BA%E7%BD%AA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="或引渡或起诉或强制出境：战争罪、危害人类罪、侵害外交代表罪" _mubu_text="%3Cspan%3E%E6%88%96%E5%BC%95%E6%B8%A1%E6%88%96%E8%B5%B7%E8%AF%89%E6%88%96%E5%BC%BA%E5%88%B6%E5%87%BA%E5%A2%83%EF%BC%9A%E6%88%98%E4%BA%89%E7%BD%AA%E3%80%81%E5%8D%B1%E5%AE%B3%E4%BA%BA%E7%B1%BB%E7%BD%AA%E3%80%81%E4%BE%B5%E5%AE%B3%E5%A4%96%E4%BA%A4%E4%BB%A3%E8%A1%A8%E7%BD%AA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="外交庇护" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E5%BA%87%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="使领馆" _mubu_text="%3Cspan%3E%E4%BD%BF%E9%A2%86%E9%A6%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无合法依据" _mubu_text="%3Cspan%3E%E6%97%A0%E5%90%88%E6%B3%95%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="05 外交关系法和领事关系法" _mubu_text="%3Cspan%3E05%20%E5%A4%96%E4%BA%A4%E5%85%B3%E7%B3%BB%E6%B3%95%E5%92%8C%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="外交关系 VS 领事关系" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E5%85%B3%E7%B3%BB%20VS%20%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="并非领导关系" _mubu_text="%3Cspan%3E%E5%B9%B6%E9%9D%9E%E9%A2%86%E5%AF%BC%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="交涉对象" _mubu_text="%3Cspan%3E%E4%BA%A4%E6%B6%89%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="外交关系：中央" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E5%85%B3%E7%B3%BB%EF%BC%9A%E4%B8%AD%E5%A4%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领事关系：地方" _mubu_text="%3Cspan%3E%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%EF%BC%9A%E5%9C%B0%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="职务范围" _mubu_text="%3Cspan%3E%E8%81%8C%E5%8A%A1%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="外交关系：全能" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E5%85%B3%E7%B3%BB%EF%BC%9A%E5%85%A8%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领事关系：非政治职能（商务、文化、侨民保护）" _mubu_text="%3Cspan%3E%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%9D%9E%E6%94%BF%E6%B2%BB%3C/span%3E%3Cspan%3E%E8%81%8C%E8%83%BD%EF%BC%88%E5%95%86%E5%8A%A1%E3%80%81%E6%96%87%E5%8C%96%E3%80%81%E4%BE%A8%E6%B0%91%E4%BF%9D%E6%8A%A4%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="外交机关和使馆人员" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E6%9C%BA%E5%85%B3%E5%92%8C%E4%BD%BF%E9%A6%86%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
          <outline text="外交机关" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="常驻：使馆" _mubu_text="%3Cspan%3E%E5%B8%B8%E9%A9%BB%EF%BC%9A%E4%BD%BF%E9%A6%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="大使：国家元首" _mubu_text="%3Cspan%3E%E5%A4%A7%E4%BD%BF%EF%BC%9A%E5%9B%BD%E5%AE%B6%E5%85%83%E9%A6%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公使：国家元首" _mubu_text="%3Cspan%3E%E5%85%AC%E4%BD%BF%EF%BC%9A%E5%9B%BD%E5%AE%B6%E5%85%83%E9%A6%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="代办：外交部长" _mubu_text="%3Cspan%3E%E4%BB%A3%E5%8A%9E%EF%BC%9A%E5%A4%96%E4%BA%A4%E9%83%A8%E9%95%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="临时：特别使团" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%EF%BC%9A%E7%89%B9%E5%88%AB%E4%BD%BF%E5%9B%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="使馆人员" _mubu_text="%3Cspan%3E%E4%BD%BF%E9%A6%86%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
            <outline text="外交人员：馆长、参赞、武官、外交秘书、随员，「不受欢迎」" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E4%BA%BA%E5%91%98%EF%BC%9A%E9%A6%86%E9%95%BF%E3%80%81%E5%8F%82%E8%B5%9E%E3%80%81%E6%AD%A6%E5%AE%98%E3%80%81%E5%A4%96%E4%BA%A4%E7%A7%98%E4%B9%A6%E3%80%81%E9%9A%8F%E5%91%98%EF%BC%8C%E3%80%8C%E4%B8%8D%E5%8F%97%E6%AC%A2%E8%BF%8E%E3%80%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="行政人员、服务人员：「不被接受」" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E4%BA%BA%E5%91%98%E3%80%81%E6%9C%8D%E5%8A%A1%E4%BA%BA%E5%91%98%EF%BC%9A%E3%80%8C%E4%B8%8D%E8%A2%AB%E6%8E%A5%E5%8F%97%E3%80%8D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="派遣" _mubu_text="%3Cspan%3E%E6%B4%BE%E9%81%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="经同意才能派遣：馆长、武官、特别使团" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%90%8C%E6%84%8F%E6%89%8D%E8%83%BD%E6%B4%BE%E9%81%A3%EF%BC%9A%E9%A6%86%E9%95%BF%E3%80%81%E6%AD%A6%E5%AE%98%E3%80%81%E7%89%B9%E5%88%AB%E4%BD%BF%E5%9B%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="经同意 + 可反悔：不具有派遣国国籍人员" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%90%8C%E6%84%8F%20+%20%E5%8F%AF%E5%8F%8D%E6%82%94%EF%BC%9A%E4%B8%8D%E5%85%B7%E6%9C%89%E6%B4%BE%E9%81%A3%E5%9B%BD%E5%9B%BD%E7%B1%8D%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="职务开始" _mubu_text="%3Cspan%3E%E8%81%8C%E5%8A%A1%E5%BC%80%E5%A7%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="使馆馆长：递交国书" _mubu_text="%3Cspan%3E%E4%BD%BF%E9%A6%86%E9%A6%86%E9%95%BF%EF%BC%9A%E9%80%92%E4%BA%A4%E5%9B%BD%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领馆馆长：接受证书" _mubu_text="%3Cspan%3E%E9%A2%86%E9%A6%86%E9%A6%86%E9%95%BF%EF%BC%9A%E6%8E%A5%E5%8F%97%E8%AF%81%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="其他人员：自动开始" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%91%98%EF%BC%9A%E8%87%AA%E5%8A%A8%E5%BC%80%E5%A7%8B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="使领馆的特权与豁免" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BD%BF%E9%A2%86%E9%A6%86%E7%9A%84%E7%89%B9%E6%9D%83%E4%B8%8E%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="馆舍不得侵犯" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%A6%86%E8%88%8D%E4%B8%8D%E5%BE%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note="">
            <outline text="使馆：非经馆长同意不得进入，包括公务、休息区域、馆长私人官邸" _mubu_text="%3Cspan%3E%E4%BD%BF%E9%A6%86%EF%BC%9A%E9%9D%9E%E7%BB%8F%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%A6%86%E9%95%BF%3C/span%3E%3Cspan%3E%E5%90%8C%E6%84%8F%E4%B8%8D%E5%BE%97%E8%BF%9B%E5%85%A5%EF%BC%8C%E5%8C%85%E6%8B%AC%E5%85%AC%E5%8A%A1%E3%80%81%E4%BC%91%E6%81%AF%E5%8C%BA%E5%9F%9F%E3%80%81%E9%A6%86%E9%95%BF%E7%A7%81%E4%BA%BA%E5%AE%98%E9%82%B8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领馆：非经馆长同意不得进入，仅限工作区域，紧急情况可未经同意，可征用 + 补偿" _mubu_text="%3Cspan%3E%E9%A2%86%E9%A6%86%EF%BC%9A%E9%9D%9E%E7%BB%8F%E9%A6%86%E9%95%BF%E5%90%8C%E6%84%8F%E4%B8%8D%E5%BE%97%E8%BF%9B%E5%85%A5%EF%BC%8C%E4%BB%85%E9%99%90%E5%B7%A5%E4%BD%9C%E5%8C%BA%E5%9F%9F%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%B4%A7%E6%80%A5%E6%83%85%E5%86%B5%3C/span%3E%3Cspan%3E%E5%8F%AF%E6%9C%AA%E7%BB%8F%E5%90%8C%E6%84%8F%EF%BC%8C%E5%8F%AF%E5%BE%81%E7%94%A8%20+%20%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="通讯自由" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%9A%E8%AE%AF%E8%87%AA%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="使馆：外交邮袋不得开拆或扣留" _mubu_text="%3Cspan%3E%E4%BD%BF%E9%A6%86%EF%BC%9A%E5%A4%96%E4%BA%A4%E9%82%AE%E8%A2%8B%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%BE%97%3C/span%3E%3Cspan%3E%E5%BC%80%E6%8B%86%E6%88%96%E6%89%A3%E7%95%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领馆：领馆邮袋可在授权代表在场下开拆，拒绝开拆退回原发送地" _mubu_text="%3Cspan%3E%E9%A2%86%E9%A6%86%EF%BC%9A%E9%A2%86%E9%A6%86%E9%82%AE%E8%A2%8B%E5%8F%AF%E5%9C%A8%E6%8E%88%E6%9D%83%E4%BB%A3%E8%A1%A8%E5%9C%A8%E5%9C%BA%E4%B8%8B%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BC%80%E6%8B%86%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%8B%92%E7%BB%9D%E5%BC%80%E6%8B%86%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%80%E5%9B%9E%3C/span%3E%3Cspan%3E%E5%8E%9F%E5%8F%91%E9%80%81%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="相同点" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%90%8C%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="装置、无线电发报机需经许可" _mubu_text="%3Cspan%3E%E8%A3%85%E7%BD%AE%E3%80%81%E6%97%A0%E7%BA%BF%E7%94%B5%E5%8F%91%E6%8A%A5%E6%9C%BA%E9%9C%80%E7%BB%8F%E8%AE%B8%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="信差执行职务时人身不得侵犯" _mubu_text="%3Cspan%3E%E4%BF%A1%E5%B7%AE%E6%89%A7%E8%A1%8C%E8%81%8C%E5%8A%A1%E6%97%B6%E4%BA%BA%E8%BA%AB%E4%B8%8D%E5%BE%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="外交人员和领事官员的特权与豁免" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%A4%96%E4%BA%A4%E4%BA%BA%E5%91%98%E5%92%8C%E9%A2%86%E4%BA%8B%E5%AE%98%E5%91%98%E7%9A%84%E7%89%B9%E6%9D%83%E4%B8%8E%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="范围：配偶、未成年子女" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%EF%BC%9A%E9%85%8D%E5%81%B6%E3%80%81%E6%9C%AA%E6%88%90%E5%B9%B4%E5%AD%90%E5%A5%B3%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="管辖豁免" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%AE%A1%E8%BE%96%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="外交人员" _mubu_text="%3Cspan%3E%E5%A4%96%E4%BA%A4%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="刑事管辖豁免，并非刑事责任豁免" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E7%AE%A1%E8%BE%96%E8%B1%81%E5%85%8D%EF%BC%8C%E5%B9%B6%E9%9D%9E%E5%88%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="民事豁免：除不动产、继承、专业商业、反诉" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E8%B1%81%E5%85%8D%EF%BC%9A%E9%99%A4%E4%B8%8D%E5%8A%A8%E4%BA%A7%E3%80%81%E7%BB%A7%E6%89%BF%E3%80%81%E4%B8%93%E4%B8%9A%E5%95%86%E4%B8%9A%E3%80%81%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="作证义务豁免" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%AF%81%E4%B9%89%E5%8A%A1%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="派遣国放弃，明示" _mubu_text="%3Cspan%3E%E6%B4%BE%E9%81%A3%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%BD%3C/span%3E%3Cspan%3E%E6%94%BE%E5%BC%83%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%98%8E%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="领事官员：执行职务行为、作证义务豁免" _mubu_text="%3Cspan%3E%E9%A2%86%E4%BA%8B%E5%AE%98%E5%91%98%EF%BC%9A%E6%89%A7%E8%A1%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%81%8C%E5%8A%A1%3C/span%3E%3Cspan%3E%E8%A1%8C%E4%B8%BA%E3%80%81%E4%BD%9C%E8%AF%81%E4%B9%89%E5%8A%A1%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="人身不得侵犯" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BA%BA%E8%BA%AB%E4%B8%8D%E5%BE%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note="">
            <outline text="外交人员：绝对不得侵犯" _mubu_text="%3Cspan%20class=%22%20text-color-blue%22%3E%E5%A4%96%E4%BA%A4%E4%BA%BA%E5%91%98%EF%BC%9A%E7%BB%9D%E5%AF%B9%E4%B8%8D%E5%BE%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="领事官员：严重、执行" _mubu_text="%3Cspan%3E%E9%A2%86%E4%BA%8B%E5%AE%98%E5%91%98%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%A5%E9%87%8D%E3%80%81%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="特别使团" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E4%BD%BF%E5%9B%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="性质：外交人员" _mubu_text="%3Cspan%3E%E6%80%A7%E8%B4%A8%EF%BC%9A%E5%A4%96%E4%BA%A4%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="特权豁免：参照领事官员" _mubu_text="%3Cspan%3E%E7%89%B9%E6%9D%83%E8%B1%81%E5%85%8D%EF%BC%9A%E5%8F%82%E7%85%A7%E9%A2%86%E4%BA%8B%E5%AE%98%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="06 条约法" _mubu_text="%3Cspan%3E06%20%E6%9D%A1%E7%BA%A6%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
        <outline text="条约的缔结" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%9D%A1%E7%BA%A6%E7%9A%84%E7%BC%94%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="实质要件" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E8%A6%81%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="形式：书面与否不影响条约效力" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%BD%A2%E5%BC%8F%EF%BC%9A%E4%B9%A6%E9%9D%A2%E4%B8%8E%E5%90%A6%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E6%9D%A1%E7%BA%A6%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="主体：国际法主体，并非国家" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E5%9B%BD%E9%99%85%E6%B3%95%E4%B8%BB%E4%BD%93%EF%BC%8C%E5%B9%B6%E9%9D%9E%E5%9B%BD%E5%AE%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="缔约权：国内法" _mubu_text="%3Cspan%3E%E7%BC%94%E7%BA%A6%E6%9D%83%EF%BC%9A%E5%9B%BD%E5%86%85%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无需全权证书：国家元首、政府首脑、外交部长、使馆馆长、派驻国际组织代表 + 正职" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%97%A0%E9%9C%80%3C/span%3E%3Cspan%3E%E5%85%A8%E6%9D%83%E8%AF%81%E4%B9%A6%EF%BC%9A%E5%9B%BD%E5%AE%B6%E5%85%83%E9%A6%96%E3%80%81%E6%94%BF%E5%BA%9C%E9%A6%96%E8%84%91%E3%80%81%E5%A4%96%E4%BA%A4%E9%83%A8%E9%95%BF%E3%80%81%E4%BD%BF%E9%A6%86%E9%A6%86%E9%95%BF%E3%80%81%E6%B4%BE%E9%A9%BB%E5%9B%BD%E9%99%85%E7%BB%84%E7%BB%87%E4%BB%A3%E8%A1%A8%20+%20%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%AD%A3%E8%81%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="意思表示：自由" _mubu_text="%3Cspan%3E%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%EF%BC%9A%E8%87%AA%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="内容：不违反国际强行法" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%EF%BC%9A%E4%B8%8D%E8%BF%9D%E5%8F%8D%E5%9B%BD%E9%99%85%E5%BC%BA%E8%A1%8C%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="联合国秘书处登记，否则联合国机构不得援引" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%81%94%E5%90%88%E5%9B%BD%E7%A7%98%E4%B9%A6%E5%A4%84%3C/span%3E%3Cspan%3E%E7%99%BB%E8%AE%B0%EF%BC%8C%E5%90%A6%E5%88%99%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%81%94%E5%90%88%E5%9B%BD%E6%9C%BA%E6%9E%84%3C/span%3E%3Cspan%3E%E4%B8%8D%E5%BE%97%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8F%B4%E5%BC%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="其他组织可以援引" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%E5%8F%AF%E4%BB%A5%E6%8F%B4%E5%BC%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不影响条约生效" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%9D%A1%E7%BA%A6%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="表示接受条约拘束的方式" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%A1%A8%E7%A4%BA%E6%8E%A5%E5%8F%97%E6%9D%A1%E7%BA%A6%E6%8B%98%E6%9D%9F%E7%9A%84%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="缔结新条约" _mubu_text="%3Cspan%3E%E7%BC%94%E7%BB%93%E6%96%B0%E6%9D%A1%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="签署" _mubu_text="%3Cspan%3E%E7%AD%BE%E7%BD%B2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="批准或核准" _mubu_text="%3Cspan%3E%E6%89%B9%E5%87%86%E6%88%96%E6%A0%B8%E5%87%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="批准：条约或重要协定" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%89%B9%E5%87%86%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%9D%A1%E7%BA%A6%E6%88%96%E9%87%8D%E8%A6%81%E5%8D%8F%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="核准：其他" _mubu_text="%3Cspan%3E%E6%A0%B8%E5%87%86%EF%BC%9A%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="进入已经缔结的条约" _mubu_text="%3Cspan%3E%E8%BF%9B%E5%85%A5%E5%B7%B2%E7%BB%8F%E7%BC%94%E7%BB%93%E7%9A%84%E6%9D%A1%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="加入" _mubu_text="%3Cspan%3E%E5%8A%A0%E5%85%A5%3C/span%3E" _note="" _mubu_note="">
                  <outline text="条约或重要协定" _mubu_text="%3Cspan%3E%E6%9D%A1%E7%BA%A6%E6%88%96%E9%87%8D%E8%A6%81%E5%8D%8F%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其他" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="接受" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%8F%97%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="条约或重要协定" _mubu_text="%3Cspan%3E%E6%9D%A1%E7%BA%A6%E6%88%96%E9%87%8D%E8%A6%81%E5%8D%8F%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="范围：友好和平、领土划界、司法协助引渡、与法律不同规定、条约规定批准" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%8B%E5%A5%BD%E5%92%8C%E5%B9%B3%E3%80%81%E9%A2%86%E5%9C%9F%E5%88%92%E7%95%8C%E3%80%81%E5%8F%B8%E6%B3%95%E5%8D%8F%E5%8A%A9%E5%BC%95%E6%B8%A1%E3%80%81%E4%B8%8E%E6%B3%95%E5%BE%8B%E4%B8%8D%E5%90%8C%E8%A7%84%E5%AE%9A%E3%80%81%E6%9D%A1%E7%BA%A6%E8%A7%84%E5%AE%9A%E6%89%B9%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="决定、签署" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E3%80%81%E7%AD%BE%E7%BD%B2%3C/span%3E" _note="" _mubu_note="">
                <outline text="条约或重要协定：人大常委会决定、主席签署" _mubu_text="%3Cspan%3E%E6%9D%A1%E7%BA%A6%E6%88%96%E9%87%8D%E8%A6%81%E5%8D%8F%E5%AE%9A%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BA%BA%E5%A4%A7%E5%B8%B8%E5%A7%94%E4%BC%9A%3C/span%3E%3Cspan%3E%E5%86%B3%E5%AE%9A%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%BB%E5%B8%AD%3C/span%3E%3Cspan%3E%E7%AD%BE%E7%BD%B2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="其他：国务院决定、外交部长签署" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%3C/span%3E%3Cspan%3E%E5%86%B3%E5%AE%9A%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%A4%96%E4%BA%A4%E9%83%A8%E9%95%BF%3C/span%3E%3Cspan%3E%E7%AD%BE%E7%BD%B2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="条约的保留" _mubu_text="%3Cspan%3E%E6%9D%A1%E7%BA%A6%E7%9A%84%E4%BF%9D%E7%95%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="性质：单方声明" _mubu_text="%3Cspan%3E%E6%80%A7%E8%B4%A8%EF%BC%9A%E5%8D%95%E6%96%B9%E5%A3%B0%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="时间限制" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="条约尚未对本国发生效力" _mubu_text="%3Cspan%3E%E6%9D%A1%E7%BA%A6%E5%B0%9A%E6%9C%AA%E5%AF%B9%E6%9C%AC%E5%9B%BD%E5%8F%91%E7%94%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无关条约是否已生效" _mubu_text="%3Cspan%3E%E6%97%A0%E5%85%B3%E6%9D%A1%E7%BA%A6%E6%98%AF%E5%90%A6%E5%B7%B2%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="保留效力" _mubu_text="%3Cspan%3E%E4%BF%9D%E7%95%99%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="明文允许：自然生效" _mubu_text="%3Cspan%3E%E6%98%8E%E6%96%87%E5%85%81%E8%AE%B8%EF%BC%9A%E8%87%AA%E7%84%B6%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
