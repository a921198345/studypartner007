            <outline text="中文译本：需要提供" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%96%87%E8%AF%91%E6%9C%AC%EF%BC%9A%E9%9C%80%E8%A6%81%E6%8F%90%E4%BE%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="认可与否裁定不服" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%8F%AF%E4%B8%8E%E5%90%A6%E8%A3%81%E5%AE%9A%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="内地作出认可与否裁定：上一级复议" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%86%85%E5%9C%B0%3C/span%3E%3Cspan%3E%E4%BD%9C%E5%87%BA%E8%AE%A4%E5%8F%AF%E4%B8%8E%E5%90%A6%E8%A3%81%E5%AE%9A%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8A%E4%B8%80%E7%BA%A7%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="港澳作出认可与否裁定：上诉" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%B8%AF%E6%BE%B3%3C/span%3E%3Cspan%3E%E4%BD%9C%E5%87%BA%E8%AE%A4%E5%8F%AF%E4%B8%8E%E5%90%A6%E8%A3%81%E5%AE%9A%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="涉港侵权案件" _mubu_text="%3Cspan%3E%E6%B6%89%E6%B8%AF%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BE%B5%E6%9D%83%3C/span%3E%3Cspan%3E%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="非侵权：认可，执行" _mubu_text="%3Cspan%20class=%22bold%22%3E%E9%9D%9E%E4%BE%B5%E6%9D%83%3C/span%3E%3Cspan%3E%EF%BC%9A%E8%AE%A4%E5%8F%AF%EF%BC%8C%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="专利侵权：不认可，不执行" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%93%E5%88%A9%3C/span%3E%3Cspan%3E%E4%BE%B5%E6%9D%83%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%AE%A4%E5%8F%AF%3C/span%3E%3Cspan%3E%EF%BC%8C%E4%B8%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="知识产权侵权：限于金钱判项" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E%3Cspan%3E%E4%BE%B5%E6%9D%83%EF%BC%9A%E9%99%90%E4%BA%8E%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%87%91%E9%92%B1%E5%88%A4%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="商业秘密侵权：包括金钱判项、非金钱判项" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E%3Cspan%3E%E4%BE%B5%E6%9D%83%EF%BC%9A%E5%8C%85%E6%8B%AC%E9%87%91%E9%92%B1%E5%88%A4%E9%A1%B9%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%9D%9E%E9%87%91%E9%92%B1%E5%88%A4%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="仲裁裁决" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院判决同时提出执行申请 VS 仲裁裁决同时提出执行申请" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%88%A4%E5%86%B3%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%90%8C%E6%97%B6%E6%8F%90%E5%87%BA%E6%89%A7%E8%A1%8C%E7%94%B3%E8%AF%B7%3C/span%3E%3Cspan%3E%20VS%20%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%E5%90%8C%E6%97%B6%E6%8F%90%E5%87%BA%E6%89%A7%E8%A1%8C%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
              <outline text="港 + 法院判决 = 能（港判能）" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E6%B8%AF%20+%20%E6%B3%95%E9%99%A2%E5%88%A4%E5%86%B3%20=%20%E8%83%BD%EF%BC%88%E6%B8%AF%E5%88%A4%E8%83%BD%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="港 + 仲裁裁决 = 不能" _mubu_text="%3Cspan%3E%E6%B8%AF%20+%20%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%20=%20%E4%B8%8D%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="澳 + 法院判决 = 不能" _mubu_text="%3Cspan%3E%E6%BE%B3%20+%20%E6%B3%95%E9%99%A2%E5%88%A4%E5%86%B3%20=%20%E4%B8%8D%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="澳 + 仲裁裁决 = 能" _mubu_text="%3Cspan%3E%E6%BE%B3%20+%20%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%20=%20%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="司法协助中的一些共同规则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E5%8D%8F%E5%8A%A9%E4%B8%AD%E7%9A%84%E4%B8%80%E4%BA%9B%E5%85%B1%E5%90%8C%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="同时向内地几个中院申请，最先立案法院受理" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%90%8C%E6%97%B6%3C/span%3E%3Cspan%3E%E5%90%91%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%86%85%E5%9C%B0%3C/span%3E%3Cspan%3E%E5%87%A0%E4%B8%AA%E4%B8%AD%E9%99%A2%E7%94%B3%E8%AF%B7%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9C%80%E5%85%88%E7%AB%8B%E6%A1%88%E6%B3%95%E9%99%A2%3C/span%3E%3Cspan%3E%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="专属管辖" _mubu_text="%3Cspan%3E%E4%B8%93%E5%B1%9E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="可以拒绝：法院判决" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8B%92%E7%BB%9D%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B3%95%E9%99%A2%3C/span%3E%3Cspan%3E%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不能拒绝：仲裁裁决、文书送达、调取证据" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%83%BD%3C/span%3E%3Cspan%3E%E6%8B%92%E7%BB%9D%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BB%B2%E8%A3%81%3C/span%3E%3Cspan%3E%E8%A3%81%E5%86%B3%E3%80%81%E6%96%87%E4%B9%A6%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%81%E8%BE%BE%3C/span%3E%3Cspan%3E%E3%80%81%E8%B0%83%E5%8F%96%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="费用" _mubu_text="%3Cspan%3E%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="执行：收费" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%EF%BC%9A%E6%94%B6%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="其他：不收费，不贴钱" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%EF%BC%9A%E4%B8%8D%E6%94%B6%E8%B4%B9%EF%BC%8C%E4%B8%8D%E8%B4%B4%E9%92%B1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="中文译本" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%96%87%E8%AF%91%E6%9C%AC%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：提交中文译本" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E6%8F%90%E4%BA%A4%E4%B8%AD%E6%96%87%E8%AF%91%E6%9C%AC%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外：国际条约规定" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E5%9B%BD%E9%99%85%E6%9D%A1%E7%BA%A6%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="国际经济法" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E7%BB%8F%E6%B5%8E%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="01 国际货物买卖法律制度" _mubu_text="%3Cspan%3E01%20%E5%9B%BD%E9%99%85%E8%B4%A7%E7%89%A9%E4%B9%B0%E5%8D%96%E6%B3%95%E5%BE%8B%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
        <outline text="《国际贸易术语解释通则》" _mubu_text="%3Cspan%3E%E3%80%8A%E5%9B%BD%E9%99%85%E8%B4%B8%E6%98%93%E6%9C%AF%E8%AF%AD%E8%A7%A3%E9%87%8A%E9%80%9A%E5%88%99%E3%80%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="EXW, DDP" _mubu_text="%3Cspan%3EEXW,%20DDP%3C/span%3E" _note="" _mubu_note="">
            <outline text="EXW：出门不管" _mubu_text="%3Cspan%3EEXW%EF%BC%9A%E5%87%BA%E9%97%A8%E4%B8%8D%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="DDP：送货上门" _mubu_text="%3Cspan%3EDDP%EF%BC%9A%E9%80%81%E8%B4%A7%E4%B8%8A%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="FOB, CIF, CFR" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3EFOB,%20CIF,%20CFR%3C/span%3E" _note="" _mubu_note="">
            <outline text="FOB：不包运不包险，港口名装运港（买方安排运输后通知卖方，卖方交货时通知买方投保）" _mubu_text="%3Cspan%20class=%22bold%22%3EFOB%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E5%8C%85%E8%BF%90%E4%B8%8D%E5%8C%85%E9%99%A9%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%B8%AF%E5%8F%A3%E5%90%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%A3%85%E8%BF%90%E6%B8%AF%3C/span%3E%3Cspan%3E%EF%BC%88%E4%B9%B0%E6%96%B9%E5%AE%89%E6%8E%92%E8%BF%90%E8%BE%93%E5%90%8E%E9%80%9A%E7%9F%A5%E5%8D%96%E6%96%B9%EF%BC%8C%E5%8D%96%E6%96%B9%E4%BA%A4%E8%B4%A7%E6%97%B6%E9%80%9A%E7%9F%A5%E4%B9%B0%E6%96%B9%E6%8A%95%E4%BF%9D%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="CFR：包运不包险，港口名目的港（卖方交货时通知买方投保）" _mubu_text="%3Cspan%20class=%22bold%22%3ECFR%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8C%85%E8%BF%90%E4%B8%8D%E5%8C%85%E9%99%A9%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%B8%AF%E5%8F%A3%E5%90%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%9B%AE%E7%9A%84%E6%B8%AF%3C/span%3E%3Cspan%3E%EF%BC%88%E5%8D%96%E6%96%B9%E4%BA%A4%E8%B4%A7%E6%97%B6%E9%80%9A%E7%9F%A5%E4%B9%B0%E6%96%B9%E6%8A%95%E4%BF%9D%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="CIF：包运包险，港口名目的港（卖方投保平安险）" _mubu_text="%3Cspan%20class=%22bold%22%3ECIF%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8C%85%E8%BF%90%E5%8C%85%E9%99%A9%3C/span%3E%3Cspan%3E%EF%BC%8C%E6%B8%AF%E5%8F%A3%E5%90%8D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%9B%AE%E7%9A%84%E6%B8%AF%3C/span%3E%3Cspan%3E%EF%BC%88%E5%8D%96%E6%96%B9%E6%8A%95%E4%BF%9D%E5%B9%B3%E5%AE%89%E9%99%A9%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="共同点" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="风险转移：装运上船" _mubu_text="%3Cspan%3E%E9%A3%8E%E9%99%A9%E8%BD%AC%E7%A7%BB%EF%BC%9A%E8%A3%85%E8%BF%90%E4%B8%8A%E8%88%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="交货地点：装运港" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%A4%E8%B4%A7%E5%9C%B0%E7%82%B9%3C/span%3E%3Cspan%3E%EF%BC%9A%E8%A3%85%E8%BF%90%E6%B8%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="FCA, CIP, CPT" _mubu_text="%3Cspan%3EFCA,%20CIP,%20CPT%3C/span%3E" _note="" _mubu_note="">
            <outline text="FCA：不包运不包险" _mubu_text="%3Cspan%3EFCA%EF%BC%9A%E4%B8%8D%E5%8C%85%E8%BF%90%E4%B8%8D%E5%8C%85%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="《2020年通则》的特殊规定：" _mubu_text="%3Cspan%3E%E3%80%8A2020%E5%B9%B4%E9%80%9A%E5%88%99%E3%80%8B%E7%9A%84%E7%89%B9%E6%AE%8A%E8%A7%84%E5%AE%9A%EF%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="买卖双方可以约定买方指示其承运人在货物装运后向卖方签发已装船提单" _mubu_text="%3Cspan%3E%E4%B9%B0%E5%8D%96%E5%8F%8C%E6%96%B9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%E7%BA%A6%E5%AE%9A%3C/span%3E%3Cspan%3E%E4%B9%B0%E6%96%B9%E6%8C%87%E7%A4%BA%E5%85%B6%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%BF%E8%BF%90%E4%BA%BA%3C/span%3E%3Cspan%3E%E5%9C%A8%E8%B4%A7%E7%89%A9%E8%A3%85%E8%BF%90%E5%90%8E%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%90%91%E5%8D%96%E6%96%B9%E7%AD%BE%E5%8F%91%E5%B7%B2%E8%A3%85%E8%88%B9%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="CPT：包运不包险" _mubu_text="%3Cspan%3ECPT%EF%BC%9A%E5%8C%85%E8%BF%90%E4%B8%8D%E5%8C%85%E9%99%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="CIP：包运包险" _mubu_text="%3Cspan%3ECIP%EF%BC%9A%E5%8C%85%E8%BF%90%E5%8C%85%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="《2020年通则》特殊规定：" _mubu_text="%3Cspan%3E%E3%80%8A2020%E5%B9%B4%E9%80%9A%E5%88%99%E3%80%8B%E7%89%B9%E6%AE%8A%E8%A7%84%E5%AE%9A%EF%BC%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="除非合同另有约定，卖方需要投保的险别提高到一切险" _mubu_text="%3Cspan%3E%E9%99%A4%E9%9D%9E%E5%90%88%E5%90%8C%E5%8F%A6%E6%9C%89%E7%BA%A6%E5%AE%9A%EF%BC%8C%E5%8D%96%E6%96%B9%E9%9C%80%E8%A6%81%E6%8A%95%E4%BF%9D%E7%9A%84%E9%99%A9%E5%88%AB%E6%8F%90%E9%AB%98%E5%88%B0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%80%E5%88%87%E9%99%A9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="共同点" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="风险转移：货交第一承运人" _mubu_text="%3Cspan%3E%E9%A3%8E%E9%99%A9%E8%BD%AC%E7%A7%BB%EF%BC%9A%E8%B4%A7%E4%BA%A4%E7%AC%AC%E4%B8%80%E6%89%BF%E8%BF%90%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="交货地点：装运地" _mubu_text="%3Cspan%3E%E4%BA%A4%E8%B4%A7%E5%9C%B0%E7%82%B9%EF%BC%9A%E8%A3%85%E8%BF%90%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="DAP, DAT" _mubu_text="%3Cspan%3EDAP,%20DAT%3C/span%3E" _note="" _mubu_note="">
            <outline text="DAP：运到" _mubu_text="%3Cspan%3EDAP%EF%BC%9A%E8%BF%90%E5%88%B0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="DAT：运到 + 卸下" _mubu_text="%3Cspan%3EDAT%EF%BC%9A%E8%BF%90%E5%88%B0%20+%20%E5%8D%B8%E4%B8%8B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="FAS" _mubu_text="%3Cspan%3EFAS%3C/span%3E" _note="" _mubu_note="">
            <outline text="FAS：船边交货" _mubu_text="%3Cspan%3EFAS%EF%BC%9A%E8%88%B9%E8%BE%B9%E4%BA%A4%E8%B4%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="FOB：船上交货" _mubu_text="%3Cspan%3EFOB%EF%BC%9A%E8%88%B9%E4%B8%8A%E4%BA%A4%E8%B4%A7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="排序：EXW &gt; (FAS) &gt; FCA(FOB) &gt; CPT(CFR) &gt; CIP(CIF) &gt; DAP &gt; DAT &gt; DDP" _mubu_text="%3Cspan%3E%E6%8E%92%E5%BA%8F%EF%BC%9AEXW%20&amp;gt;%20(FAS)%20&amp;gt;%20FCA(FOB)%20&amp;gt;%20CPT(CFR)%20&amp;gt;%20CIP(CIF)%20&amp;gt;%20DAP%20&amp;gt;%20DAT%20&amp;gt;%20DDP%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="1980年《联合国国际货物销售合同公约》" _mubu_text="%3Cspan%3E1980%E5%B9%B4%E3%80%8A%E8%81%94%E5%90%88%E5%9B%BD%E5%9B%BD%E9%99%85%E8%B4%A7%E7%89%A9%E9%94%80%E5%94%AE%E5%90%88%E5%90%8C%E5%85%AC%E7%BA%A6%E3%80%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="主体：营业地在不同缔约国" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E8%90%A5%E4%B8%9A%E5%9C%B0%E5%9C%A8%E4%B8%8D%E5%90%8C%E7%BC%94%E7%BA%A6%E5%9B%BD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不适用客体：消费者合同、拍卖、货币、船舶或飞机、电力" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E%3Cspan%3E%E5%AE%A2%E4%BD%93%EF%BC%9A%E6%B6%88%E8%B4%B9%E8%80%85%E5%90%88%E5%90%8C%E3%80%81%E6%8B%8D%E5%8D%96%E3%80%81%E8%B4%A7%E5%B8%81%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%88%B9%E8%88%B6%E6%88%96%E9%A3%9E%E6%9C%BA%3C/span%3E%3Cspan%3E%E3%80%81%E7%94%B5%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不涉及内容：合同效力、所有权转移、产品责任问题" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%B6%89%E5%8F%8A%E5%86%85%E5%AE%B9%EF%BC%9A%E5%90%88%E5%90%8C%E6%95%88%E5%8A%9B%E3%80%81%E6%89%80%E6%9C%89%E6%9D%83%E8%BD%AC%E7%A7%BB%E3%80%81%E4%BA%A7%E5%93%81%E8%B4%A3%E4%BB%BB%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="任意性效力" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BB%BB%E6%84%8F%E6%80%A7%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="选择准据法：完全排除公约适用" _mubu_text="%3Cspan%3E%E9%80%89%E6%8B%A9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%87%86%E6%8D%AE%E6%B3%95%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%AE%8C%E5%85%A8%3C/span%3E%3Cspan%3E%E6%8E%92%E9%99%A4%E5%85%AC%E7%BA%A6%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="选择贸易术语：部分排除公约适用" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E9%80%89%E6%8B%A9%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E8%B4%B8%E6%98%93%E6%9C%AF%E8%AF%AD%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%20text-color-red%22%3E%E9%83%A8%E5%88%86%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%E6%8E%92%E9%99%A4%E5%85%AC%E7%BA%A6%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="合同成立" _mubu_text="%3Cspan%3E%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="承诺生效时，合同成立（与我国合同编一致）" _mubu_text="%3Cspan%3E%E6%89%BF%E8%AF%BA%E7%94%9F%E6%95%88%E6%97%B6%EF%BC%8C%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%EF%BC%88%E4%B8%8E%E6%88%91%E5%9B%BD%E5%90%88%E5%90%8C%E7%BC%96%E4%B8%80%E8%87%B4%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="买卖合同双方的义务" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B9%B0%E5%8D%96%E5%90%88%E5%90%8C%E5%8F%8C%E6%96%B9%E7%9A%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="卖方义务规定" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8D%96%E6%96%B9%E4%B9%89%E5%8A%A1%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="交货义务" _mubu_text="%3Cspan%3E%E4%BA%A4%E8%B4%A7%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="交货地点" _mubu_text="%3Cspan%3E%E4%BA%A4%E8%B4%A7%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="卖方交货义务涉及运输：第一承运人所在地" _mubu_text="%3Cspan%3E%E5%8D%96%E6%96%B9%E4%BA%A4%E8%B4%A7%E4%B9%89%E5%8A%A1%E6%B6%89%E5%8F%8A%E8%BF%90%E8%BE%93%EF%BC%9A%E7%AC%AC%E4%B8%80%E6%89%BF%E8%BF%90%E4%BA%BA%E6%89%80%E5%9C%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="卖方交货义务不涉及运输：合同明确的特定地点或卖方营业地" _mubu_text="%3Cspan%3E%E5%8D%96%E6%96%B9%E4%BA%A4%E8%B4%A7%E4%B9%89%E5%8A%A1%E4%B8%8D%E6%B6%89%E5%8F%8A%E8%BF%90%E8%BE%93%EF%BC%9A%E5%90%88%E5%90%8C%E6%98%8E%E7%A1%AE%E7%9A%84%E7%89%B9%E5%AE%9A%E5%9C%B0%E7%82%B9%E6%88%96%E5%8D%96%E6%96%B9%E8%90%A5%E4%B8%9A%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="交货时间" _mubu_text="%3Cspan%3E%E4%BA%A4%E8%B4%A7%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="有约从约，无约合理时间" _mubu_text="%3Cspan%3E%E6%9C%89%E7%BA%A6%E4%BB%8E%E7%BA%A6%EF%BC%8C%E6%97%A0%E7%BA%A6%E5%90%88%E7%90%86%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="交单义务" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BA%A4%E5%8D%95%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="提前交单，（在交单时间届满前）有权纠正单据错误，赔偿买方损失" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%8F%90%E5%89%8D%3C/span%3E%3Cspan%3E%E4%BA%A4%E5%8D%95%EF%BC%8C%EF%BC%88%E5%9C%A8%E4%BA%A4%E5%8D%95%E6%97%B6%E9%97%B4%E5%B1%8A%E6%BB%A1%E5%89%8D%EF%BC%89%E6%9C%89%E6%9D%83%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BA%A0%E6%AD%A3%3C/span%3E%3Cspan%3E%E5%8D%95%E6%8D%AE%E9%94%99%E8%AF%AF%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%B5%94%E5%81%BF%3C/span%3E%3Cspan%3E%E4%B9%B0%E6%96%B9%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="货物相符" _mubu_text="%3Cspan%3E%E8%B4%A7%E7%89%A9%E7%9B%B8%E7%AC%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="质量担保" _mubu_text="%3Cspan%3E%E8%B4%A8%E9%87%8F%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="①适用于同一规格货物通常使用的目的；" _mubu_text="%3Cspan%3E%E2%91%A0%E9%80%82%E7%94%A8%E4%BA%8E%E5%90%8C%E4%B8%80%E8%A7%84%E6%A0%BC%E8%B4%A7%E7%89%A9%E9%80%9A%E5%B8%B8%E4%BD%BF%E7%94%A8%E7%9A%84%E7%9B%AE%E7%9A%84%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="②适用于订立合同时买方明示或默示通知卖方的任何特定目的；" _mubu_text="%3Cspan%3E%E2%91%A1%E9%80%82%E7%94%A8%E4%BA%8E%E8%AE%A2%E7%AB%8B%E5%90%88%E5%90%8C%E6%97%B6%E4%B9%B0%E6%96%B9%E6%98%8E%E7%A4%BA%E6%88%96%E9%BB%98%E7%A4%BA%E9%80%9A%E7%9F%A5%E5%8D%96%E6%96%B9%E7%9A%84%E4%BB%BB%E4%BD%95%E7%89%B9%E5%AE%9A%E7%9B%AE%E7%9A%84%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="③与卖方提供的样品或样式相符；" _mubu_text="%3Cspan%3E%E2%91%A2%E4%B8%8E%E5%8D%96%E6%96%B9%E6%8F%90%E4%BE%9B%E7%9A%84%E6%A0%B7%E5%93%81%E6%88%96%E6%A0%B7%E5%BC%8F%E7%9B%B8%E7%AC%A6%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="④货物应按照同类货物通用的方式包装或装箱，如果没有此种通用方式，则按照足以保全和保护货物的方式包装或装箱。" _mubu_text="%3Cspan%3E%E2%91%A3%E8%B4%A7%E7%89%A9%E5%BA%94%E6%8C%89%E7%85%A7%E5%90%8C%E7%B1%BB%E8%B4%A7%E7%89%A9%E9%80%9A%E7%94%A8%E7%9A%84%E6%96%B9%E5%BC%8F%E5%8C%85%E8%A3%85%E6%88%96%E8%A3%85%E7%AE%B1%EF%BC%8C%E5%A6%82%E6%9E%9C%E6%B2%A1%E6%9C%89%E6%AD%A4%E7%A7%8D%E9%80%9A%E7%94%A8%E6%96%B9%E5%BC%8F%EF%BC%8C%E5%88%99%E6%8C%89%E7%85%A7%E8%B6%B3%E4%BB%A5%E4%BF%9D%E5%85%A8%E5%92%8C%E4%BF%9D%E6%8A%A4%E8%B4%A7%E7%89%A9%E7%9A%84%E6%96%B9%E5%BC%8F%E5%8C%85%E8%A3%85%E6%88%96%E8%A3%85%E7%AE%B1%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="数量相符" _mubu_text="%3Cspan%3E%E6%95%B0%E9%87%8F%E7%9B%B8%E7%AC%A6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="与约定不符则违约" _mubu_text="%3Cspan%3E%E4%B8%8E%E7%BA%A6%E5%AE%9A%E4%B8%8D%E7%AC%A6%E5%88%99%E8%BF%9D%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="多了可收，但按合同价支付" _mubu_text="%3Cspan%3E%E5%A4%9A%E4%BA%86%E5%8F%AF%E6%94%B6%EF%BC%8C%E4%BD%86%E6%8C%89%E5%90%88%E5%90%8C%E4%BB%B7%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="权利担保" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
                <outline text="所有权担保" _mubu_text="%3Cspan%3E%E6%89%80%E6%9C%89%E6%9D%83%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="知识产权担保" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="地域限制：买方营业地、合同预期货物转售地或使用地" _mubu_text="%3Cspan%3E%E5%9C%B0%E5%9F%9F%E9%99%90%E5%88%B6%EF%BC%9A%E4%B9%B0%E6%96%B9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%90%A5%E4%B8%9A%3C/span%3E%3Cspan%3E%E5%9C%B0%E3%80%81%E5%90%88%E5%90%8C%E9%A2%84%E6%9C%9F%E8%B4%A7%E7%89%A9%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%BD%AC%E5%94%AE%E5%9C%B0%E6%88%96%E4%BD%BF%E7%94%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="免责事由：买方已知、买方指示、买方未通知" _mubu_text="%3Cspan%3E%E5%85%8D%E8%B4%A3%E4%BA%8B%E7%94%B1%EF%BC%9A%E4%B9%B0%E6%96%B9%E5%B7%B2%E7%9F%A5%E3%80%81%E4%B9%B0%E6%96%B9%E6%8C%87%E7%A4%BA%E3%80%81%E4%B9%B0%E6%96%B9%E6%9C%AA%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="买方义务规定" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B9%B0%E6%96%B9%E4%B9%89%E5%8A%A1%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="支付货款" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E8%B4%A7%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
                <outline text="支付地点" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="凭移交货物或单据支付货款：移交货物或单据的地点" _mubu_text="%3Cspan%3E%E5%87%AD%E7%A7%BB%E4%BA%A4%E8%B4%A7%E7%89%A9%E6%88%96%E5%8D%95%E6%8D%AE%E6%94%AF%E4%BB%98%E8%B4%A7%E6%AC%BE%EF%BC%9A%E7%A7%BB%E4%BA%A4%E8%B4%A7%E7%89%A9%E6%88%96%E5%8D%95%E6%8D%AE%E7%9A%84%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="其他情形：卖方营业地" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%83%85%E5%BD%A2%EF%BC%9A%E5%8D%96%E6%96%B9%E8%90%A5%E4%B8%9A%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="支付时间" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="卖方将货物或控制货物处置权的单据置于买方控制下时" _mubu_text="%3Cspan%3E%E5%8D%96%E6%96%B9%E5%B0%86%3C/span%3E%3Cspan%20class=%22underline%22%3E%E8%B4%A7%E7%89%A9%3C/span%3E%3Cspan%3E%E6%88%96%3C/span%3E%3Cspan%20class=%22underline%22%3E%E6%8E%A7%E5%88%B6%E8%B4%A7%E7%89%A9%E5%A4%84%E7%BD%AE%E6%9D%83%E7%9A%84%E5%8D%95%E6%8D%AE%3C/span%3E%3Cspan%3E%E7%BD%AE%E4%BA%8E%E4%B9%B0%E6%96%B9%E6%8E%A7%E5%88%B6%E4%B8%8B%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="接收货物" _mubu_text="%3Cspan%3E%E6%8E%A5%E6%94%B6%E8%B4%A7%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
                <outline text="接收义务：即使严重瑕疵，接收不等于接受" _mubu_text="%3Cspan%3E%E6%8E%A5%E6%94%B6%E4%B9%89%E5%8A%A1%EF%BC%9A%E5%8D%B3%E4%BD%BF%E4%B8%A5%E9%87%8D%E7%91%95%E7%96%B5%EF%BC%8C%E6%8E%A5%E6%94%B6%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E7%AD%89%E4%BA%8E%3C/span%3E%3Cspan%3E%E6%8E%A5%E5%8F%97%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="拒收权：提前交货、多交货" _mubu_text="%3Cspan%3E%E6%8B%92%E6%94%B6%E6%9D%83%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8F%90%E5%89%8D%3C/span%3E%3Cspan%3E%E4%BA%A4%E8%B4%A7%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%A4%9A%E4%BA%A4%3C/span%3E%3Cspan%3E%E8%B4%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="检验货物" _mubu_text="%3Cspan%3E%E6%A3%80%E9%AA%8C%E8%B4%A7%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
                <outline text="时间限制：最短时间内" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%E9%99%90%E5%88%B6%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9C%80%E7%9F%AD%3C/span%3E%3Cspan%3E%E6%97%B6%E9%97%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="检验地点" _mubu_text="%3Cspan%3E%E6%A3%80%E9%AA%8C%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="到达目的地后" _mubu_text="%3Cspan%3E%E5%88%B0%E8%BE%BE%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%9B%AE%E7%9A%84%E5%9C%B0%3C/span%3E%3Cspan%3E%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="合同约定须转运，到达新目的地后" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%90%88%E5%90%8C%E7%BA%A6%E5%AE%9A%3C/span%3E%3Cspan%3E%E9%A1%BB%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%BD%AC%E8%BF%90%3C/span%3E%3Cspan%3E%EF%BC%8C%E5%88%B0%E8%BE%BE%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%96%B0%E7%9B%AE%E7%9A%84%E5%9C%B0%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="声明不符时间限制：2 年" _mubu_text="%3Cspan%3E%E5%A3%B0%E6%98%8E%E4%B8%8D%E7%AC%A6%E6%97%B6%E9%97%B4%E9%99%90%E5%88%B6%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E2%20%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="违约救济和免责" _mubu_text="%3Cspan%3E%E8%BF%9D%E7%BA%A6%E6%95%91%E6%B5%8E%E5%92%8C%E5%85%8D%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="风险转移" _mubu_text="%3Cspan%3E%E9%A3%8E%E9%99%A9%E8%BD%AC%E7%A7%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="一般规则：有约从约，无约交货时" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%A7%84%E5%88%99%EF%BC%9A%E6%9C%89%E7%BA%A6%E4%BB%8E%E7%BA%A6%EF%BC%8C%E6%97%A0%E7%BA%A6%E4%BA%A4%E8%B4%A7%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="在途运输：合同成立时" _mubu_text="%3Cspan%3E%E5%9C%A8%E9%80%94%E8%BF%90%E8%BE%93%EF%BC%9A%E5%90%88%E5%90%8C%E6%88%90%E7%AB%8B%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="02 国际货物运输与保险" _mubu_text="%3Cspan%3E02%20%E5%9B%BD%E9%99%85%E8%B4%A7%E7%89%A9%E8%BF%90%E8%BE%93%E4%B8%8E%E4%BF%9D%E9%99%A9%3C/span%3E" _note="" _mubu_note="">
        <outline text="第一节 国际海上货物运输法律制度" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%80%E8%8A%82%20%E5%9B%BD%E9%99%85%E6%B5%B7%E4%B8%8A%E8%B4%A7%E7%89%A9%E8%BF%90%E8%BE%93%E6%B3%95%E5%BE%8B%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="提单和海运单" _mubu_text="%3Cspan%3E%E6%8F%90%E5%8D%95%E5%92%8C%E6%B5%B7%E8%BF%90%E5%8D%95%3C/span%3E" _note="" _mubu_note="">
            <outline text="提单" _mubu_text="%3Cspan%3E%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="作用：物权凭证" _mubu_text="%3Cspan%3E%E4%BD%9C%E7%94%A8%EF%BC%9A%E7%89%A9%E6%9D%83%E5%87%AD%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="货物是否已装船：已装船提单、收货待运提单" _mubu_text="%3Cspan%3E%E8%B4%A7%E7%89%A9%E6%98%AF%E5%90%A6%E5%B7%B2%E8%A3%85%E8%88%B9%EF%BC%9A%E5%B7%B2%E8%A3%85%E8%88%B9%E6%8F%90%E5%8D%95%E3%80%81%E6%94%B6%E8%B4%A7%E5%BE%85%E8%BF%90%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="有无不良批注：清洁提单、不清洁提单" _mubu_text="%3Cspan%3E%E6%9C%89%E6%97%A0%E4%B8%8D%E8%89%AF%E6%89%B9%E6%B3%A8%EF%BC%9A%E6%B8%85%E6%B4%81%E6%8F%90%E5%8D%95%E3%80%81%E4%B8%8D%E6%B8%85%E6%B4%81%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="收货人抬头" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%A7%E4%BA%BA%E6%8A%AC%E5%A4%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="记名提单：不能转让" _mubu_text="%3Cspan%3E%E8%AE%B0%E5%90%8D%E6%8F%90%E5%8D%95%EF%BC%9A%E4%B8%8D%E8%83%BD%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="不记名提单：交付即转让" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AE%B0%E5%90%8D%E6%8F%90%E5%8D%95%EF%BC%9A%E4%BA%A4%E4%BB%98%E5%8D%B3%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="指示提单：背书转让" _mubu_text="%3Cspan%3E%E6%8C%87%E7%A4%BA%E6%8F%90%E5%8D%95%EF%BC%9A%E8%83%8C%E4%B9%A6%E8%BD%AC%E8%AE%A9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="倒签提单和预借提单" _mubu_text="%3Cspan%3E%E5%80%92%E7%AD%BE%E6%8F%90%E5%8D%95%E5%92%8C%E9%A2%84%E5%80%9F%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note="">
                  <outline text="共同点：装船时间不真实，具有欺诈性质" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%82%B9%EF%BC%9A%E8%A3%85%E8%88%B9%E6%97%B6%E9%97%B4%E4%B8%8D%E7%9C%9F%E5%AE%9E%EF%BC%8C%E5%85%B7%E6%9C%89%E6%AC%BA%E8%AF%88%E6%80%A7%E8%B4%A8%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="区别点：签发提单时货物是否已实际装船" _mubu_text="%3Cspan%3E%E5%8C%BA%E5%88%AB%E7%82%B9%EF%BC%9A%E7%AD%BE%E5%8F%91%E6%8F%90%E5%8D%95%E6%97%B6%E8%B4%A7%E7%89%A9%E6%98%AF%E5%90%A6%E5%B7%B2%E5%AE%9E%E9%99%85%E8%A3%85%E8%88%B9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="海运单" _mubu_text="%3Cspan%3E%E6%B5%B7%E8%BF%90%E5%8D%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="短期运输" _mubu_text="%3Cspan%3E%E7%9F%AD%E6%9C%9F%E8%BF%90%E8%BE%93%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="单货一起" _mubu_text="%3Cspan%3E%E5%8D%95%E8%B4%A7%E4%B8%80%E8%B5%B7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="无单放货法律责任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%97%A0%E5%8D%95%E6%94%BE%E8%B4%A7%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="责任主体：承运人与取货人连带责任" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%E4%B8%BB%E4%BD%93%EF%BC%9A%E6%89%BF%E8%BF%90%E4%BA%BA%E4%B8%8E%E5%8F%96%E8%B4%A7%E4%BA%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%BF%9E%E5%B8%A6%3C/span%3E%3Cspan%3E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="承运人赔偿责任" _mubu_text="%3Cspan%3E%E6%89%BF%E8%BF%90%E4%BA%BA%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="违约和侵权竞合，告啥是啥" _mubu_text="%3Cspan%3E%E8%BF%9D%E7%BA%A6%E5%92%8C%E4%BE%B5%E6%9D%83%E7%AB%9E%E5%90%88%EF%BC%8C%E5%91%8A%E5%95%A5%E6%98%AF%E5%95%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="CIF 价" _mubu_text="%3Cspan%20class=%22bold%22%3ECIF%20%E4%BB%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="货物价值 + 运费 + 保险费" _mubu_text="%3Cspan%3E%E8%B4%A7%E7%89%A9%E4%BB%B7%E5%80%BC%20+%20%E8%BF%90%E8%B4%B9%20+%20%E4%BF%9D%E9%99%A9%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="足额赔偿（民事责任）" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%B6%B3%E9%A2%9D%E8%B5%94%E5%81%BF%3C/span%3E%3Cspan%3E%EF%BC%88%E6%B0%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="不适用海事赔偿责任限制" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E%3Cspan%3E%E6%B5%B7%E4%BA%8B%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="承运人免责" _mubu_text="%3Cspan%3E%E6%89%BF%E8%BF%90%E4%BA%BA%E5%85%8D%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
              <outline text="依法行为（主动/被动）" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E8%A1%8C%E4%B8%BA%EF%BC%88%E4%B8%BB%E5%8A%A8/%E8%A2%AB%E5%8A%A8%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="超过法定期限无人向海关申报" _mubu_text="%3Cspan%3E%E8%B6%85%E8%BF%87%E6%B3%95%E5%AE%9A%E6%9C%9F%E9%99%90%E6%97%A0%E4%BA%BA%E5%90%91%E6%B5%B7%E5%85%B3%E7%94%B3%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="依照提单在明的法律规定，将货物交给海关或港口当局" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%85%A7%E6%8F%90%E5%8D%95%E5%9C%A8%E6%98%8E%E7%9A%84%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%EF%BC%8C%E5%B0%86%E8%B4%A7%E7%89%A9%E4%BA%A4%E7%BB%99%E6%B5%B7%E5%85%B3%E6%88%96%E6%B8%AF%E5%8F%A3%E5%BD%93%E5%B1%80%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="记名提单 + 托运人要求" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E8%AE%B0%E5%90%8D%E6%8F%90%E5%8D%95%3C/span%3E%3Cspan%20class=%22%20text-color-red%22%3E%20+%20%E6%89%98%E8%BF%90%E4%BA%BA%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="已有正本提单" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B7%B2%E6%9C%89%E6%AD%A3%E6%9C%AC%3C/span%3E%3Cspan%3E%E6%8F%90%E5%8D%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="诉讼时效：1 年" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%EF%BC%9A1%20%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
