              <outline text="开立假信用证或“软条款”信用证" _mubu_text="%3Cspan%3E%E5%BC%80%E7%AB%8B%E5%81%87%E4%BF%A1%E7%94%A8%E8%AF%81%E6%88%96%E2%80%9C%E8%BD%AF%E6%9D%A1%E6%AC%BE%E2%80%9D%E4%BF%A1%E7%94%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="目的是使开证申请人控制整笔交易，使受益人处于受制他人的被动地位" _mubu_text="%3Cspan%3E%E7%9B%AE%E7%9A%84%E6%98%AF%E4%BD%BF%E5%BC%80%E8%AF%81%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%8E%A7%E5%88%B6%E6%95%B4%E7%AC%94%E4%BA%A4%E6%98%93%EF%BC%8C%E4%BD%BF%E5%8F%97%E7%9B%8A%E4%BA%BA%E5%A4%84%E4%BA%8E%E5%8F%97%E5%88%B6%E4%BB%96%E4%BA%BA%E7%9A%84%E8%A2%AB%E5%8A%A8%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="使开证行在“单证单单表面相符”下也无法履行付款义务" _mubu_text="%3Cspan%3E%E4%BD%BF%E5%BC%80%E8%AF%81%E8%A1%8C%E5%9C%A8%E2%80%9C%E5%8D%95%E8%AF%81%E5%8D%95%E5%8D%95%E8%A1%A8%E9%9D%A2%E7%9B%B8%E7%AC%A6%E2%80%9D%E4%B8%8B%E4%B9%9F%E6%97%A0%E6%B3%95%E5%B1%A5%E8%A1%8C%E4%BB%98%E6%AC%BE%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="伪造单据或以假货充真货（假单、假货）" _mubu_text="%3Cspan%3E%E4%BC%AA%E9%80%A0%E5%8D%95%E6%8D%AE%E6%88%96%E4%BB%A5%E5%81%87%E8%B4%A7%E5%85%85%E7%9C%9F%E8%B4%A7%3C/span%3E%3Cspan%20class=%22bold%22%3E%EF%BC%88%E5%81%87%E5%8D%95%E3%80%81%E5%81%87%E8%B4%A7%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以保函换取与信用证相符的提单（装船时间有问题、货物表面质量有问题）" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%BF%9D%E5%87%BD%E6%8D%A2%E5%8F%96%E4%B8%8E%E4%BF%A1%E7%94%A8%E8%AF%81%E7%9B%B8%E7%AC%A6%E7%9A%84%E6%8F%90%E5%8D%95%3C/span%3E%3Cspan%20class=%22bold%22%3E%EF%BC%88%E8%A3%85%E8%88%B9%E6%97%B6%E9%97%B4%E6%9C%89%E9%97%AE%E9%A2%98%E3%80%81%E8%B4%A7%E7%89%A9%E8%A1%A8%E9%9D%A2%E8%B4%A8%E9%87%8F%E6%9C%89%E9%97%AE%E9%A2%98%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="信用证欺诈例外原则" _mubu_text="%3Cspan%3E%E4%BF%A1%E7%94%A8%E8%AF%81%E6%AC%BA%E8%AF%88%E4%BE%8B%E5%A4%96%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="发出止付令条件" _mubu_text="%3Cspan%3E%E5%8F%91%E5%87%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%AD%A2%E4%BB%98%E4%BB%A4%3C/span%3E%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="（主体）法院发出" _mubu_text="%3Cspan%20class=%22bold%22%3E%EF%BC%88%E4%B8%BB%E4%BD%93%EF%BC%89%E6%B3%95%E9%99%A2%3C/span%3E%3Cspan%3E%E5%8F%91%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="（实体）证据、担保" _mubu_text="%3Cspan%20class=%22bold%22%3E%EF%BC%88%E5%AE%9E%E4%BD%93%EF%BC%89%E8%AF%81%E6%8D%AE%E3%80%81%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="（时间）任何一家关联银行均未善意付款或承兑" _mubu_text="%3Cspan%20class=%22bold%22%3E%EF%BC%88%E6%97%B6%E9%97%B4%EF%BC%89%E4%BB%BB%E4%BD%95%E4%B8%80%E5%AE%B6%3C/span%3E%3Cspan%3E%E5%85%B3%E8%81%94%E9%93%B6%E8%A1%8C%E5%9D%87%E6%9C%AA%E5%96%84%E6%84%8F%E4%BB%98%E6%AC%BE%E6%88%96%E6%89%BF%E5%85%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="时间限制：48 小时内裁定" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%E9%99%90%E5%88%B6%EF%BC%9A48%20%E5%B0%8F%E6%97%B6%E5%86%85%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="在有效信用证下开证行可不付款情形" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%9C%A8%E6%9C%89%E6%95%88%E4%BF%A1%E7%94%A8%E8%AF%81%E4%B8%8B%E5%BC%80%E8%AF%81%E8%A1%8C%E5%8F%AF%E4%B8%8D%E4%BB%98%E6%AC%BE%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="单证、单单不一致" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8D%95%E8%AF%81%E3%80%81%E5%8D%95%E5%8D%95%3C/span%3E%3Cspan%3E%E4%B8%8D%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法院发出止付令" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%91%E5%87%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%AD%A2%E4%BB%98%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="04 对外贸易管理制度" _mubu_text="%3Cspan%3E04%20%E5%AF%B9%E5%A4%96%E8%B4%B8%E6%98%93%E7%AE%A1%E7%90%86%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
        <outline text="《对外贸易法》" _mubu_text="%3Cspan%3E%E3%80%8A%E5%AF%B9%E5%A4%96%E8%B4%B8%E6%98%93%E6%B3%95%E3%80%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="货物、技术、国际服务贸易" _mubu_text="%3Cspan%3E%E8%B4%A7%E7%89%A9%E3%80%81%E6%8A%80%E6%9C%AF%E3%80%81%E5%9B%BD%E9%99%85%E6%9C%8D%E5%8A%A1%E8%B4%B8%E6%98%93%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="中国大陆（不适用港澳台）" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E5%A4%A7%E9%99%86%EF%BC%88%E4%B8%8D%E9%80%82%E7%94%A8%E6%B8%AF%E6%BE%B3%E5%8F%B0%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="外贸经营者：法人、其他组织和个人" _mubu_text="%3Cspan%3E%E5%A4%96%E8%B4%B8%E7%BB%8F%E8%90%A5%E8%80%85%EF%BC%9A%E6%B3%95%E4%BA%BA%E3%80%81%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%E5%92%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%AA%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="登记备案制" _mubu_text="%3Cspan%3E%E7%99%BB%E8%AE%B0%E5%A4%87%E6%A1%88%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="《出口管制法》" _mubu_text="%3Cspan%3E%E3%80%8A%E5%87%BA%E5%8F%A3%E7%AE%A1%E5%88%B6%E6%B3%95%E3%80%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="主体适用：出口经营者（主要）、进口商和最终用户" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E9%80%82%E7%94%A8%EF%BC%9A%E5%87%BA%E5%8F%A3%E7%BB%8F%E8%90%A5%E8%80%85%EF%BC%88%E4%B8%BB%E8%A6%81%EF%BC%89%E3%80%81%E8%BF%9B%E5%8F%A3%E5%95%86%E5%92%8C%E6%9C%80%E7%BB%88%E7%94%A8%E6%88%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="客体适用：军民两用、军品、核等管制物项相关的货物、知识产权和服务" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%E9%80%82%E7%94%A8%EF%BC%9A%E5%86%9B%E6%B0%91%E4%B8%A4%E7%94%A8%E3%80%81%E5%86%9B%E5%93%81%E3%80%81%E6%A0%B8%E7%AD%89%E7%AE%A1%E5%88%B6%E7%89%A9%E9%A1%B9%E7%9B%B8%E5%85%B3%E7%9A%84%E8%B4%A7%E7%89%A9%E3%80%81%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%E5%92%8C%E6%9C%8D%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="管制环节：出口、过境、转运、通运、再出口等" _mubu_text="%3Cspan%3E%E7%AE%A1%E5%88%B6%E7%8E%AF%E8%8A%82%EF%BC%9A%E5%87%BA%E5%8F%A3%E3%80%81%E8%BF%87%E5%A2%83%E3%80%81%E8%BD%AC%E8%BF%90%E3%80%81%E9%80%9A%E8%BF%90%E3%80%81%E5%86%8D%E5%87%BA%E5%8F%A3%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管理部门" _mubu_text="%3Cspan%3E%E7%AE%A1%E7%90%86%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="国务院、中央军委的有关部门" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E3%80%81%E4%B8%AD%E5%A4%AE%E5%86%9B%E5%A7%94%E7%9A%84%E6%9C%89%E5%85%B3%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="主要制度" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="1. 管制清单制度" _mubu_text="%3Cspan%3E1.%20%E7%AE%A1%E5%88%B6%E6%B8%85%E5%8D%95%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="﻿出口管制清单；" _mubu_text="%3Cspan%3E%EF%BB%BF%E5%87%BA%E5%8F%A3%E7%AE%A1%E5%88%B6%E6%B8%85%E5%8D%95%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="﻿临时管制清单（一般不超过2年）；" _mubu_text="%3Cspan%3E%EF%BB%BF%E4%B8%B4%E6%97%B6%E7%AE%A1%E5%88%B6%E6%B8%85%E5%8D%95%EF%BC%88%E4%B8%80%E8%88%AC%E4%B8%8D%E8%B6%85%E8%BF%872%E5%B9%B4%EF%BC%89%EF%BC%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="禁止出口清单：禁止相关管制物项的出口，或者禁止相关管制物项向特定目的国家和地区、特定组织和个人出口。" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E5%87%BA%E5%8F%A3%E6%B8%85%E5%8D%95%EF%BC%9A%E7%A6%81%E6%AD%A2%E7%9B%B8%E5%85%B3%E7%AE%A1%E5%88%B6%E7%89%A9%E9%A1%B9%E7%9A%84%E5%87%BA%E5%8F%A3%EF%BC%8C%E6%88%96%E8%80%85%E7%A6%81%E6%AD%A2%E7%9B%B8%E5%85%B3%E7%AE%A1%E5%88%B6%E7%89%A9%E9%A1%B9%E5%90%91%E7%89%B9%E5%AE%9A%E7%9B%AE%E7%9A%84%E5%9B%BD%E5%AE%B6%E5%92%8C%E5%9C%B0%E5%8C%BA%E3%80%81%E7%89%B9%E5%AE%9A%E7%BB%84%E7%BB%87%E5%92%8C%E4%B8%AA%E4%BA%BA%E5%87%BA%E5%8F%A3%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="﻿﻿﻿2.出口许可制度" _mubu_text="%3Cspan%3E%EF%BB%BF%EF%BB%BF%EF%BB%BF2.%E5%87%BA%E5%8F%A3%E8%AE%B8%E5%8F%AF%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="出口管制/临时管制清单所列物项" _mubu_text="%3Cspan%3E%E5%87%BA%E5%8F%A3%E7%AE%A1%E5%88%B6/%E4%B8%B4%E6%97%B6%E7%AE%A1%E5%88%B6%E6%B8%85%E5%8D%95%E6%89%80%E5%88%97%E7%89%A9%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="﻿﻿3.最终用户和最终用途风险管理制度（评估和核查）" _mubu_text="%3Cspan%3E%EF%BB%BF%EF%BB%BF3.%E6%9C%80%E7%BB%88%E7%94%A8%E6%88%B7%E5%92%8C%E6%9C%80%E7%BB%88%E7%94%A8%E9%80%94%E9%A3%8E%E9%99%A9%E7%AE%A1%E7%90%86%E5%88%B6%E5%BA%A6%EF%BC%88%E8%AF%84%E4%BC%B0%E5%92%8C%E6%A0%B8%E6%9F%A5%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="﻿﻿﻿4.军品出口专营制度" _mubu_text="%3Cspan%3E%EF%BB%BF%EF%BB%BF%EF%BB%BF4.%E5%86%9B%E5%93%81%E5%87%BA%E5%8F%A3%E4%B8%93%E8%90%A5%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="贸易救济措施" _mubu_text="%3Cspan%3E%E8%B4%B8%E6%98%93%E6%95%91%E6%B5%8E%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
          <outline text="反倾销措施" _mubu_text="%3Cspan%20class=%22bold%20text-color-red%22%3E%E5%8F%8D%E5%80%BE%E9%94%80%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用条件：倾销、损害、因果关系" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%EF%BC%9A%E5%80%BE%E9%94%80%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%8D%9F%E5%AE%B3%E3%80%81%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="倾销：出口价格 &lt; 正常价值" _mubu_text="%3Cspan%3E%E5%80%BE%E9%94%80%EF%BC%9A%E5%87%BA%E5%8F%A3%E4%BB%B7%E6%A0%BC%20&amp;lt;%20%E6%AD%A3%E5%B8%B8%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="正常价值（按顺序）" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%B8%B8%E4%BB%B7%E5%80%BC%EF%BC%88%E6%8C%89%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%A1%BA%E5%BA%8F%3C/span%3E%3Cspan%3E%EF%BC%89%3C/span%3E" _note="" _mubu_note="">
                  <outline text="出口国可比售价" _mubu_text="%3Cspan%3E%E5%87%BA%E5%8F%A3%E5%9B%BD%E5%8F%AF%E6%AF%94%E5%94%AE%E4%BB%B7%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="出口到第三国可比售价或原产国构成价" _mubu_text="%3Cspan%3E%E5%87%BA%E5%8F%A3%E5%88%B0%E7%AC%AC%E4%B8%89%E5%9B%BD%E5%8F%AF%E6%AF%94%E5%94%AE%E4%BB%B7%E6%88%96%E5%8E%9F%E4%BA%A7%E5%9B%BD%E6%9E%84%E6%88%90%E4%BB%B7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="损害" _mubu_text="%3Cspan%3E%E6%8D%9F%E5%AE%B3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="因果关系" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="分别是常态" _mubu_text="%3Cspan%3E%E5%88%86%E5%88%AB%E6%98%AF%E5%B8%B8%E6%80%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="累积是例外" _mubu_text="%3Cspan%3E%E7%B4%AF%E7%A7%AF%E6%98%AF%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="调查程序" _mubu_text="%3Cspan%3E%E8%B0%83%E6%9F%A5%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="商务部发起" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%95%86%E5%8A%A1%E9%83%A8%3C/span%3E%3Cspan%3E%E5%8F%91%E8%B5%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="国内产业申请，并非国内企业" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%86%85%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BA%A7%E4%B8%9A%3C/span%3E%3Cspan%3E%E7%94%B3%E8%AF%B7%EF%BC%8C%E5%B9%B6%E9%9D%9E%E5%9B%BD%E5%86%85%E4%BC%81%E4%B8%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="商务部主动" _mubu_text="%3Cspan%3E%E5%95%86%E5%8A%A1%E9%83%A8%E4%B8%BB%E5%8A%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="初步裁定" _mubu_text="%3Cspan%3E%E5%88%9D%E6%AD%A5%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="终局裁定" _mubu_text="%3Cspan%3E%E7%BB%88%E5%B1%80%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="行政复审或国内司法审查" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%A4%8D%E5%AE%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%88%96%3C/span%3E%3Cspan%3E%E5%9B%BD%E5%86%85%E5%8F%B8%E6%B3%95%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="行政复审不是行政诉讼的必经程序" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%A4%8D%E5%AE%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%98%AF%3C/span%3E%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%AF%89%E8%AE%BC%E7%9A%84%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%BF%85%E7%BB%8F%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="反倾销措施" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8F%8D%E5%80%BE%E9%94%80%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="商务部调查，海关征收" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%95%86%E5%8A%A1%E9%83%A8%3C/span%3E%3Cspan%3E%E8%B0%83%E6%9F%A5%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B5%B7%E5%85%B3%3C/span%3E%3Cspan%3E%E5%BE%81%E6%94%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="初步裁定" _mubu_text="%3Cspan%3E%E5%88%9D%E6%AD%A5%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="临时反倾销措施" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E5%8F%8D%E5%80%BE%E9%94%80%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
                  <outline text="种类：临时税或担保" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%EF%BC%9A%E4%B8%B4%E6%97%B6%E7%A8%8E%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%88%96%3C/span%3E%3Cspan%3E%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="进口经营者承担" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%BF%9B%E5%8F%A3%E7%BB%8F%E8%90%A5%E8%80%85%3C/span%3E%3Cspan%3E%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="终裁不征税（原则）：退回临时税，解除担保" _mubu_text="%3Cspan%3E%E7%BB%88%E8%A3%81%E4%B8%8D%E5%BE%81%E7%A8%8E%EF%BC%88%E5%8E%9F%E5%88%99%EF%BC%89%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%80%80%E5%9B%9E%3C/span%3E%3Cspan%3E%E4%B8%B4%E6%97%B6%E7%A8%8E%EF%BC%8C%E8%A7%A3%E9%99%A4%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
                    <outline text="对终局裁定公告后再进口的产品征收" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%BB%88%E5%B1%80%E8%A3%81%E5%AE%9A%E5%85%AC%E5%91%8A%E5%90%8E%E5%86%8D%E8%BF%9B%E5%8F%A3%E7%9A%84%E4%BA%A7%E5%93%81%E5%BE%81%E6%94%B6%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="终裁追溯征收：多退少不补" _mubu_text="%3Cspan%3E%E7%BB%88%E8%A3%81%E8%BF%BD%E6%BA%AF%E5%BE%81%E6%94%B6%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%A4%9A%E9%80%80%E5%B0%91%E4%B8%8D%E8%A1%A5%3C/span%3E" _note="" _mubu_note="">
                    <outline text="存在实质损害或实质损害威胁且已采取临时措施的——可以追溯至临时措施" _mubu_text="%3Cspan%3E%E5%AD%98%E5%9C%A8%E5%AE%9E%E8%B4%A8%E6%8D%9F%E5%AE%B3%E6%88%96%E5%AE%9E%E8%B4%A8%E6%8D%9F%E5%AE%B3%E5%A8%81%E8%83%81%E4%B8%94%E5%B7%B2%E9%87%87%E5%8F%96%E4%B8%B4%E6%97%B6%E6%8E%AA%E6%96%BD%E7%9A%84%E2%80%94%E2%80%94%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E8%BF%BD%E6%BA%AF%E8%87%B3%E4%B8%B4%E6%97%B6%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="有倾销历史或进口商明知且短期大量进口的——可以追溯至临时措施前90天" _mubu_text="%3Cspan%3E%E6%9C%89%E5%80%BE%E9%94%80%E5%8E%86%E5%8F%B2%E6%88%96%E8%BF%9B%E5%8F%A3%E5%95%86%E6%98%8E%E7%9F%A5%E4%B8%94%E7%9F%AD%E6%9C%9F%E5%A4%A7%E9%87%8F%E8%BF%9B%E5%8F%A3%E7%9A%84%E2%80%94%E2%80%94%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E8%BF%BD%E6%BA%AF%E8%87%B3%E4%B8%B4%E6%97%B6%E6%8E%AA%E6%96%BD%E5%89%8D90%E5%A4%A9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="终局裁定" _mubu_text="%3Cspan%3E%E7%BB%88%E5%B1%80%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="价格承诺" _mubu_text="%3Cspan%3E%E4%BB%B7%E6%A0%BC%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="出口经营者承诺" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%87%BA%E5%8F%A3%E7%BB%8F%E8%90%A5%E8%80%85%3C/span%3E%3Cspan%3E%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="可以承诺" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="主管机关可以接受" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%AE%A1%E6%9C%BA%E5%85%B3%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E6%8E%A5%E5%8F%97%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="主管机关初裁之前不能接受价格承诺" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%AE%A1%E6%9C%BA%E5%85%B3%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%88%9D%E8%A3%81%E4%B9%8B%E5%89%8D%3C/span%3E%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%8E%A5%E5%8F%97%E4%BB%B7%E6%A0%BC%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="或反倾销税" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%88%96%3C/span%3E%3Cspan%3E%E5%8F%8D%E5%80%BE%E9%94%80%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="进口经营者缴纳" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%BF%9B%E5%8F%A3%E7%BB%8F%E8%90%A5%E8%80%85%3C/span%3E%3Cspan%3E%E7%BC%B4%E7%BA%B3%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="原则上不追溯" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%BF%BD%E6%BA%AF%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="一般不超过 5 年" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%B8%8D%E8%B6%85%E8%BF%87%20%3C/span%3E%3Cspan%20class=%22bold%22%3E5%20%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="税额不超过倾销幅度" _mubu_text="%3Cspan%3E%E7%A8%8E%E9%A2%9D%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E8%B6%85%E8%BF%87%3C/span%3E%3Cspan%3E%E5%80%BE%E9%94%80%E5%B9%85%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="反倾销税" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%80%BE%E9%94%80%E7%A8%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="应当根据不同倾销幅度分别确定" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E6%A0%B9%E6%8D%AE%E4%B8%8D%E5%90%8C%E5%80%BE%E9%94%80%E5%B9%85%E5%BA%A6%E5%88%86%E5%88%AB%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="倾销幅度不同，反倾销税率也应当不同" _mubu_text="%3Cspan%3E%E5%80%BE%E9%94%80%E5%B9%85%E5%BA%A6%E4%B8%8D%E5%90%8C%EF%BC%8C%E5%8F%8D%E5%80%BE%E9%94%80%E7%A8%8E%E7%8E%87%E4%B9%9F%E5%BA%94%E5%BD%93%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="征收期间" _mubu_text="%3Cspan%3E%E5%BE%81%E6%94%B6%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="﻿反倾销税原则上只对终局裁定公告后再进口的产品征收。" _mubu_text="%3Cspan%3E%EF%BB%BF%E5%8F%8D%E5%80%BE%E9%94%80%E7%A8%8E%E5%8E%9F%E5%88%99%E4%B8%8A%E5%8F%AA%E5%AF%B9%E7%BB%88%E5%B1%80%E8%A3%81%E5%AE%9A%E5%85%AC%E5%91%8A%E5%90%8E%E5%86%8D%E8%BF%9B%E5%8F%A3%E7%9A%84%E4%BA%A7%E5%93%81%E5%BE%81%E6%94%B6%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="﻿存在实质损害或实质损害威胁且已采取临时措施的，可以追溯至实施临时反倾销措施期间。" _mubu_text="%3Cspan%3E%EF%BB%BF%E5%AD%98%E5%9C%A8%E5%AE%9E%E8%B4%A8%E6%8D%9F%E5%AE%B3%E6%88%96%E5%AE%9E%E8%B4%A8%E6%8D%9F%E5%AE%B3%E5%A8%81%E8%83%81%E4%B8%94%E5%B7%B2%E9%87%87%E5%8F%96%E4%B8%B4%E6%97%B6%E6%8E%AA%E6%96%BD%E7%9A%84%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E8%BF%BD%E6%BA%AF%E8%87%B3%E5%AE%9E%E6%96%BD%E4%B8%B4%E6%97%B6%E5%8F%8D%E5%80%BE%E9%94%80%E6%8E%AA%E6%96%BD%E6%9C%9F%E9%97%B4%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="﻿有倾销历史或进口商明知且短期大量进口的，可以追湖至临时措施前90天。" _mubu_text="%3Cspan%3E%EF%BB%BF%E6%9C%89%E5%80%BE%E9%94%80%E5%8E%86%E5%8F%B2%E6%88%96%E8%BF%9B%E5%8F%A3%E5%95%86%E6%98%8E%E7%9F%A5%E4%B8%94%E7%9F%AD%E6%9C%9F%E5%A4%A7%E9%87%8F%E8%BF%9B%E5%8F%A3%E7%9A%84%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E8%BF%BD%E6%B9%96%E8%87%B3%E4%B8%B4%E6%97%B6%E6%8E%AA%E6%96%BD%E5%89%8D90%E5%A4%A9%E3%80%82%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="反补贴措施" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8F%8D%E8%A1%A5%E8%B4%B4%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用条件：专项补贴、损害、因果关系" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%EF%BC%9A%E4%B8%93%E9%A1%B9%E8%A1%A5%E8%B4%B4%E3%80%81%E6%8D%9F%E5%AE%B3%E3%80%81%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="专项补贴" _mubu_text="%3Cspan%3E%E4%B8%93%E9%A1%B9%E8%A1%A5%E8%B4%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="政府提供财政资助" _mubu_text="%3Cspan%3E%E6%94%BF%E5%BA%9C%E6%8F%90%E4%BE%9B%E8%B4%A2%E6%94%BF%E8%B5%84%E5%8A%A9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="接受者获得利益" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%8F%97%E8%80%85%E8%8E%B7%E5%BE%97%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="专向性：部分" _mubu_text="%3Cspan%3E%E4%B8%93%E5%90%91%E6%80%A7%EF%BC%9A%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="反补贴措施" _mubu_text="%3Cspan%3E%E5%8F%8D%E8%A1%A5%E8%B4%B4%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
              <outline text="承诺主体为出口国政府或出口经营者承诺" _mubu_text="%3Cspan%3E%E6%89%BF%E8%AF%BA%E4%B8%BB%E4%BD%93%E4%B8%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%87%BA%E5%8F%A3%E5%9B%BD%E6%94%BF%E5%BA%9C%3C/span%3E%3Cspan%3E%E6%88%96%E5%87%BA%E5%8F%A3%E7%BB%8F%E8%90%A5%E8%80%85%E6%89%BF%E8%AF%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="保障措施" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%BF%9D%E9%9A%9C%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用条件：" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%EF%BC%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="进口数量增加（相对或绝对）" _mubu_text="%3Cspan%3E%E8%BF%9B%E5%8F%A3%E6%95%B0%E9%87%8F%E5%A2%9E%E5%8A%A0%EF%BC%88%E7%9B%B8%E5%AF%B9%E6%88%96%E7%BB%9D%E5%AF%B9%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="严重损害或严重损害威胁" _mubu_text="%3Cspan%3E%E4%B8%A5%E9%87%8D%E6%8D%9F%E5%AE%B3%E6%88%96%E4%B8%A5%E9%87%8D%E6%8D%9F%E5%AE%B3%E5%A8%81%E8%83%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="因果关系" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
