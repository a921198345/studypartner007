<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <head>
    <title>三国法 - 杨帆</title>
  </head>
  <body>
    <outline text="国际法" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="01 国际法的渊源和效力" _mubu_text="%3Cspan%3E01%20%E5%9B%BD%E9%99%85%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%E5%92%8C%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
        <outline text="国际法的渊源" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%3C/span%3E" _note="" _mubu_note="">
          <outline text="渊源（作为法律依据）：仅限国际条约、国际习惯、一般法律原则" _mubu_text="%3Cspan%3E%E6%B8%8A%E6%BA%90%EF%BC%88%E4%BD%9C%E4%B8%BA%E6%B3%95%E5%BE%8B%E4%BE%9D%E6%8D%AE%EF%BC%89%EF%BC%9A%E4%BB%85%E9%99%90%E5%9B%BD%E9%99%85%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%9D%A1%E7%BA%A6%E3%80%81%3C/span%3E%3Cspan%3E%E5%9B%BD%E9%99%85%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B9%A0%E6%83%AF%E3%80%81%E4%B8%80%E8%88%AC%E6%B3%95%E5%BE%8B%3C/span%3E%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="区别" _mubu_text="%3Cspan%3E%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
            <outline text="国际条约：约束缔约国，成文" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%9D%A1%E7%BA%A6%EF%BC%9A%E7%BA%A6%E6%9D%9F%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BC%94%E7%BA%A6%E5%9B%BD%3C/span%3E%3Cspan%3E%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%88%90%E6%96%87%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="国际习惯：约束所有，不成文，实践形成、官方证据" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E4%B9%A0%E6%83%AF%EF%BC%9A%E7%BA%A6%E6%9D%9F%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%80%E6%9C%89%3C/span%3E%3Cspan%3E%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%88%90%E6%96%87%3C/span%3E%3Cspan%3E%EF%BC%8C%E5%AE%9E%E8%B7%B5%E5%BD%A2%E6%88%90%E3%80%81%E5%AE%98%E6%96%B9%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一般法律原则：约束所有，不成文" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99%EF%BC%9A%E7%BA%A6%E6%9D%9F%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%80%E6%9C%89%3C/span%3E%3Cspan%3E%EF%BC%8C%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8D%E6%88%90%E6%96%87%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="国际条约在中国的适用" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%9D%A1%E7%BA%A6%E5%9C%A8%E4%B8%AD%E5%9B%BD%E7%9A%84%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="宪法无规定" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E6%97%A0%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="直接并优先适用：民商事，除知识产权" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%9B%B4%E6%8E%A5%3C/span%3E%3Cspan%3E%E5%B9%B6%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BC%98%E5%85%88%3C/span%3E%3Cspan%3E%E9%80%82%E7%94%A8%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B0%91%E5%95%86%3C/span%3E%3Cspan%3E%E4%BA%8B%EF%BC%8C%E9%99%A4%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="转化适用：WTO 协议、知识产权" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%BD%AC%E5%8C%96%3C/span%3E%3Cspan%3E%E9%80%82%E7%94%A8%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3EWTO%3C/span%3E%3Cspan%3E%20%E5%8D%8F%E8%AE%AE%E3%80%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="并行适用：外交关系、领事关系" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%B9%B6%E8%A1%8C%3C/span%3E%3Cspan%3E%E9%80%82%E7%94%A8%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%A4%96%E4%BA%A4%3C/span%3E%3Cspan%3E%E5%85%B3%E7%B3%BB%E3%80%81%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="国际法基本原则" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%B3%95%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="性质：一般法律原则的一部分，国际法的渊源" _mubu_text="%3Cspan%3E%E6%80%A7%E8%B4%A8%EF%BC%9A%E4%B8%80%E8%88%AC%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99%E7%9A%84%E4%B8%80%E9%83%A8%E5%88%86%EF%BC%8C%E5%9B%BD%E9%99%85%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="国家主权平等原则" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%B8%BB%E6%9D%83%E5%B9%B3%E7%AD%89%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不干涉内政原则：国内管辖 + 不违反基本原则（代表在国外发言）" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%B9%B2%E6%B6%89%E5%86%85%E6%94%BF%E5%8E%9F%E5%88%99%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%BD%E5%86%85%E7%AE%A1%E8%BE%96%3C/span%3E%3Cspan%3E%20+%20%E4%B8%8D%E8%BF%9D%E5%8F%8D%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%EF%BC%88%E4%BB%A3%E8%A1%A8%E5%9C%A8%E5%9B%BD%E5%A4%96%E5%8F%91%E8%A8%80%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不使用武力和武力威胁原则：允许自卫、安理会授权" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BD%BF%E7%94%A8%E6%AD%A6%E5%8A%9B%E5%92%8C%E6%AD%A6%E5%8A%9B%E5%A8%81%E8%83%81%E5%8E%9F%E5%88%99%EF%BC%9A%E5%85%81%E8%AE%B8%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%87%AA%E5%8D%AB%E3%80%81%E5%AE%89%E7%90%86%E4%BC%9A%E6%8E%88%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="和平解决国际争端原则：和平 ≠ 非武力" _mubu_text="%3Cspan%3E%E5%92%8C%E5%B9%B3%E8%A7%A3%E5%86%B3%E5%9B%BD%E9%99%85%E4%BA%89%E7%AB%AF%E5%8E%9F%E5%88%99%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%92%8C%E5%B9%B3%20%E2%89%A0%20%E9%9D%9E%E6%AD%A6%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="民族自决原则：独立权只严格适用殖民" _mubu_text="%3Cspan%3E%E6%B0%91%E6%97%8F%E8%87%AA%E5%86%B3%E5%8E%9F%E5%88%99%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%8B%AC%E7%AB%8B%E6%9D%83%3C/span%3E%3Cspan%3E%E5%8F%AA%E4%B8%A5%E6%A0%BC%E9%80%82%E7%94%A8%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%AE%96%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="善意履行国际义务原则" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E5%B1%A5%E8%A1%8C%E5%9B%BD%E9%99%85%E4%B9%89%E5%8A%A1%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="02 国际法的主体与国际法律责任" _mubu_text="%3Cspan%3E02%20%E5%9B%BD%E9%99%85%E6%B3%95%E7%9A%84%E4%B8%BB%E4%BD%93%E4%B8%8E%E5%9B%BD%E9%99%85%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
        <outline text="国家主权豁免" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%B8%BB%E6%9D%83%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="现状：绝对豁免" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%8E%B0%E7%8A%B6%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BB%9D%E5%AF%B9%3C/span%3E%3Cspan%3E%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="含义：国家行为不受他国法院管辖，不包括国际法院" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%EF%BC%9A%E5%9B%BD%E5%AE%B6%E8%A1%8C%E4%B8%BA%E4%B8%8D%E5%8F%97%E4%BB%96%E5%9B%BD%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%EF%BC%8C%E4%B8%8D%E5%8C%85%E6%8B%AC%E5%9B%BD%E9%99%85%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="放弃：自愿 + 特定 + 明确" _mubu_text="%3Cspan%3E%E6%94%BE%E5%BC%83%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%87%AA%E6%84%BF%20+%20%E7%89%B9%E5%AE%9A%20+%20%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="明示" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="默示：「诉」行为，不含抗议管辖权等" _mubu_text="%3Cspan%3E%E9%BB%98%E7%A4%BA%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E3%80%8C%E8%AF%89%E3%80%8D%E8%A1%8C%E4%B8%BA%3C/span%3E%3Cspan%3E%EF%BC%8C%E4%B8%8D%E5%90%AB%E6%8A%97%E8%AE%AE%E7%AE%A1%E8%BE%96%E6%9D%83%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="管辖豁免 ≠ 执行豁免" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E8%B1%81%E5%85%8D%20%E2%89%A0%20%E6%89%A7%E8%A1%8C%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="理论：限制豁免" _mubu_text="%3Cspan%20class=%22bold%22%3E%E7%90%86%E8%AE%BA%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%99%90%E5%88%B6%3C/span%3E%3Cspan%3E%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note="">
            <outline text="统治行为：管辖豁免" _mubu_text="%3Cspan%3E%E7%BB%9F%E6%B2%BB%E8%A1%8C%E4%B8%BA%EF%BC%9A%E7%AE%A1%E8%BE%96%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="商业行为：没有管辖豁免" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%95%86%E4%B8%9A%3C/span%3E%3Cspan%3E%E8%A1%8C%E4%B8%BA%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%B2%A1%E6%9C%89%3C/span%3E%3Cspan%3E%E7%AE%A1%E8%BE%96%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="享有绝对执行豁免" _mubu_text="%3Cspan%3E%E4%BA%AB%E6%9C%89%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%BB%9D%E5%AF%B9%E6%89%A7%E8%A1%8C%E8%B1%81%E5%85%8D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="国际法上的承认" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%B3%95%E4%B8%8A%E7%9A%84%E6%89%BF%E8%AE%A4%3C/span%3E" _note="" _mubu_note="">
          <outline text="主体：国家、国际组织" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E5%9B%BD%E5%AE%B6%E3%80%81%E5%9B%BD%E9%99%85%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="对象" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="新国家：领土变更" _mubu_text="%3Cspan%3E%E6%96%B0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%BD%E5%AE%B6%3C/span%3E%3Cspan%3E%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E9%A2%86%E5%9C%9F%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="新政府：政权更迭" _mubu_text="%3Cspan%3E%E6%96%B0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%94%BF%E5%BA%9C%3C/span%3E%3Cspan%3E%EF%BC%9A%E6%94%BF%E6%9D%83%E6%9B%B4%E8%BF%AD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="明示" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A4%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="默示" _mubu_text="%3Cspan%3E%E9%BB%98%E7%A4%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="外交关系、领事关系" _mubu_text="%3Cspan%20class=%22bold%22%3E%E5%A4%96%E4%BA%A4%3C/span%3E%3Cspan%3E%E5%85%B3%E7%B3%BB%E3%80%81%E9%A2%86%E4%BA%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="政治性条约" _mubu_text="%3Cspan%20class=%22bold%22%3E%E6%94%BF%E6%B2%BB%E6%80%A7%3C/span%3E%3Cspan%3E%E6%9D%A1%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="投票支持参加仅对国家开放国际组织（联合国，不含 WTO）" _mubu_text="%3Cspan%3E%E6%8A%95%E7%A5%A8%E6%94%AF%E6%8C%81%E5%8F%82%E5%8A%A0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%BB%85%E5%AF%B9%E5%9B%BD%E5%AE%B6%E5%BC%80%E6%94%BE%3C/span%3E%3Cspan%3E%E5%9B%BD%E9%99%85%E7%BB%84%E7%BB%87%EF%BC%88%E8%81%94%E5%90%88%E5%9B%BD%EF%BC%8C%E4%B8%8D%E5%90%AB%20WTO%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="性质：单方行为" _mubu_text="%3Cspan%3E%E6%80%A7%E8%B4%A8%EF%BC%9A%E5%8D%95%E6%96%B9%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="后果" _mubu_text="%3Cspan%3E%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="对新国家承认：无法撤回" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B0%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%9B%BD%E5%AE%B6%3C/span%3E%3Cspan%3E%E6%89%BF%E8%AE%A4%EF%BC%9A%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%97%A0%E6%B3%95%3C/span%3E%3Cspan%3E%E6%92%A4%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
