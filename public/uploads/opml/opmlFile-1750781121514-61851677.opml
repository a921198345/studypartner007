              </outline>
              <outline text="在权限划分方面" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%9D%83%E9%99%90%E5%88%92%E5%88%86%E6%96%B9%E9%9D%A2%3C/span%3E" _note="人大与国家机关" _mubu_note="%3Cspan%3E%E4%BA%BA%E5%A4%A7%E4%B8%8E%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%3C/span%3E">
                <outline text="国家行政、监察、审判、检察、军事机关都由人大产生" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E8%A1%8C%E6%94%BF%E3%80%81%E7%9B%91%E5%AF%9F%E3%80%81%E5%AE%A1%E5%88%A4%E3%80%81%E6%A3%80%E5%AF%9F%E3%80%81%E5%86%9B%E4%BA%8B%E6%9C%BA%E5%85%B3%E9%83%BD%E7%94%B1%E4%BA%BA%E5%A4%A7%E4%BA%A7%E7%94%9F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="在中央和地方关系方面" _mubu_text="%3Cspan%3E%E5%9C%A8%E4%B8%AD%E5%A4%AE%E5%92%8C%E5%9C%B0%E6%96%B9%E5%85%B3%E7%B3%BB%E6%96%B9%E9%9D%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="在中央统一领导下，充分发挥地方积极性、主动性" _mubu_text="%3Cspan%3E%E5%9C%A8%E4%B8%AD%E5%A4%AE%E7%BB%9F%E4%B8%80%E9%A2%86%E5%AF%BC%E4%B8%8B%EF%BC%8C%E5%85%85%E5%88%86%E5%8F%91%E6%8C%A5%E5%9C%B0%E6%96%B9%E7%A7%AF%E6%9E%81%E6%80%A7%E3%80%81%E4%B8%BB%E5%8A%A8%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="在国家机关内部方面" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E5%86%85%E9%83%A8%E6%96%B9%E9%9D%A2%3C/span%3E" _note="民主" _mubu_note="%3Cspan%3E%E6%B0%91%E4%B8%BB%3C/span%3E">
                <outline text="人大及其常委会实行集体领导制" _mubu_text="%3Cspan%3E%E4%BA%BA%E5%A4%A7%E5%8F%8A%E5%85%B6%E5%B8%B8%E5%A7%94%E4%BC%9A%E5%AE%9E%E8%A1%8C%E9%9B%86%E4%BD%93%E9%A2%86%E5%AF%BC%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="行政和军事机关实行首长负责制" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%92%8C%E5%86%9B%E4%BA%8B%E6%9C%BA%E5%85%B3%E5%AE%9E%E8%A1%8C%E9%A6%96%E9%95%BF%E8%B4%9F%E8%B4%A3%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="在具体工作方面" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%85%B7%E4%BD%93%E5%B7%A5%E4%BD%9C%E6%96%B9%E9%9D%A2%3C/span%3E" _note="集中" _mubu_note="%3Cspan%3E%E9%9B%86%E4%B8%AD%3C/span%3E">
                <outline text="各国家机关须遵循民主集中制原则" _mubu_text="%3Cspan%3E%E5%90%84%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E9%A1%BB%E9%81%B5%E5%BE%AA%E6%B0%91%E4%B8%BB%E9%9B%86%E4%B8%AD%E5%88%B6%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
