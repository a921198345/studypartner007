            <outline text="宪法委员会和德国的宪法法院" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E5%A7%94%E5%91%98%E4%BC%9A%E5%92%8C%E5%BE%B7%E5%9B%BD%E7%9A%84%E5%AE%AA%E6%B3%95%E6%B3%95%E9%99%A2%3C/span%3E" _note="法国、西班牙、意大利、德国、奥地利、韩国、俄罗斯等" _mubu_note="%3Cspan%3E%E6%B3%95%E5%9B%BD%E3%80%81%E8%A5%BF%E7%8F%AD%E7%89%99%E3%80%81%E6%84%8F%E5%A4%A7%E5%88%A9%E3%80%81%E5%BE%B7%E5%9B%BD%E3%80%81%E5%A5%A5%E5%9C%B0%E5%88%A9%E3%80%81%E9%9F%A9%E5%9B%BD%E3%80%81%E4%BF%84%E7%BD%97%E6%96%AF%E7%AD%89%3C/span%3E"/>
          </outline>
          <outline text="宪法解释与合宪性审查是一致的" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E8%A7%A3%E9%87%8A%E4%B8%8E%E5%90%88%E5%AE%AA%E6%80%A7%E5%AE%A1%E6%9F%A5%E6%98%AF%E4%B8%80%E8%87%B4%E7%9A%84%3C/span%3E" _note="宪法解释是合宪性审查的前奏" _mubu_note="%3Cspan%3E%E5%AE%AA%E6%B3%95%E8%A7%A3%E9%87%8A%E6%98%AF%E5%90%88%E5%AE%AA%E6%80%A7%E5%AE%A1%E6%9F%A5%E7%9A%84%E5%89%8D%E5%A5%8F%3C/span%3E"/>
        </outline>
        <outline text="宪法实施的监督和保障" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E7%9B%91%E7%9D%A3%E5%92%8C%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note="">
          <outline text="宪法的监督对象" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E7%9A%84%E7%9B%91%E7%9D%A3%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="法律法规的合宪性审查" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%B3%95%E8%A7%84%E7%9A%84%E5%90%88%E5%AE%AA%E6%80%A7%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="行为的合宪性审查" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%90%88%E5%AE%AA%E6%80%A7%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="行为可通过遵守下位法达到合宪的目的" _mubu_note="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E5%8F%AF%E9%80%9A%E8%BF%87%E9%81%B5%E5%AE%88%E4%B8%8B%E4%BD%8D%E6%B3%95%E8%BE%BE%E5%88%B0%E5%90%88%E5%AE%AA%E7%9A%84%E7%9B%AE%E7%9A%84%3C/span%3E"/>
          </outline>
          <outline text="宪法实施的保障体制" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E4%BF%9D%E9%9A%9C%E4%BD%93%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="同宪法解释对应" _mubu_text="%3Cspan%3E%E5%90%8C%E5%AE%AA%E6%B3%95%E8%A7%A3%E9%87%8A%E5%AF%B9%E5%BA%94%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="立法机关保障" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%B3%95%E6%9C%BA%E5%85%B3%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="议会模式" _mubu_note="%3Cspan%3E%E8%AE%AE%E4%BC%9A%E6%A8%A1%E5%BC%8F%3C/span%3E"/>
            <outline text="司法机关保障" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="专门机关保障" _mubu_text="%3Cspan%3E%E4%B8%93%E9%97%A8%E6%9C%BA%E5%85%B3%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="宪法实施的保障方式" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E4%BF%9D%E9%9A%9C%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="事前审查or事后审查" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%89%8D%E5%AE%A1%E6%9F%A5or%E4%BA%8B%E5%90%8E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="立法机关模式" _mubu_note="%3Cspan%3E%E7%AB%8B%E6%B3%95%E6%9C%BA%E5%85%B3%E6%A8%A1%E5%BC%8F%3C/span%3E">
              <outline text="事前审查：公布实施以前，由有权机关对其合宪性进行审查" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%89%8D%E5%AE%A1%E6%9F%A5%EF%BC%9A%E5%85%AC%E5%B8%83%E5%AE%9E%E6%96%BD%E4%BB%A5%E5%89%8D%EF%BC%8C%E7%94%B1%E6%9C%89%E6%9D%83%E6%9C%BA%E5%85%B3%E5%AF%B9%E5%85%B6%E5%90%88%E5%AE%AA%E6%80%A7%E8%BF%9B%E8%A1%8C%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="事后审查：颁行后，有关主体对其合宪性提出质疑所引发的审查" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%90%8E%E5%AE%A1%E6%9F%A5%EF%BC%9A%E9%A2%81%E8%A1%8C%E5%90%8E%EF%BC%8C%E6%9C%89%E5%85%B3%E4%B8%BB%E4%BD%93%E5%AF%B9%E5%85%B6%E5%90%88%E5%AE%AA%E6%80%A7%E6%8F%90%E5%87%BA%E8%B4%A8%E7%96%91%E6%89%80%E5%BC%95%E5%8F%91%E7%9A%84%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="附带性审查or宪法控诉(宪法诉愿)" _mubu_text="%3Cspan%3E%E9%99%84%E5%B8%A6%E6%80%A7%E5%AE%A1%E6%9F%A5or%E5%AE%AA%E6%B3%95%E6%8E%A7%E8%AF%89(%E5%AE%AA%E6%B3%95%E8%AF%89%E6%84%BF)%3C/span%3E" _note="我国没有" _mubu_note="%3Cspan%3E%E6%88%91%E5%9B%BD%E6%B2%A1%E6%9C%89%3C/span%3E">
              <outline text="附带性审查：司法机关在审理具体案件的过程中，当事人或司法机关对案件涉及的规范性文件的合宪性提出质疑进而引发的审查" _mubu_text="%3Cspan%3E%E9%99%84%E5%B8%A6%E6%80%A7%E5%AE%A1%E6%9F%A5%EF%BC%9A%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E5%9C%A8%E5%AE%A1%E7%90%86%E5%85%B7%E4%BD%93%E6%A1%88%E4%BB%B6%E7%9A%84%E8%BF%87%E7%A8%8B%E4%B8%AD%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%88%96%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E5%AF%B9%E6%A1%88%E4%BB%B6%E6%B6%89%E5%8F%8A%E7%9A%84%E8%A7%84%E8%8C%83%E6%80%A7%E6%96%87%E4%BB%B6%E7%9A%84%E5%90%88%E5%AE%AA%E6%80%A7%E6%8F%90%E5%87%BA%E8%B4%A8%E7%96%91%E8%BF%9B%E8%80%8C%E5%BC%95%E5%8F%91%E7%9A%84%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="普通法院模式" _mubu_note="%3Cspan%3E%E6%99%AE%E9%80%9A%E6%B3%95%E9%99%A2%E6%A8%A1%E5%BC%8F%3C/span%3E"/>
              <outline text="宪法控诉：公民在宪法权利受到侵犯时向宪法法院或其他专门机构提出控诉，要求对有关行为或规范性文件的合宪性进行审查" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E6%8E%A7%E8%AF%89%EF%BC%9A%E5%85%AC%E6%B0%91%E5%9C%A8%E5%AE%AA%E6%B3%95%E6%9D%83%E5%88%A9%E5%8F%97%E5%88%B0%E4%BE%B5%E7%8A%AF%E6%97%B6%E5%90%91%E5%AE%AA%E6%B3%95%E6%B3%95%E9%99%A2%E6%88%96%E5%85%B6%E4%BB%96%E4%B8%93%E9%97%A8%E6%9C%BA%E6%9E%84%E6%8F%90%E5%87%BA%E6%8E%A7%E8%AF%89%EF%BC%8C%E8%A6%81%E6%B1%82%E5%AF%B9%E6%9C%89%E5%85%B3%E8%A1%8C%E4%B8%BA%E6%88%96%E8%A7%84%E8%8C%83%E6%80%A7%E6%96%87%E4%BB%B6%E7%9A%84%E5%90%88%E5%AE%AA%E6%80%A7%E8%BF%9B%E8%A1%8C%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="专门机关模式" _mubu_note="%3Cspan%3E%E4%B8%93%E9%97%A8%E6%9C%BA%E5%85%B3%E6%A8%A1%E5%BC%8F%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="全人常的宪法监督程序" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E7%9A%84%E5%AE%AA%E6%B3%95%E7%9B%91%E7%9D%A3%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="依据" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="全国各组织或个人都必须以宪法为根本的活动准则" _mubu_text="%3Cspan%3E%E5%85%A8%E5%9B%BD%E5%90%84%E7%BB%84%E7%BB%87%E6%88%96%E4%B8%AA%E4%BA%BA%E9%83%BD%E5%BF%85%E9%A1%BB%E4%BB%A5%E5%AE%AA%E6%B3%95%E4%B8%BA%E6%A0%B9%E6%9C%AC%E7%9A%84%E6%B4%BB%E5%8A%A8%E5%87%86%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="且负有维护宪法的尊严、保证宪法实施的职责" _mubu_text="%3Cspan%3E%E4%B8%94%E8%B4%9F%E6%9C%89%E7%BB%B4%E6%8A%A4%E5%AE%AA%E6%B3%95%E7%9A%84%E5%B0%8A%E4%B8%A5%E3%80%81%E4%BF%9D%E8%AF%81%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E8%81%8C%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="一切法律法规都不得同宪法相抵触" _mubu_text="%3Cspan%3E%E4%B8%80%E5%88%87%E6%B3%95%E5%BE%8B%E6%B3%95%E8%A7%84%E9%83%BD%E4%B8%8D%E5%BE%97%E5%90%8C%E5%AE%AA%E6%B3%95%E7%9B%B8%E6%8A%B5%E8%A7%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="任何组织或个人都不能有超越宪法的特权" _mubu_text="%3Cspan%3E%E4%BB%BB%E4%BD%95%E7%BB%84%E7%BB%87%E6%88%96%E4%B8%AA%E4%BA%BA%E9%83%BD%E4%B8%8D%E8%83%BD%E6%9C%89%E8%B6%85%E8%B6%8A%E5%AE%AA%E6%B3%95%E7%9A%84%E7%89%B9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="机关" _mubu_text="%3Cspan%3E%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="54宪法规定了全人大监督宪法实施的权力" _mubu_text="%3Cspan%3E54%E5%AE%AA%E6%B3%95%E8%A7%84%E5%AE%9A%E4%BA%86%E5%85%A8%E4%BA%BA%E5%A4%A7%E7%9B%91%E7%9D%A3%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E6%9D%83%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="82宪法增加了全人常监督宪法实施的权力" _mubu_text="%3Cspan%3E82%E5%AE%AA%E6%B3%95%E5%A2%9E%E5%8A%A0%E4%BA%86%E5%85%A8%E4%BA%BA%E5%B8%B8%E7%9B%91%E7%9D%A3%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E7%9A%84%E6%9D%83%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="主动审查" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="全人大有关专委和常委工作机构可对报备案的规范性文件进行主动审查" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E6%9C%89%E5%85%B3%E4%B8%93%E5%A7%94%E5%92%8C%E5%B8%B8%E5%A7%94%E5%B7%A5%E4%BD%9C%E6%9C%BA%E6%9E%84%E5%8F%AF%E5%AF%B9%E6%8A%A5%E5%A4%87%E6%A1%88%E7%9A%84%E8%A7%84%E8%8C%83%E6%80%A7%E6%96%87%E4%BB%B6%E8%BF%9B%E8%A1%8C%E4%B8%BB%E5%8A%A8%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="并可根据需要进行专项审查" _mubu_text="%3Cspan%3E%E5%B9%B6%E5%8F%AF%E6%A0%B9%E6%8D%AE%E9%9C%80%E8%A6%81%E8%BF%9B%E8%A1%8C%E4%B8%93%E9%A1%B9%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E"/>
              </outline>
              <outline text="被动审查" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%8A%A8%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="审查要求" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                  <outline text="“两央两高监省常”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%A4%E5%A4%AE%E4%B8%A4%E9%AB%98%E7%9B%91%E7%9C%81%E5%B8%B8%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="审查建议" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E5%BB%BA%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                  <outline text="其他机关、团体、组织以及公民" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%9C%BA%E5%85%B3%E3%80%81%E5%9B%A2%E4%BD%93%E3%80%81%E7%BB%84%E7%BB%87%E4%BB%A5%E5%8F%8A%E5%85%AC%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="联动审查" _mubu_text="%3Cspan%3E%E8%81%94%E5%8A%A8%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="新增" _mubu_note="%3Cspan%3E%E6%96%B0%E5%A2%9E%3C/span%3E">
                  <outline text="对应由其他机关处理的审查要求或建议，及时移送有关机关处理" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%BA%94%E7%94%B1%E5%85%B6%E4%BB%96%E6%9C%BA%E5%85%B3%E5%A4%84%E7%90%86%E7%9A%84%E5%AE%A1%E6%9F%A5%E8%A6%81%E6%B1%82%E6%88%96%E5%BB%BA%E8%AE%AE%EF%BC%8C%E5%8F%8A%E6%97%B6%E7%A7%BB%E9%80%81%E6%9C%89%E5%85%B3%E6%9C%BA%E5%85%B3%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
