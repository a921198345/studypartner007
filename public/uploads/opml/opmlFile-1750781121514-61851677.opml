            <outline text="当代中国的法律体系" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BB%A3%E4%B8%AD%E5%9B%BD%E7%9A%84%E6%B3%95%E5%BE%8B%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="概念" _mubu_text="%3Cspan%3E%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
                <outline text="以宪法为统帅" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%AE%AA%E6%B3%95%E4%B8%BA%E7%BB%9F%E5%B8%85%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="以法律为主干" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%B3%95%E5%BE%8B%E4%B8%BA%E4%B8%BB%E5%B9%B2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="以行政法规、地方性去规为重要组成部分" _mubu_text="%3Cspan%3E%E4%BB%A5%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E3%80%81%E5%9C%B0%E6%96%B9%E6%80%A7%E5%8E%BB%E8%A7%84%E4%B8%BA%E9%87%8D%E8%A6%81%E7%BB%84%E6%88%90%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="由多个法律部门组成的有机统一整体" _mubu_text="%3Cspan%3E%E7%94%B1%E5%A4%9A%E4%B8%AA%E6%B3%95%E5%BE%8B%E9%83%A8%E9%97%A8%E7%BB%84%E6%88%90%E7%9A%84%E6%9C%89%E6%9C%BA%E7%BB%9F%E4%B8%80%E6%95%B4%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="中特社义法律体系" _mubu_text="%3Cspan%3E%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E6%B3%95%E5%BE%8B%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="2011年建立完成" _mubu_text="%3Cspan%3E2011%E5%B9%B4%E5%BB%BA%E7%AB%8B%E5%AE%8C%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="当前工作重点在进一步健全体系" _mubu_text="%3Cspan%3E%E5%BD%93%E5%89%8D%E5%B7%A5%E4%BD%9C%E9%87%8D%E7%82%B9%E5%9C%A8%E8%BF%9B%E4%B8%80%E6%AD%A5%E5%81%A5%E5%85%A8%E4%BD%93%E7%B3%BB%3C/span%3E" _note="非建立" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%BB%BA%E7%AB%8B%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="法的效力" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
          <outline text="法的效力的含义" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%95%88%E5%8A%9B%E7%9A%84%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="即法律对人们行为所产生的约束力" _mubu_text="%3Cspan%3E%E5%8D%B3%E6%B3%95%E5%BE%8B%E5%AF%B9%E4%BA%BA%E4%BB%AC%E8%A1%8C%E4%B8%BA%E6%89%80%E4%BA%A7%E7%94%9F%E7%9A%84%E7%BA%A6%E6%9D%9F%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="广义的法律效力" _mubu_text="%3Cspan%3E%E5%B9%BF%E4%B9%89%E7%9A%84%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="还包括非规范性法律文件的效力" _mubu_text="%3Cspan%3E%E8%BF%98%E5%8C%85%E6%8B%AC%E9%9D%9E%E8%A7%84%E8%8C%83%E6%80%A7%E6%B3%95%E5%BE%8B%E6%96%87%E4%BB%B6%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="题目中未作特别说明，一律当作狭义理解" _mubu_text="%3Cspan%3E%E9%A2%98%E7%9B%AE%E4%B8%AD%E6%9C%AA%E4%BD%9C%E7%89%B9%E5%88%AB%E8%AF%B4%E6%98%8E%EF%BC%8C%E4%B8%80%E5%BE%8B%E5%BD%93%E4%BD%9C%E7%8B%AD%E4%B9%89%E7%90%86%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="法的对人效力" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AF%B9%E4%BA%BA%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="属人主义" _mubu_text="%3Cspan%3E%E5%B1%9E%E4%BA%BA%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="属地主义" _mubu_text="%3Cspan%3E%E5%B1%9E%E5%9C%B0%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="保护主义" _mubu_text="%3Cspan%3E%E4%BF%9D%E6%8A%A4%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="折中主义" _mubu_text="%3Cspan%3E%E6%8A%98%E4%B8%AD%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="属地主义为主，与属人主义、保护主义相结合" _mubu_text="%3Cspan%3E%E5%B1%9E%E5%9C%B0%E4%B8%BB%E4%B9%89%E4%B8%BA%E4%B8%BB%EF%BC%8C%E4%B8%8E%E5%B1%9E%E4%BA%BA%E4%B8%BB%E4%B9%89%E3%80%81%E4%BF%9D%E6%8A%A4%E4%B8%BB%E4%B9%89%E7%9B%B8%E7%BB%93%E5%90%88%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="中国法律效力" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="对中国人" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%B8%AD%E5%9B%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="领域内" _mubu_text="%3Cspan%3E%E9%A2%86%E5%9F%9F%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法律面前人人平等，一律适用" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E9%9D%A2%E5%89%8D%E4%BA%BA%E4%BA%BA%E5%B9%B3%E7%AD%89%EF%BC%8C%E4%B8%80%E5%BE%8B%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="领域外" _mubu_text="%3Cspan%3E%E9%A2%86%E5%9F%9F%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="原则上适用，但有例外" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E9%80%82%E7%94%A8%EF%BC%8C%E4%BD%86%E6%9C%89%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="对外国人" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A4%96%E5%9B%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="中国境内" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E5%A2%83%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                  <outline text="原则上适用" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E9%80%82%E7%94%A8%3C/span%3E" _note="但享有外交特权与豁免权的除外" _mubu_note="%3Cspan%3E%E4%BD%86%E4%BA%AB%E6%9C%89%E5%A4%96%E4%BA%A4%E7%89%B9%E6%9D%83%E4%B8%8E%E8%B1%81%E5%85%8D%E6%9D%83%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
                </outline>
                <outline text="中国境外" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E5%A2%83%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="重罪＋双重犯罪" _mubu_text="%3Cspan%3E%E9%87%8D%E7%BD%AA%EF%BC%8B%E5%8F%8C%E9%87%8D%E7%8A%AF%E7%BD%AA%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="法的空间效力" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E7%A9%BA%E9%97%B4%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="域内效力" _mubu_text="%3Cspan%3E%E5%9F%9F%E5%86%85%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="全国生效" _mubu_text="%3Cspan%3E%E5%85%A8%E5%9B%BD%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="地方生效" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="部分地方暂时调整或暂停" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E5%9C%B0%E6%96%B9%E6%9A%82%E6%97%B6%E8%B0%83%E6%95%B4%E6%88%96%E6%9A%82%E5%81%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="全人大及常可根据需要，决定在一定期限内部分地方暂时调整或暂停适用法律的部分规定" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E5%8F%8A%E5%B8%B8%E5%8F%AF%E6%A0%B9%E6%8D%AE%E9%9C%80%E8%A6%81%EF%BC%8C%E5%86%B3%E5%AE%9A%E5%9C%A8%E4%B8%80%E5%AE%9A%E6%9C%9F%E9%99%90%E5%86%85%E9%83%A8%E5%88%86%E5%9C%B0%E6%96%B9%E6%9A%82%E6%97%B6%E8%B0%83%E6%95%B4%E6%88%96%E6%9A%82%E5%81%9C%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E7%9A%84%E9%83%A8%E5%88%86%E8%A7%84%E5%AE%9A%3C/span%3E" _note="行政法规的调整权限与此类似，由国务院决定" _mubu_note="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E7%9A%84%E8%B0%83%E6%95%B4%E6%9D%83%E9%99%90%E4%B8%8E%E6%AD%A4%E7%B1%BB%E4%BC%BC%EF%BC%8C%E7%94%B1%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%86%B3%E5%AE%9A%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
            <outline text="域外效力" _mubu_text="%3Cspan%3E%E5%9F%9F%E5%A4%96%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法的时间效力" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%97%B6%E9%97%B4%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="生效时间" _mubu_text="%3Cspan%3E%E7%94%9F%E6%95%88%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="效力终止" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%E7%BB%88%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="明/默示废止" _mubu_text="%3Cspan%3E%E6%98%8E/%E9%BB%98%E7%A4%BA%E5%BA%9F%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="明示都是立法，默示都是司法" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A4%BA%E9%83%BD%E6%98%AF%E7%AB%8B%E6%B3%95%EF%BC%8C%E9%BB%98%E7%A4%BA%E9%83%BD%E6%98%AF%E5%8F%B8%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="法律的溯及力" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E6%BA%AF%E5%8F%8A%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="从旧兼从轻" _mubu_text="%3Cspan%3E%E4%BB%8E%E6%97%A7%E5%85%BC%E4%BB%8E%E8%BD%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="法律关系" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="概述" _mubu_text="%3Cspan%3E%E6%A6%82%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="是法律规范调整社会关系的过程中所形成的人与人之间的权利义务关系" _mubu_text="%3Cspan%3E%E6%98%AF%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E8%B0%83%E6%95%B4%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%E7%9A%84%E8%BF%87%E7%A8%8B%E4%B8%AD%E6%89%80%E5%BD%A2%E6%88%90%E7%9A%84%E4%BA%BA%E4%B8%8E%E4%BA%BA%E4%B9%8B%E9%97%B4%E7%9A%84%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="是依法建立的社会关系" _mubu_text="%3Cspan%3E%E6%98%AF%E4%BE%9D%E6%B3%95%E5%BB%BA%E7%AB%8B%E7%9A%84%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="法律规范是法律关系产生的前提" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E6%98%AF%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E4%BA%A7%E7%94%9F%E7%9A%84%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法律关系＜社会关系" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%EF%BC%9C%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法律关系是法律规范的实现形式" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%98%AF%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E7%9A%84%E5%AE%9E%E7%8E%B0%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法律关系是人与人之间符合法律规范的社会关系" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%98%AF%E4%BA%BA%E4%B8%8E%E4%BA%BA%E4%B9%8B%E9%97%B4%E7%AC%A6%E5%90%88%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E7%9A%84%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="是一种体现意志性的特殊社会关系" _mubu_text="%3Cspan%3E%E6%98%AF%E4%B8%80%E7%A7%8D%E4%BD%93%E7%8E%B0%E6%84%8F%E5%BF%97%E6%80%A7%E7%9A%84%E7%89%B9%E6%AE%8A%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="法律关系是根据法律规范建立的，而法律规范是国家意志的体现" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%98%AF%E6%A0%B9%E6%8D%AE%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E5%BB%BA%E7%AB%8B%E7%9A%84%EF%BC%8C%E8%80%8C%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E6%98%AF%E5%9B%BD%E5%AE%B6%E6%84%8F%E5%BF%97%E7%9A%84%E4%BD%93%E7%8E%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法律关系参加者的意志对于法律关系的建立和实现也有着重要的作用" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E5%8F%82%E5%8A%A0%E8%80%85%E7%9A%84%E6%84%8F%E5%BF%97%E5%AF%B9%E4%BA%8E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E7%9A%84%E5%BB%BA%E7%AB%8B%E5%92%8C%E5%AE%9E%E7%8E%B0%E4%B9%9F%E6%9C%89%E7%9D%80%E9%87%8D%E8%A6%81%E7%9A%84%E4%BD%9C%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="是以法律上的权利义务为内容的社会关系" _mubu_text="%3Cspan%3E%E6%98%AF%E4%BB%A5%E6%B3%95%E5%BE%8B%E4%B8%8A%E7%9A%84%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%86%85%E5%AE%B9%E7%9A%84%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="是法律关系区别于其他社会关系的重要标志" _mubu_text="%3Cspan%3E%E6%98%AF%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E5%8C%BA%E5%88%AB%E4%BA%8E%E5%85%B6%E4%BB%96%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%E7%9A%84%E9%87%8D%E8%A6%81%E6%A0%87%E5%BF%97%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="调整性法律关系、保护性法律关系" _mubu_text="%3Cspan%3E%E8%B0%83%E6%95%B4%E6%80%A7%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E4%BF%9D%E6%8A%A4%E6%80%A7%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="调整性：基于合法行为" _mubu_text="%3Cspan%3E%E8%B0%83%E6%95%B4%E6%80%A7%EF%BC%9A%E5%9F%BA%E4%BA%8E%E5%90%88%E6%B3%95%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="保护性：基于违法行为" _mubu_text="%3Cspan%3E%E4%BF%9D%E6%8A%A4%E6%80%A7%EF%BC%9A%E5%9F%BA%E4%BA%8E%E8%BF%9D%E6%B3%95%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="纵向(隶属)的法律关系、横向(平权)的法律关系" _mubu_text="%3Cspan%3E%E7%BA%B5%E5%90%91(%E9%9A%B6%E5%B1%9E)%E7%9A%84%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E6%A8%AA%E5%90%91(%E5%B9%B3%E6%9D%83)%E7%9A%84%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="纵向" _mubu_text="%3Cspan%3E%E7%BA%B5%E5%90%91%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不平等主体之间" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%B9%B3%E7%AD%89%E4%B8%BB%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="横向" _mubu_text="%3Cspan%3E%E6%A8%AA%E5%90%91%3C/span%3E" _note="" _mubu_note="">
                  <outline text="平等主体之间" _mubu_text="%3Cspan%3E%E5%B9%B3%E7%AD%89%E4%B8%BB%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="单向(单务)法律关系、双向(双边)法律关系、多向(多边)法律关系" _mubu_text="%3Cspan%3E%E5%8D%95%E5%90%91(%E5%8D%95%E5%8A%A1)%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E5%8F%8C%E5%90%91(%E5%8F%8C%E8%BE%B9)%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E5%A4%9A%E5%90%91(%E5%A4%9A%E8%BE%B9)%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="第一性法律关系、第二性法律关系" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%80%E6%80%A7%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E7%AC%AC%E4%BA%8C%E6%80%A7%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="主法律关系、从法律关系" _mubu_note="%3Cspan%3E%E4%B8%BB%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E4%BB%8E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E"/>
              <outline text="绝对法律关系、相对法律关系" _mubu_text="%3Cspan%3E%E7%BB%9D%E5%AF%B9%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E7%9B%B8%E5%AF%B9%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="绝对：义务主体不特定" _mubu_text="%3Cspan%3E%E7%BB%9D%E5%AF%B9%EF%BC%9A%E4%B9%89%E5%8A%A1%E4%B8%BB%E4%BD%93%E4%B8%8D%E7%89%B9%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="相对：义务主体特定" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%EF%BC%9A%E4%B9%89%E5%8A%A1%E4%B8%BB%E4%BD%93%E7%89%B9%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="基本法律关系、普通法律关系、诉讼法律关系" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E6%99%AE%E9%80%9A%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E3%80%81%E8%AF%89%E8%AE%BC%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="基本：依据宪法/宪法性法律文件形成" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%EF%BC%9A%E4%BE%9D%E6%8D%AE%E5%AE%AA%E6%B3%95/%E5%AE%AA%E6%B3%95%E6%80%A7%E6%B3%95%E5%BE%8B%E6%96%87%E4%BB%B6%E5%BD%A2%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="普通：依据宪法以外的法律形成" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%EF%BC%9A%E4%BE%9D%E6%8D%AE%E5%AE%AA%E6%B3%95%E4%BB%A5%E5%A4%96%E7%9A%84%E6%B3%95%E5%BE%8B%E5%BD%A2%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="诉讼：依据诉讼法律规范形成" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%EF%BC%9A%E4%BE%9D%E6%8D%AE%E8%AF%89%E8%AE%BC%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E5%BD%A2%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="构成要素" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%E8%A6%81%E7%B4%A0%3C/span%3E" _note="" _mubu_note="">
            <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="含义" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="法律关系的参加者" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E7%9A%84%E5%8F%82%E5%8A%A0%E8%80%85%3C/span%3E" _note="即法律关系中权利享有者和义务承担者" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E4%B8%AD%E6%9D%83%E5%88%A9%E4%BA%AB%E6%9C%89%E8%80%85%E5%92%8C%E4%B9%89%E5%8A%A1%E6%89%BF%E6%8B%85%E8%80%85%3C/span%3E"/>
                <outline text="具有法律性和社会性的双重属性" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%B3%95%E5%BE%8B%E6%80%A7%E5%92%8C%E7%A4%BE%E4%BC%9A%E6%80%A7%E7%9A%84%E5%8F%8C%E9%87%8D%E5%B1%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="自然人" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E4%BA%BA%3C/span%3E" _note="公民" _mubu_note="%3Cspan%3E%E5%85%AC%E6%B0%91%3C/span%3E"/>
                <outline text="法人" _mubu_text="%3Cspan%3E%E6%B3%95%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="其他社会组织和国家" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E7%A4%BE%E4%BC%9A%E7%BB%84%E7%BB%87%E5%92%8C%E5%9B%BD%E5%AE%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="权利能力和行为能力" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E8%83%BD%E5%8A%9B%E5%92%8C%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="具体当事人之间的权利义务" _mubu_text="%3Cspan%3E%E5%85%B7%E4%BD%93%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B9%8B%E9%97%B4%E7%9A%84%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="结构上，二者紧密联系，不可分割" _mubu_text="%3Cspan%3E%E7%BB%93%E6%9E%84%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E7%B4%A7%E5%AF%86%E8%81%94%E7%B3%BB%EF%BC%8C%E4%B8%8D%E5%8F%AF%E5%88%86%E5%89%B2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="数量上，二者一一对应，总量相等" _mubu_text="%3Cspan%3E%E6%95%B0%E9%87%8F%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E4%B8%80%E4%B8%80%E5%AF%B9%E5%BA%94%EF%BC%8C%E6%80%BB%E9%87%8F%E7%9B%B8%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="从产生和发展看，浑然一体→分裂对立→相对一致" _mubu_text="%3Cspan%3E%E4%BB%8E%E4%BA%A7%E7%94%9F%E5%92%8C%E5%8F%91%E5%B1%95%E7%9C%8B%EF%BC%8C%E6%B5%91%E7%84%B6%E4%B8%80%E4%BD%93%E2%86%92%E5%88%86%E8%A3%82%E5%AF%B9%E7%AB%8B%E2%86%92%E7%9B%B8%E5%AF%B9%E4%B8%80%E8%87%B4%3C/span%3E" _note="原始社会→阶级社会→社会主义社会" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%A7%8B%E7%A4%BE%E4%BC%9A%E2%86%92%E9%98%B6%E7%BA%A7%E7%A4%BE%E4%BC%9A%E2%86%92%E7%A4%BE%E4%BC%9A%E4%B8%BB%E4%B9%89%E7%A4%BE%E4%BC%9A%3C/span%3E"/>
                <outline text="价值上，权利和义务代表着不同的法律精神" _mubu_text="%3Cspan%3E%E4%BB%B7%E5%80%BC%E4%B8%8A%EF%BC%8C%E6%9D%83%E5%88%A9%E5%92%8C%E4%B9%89%E5%8A%A1%E4%BB%A3%E8%A1%A8%E7%9D%80%E4%B8%8D%E5%90%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E7%B2%BE%E7%A5%9E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="作用上，二者相互限定、相互制约" _mubu_text="%3Cspan%3E%E4%BD%9C%E7%94%A8%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E7%9B%B8%E4%BA%92%E9%99%90%E5%AE%9A%E3%80%81%E7%9B%B8%E4%BA%92%E5%88%B6%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="客体" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="概念" _mubu_text="%3Cspan%3E%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
                <outline text="法律关系主体之间权利义务所指向的对象" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E4%B8%BB%E4%BD%93%E4%B9%8B%E9%97%B4%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E6%89%80%E6%8C%87%E5%90%91%E7%9A%84%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="物" _mubu_text="%3Cspan%3E%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应得到法律认可" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BE%97%E5%88%B0%E6%B3%95%E5%BE%8B%E8%AE%A4%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="应为人类所认识和控制" _mubu_text="%3Cspan%3E%E5%BA%94%E4%B8%BA%E4%BA%BA%E7%B1%BB%E6%89%80%E8%AE%A4%E8%AF%86%E5%92%8C%E6%8E%A7%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="能够给人们带来某种物质利益，具有经济价值" _mubu_text="%3Cspan%3E%E8%83%BD%E5%A4%9F%E7%BB%99%E4%BA%BA%E4%BB%AC%E5%B8%A6%E6%9D%A5%E6%9F%90%E7%A7%8D%E7%89%A9%E8%B4%A8%E5%88%A9%E7%9B%8A%EF%BC%8C%E5%85%B7%E6%9C%89%E7%BB%8F%E6%B5%8E%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="须具有独立性" _mubu_text="%3Cspan%3E%E9%A1%BB%E5%85%B7%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="不能成为私人法律关系的客体" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%88%90%E4%B8%BA%E7%A7%81%E4%BA%BA%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E7%9A%84%E5%AE%A2%E4%BD%93%3C/span%3E" _note="却是公共法律关系的客体" _mubu_note="%3Cspan%3E%E5%8D%B4%E6%98%AF%E5%85%AC%E5%85%B1%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E7%9A%84%E5%AE%A2%E4%BD%93%3C/span%3E">
                    <outline text="人类公共之物或国家专有之物" _mubu_text="%3Cspan%3E%E4%BA%BA%E7%B1%BB%E5%85%AC%E5%85%B1%E4%B9%8B%E7%89%A9%E6%88%96%E5%9B%BD%E5%AE%B6%E4%B8%93%E6%9C%89%E4%B9%8B%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="文物" _mubu_text="%3Cspan%3E%E6%96%87%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="军事设施、武器" _mubu_text="%3Cspan%3E%E5%86%9B%E4%BA%8B%E8%AE%BE%E6%96%BD%E3%80%81%E6%AD%A6%E5%99%A8%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="危害人类之物" _mubu_text="%3Cspan%3E%E5%8D%B1%E5%AE%B3%E4%BA%BA%E7%B1%BB%E4%B9%8B%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="人身" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%3C/span%3E" _note="" _mubu_note="">
                  <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="活人的(整个)身体，不得视为法律上之&quot;物&quot;" _mubu_text="%3Cspan%3E%E6%B4%BB%E4%BA%BA%E7%9A%84(%E6%95%B4%E4%B8%AA)%E8%BA%AB%E4%BD%93%EF%BC%8C%E4%B8%8D%E5%BE%97%E8%A7%86%E4%B8%BA%E6%B3%95%E5%BE%8B%E4%B8%8A%E4%B9%8B%22%E7%89%A9%22%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="权利人对自己的人身不得进行违法或有伤风化的活动" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E5%AF%B9%E8%87%AA%E5%B7%B1%E7%9A%84%E4%BA%BA%E8%BA%AB%E4%B8%8D%E5%BE%97%E8%BF%9B%E8%A1%8C%E8%BF%9D%E6%B3%95%E6%88%96%E6%9C%89%E4%BC%A4%E9%A3%8E%E5%8C%96%E7%9A%84%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="不得滥用人身，或自贱人身和人格" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%BB%A5%E7%94%A8%E4%BA%BA%E8%BA%AB%EF%BC%8C%E6%88%96%E8%87%AA%E8%B4%B1%E4%BA%BA%E8%BA%AB%E5%92%8C%E4%BA%BA%E6%A0%BC%3C/span%3E"/>
                    <outline text="对人身行使权利必须依法进行" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BA%BA%E8%BA%AB%E8%A1%8C%E4%BD%BF%E6%9D%83%E5%88%A9%E5%BF%85%E9%A1%BB%E4%BE%9D%E6%B3%95%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="人身部分" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="部分尚未脱离人的整体时，即属人身本身" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E5%B0%9A%E6%9C%AA%E8%84%B1%E7%A6%BB%E4%BA%BA%E7%9A%84%E6%95%B4%E4%BD%93%E6%97%B6%EF%BC%8C%E5%8D%B3%E5%B1%9E%E4%BA%BA%E8%BA%AB%E6%9C%AC%E8%BA%AB%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="部分自然地从身体分离，亦可视为法律上之&quot;物&quot;" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E8%87%AA%E7%84%B6%E5%9C%B0%E4%BB%8E%E8%BA%AB%E4%BD%93%E5%88%86%E7%A6%BB%EF%BC%8C%E4%BA%A6%E5%8F%AF%E8%A7%86%E4%B8%BA%E6%B3%95%E5%BE%8B%E4%B8%8A%E4%B9%8B%22%E7%89%A9%22%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="部分已植人他人身体时，即为他人人身之组成部分" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E5%B7%B2%E6%A4%8D%E4%BA%BA%E4%BB%96%E4%BA%BA%E8%BA%AB%E4%BD%93%E6%97%B6%EF%BC%8C%E5%8D%B3%E4%B8%BA%E4%BB%96%E4%BA%BA%E4%BA%BA%E8%BA%AB%E4%B9%8B%E7%BB%84%E6%88%90%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="精神产品" _mubu_text="%3Cspan%3E%E7%B2%BE%E7%A5%9E%E4%BA%A7%E5%93%81%3C/span%3E" _note="" _mubu_note="">
                  <outline text="知识产权" _mubu_text="%3Cspan%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="行为结果" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E7%BB%93%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="物化结果" _mubu_text="%3Cspan%3E%E7%89%A9%E5%8C%96%E7%BB%93%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                    <outline text="义务人的行为（劳动）凝结于一定的物体" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%E4%BA%BA%E7%9A%84%E8%A1%8C%E4%B8%BA%EF%BC%88%E5%8A%B3%E5%8A%A8%EF%BC%89%E5%87%9D%E7%BB%93%E4%BA%8E%E4%B8%80%E5%AE%9A%E7%9A%84%E7%89%A9%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="非物化结果" _mubu_text="%3Cspan%3E%E9%9D%9E%E7%89%A9%E5%8C%96%E7%BB%93%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                    <outline text="仅表现为行为过程，直至终了，最后产生权利人期望结果/效果" _mubu_text="%3Cspan%3E%E4%BB%85%E8%A1%A8%E7%8E%B0%E4%B8%BA%E8%A1%8C%E4%B8%BA%E8%BF%87%E7%A8%8B%EF%BC%8C%E7%9B%B4%E8%87%B3%E7%BB%88%E4%BA%86%EF%BC%8C%E6%9C%80%E5%90%8E%E4%BA%A7%E7%94%9F%E6%9D%83%E5%88%A9%E4%BA%BA%E6%9C%9F%E6%9C%9B%E7%BB%93%E6%9E%9C/%E6%95%88%E6%9E%9C%3C/span%3E" _note="行为没有转化为物化实体" _mubu_note="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E6%B2%A1%E6%9C%89%E8%BD%AC%E5%8C%96%E4%B8%BA%E7%89%A9%E5%8C%96%E5%AE%9E%E4%BD%93%3C/span%3E"/>
                  </outline>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="产生、变更和消灭" _mubu_text="%3Cspan%3E%E4%BA%A7%E7%94%9F%E3%80%81%E5%8F%98%E6%9B%B4%E5%92%8C%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="法律规范(抽象条件)＋法律事实(具体条件)＝法律关系" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83(%E6%8A%BD%E8%B1%A1%E6%9D%A1%E4%BB%B6)%EF%BC%8B%E6%B3%95%E5%BE%8B%E4%BA%8B%E5%AE%9E(%E5%85%B7%E4%BD%93%E6%9D%A1%E4%BB%B6)%EF%BC%9D%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
