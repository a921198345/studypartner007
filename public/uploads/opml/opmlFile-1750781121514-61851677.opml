              <outline text="或30名以上代表" _mubu_text="%3Cspan%3E%E6%88%9630%E5%90%8D%E4%BB%A5%E4%B8%8A%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="提出质询案" _mubu_text="%3Cspan%3E%E6%8F%90%E5%87%BA%E8%B4%A8%E8%AF%A2%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="1个代表团或30名以上代表" _mubu_text="%3Cspan%3E1%E4%B8%AA%E4%BB%A3%E8%A1%A8%E5%9B%A2%E6%88%9630%E5%90%8D%E4%BB%A5%E4%B8%8A%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="常委会组成人员10名以上" _mubu_text="%3Cspan%3E%E5%B8%B8%E5%A7%94%E4%BC%9A%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%9810%E5%90%8D%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="提出罢免案" _mubu_text="%3Cspan%3E%E6%8F%90%E5%87%BA%E7%BD%A2%E5%85%8D%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="3个以上代表团" _mubu_text="%3Cspan%3E3%E4%B8%AA%E4%BB%A5%E4%B8%8A%E4%BB%A3%E8%A1%A8%E5%9B%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="或1/10以上代表" _mubu_text="%3Cspan%3E%E6%88%961/10%E4%BB%A5%E4%B8%8A%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="享受适当补贴和物质上的便利的权利" _mubu_text="%3Cspan%3E%E4%BA%AB%E5%8F%97%E9%80%82%E5%BD%93%E8%A1%A5%E8%B4%B4%E5%92%8C%E7%89%A9%E8%B4%A8%E4%B8%8A%E7%9A%84%E4%BE%BF%E5%88%A9%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
