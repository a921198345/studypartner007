                <outline text="由居委会成员分工负责" _mubu_text="%3Cspan%3E%E7%94%B1%E5%B1%85%E5%A7%94%E4%BC%9A%E6%88%90%E5%91%98%E5%88%86%E5%B7%A5%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
