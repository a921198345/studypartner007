              </outline>
            </outline>
            <outline text="效力及制定修改程序" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%E5%8F%8A%E5%88%B6%E5%AE%9A%E4%BF%AE%E6%94%B9%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="与普通法律有无差异" _mubu_note="%3Cspan%3E%E4%B8%8E%E6%99%AE%E9%80%9A%E6%B3%95%E5%BE%8B%E6%9C%89%E6%97%A0%E5%B7%AE%E5%BC%82%3C/span%3E">
