          </outline>
          <outline text="正式渊源的效力原则" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%E7%9A%84%E6%95%88%E5%8A%9B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="不同位阶" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%90%8C%E4%BD%8D%E9%98%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：位阶理论" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%BD%8D%E9%98%B6%E7%90%86%E8%AE%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="合宪性原则" _mubu_text="%3Cspan%3E%E5%90%88%E5%AE%AA%E6%80%A7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="下位法必须服从上位法" _mubu_text="%3Cspan%3E%E4%B8%8B%E4%BD%8D%E6%B3%95%E5%BF%85%E9%A1%BB%E6%9C%8D%E4%BB%8E%E4%B8%8A%E4%BD%8D%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
                  <outline text="与上位法抵触" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%B8%8A%E4%BD%8D%E6%B3%95%E6%8A%B5%E8%A7%A6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="下位法丧失法律效力" _mubu_text="%3Cspan%3E%E4%B8%8B%E4%BD%8D%E6%B3%95%E4%B8%A7%E5%A4%B1%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="不抵触" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%8A%B5%E8%A7%A6%3C/span%3E" _note="" _mubu_note="">
                    <outline text="应优先适用下位法" _mubu_text="%3Cspan%3E%E5%BA%94%E4%BC%98%E5%85%88%E9%80%82%E7%94%A8%E4%B8%8B%E4%BD%8D%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="特例" _mubu_text="%3Cspan%3E%E7%89%B9%E4%BE%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="自治条例、单行条例" _mubu_text="%3Cspan%3E%E8%87%AA%E6%B2%BB%E6%9D%A1%E4%BE%8B%E3%80%81%E5%8D%95%E8%A1%8C%E6%9D%A1%E4%BE%8B%3C/span%3E" _note="" _mubu_note="">
                  <outline text="与上位法矛盾并不必然丧失法律效力" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%B8%8A%E4%BD%8D%E6%B3%95%E7%9F%9B%E7%9B%BE%E5%B9%B6%E4%B8%8D%E5%BF%85%E7%84%B6%E4%B8%A7%E5%A4%B1%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="有一定的变通权限" _mubu_note="%3Cspan%3E%E6%9C%89%E4%B8%80%E5%AE%9A%E7%9A%84%E5%8F%98%E9%80%9A%E6%9D%83%E9%99%90%3C/span%3E"/>
                </outline>
                <outline text="“三区”法律与法律" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%89%E5%8C%BA%E2%80%9D%E6%B3%95%E5%BE%8B%E4%B8%8E%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note="">
                  <outline text="由全人常裁决" _mubu_text="%3Cspan%3E%E7%94%B1%E5%85%A8%E4%BA%BA%E5%B8%B8%E8%A3%81%E5%86%B3%3C/span%3E" _note="三区属于授权立法" _mubu_note="%3Cspan%3E%E4%B8%89%E5%8C%BA%E5%B1%9E%E4%BA%8E%E6%8E%88%E6%9D%83%E7%AB%8B%E6%B3%95%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
            <outline text="同一位阶" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E4%BD%8D%E9%98%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="特别法优于普通法" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E6%B3%95%E4%BC%98%E4%BA%8E%E6%99%AE%E9%80%9A%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="新法优于旧法" _mubu_text="%3Cspan%3E%E6%96%B0%E6%B3%95%E4%BC%98%E4%BA%8E%E6%97%A7%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="同一位阶的交叉" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E4%BD%8D%E9%98%B6%E7%9A%84%E4%BA%A4%E5%8F%89%3C/span%3E" _note="" _mubu_note="">
                  <outline text="即新法为一般法，旧法为特别法" _mubu_text="%3Cspan%3E%E5%8D%B3%E6%96%B0%E6%B3%95%E4%B8%BA%E4%B8%80%E8%88%AC%E6%B3%95%EF%BC%8C%E6%97%A7%E6%B3%95%E4%B8%BA%E7%89%B9%E5%88%AB%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
