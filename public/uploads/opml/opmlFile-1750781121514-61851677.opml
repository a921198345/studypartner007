              </outline>
            </outline>
          </outline>
          <outline text="马克思主义关于法律本质的看法" _mubu_text="%3Cspan%3E%E9%A9%AC%E5%85%8B%E6%80%9D%E4%B8%BB%E4%B9%89%E5%85%B3%E4%BA%8E%E6%B3%95%E5%BE%8B%E6%9C%AC%E8%B4%A8%E7%9A%84%E7%9C%8B%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
            <outline text="正式性" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E6%80%A7%3C/span%3E" _note="国家性" _mubu_note="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%80%A7%3C/span%3E"/>
            <outline text="阶级性" _mubu_text="%3Cspan%3E%E9%98%B6%E7%BA%A7%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="统治阶级意志" _mubu_text="%3Cspan%3E%E7%BB%9F%E6%B2%BB%E9%98%B6%E7%BA%A7%E6%84%8F%E5%BF%97%3C/span%3E" _note="国家意志就是法律化的统治阶级意志" _mubu_note="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%84%8F%E5%BF%97%E5%B0%B1%E6%98%AF%E6%B3%95%E5%BE%8B%E5%8C%96%E7%9A%84%E7%BB%9F%E6%B2%BB%E9%98%B6%E7%BA%A7%E6%84%8F%E5%BF%97%3C/span%3E"/>
              <outline text="也可能反应被统治阶级的某些要求" _mubu_text="%3Cspan%3E%E4%B9%9F%E5%8F%AF%E8%83%BD%E5%8F%8D%E5%BA%94%E8%A2%AB%E7%BB%9F%E6%B2%BB%E9%98%B6%E7%BA%A7%E7%9A%84%E6%9F%90%E4%BA%9B%E8%A6%81%E6%B1%82%3C/span%3E" _note="出于缓和阶级矛盾的需要" _mubu_note="%3Cspan%3E%E5%87%BA%E4%BA%8E%E7%BC%93%E5%92%8C%E9%98%B6%E7%BA%A7%E7%9F%9B%E7%9B%BE%E7%9A%84%E9%9C%80%E8%A6%81%3C/span%3E"/>
              <outline text="反应统治阶级整体长远利益的要求" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%BA%94%E7%BB%9F%E6%B2%BB%E9%98%B6%E7%BA%A7%E6%95%B4%E4%BD%93%E9%95%BF%E8%BF%9C%E5%88%A9%E7%9B%8A%E7%9A%84%E8%A6%81%E6%B1%82%3C/span%3E" _note="而非个别" _mubu_note="%3Cspan%3E%E8%80%8C%E9%9D%9E%E4%B8%AA%E5%88%AB%3C/span%3E"/>
            </outline>
            <outline text="社会性" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E6%80%A7%3C/span%3E" _note="物质制约性" _mubu_note="%3Cspan%3E%E7%89%A9%E8%B4%A8%E5%88%B6%E7%BA%A6%E6%80%A7%3C/span%3E">
              <outline text="被社会物质生活条件决定，也会受到其他非经济因素影响" _mubu_text="%3Cspan%3E%E8%A2%AB%E7%A4%BE%E4%BC%9A%E7%89%A9%E8%B4%A8%E7%94%9F%E6%B4%BB%E6%9D%A1%E4%BB%B6%E5%86%B3%E5%AE%9A%EF%BC%8C%E4%B9%9F%E4%BC%9A%E5%8F%97%E5%88%B0%E5%85%B6%E4%BB%96%E9%9D%9E%E7%BB%8F%E6%B5%8E%E5%9B%A0%E7%B4%A0%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="国家不是在创造法律，只是在表述法律" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%B8%8D%E6%98%AF%E5%9C%A8%E5%88%9B%E9%80%A0%E6%B3%95%E5%BE%8B%EF%BC%8C%E5%8F%AA%E6%98%AF%E5%9C%A8%E8%A1%A8%E8%BF%B0%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="三者有机统一，共同构成" _mubu_text="%3Cspan%3E%E4%B8%89%E8%80%85%E6%9C%89%E6%9C%BA%E7%BB%9F%E4%B8%80%EF%BC%8C%E5%85%B1%E5%90%8C%E6%9E%84%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="国法" _mubu_text="%3Cspan%3E%E5%9B%BD%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
