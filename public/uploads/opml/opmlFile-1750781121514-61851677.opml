                </outline>
                <outline text="会议有效" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E6%9C%89%E6%95%88%3C/span%3E" _note="人数条件" _mubu_note="%3Cspan%3E%E4%BA%BA%E6%95%B0%E6%9D%A1%E4%BB%B6%3C/span%3E">
                  <outline text="本村年满18周岁的村民过半数参加" _mubu_text="%3Cspan%3E%E6%9C%AC%E6%9D%91%E5%B9%B4%E6%BB%A118%E5%91%A8%E5%B2%81%E7%9A%84%E6%9D%91%E6%B0%91%E8%BF%87%E5%8D%8A%E6%95%B0%E5%8F%82%E5%8A%A0%3C/span%3E" _note="未规定未被剥夺政治权利" _mubu_note="%3Cspan%3E%E6%9C%AA%E8%A7%84%E5%AE%9A%E6%9C%AA%E8%A2%AB%E5%89%A5%E5%A4%BA%E6%94%BF%E6%B2%BB%E6%9D%83%E5%88%A9%3C/span%3E"/>
                  <outline text="或2/3以上户的代表参加" _mubu_text="%3Cspan%3E%E6%88%962/3%E4%BB%A5%E4%B8%8A%E6%88%B7%E7%9A%84%E4%BB%A3%E8%A1%A8%E5%8F%82%E5%8A%A0%3C/span%3E" _note="代表参会都是2/3" _mubu_note="%3Cspan%3E%E4%BB%A3%E8%A1%A8%E5%8F%82%E4%BC%9A%E9%83%BD%E6%98%AF2/3%3C/span%3E"/>
                </outline>
                <outline text="所作决定应经到会人员过半数通过" _mubu_text="%3Cspan%3E%E6%89%80%E4%BD%9C%E5%86%B3%E5%AE%9A%E5%BA%94%E7%BB%8F%E5%88%B0%E4%BC%9A%E4%BA%BA%E5%91%98%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E" _note="与居委会是唯二到会过半数通过" _mubu_note="%3Cspan%3E%E4%B8%8E%E5%B1%85%E5%A7%94%E4%BC%9A%E6%98%AF%E5%94%AF%E4%BA%8C%E5%88%B0%E4%BC%9A%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E"/>
              </outline>
              <outline text="村民代表会议" _mubu_text="%3Cspan%3E%E6%9D%91%E6%B0%91%E4%BB%A3%E8%A1%A8%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
