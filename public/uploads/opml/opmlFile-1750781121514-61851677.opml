                <outline text="国家机关、社会组织、公民" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E3%80%81%E7%A4%BE%E4%BC%9A%E7%BB%84%E7%BB%87%E3%80%81%E5%85%AC%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="客体" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
                <outline text="既是监督的主体，又是监督的客体" _mubu_text="%3Cspan%3E%E6%97%A2%E6%98%AF%E7%9B%91%E7%9D%A3%E7%9A%84%E4%B8%BB%E4%BD%93%EF%BC%8C%E5%8F%88%E6%98%AF%E7%9B%91%E7%9D%A3%E7%9A%84%E5%AE%A2%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="包括与监督客体行为的合法性有关的所有问题" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E4%B8%8E%E7%9B%91%E7%9D%A3%E5%AE%A2%E4%BD%93%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%90%88%E6%B3%95%E6%80%A7%E6%9C%89%E5%85%B3%E7%9A%84%E6%89%80%E6%9C%89%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="重点是司法行为和执法行为" _mubu_text="%3Cspan%3E%E9%87%8D%E7%82%B9%E6%98%AF%E5%8F%B8%E6%B3%95%E8%A1%8C%E4%B8%BA%E5%92%8C%E6%89%A7%E6%B3%95%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="体系" _mubu_text="%3Cspan%3E%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
