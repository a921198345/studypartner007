              <outline text="在意志代表方面" _mubu_text="%3Cspan%3E%E5%9C%A8%E6%84%8F%E5%BF%97%E4%BB%A3%E8%A1%A8%E6%96%B9%E9%9D%A2%3C/span%3E" _note="人大与人民" _mubu_note="%3Cspan%3E%E4%BA%BA%E5%A4%A7%E4%B8%8E%E4%BA%BA%E6%B0%91%3C/span%3E">
