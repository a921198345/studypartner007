            <outline text="根本性" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%9C%AC%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="最高性" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="原则性" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="纲领性" _mubu_text="%3Cspan%3E%E7%BA%B2%E9%A2%86%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="稳定性" _mubu_text="%3Cspan%3E%E7%A8%B3%E5%AE%9A%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="确认性规范" _mubu_text="%3Cspan%3E%E7%A1%AE%E8%AE%A4%E6%80%A7%E8%A7%84%E8%8C%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="禁止性规范" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%80%A7%E8%A7%84%E8%8C%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="权利义务规范" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E8%A7%84%E8%8C%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="程序性规范" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E6%80%A7%E8%A7%84%E8%8C%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="宪法的效力" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
          <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
            <outline text="最高性和直接性" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%80%A7%E5%92%8C%E7%9B%B4%E6%8E%A5%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="对人效力" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BA%BA%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用于所有中国公民" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8E%E6%89%80%E6%9C%89%E4%B8%AD%E5%9B%BD%E5%85%AC%E6%B0%91%3C/span%3E" _note="属人" _mubu_note="%3Cspan%3E%E5%B1%9E%E4%BA%BA%3C/span%3E"/>
            <outline text="适用于外国人和法人" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8E%E5%A4%96%E5%9B%BD%E4%BA%BA%E5%92%8C%E6%B3%95%E4%BA%BA%3C/span%3E" _note="属地" _mubu_note="%3Cspan%3E%E5%B1%9E%E5%9C%B0%3C/span%3E"/>
          </outline>
          <outline text="领土效力" _mubu_text="%3Cspan%3E%E9%A2%86%E5%9C%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="国家基本制度" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%9F%BA%E6%9C%AC%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
        <outline text="中国的国家性质" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E7%9A%84%E5%9B%BD%E5%AE%B6%E6%80%A7%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="人民民主专政" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%B0%91%E6%B0%91%E4%B8%BB%E4%B8%93%E6%94%BF%3C/span%3E" _note="" _mubu_note="">
            <outline text="是我国的国体，即国家的阶级本质" _mubu_text="%3Cspan%3E%E6%98%AF%E6%88%91%E5%9B%BD%E7%9A%84%E5%9B%BD%E4%BD%93%EF%BC%8C%E5%8D%B3%E5%9B%BD%E5%AE%B6%E7%9A%84%E9%98%B6%E7%BA%A7%E6%9C%AC%E8%B4%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="是我国的国家性质，是对马列无级专政的中国化发展" _mubu_text="%3Cspan%3E%E6%98%AF%E6%88%91%E5%9B%BD%E7%9A%84%E5%9B%BD%E5%AE%B6%E6%80%A7%E8%B4%A8%EF%BC%8C%E6%98%AF%E5%AF%B9%E9%A9%AC%E5%88%97%E6%97%A0%E7%BA%A7%E4%B8%93%E6%94%BF%E7%9A%84%E4%B8%AD%E5%9B%BD%E5%8C%96%E5%8F%91%E5%B1%95%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="本质" _mubu_text="%3Cspan%3E%E6%9C%AC%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
              <outline text="对人民的民主和对少数敌对分子专政的有机统一" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BA%BA%E6%B0%91%E7%9A%84%E6%B0%91%E4%B8%BB%E5%92%8C%E5%AF%B9%E5%B0%91%E6%95%B0%E6%95%8C%E5%AF%B9%E5%88%86%E5%AD%90%E4%B8%93%E6%94%BF%E7%9A%84%E6%9C%89%E6%9C%BA%E7%BB%9F%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="主要特色" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E7%89%B9%E8%89%B2%3C/span%3E" _note="" _mubu_note="">
              <outline text="共产党领导的多党合作；爱国统一战线" _mubu_text="%3Cspan%3E%E5%85%B1%E4%BA%A7%E5%85%9A%E9%A2%86%E5%AF%BC%E7%9A%84%E5%A4%9A%E5%85%9A%E5%90%88%E4%BD%9C%EF%BC%9B%E7%88%B1%E5%9B%BD%E7%BB%9F%E4%B8%80%E6%88%98%E7%BA%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="党领导的多党合作、政治协商制度" _mubu_text="%3Cspan%3E%E5%85%9A%E9%A2%86%E5%AF%BC%E7%9A%84%E5%A4%9A%E5%85%9A%E5%90%88%E4%BD%9C%E3%80%81%E6%94%BF%E6%B2%BB%E5%8D%8F%E5%95%86%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="共产党是执政党，民主党是参政党" _mubu_text="%3Cspan%3E%E5%85%B1%E4%BA%A7%E5%85%9A%E6%98%AF%E6%89%A7%E6%94%BF%E5%85%9A%EF%BC%8C%E6%B0%91%E4%B8%BB%E5%85%9A%E6%98%AF%E5%8F%82%E6%94%BF%E5%85%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="政治基础" _mubu_text="%3Cspan%3E%E6%94%BF%E6%B2%BB%E5%9F%BA%E7%A1%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="坚持共产党的领导" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E5%85%B1%E4%BA%A7%E5%85%9A%E7%9A%84%E9%A2%86%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="坚持四项基本原则" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E5%9B%9B%E9%A1%B9%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="基本方针" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%96%B9%E9%92%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="长期共存、互相监督、肝胆相照、荣辱与共" _mubu_text="%3Cspan%3E%E9%95%BF%E6%9C%9F%E5%85%B1%E5%AD%98%E3%80%81%E4%BA%92%E7%9B%B8%E7%9B%91%E7%9D%A3%E3%80%81%E8%82%9D%E8%83%86%E7%9B%B8%E7%85%A7%E3%80%81%E8%8D%A3%E8%BE%B1%E4%B8%8E%E5%85%B1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="领导包括" _mubu_text="%3Cspan%3E%E9%A2%86%E5%AF%BC%E5%8C%85%E6%8B%AC%3C/span%3E" _note="" _mubu_note="">
              <outline text="政治原则、政治方向、重大方针政策的领导" _mubu_text="%3Cspan%3E%E6%94%BF%E6%B2%BB%E5%8E%9F%E5%88%99%E3%80%81%E6%94%BF%E6%B2%BB%E6%96%B9%E5%90%91%E3%80%81%E9%87%8D%E5%A4%A7%E6%96%B9%E9%92%88%E6%94%BF%E7%AD%96%E7%9A%84%E9%A2%86%E5%AF%BC%3C/span%3E" _note="各民主党派享有宪法规定范围内的政治自由、组织独立、法律地位平等的权利" _mubu_note="%3Cspan%3E%E5%90%84%E6%B0%91%E4%B8%BB%E5%85%9A%E6%B4%BE%E4%BA%AB%E6%9C%89%E5%AE%AA%E6%B3%95%E8%A7%84%E5%AE%9A%E8%8C%83%E5%9B%B4%E5%86%85%E7%9A%84%E6%94%BF%E6%B2%BB%E8%87%AA%E7%94%B1%E3%80%81%E7%BB%84%E7%BB%87%E7%8B%AC%E7%AB%8B%E3%80%81%E6%B3%95%E5%BE%8B%E5%9C%B0%E4%BD%8D%E5%B9%B3%E7%AD%89%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E"/>
              <outline text="非组织、人事领导" _mubu_text="%3Cspan%3E%E9%9D%9E%E7%BB%84%E7%BB%87%E3%80%81%E4%BA%BA%E4%BA%8B%E9%A2%86%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="政协" _mubu_text="%3Cspan%3E%E6%94%BF%E5%8D%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="非国家机关" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
                <outline text="无公权力" _mubu_text="%3Cspan%3E%E6%97%A0%E5%85%AC%E6%9D%83%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="有政治影响力" _mubu_text="%3Cspan%3E%E6%9C%89%E6%94%BF%E6%B2%BB%E5%BD%B1%E5%93%8D%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="职能" _mubu_text="%3Cspan%3E%E8%81%8C%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
                <outline text="政治协商、民主监督、参政议政" _mubu_text="%3Cspan%3E%E6%94%BF%E6%B2%BB%E5%8D%8F%E5%95%86%E3%80%81%E6%B0%91%E4%B8%BB%E7%9B%91%E7%9D%A3%E3%80%81%E5%8F%82%E6%94%BF%E8%AE%AE%E6%94%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="政协委员" _mubu_text="%3Cspan%3E%E6%94%BF%E5%8D%8F%E5%A7%94%E5%91%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="产生方式：推荐＋协商" _mubu_text="%3Cspan%3E%E4%BA%A7%E7%94%9F%E6%96%B9%E5%BC%8F%EF%BC%9A%E6%8E%A8%E8%8D%90%EF%BC%8B%E5%8D%8F%E5%95%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="“陪开会、陪讨论、陪视察“" _mubu_text="%3Cspan%3E%E2%80%9C%E9%99%AA%E5%BC%80%E4%BC%9A%E3%80%81%E9%99%AA%E8%AE%A8%E8%AE%BA%E3%80%81%E9%99%AA%E8%A7%86%E5%AF%9F%E2%80%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="爱国统一战线" _mubu_text="%3Cspan%3E%E7%88%B1%E5%9B%BD%E7%BB%9F%E4%B8%80%E6%88%98%E7%BA%BF%3C/span%3E" _note="现阶段我国的统一战线" _mubu_note="%3Cspan%3E%E7%8E%B0%E9%98%B6%E6%AE%B5%E6%88%91%E5%9B%BD%E7%9A%84%E7%BB%9F%E4%B8%80%E6%88%98%E7%BA%BF%3C/span%3E">
            <outline text="含义" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%3C/span%3E" _note="“三爱一劳建”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%B8%89%E7%88%B1%E4%B8%80%E5%8A%B3%E5%BB%BA%E2%80%9D%3C/span%3E">
              <outline text="由中国共产党领导的，" _mubu_text="%3Cspan%3E%E7%94%B1%E4%B8%AD%E5%9B%BD%E5%85%B1%E4%BA%A7%E5%85%9A%E9%A2%86%E5%AF%BC%E7%9A%84%EF%BC%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="由各民主党派和各人民团体参加的，" _mubu_text="%3Cspan%3E%E7%94%B1%E5%90%84%E6%B0%91%E4%B8%BB%E5%85%9A%E6%B4%BE%E5%92%8C%E5%90%84%E4%BA%BA%E6%B0%91%E5%9B%A2%E4%BD%93%E5%8F%82%E5%8A%A0%E7%9A%84%EF%BC%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="包括全体社义劳动者、" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E5%85%A8%E4%BD%93%E7%A4%BE%E4%B9%89%E5%8A%B3%E5%8A%A8%E8%80%85%E3%80%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="社义事业的建设者、" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%B9%89%E4%BA%8B%E4%B8%9A%E7%9A%84%E5%BB%BA%E8%AE%BE%E8%80%85%E3%80%81%3C/span%3E" _note="04增加" _mubu_note="%3Cspan%3E04%E5%A2%9E%E5%8A%A0%3C/span%3E"/>
              <outline text="拥护社义的爱国者、" _mubu_text="%3Cspan%3E%E6%8B%A5%E6%8A%A4%E7%A4%BE%E4%B9%89%E7%9A%84%E7%88%B1%E5%9B%BD%E8%80%85%E3%80%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="拥护祖国统一和致力于中华民族伟大复兴的爱国者的政治联盟" _mubu_text="%3Cspan%3E%E6%8B%A5%E6%8A%A4%E7%A5%96%E5%9B%BD%E7%BB%9F%E4%B8%80%E5%92%8C%E8%87%B4%E5%8A%9B%E4%BA%8E%E4%B8%AD%E5%8D%8E%E6%B0%91%E6%97%8F%E4%BC%9F%E5%A4%A7%E5%A4%8D%E5%85%B4%E7%9A%84%E7%88%B1%E5%9B%BD%E8%80%85%E7%9A%84%E6%94%BF%E6%B2%BB%E8%81%94%E7%9B%9F%3C/span%3E" _note="18增加" _mubu_note="%3Cspan%3E18%E5%A2%9E%E5%8A%A0%3C/span%3E"/>
            </outline>
            <outline text="三大任务" _mubu_text="%3Cspan%3E%E4%B8%89%E5%A4%A7%E4%BB%BB%E5%8A%A1%3C/span%3E" _note="政协任务" _mubu_note="%3Cspan%3E%E6%94%BF%E5%8D%8F%E4%BB%BB%E5%8A%A1%3C/span%3E">
              <outline text="社义现代化建设" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%B9%89%E7%8E%B0%E4%BB%A3%E5%8C%96%E5%BB%BA%E8%AE%BE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="祖国统一" _mubu_text="%3Cspan%3E%E7%A5%96%E5%9B%BD%E7%BB%9F%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
