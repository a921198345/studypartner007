              <outline text="人大向人民负责" _mubu_text="%3Cspan%3E%E4%BA%BA%E5%A4%A7%E5%90%91%E4%BA%BA%E6%B0%91%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="其他国家机关向人大负责" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E5%90%91%E4%BA%BA%E5%A4%A7%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="集体负责制" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E8%B4%9F%E8%B4%A3%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
