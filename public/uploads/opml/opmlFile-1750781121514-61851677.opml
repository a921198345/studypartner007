              </outline>
            </outline>
            <outline text="恶法亦法 VS 恶法非法" _mubu_text="%3Cspan%3E%E6%81%B6%E6%B3%95%E4%BA%A6%E6%B3%95%20VS%20%E6%81%B6%E6%B3%95%E9%9D%9E%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="法实证主义" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9E%E8%AF%81%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="侧重于研究实际存在的法律" _mubu_text="%3Cspan%3E%E4%BE%A7%E9%87%8D%E4%BA%8E%E7%A0%94%E7%A9%B6%E5%AE%9E%E9%99%85%E5%AD%98%E5%9C%A8%E7%9A%84%E6%B3%95%E5%BE%8B%3C/span%3E" _note="实然法" _mubu_note="%3Cspan%3E%E5%AE%9E%E7%84%B6%E6%B3%95%3C/span%3E"/>
                <outline text="法律的存在与法律在道德上的好坏是两个不能混同的议题" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E5%AD%98%E5%9C%A8%E4%B8%8E%E6%B3%95%E5%BE%8B%E5%9C%A8%E9%81%93%E5%BE%B7%E4%B8%8A%E7%9A%84%E5%A5%BD%E5%9D%8F%E6%98%AF%E4%B8%A4%E4%B8%AA%E4%B8%8D%E8%83%BD%E6%B7%B7%E5%90%8C%E7%9A%84%E8%AE%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="恶法亦法" _mubu_text="%3Cspan%3E%E6%81%B6%E6%B3%95%E4%BA%A6%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="非实证主义" _mubu_text="%3Cspan%3E%E9%9D%9E%E5%AE%9E%E8%AF%81%E4%B8%BB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="侧重于研究法律在道德上应当是什么的问题" _mubu_text="%3Cspan%3E%E4%BE%A7%E9%87%8D%E4%BA%8E%E7%A0%94%E7%A9%B6%E6%B3%95%E5%BE%8B%E5%9C%A8%E9%81%93%E5%BE%B7%E4%B8%8A%E5%BA%94%E5%BD%93%E6%98%AF%E4%BB%80%E4%B9%88%E7%9A%84%E9%97%AE%E9%A2%98%3C/span%3E" _note="应然法" _mubu_note="%3Cspan%3E%E5%BA%94%E7%84%B6%E6%B3%95%3C/span%3E"/>
                <outline text="法律的要求不能同道德的要求相抵触" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E8%A6%81%E6%B1%82%E4%B8%8D%E8%83%BD%E5%90%8C%E9%81%93%E5%BE%B7%E7%9A%84%E8%A6%81%E6%B1%82%E7%9B%B8%E6%8A%B5%E8%A7%A6%3C/span%3E" _note="法律的效力依附于道德" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E6%95%88%E5%8A%9B%E4%BE%9D%E9%99%84%E4%BA%8E%E9%81%93%E5%BE%B7%3C/span%3E"/>
