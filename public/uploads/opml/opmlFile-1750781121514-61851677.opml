                <outline text="人权是历史发展的产物" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%9D%83%E6%98%AF%E5%8E%86%E5%8F%B2%E5%8F%91%E5%B1%95%E7%9A%84%E4%BA%A7%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="既不依赖于国家更不依赖于国家的法" _mubu_text="%3Cspan%3E%E6%97%A2%E4%B8%8D%E4%BE%9D%E8%B5%96%E4%BA%8E%E5%9B%BD%E5%AE%B6%E6%9B%B4%E4%B8%8D%E4%BE%9D%E8%B5%96%E4%BA%8E%E5%9B%BD%E5%AE%B6%E7%9A%84%E6%B3%95%3C/span%3E" _note="凭自己是人而享有的权利" _mubu_note="%3Cspan%3E%E5%87%AD%E8%87%AA%E5%B7%B1%E6%98%AF%E4%BA%BA%E8%80%8C%E4%BA%AB%E6%9C%89%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E">
                <outline text="在逻辑上是先于国家和法的，在根本上是一种道德权利" _mubu_text="%3Cspan%3E%E5%9C%A8%E9%80%BB%E8%BE%91%E4%B8%8A%E6%98%AF%E5%85%88%E4%BA%8E%E5%9B%BD%E5%AE%B6%E5%92%8C%E6%B3%95%E7%9A%84%EF%BC%8C%E5%9C%A8%E6%A0%B9%E6%9C%AC%E4%B8%8A%E6%98%AF%E4%B8%80%E7%A7%8D%E9%81%93%E5%BE%B7%E6%9D%83%E5%88%A9%3C/span%3E" _note="人权在逻辑上先于法律权利，可以作为法的评价标准" _mubu_note="%3Cspan%3E%E4%BA%BA%E6%9D%83%E5%9C%A8%E9%80%BB%E8%BE%91%E4%B8%8A%E5%85%88%E4%BA%8E%E6%B3%95%E5%BE%8B%E6%9D%83%E5%88%A9%EF%BC%8C%E5%8F%AF%E4%BB%A5%E4%BD%9C%E4%B8%BA%E6%B3%95%E7%9A%84%E8%AF%84%E4%BB%B7%E6%A0%87%E5%87%86%3C/span%3E"/>
              </outline>
              <outline text="人权应当尽可能地被法律化" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%9D%83%E5%BA%94%E5%BD%93%E5%B0%BD%E5%8F%AF%E8%83%BD%E5%9C%B0%E8%A2%AB%E6%B3%95%E5%BE%8B%E5%8C%96%3C/span%3E" _note="保证在事实上享有" _mubu_note="%3Cspan%3E%E4%BF%9D%E8%AF%81%E5%9C%A8%E4%BA%8B%E5%AE%9E%E4%B8%8A%E4%BA%AB%E6%9C%89%3C/span%3E"/>
              <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="个体人权" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BD%93%E4%BA%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="集体人权" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E4%BA%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="正义" _mubu_text="%3Cspan%3E%E6%AD%A3%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="是法的基本标准" _mubu_text="%3Cspan%3E%E6%98%AF%E6%B3%95%E7%9A%84%E5%9F%BA%E6%9C%AC%E6%A0%87%E5%87%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="是法的评价体系" _mubu_text="%3Cspan%3E%E6%98%AF%E6%B3%95%E7%9A%84%E8%AF%84%E4%BB%B7%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="极大地推动着法律的进化" _mubu_text="%3Cspan%3E%E6%9E%81%E5%A4%A7%E5%9C%B0%E6%8E%A8%E5%8A%A8%E7%9D%80%E6%B3%95%E5%BE%8B%E7%9A%84%E8%BF%9B%E5%8C%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="同时包括实体正义与程序正义" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%8C%85%E6%8B%AC%E5%AE%9E%E4%BD%93%E6%AD%A3%E4%B9%89%E4%B8%8E%E7%A8%8B%E5%BA%8F%E6%AD%A3%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="同等重要，如车之两轮，鸟之两翼，相辅相成，不可偏废" _mubu_text="%3Cspan%3E%E5%90%8C%E7%AD%89%E9%87%8D%E8%A6%81%EF%BC%8C%E5%A6%82%E8%BD%A6%E4%B9%8B%E4%B8%A4%E8%BD%AE%EF%BC%8C%E9%B8%9F%E4%B9%8B%E4%B8%A4%E7%BF%BC%EF%BC%8C%E7%9B%B8%E8%BE%85%E7%9B%B8%E6%88%90%EF%BC%8C%E4%B8%8D%E5%8F%AF%E5%81%8F%E5%BA%9F%3C/span%3E" _note="分别都有独立性价值" _mubu_note="%3Cspan%3E%E5%88%86%E5%88%AB%E9%83%BD%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%80%A7%E4%BB%B7%E5%80%BC%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="价值冲突" _mubu_text="%3Cspan%3E%E4%BB%B7%E5%80%BC%E5%86%B2%E7%AA%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="冲突场合" _mubu_text="%3Cspan%3E%E5%86%B2%E7%AA%81%E5%9C%BA%E5%90%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="个体之间" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="共同体之间" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="个体和共同体之间" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BD%93%E5%92%8C%E5%85%B1%E5%90%8C%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="解决原则" _mubu_text="%3Cspan%3E%E8%A7%A3%E5%86%B3%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="价值位阶" _mubu_text="%3Cspan%3E%E4%BB%B7%E5%80%BC%E4%BD%8D%E9%98%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="高位阶优于低位阶" _mubu_text="%3Cspan%3E%E9%AB%98%E4%BD%8D%E9%98%B6%E4%BC%98%E4%BA%8E%E4%BD%8E%E4%BD%8D%E9%98%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="个案平衡原则" _mubu_text="%3Cspan%3E%E4%B8%AA%E6%A1%88%E5%B9%B3%E8%A1%A1%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="同一位阶，权衡考虑，兼顾双方" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E4%BD%8D%E9%98%B6%EF%BC%8C%E6%9D%83%E8%A1%A1%E8%80%83%E8%99%91%EF%BC%8C%E5%85%BC%E9%A1%BE%E5%8F%8C%E6%96%B9%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="比例原则" _mubu_text="%3Cspan%3E%E6%AF%94%E4%BE%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="最小损害、最少限制" _mubu_text="%3Cspan%3E%E6%9C%80%E5%B0%8F%E6%8D%9F%E5%AE%B3%E3%80%81%E6%9C%80%E5%B0%91%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="只要有高下判断就是位阶，只要有关注成本限度就是比例" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%A6%81%E6%9C%89%E9%AB%98%E4%B8%8B%E5%88%A4%E6%96%AD%E5%B0%B1%E6%98%AF%E4%BD%8D%E9%98%B6%EF%BC%8C%E5%8F%AA%E8%A6%81%E6%9C%89%E5%85%B3%E6%B3%A8%E6%88%90%E6%9C%AC%E9%99%90%E5%BA%A6%E5%B0%B1%E6%98%AF%E6%AF%94%E4%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="法律的要素" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E8%A6%81%E7%B4%A0%3C/span%3E" _note="" _mubu_note="">
          <outline text="法律规则" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="&quot;新三要素说&quot;" _mubu_text="%3Cspan%3E%22%E6%96%B0%E4%B8%89%E8%A6%81%E7%B4%A0%E8%AF%B4%22%3C/span%3E" _note="" _mubu_note="">
              <outline text="假定条件" _mubu_text="%3Cspan%3E%E5%81%87%E5%AE%9A%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="主体条件" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="事项条件" _mubu_text="%3Cspan%3E%E4%BA%8B%E9%A1%B9%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="时间、地点、行为类型" _mubu_note="%3Cspan%3E%E6%97%B6%E9%97%B4%E3%80%81%E5%9C%B0%E7%82%B9%E3%80%81%E8%A1%8C%E4%B8%BA%E7%B1%BB%E5%9E%8B%3C/span%3E"/>
              </outline>
              <outline text="行为模式" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E6%A8%A1%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="可为、应为、勿为" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%BA%E3%80%81%E5%BA%94%E4%B8%BA%E3%80%81%E5%8B%BF%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法律后果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="肯定性" _mubu_text="%3Cspan%3E%E8%82%AF%E5%AE%9A%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="否定性" _mubu_text="%3Cspan%3E%E5%90%A6%E5%AE%9A%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="三要素中任何部分，均可能被省略" _mubu_text="%3Cspan%3E%E4%B8%89%E8%A6%81%E7%B4%A0%E4%B8%AD%E4%BB%BB%E4%BD%95%E9%83%A8%E5%88%86%EF%BC%8C%E5%9D%87%E5%8F%AF%E8%83%BD%E8%A2%AB%E7%9C%81%E7%95%A5%3C/span%3E" _note="即不明确表达出来" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%B8%8D%E6%98%8E%E7%A1%AE%E8%A1%A8%E8%BE%BE%E5%87%BA%E6%9D%A5%3C/span%3E">
                <outline text="并不意味着逻辑上不存在" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%B8%8D%E6%84%8F%E5%91%B3%E7%9D%80%E9%80%BB%E8%BE%91%E4%B8%8A%E4%B8%8D%E5%AD%98%E5%9C%A8%3C/span%3E" _note="逻辑上缺一不可" _mubu_note="%3Cspan%3E%E9%80%BB%E8%BE%91%E4%B8%8A%E7%BC%BA%E4%B8%80%E4%B8%8D%E5%8F%AF%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="法律规则与法律条文" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E5%88%99%E4%B8%8E%E6%B3%95%E5%BE%8B%E6%9D%A1%E6%96%87%3C/span%3E" _note="" _mubu_note="">
              <outline text="任意对应关系" _mubu_text="%3Cspan%3E%E4%BB%BB%E6%84%8F%E5%AF%B9%E5%BA%94%E5%85%B3%E7%B3%BB%3C/span%3E" _note="一对多、多对一" _mubu_note="%3Cspan%3E%E4%B8%80%E5%AF%B9%E5%A4%9A%E3%80%81%E5%A4%9A%E5%AF%B9%E4%B8%80%3C/span%3E"/>
            </outline>
            <outline text="法律规则与语言" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E5%88%99%E4%B8%8E%E8%AF%AD%E8%A8%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="一切法律规范都须以语句形式表达出来" _mubu_text="%3Cspan%3E%E4%B8%80%E5%88%87%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E9%83%BD%E9%A1%BB%E4%BB%A5%E8%AF%AD%E5%8F%A5%E5%BD%A2%E5%BC%8F%E8%A1%A8%E8%BE%BE%E5%87%BA%E6%9D%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="通过解释实现对法律含义的说明" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E8%A7%A3%E9%87%8A%E5%AE%9E%E7%8E%B0%E5%AF%B9%E6%B3%95%E5%BE%8B%E5%90%AB%E4%B9%89%E7%9A%84%E8%AF%B4%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="规范性语句" _mubu_text="%3Cspan%3E%E8%A7%84%E8%8C%83%E6%80%A7%E8%AF%AD%E5%8F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="命令句和允许句" _mubu_text="%3Cspan%3E%E5%91%BD%E4%BB%A4%E5%8F%A5%E5%92%8C%E5%85%81%E8%AE%B8%E5%8F%A5%3C/span%3E" _note="所运用的道义助动词不同" _mubu_note="%3Cspan%3E%E6%89%80%E8%BF%90%E7%94%A8%E7%9A%84%E9%81%93%E4%B9%89%E5%8A%A9%E5%8A%A8%E8%AF%8D%E4%B8%8D%E5%90%8C%3C/span%3E">
                <outline text="&quot;必须、应该、禁止、可以&quot;等道义助动词" _mubu_text="%3Cspan%3E%22%E5%BF%85%E9%A1%BB%E3%80%81%E5%BA%94%E8%AF%A5%E3%80%81%E7%A6%81%E6%AD%A2%E3%80%81%E5%8F%AF%E4%BB%A5%22%E7%AD%89%E9%81%93%E4%B9%89%E5%8A%A9%E5%8A%A8%E8%AF%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="并非所有都以规范语句形式表达，也可用陈述语句或陈述句表达" _mubu_text="%3Cspan%3E%E5%B9%B6%E9%9D%9E%E6%89%80%E6%9C%89%E9%83%BD%E4%BB%A5%E8%A7%84%E8%8C%83%E8%AF%AD%E5%8F%A5%E5%BD%A2%E5%BC%8F%E8%A1%A8%E8%BE%BE%EF%BC%8C%E4%B9%9F%E5%8F%AF%E7%94%A8%E9%99%88%E8%BF%B0%E8%AF%AD%E5%8F%A5%E6%88%96%E9%99%88%E8%BF%B0%E5%8F%A5%E8%A1%A8%E8%BE%BE%3C/span%3E" _note="即无道义助动词" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%97%A0%E9%81%93%E4%B9%89%E5%8A%A9%E5%8A%A8%E8%AF%8D%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="种类" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="按行为模式" _mubu_text="%3Cspan%3E%E6%8C%89%E8%A1%8C%E4%B8%BA%E6%A8%A1%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
                <outline text="授权性规则、义务性规则、权义复合性规则" _mubu_text="%3Cspan%3E%E6%8E%88%E6%9D%83%E6%80%A7%E8%A7%84%E5%88%99%E3%80%81%E4%B9%89%E5%8A%A1%E6%80%A7%E8%A7%84%E5%88%99%E3%80%81%E6%9D%83%E4%B9%89%E5%A4%8D%E5%90%88%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="可为、应为、勿为" _mubu_note="%3Cspan%3E%E5%8F%AF%E4%B8%BA%E3%80%81%E5%BA%94%E4%B8%BA%E3%80%81%E5%8B%BF%E4%B8%BA%3C/span%3E"/>
              </outline>
              <outline text="按照内容确定性程度" _mubu_text="%3Cspan%3E%E6%8C%89%E7%85%A7%E5%86%85%E5%AE%B9%E7%A1%AE%E5%AE%9A%E6%80%A7%E7%A8%8B%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="确定性规则、委任性规则、准用性规则" _mubu_text="%3Cspan%3E%E7%A1%AE%E5%AE%9A%E6%80%A7%E8%A7%84%E5%88%99%E3%80%81%E5%A7%94%E4%BB%BB%E6%80%A7%E8%A7%84%E5%88%99%E3%80%81%E5%87%86%E7%94%A8%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="不需帮忙、找人帮忙、找法帮忙" _mubu_note="%3Cspan%3E%E4%B8%8D%E9%9C%80%E5%B8%AE%E5%BF%99%E3%80%81%E6%89%BE%E4%BA%BA%E5%B8%AE%E5%BF%99%E3%80%81%E6%89%BE%E6%B3%95%E5%B8%AE%E5%BF%99%3C/span%3E"/>
              </outline>
              <outline text="按适用谁说了算" _mubu_text="%3Cspan%3E%E6%8C%89%E9%80%82%E7%94%A8%E8%B0%81%E8%AF%B4%E4%BA%86%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
                <outline text="强行性规则、任意性规则" _mubu_text="%3Cspan%3E%E5%BC%BA%E8%A1%8C%E6%80%A7%E8%A7%84%E5%88%99%E3%80%81%E4%BB%BB%E6%84%8F%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="按法律规则功能" _mubu_text="%3Cspan%3E%E6%8C%89%E6%B3%95%E5%BE%8B%E8%A7%84%E5%88%99%E5%8A%9F%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
                <outline text="调整性规则" _mubu_text="%3Cspan%3E%E8%B0%83%E6%95%B4%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="行为在前，规则在后" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E5%9C%A8%E5%89%8D%EF%BC%8C%E8%A7%84%E5%88%99%E5%9C%A8%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="构成性规则" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="规则在前，行为在后" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%99%E5%9C%A8%E5%89%8D%EF%BC%8C%E8%A1%8C%E4%B8%BA%E5%9C%A8%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="法律原则" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="种类" _mubu_text="%3Cspan%3E%E7%A7%8D%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="公理性原则和政策性原则" _mubu_text="%3Cspan%3E%E5%85%AC%E7%90%86%E6%80%A7%E5%8E%9F%E5%88%99%E5%92%8C%E6%94%BF%E7%AD%96%E6%80%A7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="公理性原则" _mubu_text="%3Cspan%3E%E5%85%AC%E7%90%86%E6%80%A7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="由法理构成的原则" _mubu_text="%3Cspan%3E%E7%94%B1%E6%B3%95%E7%90%86%E6%9E%84%E6%88%90%E7%9A%84%E5%8E%9F%E5%88%99%3C/span%3E" _note="有普遍性" _mubu_note="%3Cspan%3E%E6%9C%89%E6%99%AE%E9%81%8D%E6%80%A7%3C/span%3E"/>
                </outline>
                <outline text="政策性原则" _mubu_text="%3Cspan%3E%E6%94%BF%E7%AD%96%E6%80%A7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国家或民族出于一定的政策考量而制定" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%88%96%E6%B0%91%E6%97%8F%E5%87%BA%E4%BA%8E%E4%B8%80%E5%AE%9A%E7%9A%84%E6%94%BF%E7%AD%96%E8%80%83%E9%87%8F%E8%80%8C%E5%88%B6%E5%AE%9A%3C/span%3E" _note="有特殊性" _mubu_note="%3Cspan%3E%E6%9C%89%E7%89%B9%E6%AE%8A%E6%80%A7%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="基本原则和具体原则" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%E5%92%8C%E5%85%B7%E4%BD%93%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="基本法律原则" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="整个法律体系或部门所适用" _mubu_text="%3Cspan%3E%E6%95%B4%E4%B8%AA%E6%B3%95%E5%BE%8B%E4%BD%93%E7%B3%BB%E6%88%96%E9%83%A8%E9%97%A8%E6%89%80%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="具体法律原则" _mubu_text="%3Cspan%3E%E5%85%B7%E4%BD%93%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="适用于某一法律部门中特定情形" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8E%E6%9F%90%E4%B8%80%E6%B3%95%E5%BE%8B%E9%83%A8%E9%97%A8%E4%B8%AD%E7%89%B9%E5%AE%9A%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="实体性原则和程序性原则" _mubu_text="%3Cspan%3E%E5%AE%9E%E4%BD%93%E6%80%A7%E5%8E%9F%E5%88%99%E5%92%8C%E7%A8%8B%E5%BA%8F%E6%80%A7%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法律原则or法律规则" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%8E%9F%E5%88%99or%E6%B3%95%E5%BE%8B%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="内容上" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%E4%B8%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="规则" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="明确具体" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A1%AE%E5%85%B7%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="着眼于主体行为及各种条件的共性" _mubu_text="%3Cspan%3E%E7%9D%80%E7%9C%BC%E4%BA%8E%E4%B8%BB%E4%BD%93%E8%A1%8C%E4%B8%BA%E5%8F%8A%E5%90%84%E7%A7%8D%E6%9D%A1%E4%BB%B6%E7%9A%84%E5%85%B1%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="宽泛抽象" _mubu_text="%3Cspan%3E%E5%AE%BD%E6%B3%9B%E6%8A%BD%E8%B1%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="不仅局限于共性，而且关注个性" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BB%85%E5%B1%80%E9%99%90%E4%BA%8E%E5%85%B1%E6%80%A7%EF%BC%8C%E8%80%8C%E4%B8%94%E5%85%B3%E6%B3%A8%E4%B8%AA%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="适用范围上" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%E4%B8%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则具有宏观的指导性，其适用范围比规则宽广" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E5%85%B7%E6%9C%89%E5%AE%8F%E8%A7%82%E7%9A%84%E6%8C%87%E5%AF%BC%E6%80%A7%EF%BC%8C%E5%85%B6%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%E6%AF%94%E8%A7%84%E5%88%99%E5%AE%BD%E5%B9%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="适用方式上" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%96%B9%E5%BC%8F%E4%B8%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="规则" _mubu_text="%3Cspan%3E%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="&quot;全有或全无的方式&quot;" _mubu_text="%3Cspan%3E%22%E5%85%A8%E6%9C%89%E6%88%96%E5%85%A8%E6%97%A0%E7%9A%84%E6%96%B9%E5%BC%8F%22%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                  <outline text="&quot;权衡强度或分量的方式&quot;" _mubu_text="%3Cspan%3E%22%E6%9D%83%E8%A1%A1%E5%BC%BA%E5%BA%A6%E6%88%96%E5%88%86%E9%87%8F%E7%9A%84%E6%96%B9%E5%BC%8F%22%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="功能上" _mubu_text="%3Cspan%3E%E5%8A%9F%E8%83%BD%E4%B8%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则是规则的基础和本源，规则是原则具体化结果" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E6%98%AF%E8%A7%84%E5%88%99%E7%9A%84%E5%9F%BA%E7%A1%80%E5%92%8C%E6%9C%AC%E6%BA%90%EF%BC%8C%E8%A7%84%E5%88%99%E6%98%AF%E5%8E%9F%E5%88%99%E5%85%B7%E4%BD%93%E5%8C%96%E7%BB%93%E6%9E%9C%3C/span%3E" _note="能协调矛盾，弥补不足" _mubu_note="%3Cspan%3E%E8%83%BD%E5%8D%8F%E8%B0%83%E7%9F%9B%E7%9B%BE%EF%BC%8C%E5%BC%A5%E8%A1%A5%E4%B8%8D%E8%B6%B3%3C/span%3E"/>
                <outline text="原则弹性，规则刚性" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E5%BC%B9%E6%80%A7%EF%BC%8C%E8%A7%84%E5%88%99%E5%88%9A%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="原则是最佳化命令，规则是确定性命令" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E6%98%AF%E6%9C%80%E4%BD%B3%E5%8C%96%E5%91%BD%E4%BB%A4%EF%BC%8C%E8%A7%84%E5%88%99%E6%98%AF%E7%A1%AE%E5%AE%9A%E6%80%A7%E5%91%BD%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="原则适用条件" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="核心目的是限制法官自由裁量" _mubu_note="%3Cspan%3E%E6%A0%B8%E5%BF%83%E7%9B%AE%E7%9A%84%E6%98%AF%E9%99%90%E5%88%B6%E6%B3%95%E5%AE%98%E8%87%AA%E7%94%B1%E8%A3%81%E9%87%8F%3C/span%3E">
              <outline text="穷尽规则" _mubu_text="%3Cspan%3E%E7%A9%B7%E5%B0%BD%E8%A7%84%E5%88%99%3C/span%3E" _note="是原则弥补规则漏洞的条件" _mubu_note="%3Cspan%3E%E6%98%AF%E5%8E%9F%E5%88%99%E5%BC%A5%E8%A1%A5%E8%A7%84%E5%88%99%E6%BC%8F%E6%B4%9E%E7%9A%84%E6%9D%A1%E4%BB%B6%3C/span%3E"/>
              <outline text="实现个案正义" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%8E%B0%E4%B8%AA%E6%A1%88%E6%AD%A3%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有更强理由" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9B%B4%E5%BC%BA%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="权利与义务" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%B8%8E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="权利" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="遵从法定原则" _mubu_text="%3Cspan%3E%E9%81%B5%E4%BB%8E%E6%B3%95%E5%AE%9A%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有一定自主性" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%80%E5%AE%9A%E8%87%AA%E4%B8%BB%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="总与义务相关联" _mubu_text="%3Cspan%3E%E6%80%BB%E4%B8%8E%E4%B9%89%E5%8A%A1%E7%9B%B8%E5%85%B3%E8%81%94%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="指向&quot;应然&quot;和&quot;未来&quot;" _mubu_text="%3Cspan%3E%E6%8C%87%E5%90%91%22%E5%BA%94%E7%84%B6%22%E5%92%8C%22%E6%9C%AA%E6%9D%A5%22%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="具有强制履行性" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E5%BC%BA%E5%88%B6%E5%B1%A5%E8%A1%8C%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="基本权利义务与普通权利义务" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E4%B8%8E%E6%99%AE%E9%80%9A%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%3C/span%3E" _note="根据根本法和普通法律规定不同" _mubu_note="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E6%A0%B9%E6%9C%AC%E6%B3%95%E5%92%8C%E6%99%AE%E9%80%9A%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E4%B8%8D%E5%90%8C%3C/span%3E"/>
              <outline text="绝对权利义务和相对权利义务" _mubu_text="%3Cspan%3E%E7%BB%9D%E5%AF%B9%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%92%8C%E7%9B%B8%E5%AF%B9%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%3C/span%3E" _note="对世与对人" _mubu_note="%3Cspan%3E%E5%AF%B9%E4%B8%96%E4%B8%8E%E5%AF%B9%E4%BA%BA%3C/span%3E"/>
            </outline>
            <outline text="相互联系" _mubu_text="%3Cspan%3E%E7%9B%B8%E4%BA%92%E8%81%94%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="结构上，二者紧密联系，不可分割" _mubu_text="%3Cspan%3E%E7%BB%93%E6%9E%84%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E7%B4%A7%E5%AF%86%E8%81%94%E7%B3%BB%EF%BC%8C%E4%B8%8D%E5%8F%AF%E5%88%86%E5%89%B2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="数量上，二者一一对应，总量相等" _mubu_text="%3Cspan%3E%E6%95%B0%E9%87%8F%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E4%B8%80%E4%B8%80%E5%AF%B9%E5%BA%94%EF%BC%8C%E6%80%BB%E9%87%8F%E7%9B%B8%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="从产生和发展看，浑然一体→分裂对立→相对一致" _mubu_text="%3Cspan%3E%E4%BB%8E%E4%BA%A7%E7%94%9F%E5%92%8C%E5%8F%91%E5%B1%95%E7%9C%8B%EF%BC%8C%E6%B5%91%E7%84%B6%E4%B8%80%E4%BD%93%E2%86%92%E5%88%86%E8%A3%82%E5%AF%B9%E7%AB%8B%E2%86%92%E7%9B%B8%E5%AF%B9%E4%B8%80%E8%87%B4%3C/span%3E" _note="原始社会→阶级社会→社会主义社会" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%A7%8B%E7%A4%BE%E4%BC%9A%E2%86%92%E9%98%B6%E7%BA%A7%E7%A4%BE%E4%BC%9A%E2%86%92%E7%A4%BE%E4%BC%9A%E4%B8%BB%E4%B9%89%E7%A4%BE%E4%BC%9A%3C/span%3E"/>
              <outline text="价值上，权利和义务代表着不同的法律精神" _mubu_text="%3Cspan%3E%E4%BB%B7%E5%80%BC%E4%B8%8A%EF%BC%8C%E6%9D%83%E5%88%A9%E5%92%8C%E4%B9%89%E5%8A%A1%E4%BB%A3%E8%A1%A8%E7%9D%80%E4%B8%8D%E5%90%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E7%B2%BE%E7%A5%9E%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="作用上，二者相互限定、相互制约" _mubu_text="%3Cspan%3E%E4%BD%9C%E7%94%A8%E4%B8%8A%EF%BC%8C%E4%BA%8C%E8%80%85%E7%9B%B8%E4%BA%92%E9%99%90%E5%AE%9A%E3%80%81%E7%9B%B8%E4%BA%92%E5%88%B6%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="法的渊源与分类" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%E4%B8%8E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="法的渊源的概念" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%E7%9A%84%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
            <outline text="实质渊源or形式渊源" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E6%B8%8A%E6%BA%90or%E5%BD%A2%E5%BC%8F%E6%B8%8A%E6%BA%90%3C/span%3E" _note="形式渊源即效力渊源" _mubu_note="%3Cspan%3E%E5%BD%A2%E5%BC%8F%E6%B8%8A%E6%BA%90%E5%8D%B3%E6%95%88%E5%8A%9B%E6%B8%8A%E6%BA%90%3C/span%3E"/>
            <outline text="正式渊源or非正式渊源" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90or%E9%9D%9E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%3C/span%3E" _note="" _mubu_note="">
              <outline text="正式渊源：具有明文规定的法律效力和直接作为" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%EF%BC%9A%E5%85%B7%E6%9C%89%E6%98%8E%E6%96%87%E8%A7%84%E5%AE%9A%E7%9A%84%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%E5%92%8C%E7%9B%B4%E6%8E%A5%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="法律人必须予以考虑" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E4%BA%BA%E5%BF%85%E9%A1%BB%E4%BA%88%E4%BB%A5%E8%80%83%E8%99%91%3C/span%3E"/>
              <outline text="非正式渊源：不具有法律明文规定的法律效力" _mubu_text="%3Cspan%3E%E9%9D%9E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%EF%BC%9A%E4%B8%8D%E5%85%B7%E6%9C%89%E6%B3%95%E5%BE%8B%E6%98%8E%E6%96%87%E8%A7%84%E5%AE%9A%E7%9A%84%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
                <outline text="核心价值" _mubu_text="%3Cspan%3E%E6%A0%B8%E5%BF%83%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note="">
                  <outline text="填补漏洞和辅助理解" _mubu_text="%3Cspan%3E%E5%A1%AB%E8%A1%A5%E6%BC%8F%E6%B4%9E%E5%92%8C%E8%BE%85%E5%8A%A9%E7%90%86%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="法官不得以法律没有规定或规定不清楚为理由拒绝裁判" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%98%E4%B8%8D%E5%BE%97%E4%BB%A5%E6%B3%95%E5%BE%8B%E6%B2%A1%E6%9C%89%E8%A7%84%E5%AE%9A%E6%88%96%E8%A7%84%E5%AE%9A%E4%B8%8D%E6%B8%85%E6%A5%9A%E4%B8%BA%E7%90%86%E7%94%B1%E6%8B%92%E7%BB%9D%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="正式渊源代表了&quot;官方认同&quot;，非正式渊源代表了&quot;社会认同&quot;" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%E4%BB%A3%E8%A1%A8%E4%BA%86%22%E5%AE%98%E6%96%B9%E8%AE%A4%E5%90%8C%22%EF%BC%8C%E9%9D%9E%E6%AD%A3%E5%BC%8F%E6%B8%8A%E6%BA%90%E4%BB%A3%E8%A1%A8%E4%BA%86%22%E7%A4%BE%E4%BC%9A%E8%AE%A4%E5%90%8C%22%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="当代中国的正式法的渊源" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BB%A3%E4%B8%AD%E5%9B%BD%E7%9A%84%E6%AD%A3%E5%BC%8F%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="宪法" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="我国当前《宪法》并未司法化，实践中无法独立作为裁判依据" _mubu_text="%3Cspan%3E%E6%88%91%E5%9B%BD%E5%BD%93%E5%89%8D%E3%80%8A%E5%AE%AA%E6%B3%95%E3%80%8B%E5%B9%B6%E6%9C%AA%E5%8F%B8%E6%B3%95%E5%8C%96%EF%BC%8C%E5%AE%9E%E8%B7%B5%E4%B8%AD%E6%97%A0%E6%B3%95%E7%8B%AC%E7%AB%8B%E4%BD%9C%E4%B8%BA%E8%A3%81%E5%88%A4%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="将宪法列入正式法律渊源只是从形式上对《宪法》的尊重" _mubu_note="%3Cspan%3E%E5%B0%86%E5%AE%AA%E6%B3%95%E5%88%97%E5%85%A5%E6%AD%A3%E5%BC%8F%E6%B3%95%E5%BE%8B%E6%B8%8A%E6%BA%90%E5%8F%AA%E6%98%AF%E4%BB%8E%E5%BD%A2%E5%BC%8F%E4%B8%8A%E5%AF%B9%E3%80%8A%E5%AE%AA%E6%B3%95%E3%80%8B%E7%9A%84%E5%B0%8A%E9%87%8D%3C/span%3E"/>
            </outline>
            <outline text="法律" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="基本法律" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="全人大制定和修改" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E5%88%B6%E5%AE%9A%E5%92%8C%E4%BF%AE%E6%94%B9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="全人常修改" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E4%BF%AE%E6%94%B9%3C/span%3E" _note="" _mubu_note="">
                    <outline text="全人大闭会期间" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E9%97%AD%E4%BC%9A%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="进行部分修改" _mubu_text="%3Cspan%3E%E8%BF%9B%E8%A1%8C%E9%83%A8%E5%88%86%E4%BF%AE%E6%94%B9%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
              <outline text="普通法律" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note="">
                <outline text="全人常制定和修改" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E5%88%B6%E5%AE%9A%E5%92%8C%E4%BF%AE%E6%94%B9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="与基本法律在效力上无高下之分" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%9F%BA%E6%9C%AC%E6%B3%95%E5%BE%8B%E5%9C%A8%E6%95%88%E5%8A%9B%E4%B8%8A%E6%97%A0%E9%AB%98%E4%B8%8B%E4%B9%8B%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="矛盾时，由全人大常裁决" _mubu_text="%3Cspan%3E%E7%9F%9B%E7%9B%BE%E6%97%B6%EF%BC%8C%E7%94%B1%E5%85%A8%E4%BA%BA%E5%A4%A7%E5%B8%B8%E8%A3%81%E5%86%B3%3C/span%3E" _note="视为同一位阶的法的渊源冲突" _mubu_note="%3Cspan%3E%E8%A7%86%E4%B8%BA%E5%90%8C%E4%B8%80%E4%BD%8D%E9%98%B6%E7%9A%84%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%E5%86%B2%E7%AA%81%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="全人大及常作出的具有规范性的决议、决定、规定、办法等" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E5%8F%8A%E5%B8%B8%E4%BD%9C%E5%87%BA%E7%9A%84%E5%85%B7%E6%9C%89%E8%A7%84%E8%8C%83%E6%80%A7%E7%9A%84%E5%86%B3%E8%AE%AE%E3%80%81%E5%86%B3%E5%AE%9A%E3%80%81%E8%A7%84%E5%AE%9A%E3%80%81%E5%8A%9E%E6%B3%95%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="行政法规" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="效力仅次于宪法和法律" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%E4%BB%85%E6%AC%A1%E4%BA%8E%E5%AE%AA%E6%B3%95%E5%92%8C%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="国务院发布决定和命令中具有规范性内容的，也是法律渊源" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%91%E5%B8%83%E5%86%B3%E5%AE%9A%E5%92%8C%E5%91%BD%E4%BB%A4%E4%B8%AD%E5%85%B7%E6%9C%89%E8%A7%84%E8%8C%83%E6%80%A7%E5%86%85%E5%AE%B9%E7%9A%84%EF%BC%8C%E4%B9%9F%E6%98%AF%E6%B3%95%E5%BE%8B%E6%B8%8A%E6%BA%90%3C/span%3E" _note="与行政法规具有同等效力" _mubu_note="%3Cspan%3E%E4%B8%8E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E5%85%B7%E6%9C%89%E5%90%8C%E7%AD%89%E6%95%88%E5%8A%9B%3C/span%3E"/>
            </outline>
            <outline text="行政规章" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%A7%84%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="部门规章" _mubu_text="%3Cspan%3E%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="地方政府规章" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%94%BF%E5%BA%9C%E8%A7%84%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="省级、地级市级" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E3%80%81%E5%9C%B0%E7%BA%A7%E5%B8%82%E7%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                  <outline text="城乡建设与管理、生态文明建设、历史文化保护、基层治理" _mubu_text="%3Cspan%3E%E5%9F%8E%E4%B9%A1%E5%BB%BA%E8%AE%BE%E4%B8%8E%E7%AE%A1%E7%90%86%E3%80%81%E7%94%9F%E6%80%81%E6%96%87%E6%98%8E%E5%BB%BA%E8%AE%BE%E3%80%81%E5%8E%86%E5%8F%B2%E6%96%87%E5%8C%96%E4%BF%9D%E6%8A%A4%E3%80%81%E5%9F%BA%E5%B1%82%E6%B2%BB%E7%90%86%3C/span%3E" _note="“城乡生态历史基”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%9F%8E%E4%B9%A1%E7%94%9F%E6%80%81%E5%8E%86%E5%8F%B2%E5%9F%BA%E2%80%9D%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
            <outline text="地方性法规" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%80%A7%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="省级、地级市级人大及常" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E3%80%81%E5%9C%B0%E7%BA%A7%E5%B8%82%E7%BA%A7%E4%BA%BA%E5%A4%A7%E5%8F%8A%E5%B8%B8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="城乡建设与管理、生态文明建设、历史文化保护、基层治理" _mubu_text="%3Cspan%3E%E5%9F%8E%E4%B9%A1%E5%BB%BA%E8%AE%BE%E4%B8%8E%E7%AE%A1%E7%90%86%E3%80%81%E7%94%9F%E6%80%81%E6%96%87%E6%98%8E%E5%BB%BA%E8%AE%BE%E3%80%81%E5%8E%86%E5%8F%B2%E6%96%87%E5%8C%96%E4%BF%9D%E6%8A%A4%E3%80%81%E5%9F%BA%E5%B1%82%E6%B2%BB%E7%90%86%3C/span%3E" _note="“城乡生态历史基”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%9F%8E%E4%B9%A1%E7%94%9F%E6%80%81%E5%8E%86%E5%8F%B2%E5%9F%BA%E2%80%9D%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="民族自治法规" _mubu_text="%3Cspan%3E%E6%B0%91%E6%97%8F%E8%87%AA%E6%B2%BB%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="民族自治地方的人大制定" _mubu_text="%3Cspan%3E%E6%B0%91%E6%97%8F%E8%87%AA%E6%B2%BB%E5%9C%B0%E6%96%B9%E7%9A%84%E4%BA%BA%E5%A4%A7%E5%88%B6%E5%AE%9A%3C/span%3E" _note="无其常委会" _mubu_note="%3Cspan%3E%E6%97%A0%E5%85%B6%E5%B8%B8%E5%A7%94%E4%BC%9A%3C/span%3E"/>
              <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="除“宪民基专”不得变" _mubu_text="%3Cspan%3E%E9%99%A4%E2%80%9C%E5%AE%AA%E6%B0%91%E5%9F%BA%E4%B8%93%E2%80%9D%E4%B8%8D%E5%BE%97%E5%8F%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
