            <outline text="国家通过一定方式认可的习惯" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E9%80%9A%E8%BF%87%E4%B8%80%E5%AE%9A%E6%96%B9%E5%BC%8F%E8%AE%A4%E5%8F%AF%E7%9A%84%E4%B9%A0%E6%83%AF%3C/span%3E" _note="习惯法" _mubu_note="%3Cspan%3E%E4%B9%A0%E6%83%AF%E6%B3%95%3C/span%3E"/>
