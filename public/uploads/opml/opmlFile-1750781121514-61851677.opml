              </outline>
            </outline>
          </outline>
          <outline text="监督权和获得赔偿权" _mubu_text="%3Cspan%3E%E7%9B%91%E7%9D%A3%E6%9D%83%E5%92%8C%E8%8E%B7%E5%BE%97%E8%B5%94%E5%81%BF%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="批评、建议权" _mubu_text="%3Cspan%3E%E6%89%B9%E8%AF%84%E3%80%81%E5%BB%BA%E8%AE%AE%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="公民对任何国家机关和国家工作人员，有提出批评和建议的权利" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E5%AF%B9%E4%BB%BB%E4%BD%95%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E5%92%8C%E5%9B%BD%E5%AE%B6%E5%B7%A5%E4%BD%9C%E4%BA%BA%E5%91%98%EF%BC%8C%E6%9C%89%E6%8F%90%E5%87%BA%E6%89%B9%E8%AF%84%E5%92%8C%E5%BB%BA%E8%AE%AE%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="检举、控告权" _mubu_text="%3Cspan%3E%E6%A3%80%E4%B8%BE%E3%80%81%E6%8E%A7%E5%91%8A%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="对任何国家机关和国家工作人员的违法失职行为，" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BB%BB%E4%BD%95%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E5%92%8C%E5%9B%BD%E5%AE%B6%E5%B7%A5%E4%BD%9C%E4%BA%BA%E5%91%98%E7%9A%84%E8%BF%9D%E6%B3%95%E5%A4%B1%E8%81%8C%E8%A1%8C%E4%B8%BA%EF%BC%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="有向有关国家机关提出申诉、控告或者检举的权利" _mubu_text="%3Cspan%3E%E6%9C%89%E5%90%91%E6%9C%89%E5%85%B3%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%89%E3%80%81%E6%8E%A7%E5%91%8A%E6%88%96%E8%80%85%E6%A3%80%E4%B8%BE%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="但不得捏造或歪曲事实进行诬告陷害" _mubu_note="%3Cspan%3E%E4%BD%86%E4%B8%8D%E5%BE%97%E6%8D%8F%E9%80%A0%E6%88%96%E6%AD%AA%E6%9B%B2%E4%BA%8B%E5%AE%9E%E8%BF%9B%E8%A1%8C%E8%AF%AC%E5%91%8A%E9%99%B7%E5%AE%B3%3C/span%3E"/>
              <outline text="对公民申诉、控告或检举，必须查清事实，负责处理" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%85%AC%E6%B0%91%E7%94%B3%E8%AF%89%E3%80%81%E6%8E%A7%E5%91%8A%E6%88%96%E6%A3%80%E4%B8%BE%EF%BC%8C%E5%BF%85%E9%A1%BB%E6%9F%A5%E6%B8%85%E4%BA%8B%E5%AE%9E%EF%BC%8C%E8%B4%9F%E8%B4%A3%E5%A4%84%E7%90%86%3C/span%3E" _note="任何人不得压制和打击报复" _mubu_note="%3Cspan%3E%E4%BB%BB%E4%BD%95%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%8E%8B%E5%88%B6%E5%92%8C%E6%89%93%E5%87%BB%E6%8A%A5%E5%A4%8D%3C/span%3E"/>
            </outline>
            <outline text="申诉权" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%89%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="诉讼上的申诉" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%8A%E7%9A%84%E7%94%B3%E8%AF%89%3C/span%3E" _note="对司法机关的生效判决、裁定不服" _mubu_note="%3Cspan%3E%E5%AF%B9%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E7%9A%84%E7%94%9F%E6%95%88%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E4%B8%8D%E6%9C%8D%3C/span%3E"/>
              <outline text="非诉讼上的申诉" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%AF%89%E8%AE%BC%E4%B8%8A%E7%9A%84%E7%94%B3%E8%AF%89%3C/span%3E" _note="对行政机关的决定不服" _mubu_note="%3Cspan%3E%E5%AF%B9%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E7%9A%84%E5%86%B3%E5%AE%9A%E4%B8%8D%E6%9C%8D%3C/span%3E"/>
            </outline>
            <outline text="获得国家赔偿权" _mubu_text="%3Cspan%3E%E8%8E%B7%E5%BE%97%E5%9B%BD%E5%AE%B6%E8%B5%94%E5%81%BF%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="行政赔偿" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="刑事赔偿" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E8%B5%94%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="宗教信仰自由" _mubu_text="%3Cspan%3E%E5%AE%97%E6%95%99%E4%BF%A1%E4%BB%B0%E8%87%AA%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="中国法律的规定" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E6%B3%95%E5%BE%8B%E7%9A%84%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="公民有宗教信仰自由" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E6%9C%89%E5%AE%97%E6%95%99%E4%BF%A1%E4%BB%B0%E8%87%AA%E7%94%B1%3C/span%3E" _note="无传教与办教自由" _mubu_note="%3Cspan%3E%E6%97%A0%E4%BC%A0%E6%95%99%E4%B8%8E%E5%8A%9E%E6%95%99%E8%87%AA%E7%94%B1%3C/span%3E"/>
              <outline text="不得强制公民(不)信仰宗教，不得歧视(不)信仰宗教的公民" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%BC%BA%E5%88%B6%E5%85%AC%E6%B0%91(%E4%B8%8D)%E4%BF%A1%E4%BB%B0%E5%AE%97%E6%95%99%EF%BC%8C%E4%B8%8D%E5%BE%97%E6%AD%A7%E8%A7%86(%E4%B8%8D)%E4%BF%A1%E4%BB%B0%E5%AE%97%E6%95%99%E7%9A%84%E5%85%AC%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="国家保护正常的宗教活动" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%BF%9D%E6%8A%A4%E6%AD%A3%E5%B8%B8%E7%9A%84%E5%AE%97%E6%95%99%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宗教团体和事务不受外国势力支配" _mubu_text="%3Cspan%3E%E5%AE%97%E6%95%99%E5%9B%A2%E4%BD%93%E5%92%8C%E4%BA%8B%E5%8A%A1%E4%B8%8D%E5%8F%97%E5%A4%96%E5%9B%BD%E5%8A%BF%E5%8A%9B%E6%94%AF%E9%85%8D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="人身自由" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="人身自由是一切公民权利和自由的基础和前提" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%E6%98%AF%E4%B8%80%E5%88%87%E5%85%AC%E6%B0%91%E6%9D%83%E5%88%A9%E5%92%8C%E8%87%AA%E7%94%B1%E7%9A%84%E5%9F%BA%E7%A1%80%E5%92%8C%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="生命权" _mubu_text="%3Cspan%3E%E7%94%9F%E5%91%BD%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="最高性，主体自然人" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%80%A7%EF%BC%8C%E4%B8%BB%E4%BD%93%E8%87%AA%E7%84%B6%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="现行宪法未作规定" _mubu_text="%3Cspan%3E%E7%8E%B0%E8%A1%8C%E5%AE%AA%E6%B3%95%E6%9C%AA%E4%BD%9C%E8%A7%84%E5%AE%9A%3C/span%3E" _note="不言自明" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%A8%80%E8%87%AA%E6%98%8E%3C/span%3E"/>
            </outline>
            <outline text="人身自由不受侵犯" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%E4%B8%8D%E5%8F%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="非经检察院批准/决定或法院决定，并由公安执行，不受逮捕" _mubu_text="%3Cspan%3E%E9%9D%9E%E7%BB%8F%E6%A3%80%E5%AF%9F%E9%99%A2%E6%89%B9%E5%87%86/%E5%86%B3%E5%AE%9A%E6%88%96%E6%B3%95%E9%99%A2%E5%86%B3%E5%AE%9A%EF%BC%8C%E5%B9%B6%E7%94%B1%E5%85%AC%E5%AE%89%E6%89%A7%E8%A1%8C%EF%BC%8C%E4%B8%8D%E5%8F%97%E9%80%AE%E6%8D%95%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="禁止非法剥夺或者限制公民人身自由，禁止非法搜查公民身体" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E9%9D%9E%E6%B3%95%E5%89%A5%E5%A4%BA%E6%88%96%E8%80%85%E9%99%90%E5%88%B6%E5%85%AC%E6%B0%91%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%EF%BC%8C%E7%A6%81%E6%AD%A2%E9%9D%9E%E6%B3%95%E6%90%9C%E6%9F%A5%E5%85%AC%E6%B0%91%E8%BA%AB%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="人格尊严不受侵犯" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%A0%BC%E5%B0%8A%E4%B8%A5%E4%B8%8D%E5%8F%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="中华人民共和国公民的人格尊严不受侵犯。禁止采用任何方法对公民进行侮辱、诽谤和诬告陷害(宪法38)" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%8D%8E%E4%BA%BA%E6%B0%91%E5%85%B1%E5%92%8C%E5%9B%BD%E5%85%AC%E6%B0%91%E7%9A%84%E4%BA%BA%E6%A0%BC%E5%B0%8A%E4%B8%A5%E4%B8%8D%E5%8F%97%E4%BE%B5%E7%8A%AF%E3%80%82%E7%A6%81%E6%AD%A2%E9%87%87%E7%94%A8%E4%BB%BB%E4%BD%95%E6%96%B9%E6%B3%95%E5%AF%B9%E5%85%AC%E6%B0%91%E8%BF%9B%E8%A1%8C%E4%BE%AE%E8%BE%B1%E3%80%81%E8%AF%BD%E8%B0%A4%E5%92%8C%E8%AF%AC%E5%91%8A%E9%99%B7%E5%AE%B3(%E5%AE%AA%E6%B3%9538)%3C/span%3E" _note="人格权和人的尊严紧密联系" _mubu_note="%3Cspan%3E%E4%BA%BA%E6%A0%BC%E6%9D%83%E5%92%8C%E4%BA%BA%E7%9A%84%E5%B0%8A%E4%B8%A5%E7%B4%A7%E5%AF%86%E8%81%94%E7%B3%BB%3C/span%3E"/>
            </outline>
            <outline text="住宅不受侵犯" _mubu_text="%3Cspan%3E%E4%BD%8F%E5%AE%85%E4%B8%8D%E5%8F%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="不受非法侵入和搜查" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%97%E9%9D%9E%E6%B3%95%E4%BE%B5%E5%85%A5%E5%92%8C%E6%90%9C%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="核心法益是居住安全和生活安宁" _mubu_text="%3Cspan%3E%E6%A0%B8%E5%BF%83%E6%B3%95%E7%9B%8A%E6%98%AF%E5%B1%85%E4%BD%8F%E5%AE%89%E5%85%A8%E5%92%8C%E7%94%9F%E6%B4%BB%E5%AE%89%E5%AE%81%3C/span%3E" _note="在其中起居生活" _mubu_note="%3Cspan%3E%E5%9C%A8%E5%85%B6%E4%B8%AD%E8%B5%B7%E5%B1%85%E7%94%9F%E6%B4%BB%3C/span%3E"/>
            </outline>
            <outline text="通信自由和通信秘密受法律保护" _mubu_text="%3Cspan%3E%E9%80%9A%E4%BF%A1%E8%87%AA%E7%94%B1%E5%92%8C%E9%80%9A%E4%BF%A1%E7%A7%98%E5%AF%86%E5%8F%97%E6%B3%95%E5%BE%8B%E4%BF%9D%E6%8A%A4%3C/span%3E" _note="" _mubu_note="">
              <outline text="除非因国家安全和追查刑事犯罪的需要并由公检机关进行检查" _mubu_text="%3Cspan%3E%E9%99%A4%E9%9D%9E%E5%9B%A0%E5%9B%BD%E5%AE%B6%E5%AE%89%E5%85%A8%E5%92%8C%E8%BF%BD%E6%9F%A5%E5%88%91%E4%BA%8B%E7%8A%AF%E7%BD%AA%E7%9A%84%E9%9C%80%E8%A6%81%E5%B9%B6%E7%94%B1%E5%85%AC%E6%A3%80%E6%9C%BA%E5%85%B3%E8%BF%9B%E8%A1%8C%E6%A3%80%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检查时，必须遵守法定程序" _mubu_text="%3Cspan%3E%E6%A3%80%E6%9F%A5%E6%97%B6%EF%BC%8C%E5%BF%85%E9%A1%BB%E9%81%B5%E5%AE%88%E6%B3%95%E5%AE%9A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="社会经济文化教育权利" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E7%BB%8F%E6%B5%8E%E6%96%87%E5%8C%96%E6%95%99%E8%82%B2%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="财产权" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="规定" _mubu_text="%3Cspan%3E%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="04年私有财产权入宪" _mubu_text="%3Cspan%3E04%E5%B9%B4%E7%A7%81%E6%9C%89%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%85%A5%E5%AE%AA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公民的合法的私有财产不受侵犯" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E7%9A%84%E5%90%88%E6%B3%95%E7%9A%84%E7%A7%81%E6%9C%89%E8%B4%A2%E4%BA%A7%E4%B8%8D%E5%8F%97%E4%BE%B5%E7%8A%AF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="国家依照法律规定保护公民的私有财产权和继承权" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%BE%9D%E7%85%A7%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E4%BF%9D%E6%8A%A4%E5%85%AC%E6%B0%91%E7%9A%84%E7%A7%81%E6%9C%89%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%92%8C%E7%BB%A7%E6%89%BF%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="国家为公共利益的需要，可依法对私有财产征收或征用并给予补偿" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E4%B8%BA%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E7%9A%84%E9%9C%80%E8%A6%81%EF%BC%8C%E5%8F%AF%E4%BE%9D%E6%B3%95%E5%AF%B9%E7%A7%81%E6%9C%89%E8%B4%A2%E4%BA%A7%E5%BE%81%E6%94%B6%E6%88%96%E5%BE%81%E7%94%A8%E5%B9%B6%E7%BB%99%E4%BA%88%E8%A1%A5%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="规定于&quot;总纲&quot;而非&quot;公民基本权利和义务&quot;部分" _mubu_text="%3Cspan%3E%E8%A7%84%E5%AE%9A%E4%BA%8E%22%E6%80%BB%E7%BA%B2%22%E8%80%8C%E9%9D%9E%22%E5%85%AC%E6%B0%91%E5%9F%BA%E6%9C%AC%E6%9D%83%E5%88%A9%E5%92%8C%E4%B9%89%E5%8A%A1%22%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
