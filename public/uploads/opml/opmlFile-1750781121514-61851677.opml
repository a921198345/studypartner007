              <outline text="不诉及协议免责" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AF%89%E5%8F%8A%E5%8D%8F%E8%AE%AE%E5%85%8D%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="自首、立功免责" _mubu_text="%3Cspan%3E%E8%87%AA%E9%A6%96%E3%80%81%E7%AB%8B%E5%8A%9F%E5%85%8D%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="履行不能而免责" _mubu_text="%3Cspan%3E%E5%B1%A5%E8%A1%8C%E4%B8%8D%E8%83%BD%E8%80%8C%E5%85%8D%E8%B4%A3%3C/span%3E" _note="法律不强人所难" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E4%B8%8D%E5%BC%BA%E4%BA%BA%E6%89%80%E9%9A%BE%3C/span%3E"/>
              <outline text="不可抗力、正当防卫、紧急避险" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E6%8A%97%E5%8A%9B%E3%80%81%E6%AD%A3%E5%BD%93%E9%98%B2%E5%8D%AB%E3%80%81%E7%B4%A7%E6%80%A5%E9%81%BF%E9%99%A9%3C/span%3E" _note="一般直接当无责" _mubu_note="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%9B%B4%E6%8E%A5%E5%BD%93%E6%97%A0%E8%B4%A3%3C/span%3E">
                <outline text="免责前提是有责可免" _mubu_text="%3Cspan%3E%E5%85%8D%E8%B4%A3%E5%89%8D%E6%8F%90%E6%98%AF%E6%9C%89%E8%B4%A3%E5%8F%AF%E5%85%8D%3C/span%3E" _note="无刑责能力、正当防卫、紧急避险等均不属于免责" _mubu_note="%3Cspan%3E%E6%97%A0%E5%88%91%E8%B4%A3%E8%83%BD%E5%8A%9B%E3%80%81%E6%AD%A3%E5%BD%93%E9%98%B2%E5%8D%AB%E3%80%81%E7%B4%A7%E6%80%A5%E9%81%BF%E9%99%A9%E7%AD%89%E5%9D%87%E4%B8%8D%E5%B1%9E%E4%BA%8E%E5%85%8D%E8%B4%A3%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="法律责任与法律制裁" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%E4%B8%8E%E6%B3%95%E5%BE%8B%E5%88%B6%E8%A3%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="有法律责任不一定有法律制裁，有法律制裁一定有法律责任" _mubu_text="%3Cspan%3E%E6%9C%89%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%E4%B8%8D%E4%B8%80%E5%AE%9A%E6%9C%89%E6%B3%95%E5%BE%8B%E5%88%B6%E8%A3%81%EF%BC%8C%E6%9C%89%E6%B3%95%E5%BE%8B%E5%88%B6%E8%A3%81%E4%B8%80%E5%AE%9A%E6%9C%89%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律制裁分类" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%88%B6%E8%A3%81%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="刑事制裁、民事制裁、行政制裁(行政处罚/行政处分）、违宪制裁" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E5%88%B6%E8%A3%81%E3%80%81%E6%B0%91%E4%BA%8B%E5%88%B6%E8%A3%81%E3%80%81%E8%A1%8C%E6%94%BF%E5%88%B6%E8%A3%81(%E8%A1%8C%E6%94%BF%E5%A4%84%E7%BD%9A/%E8%A1%8C%E6%94%BF%E5%A4%84%E5%88%86%EF%BC%89%E3%80%81%E8%BF%9D%E5%AE%AA%E5%88%B6%E8%A3%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="法的运行" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E8%BF%90%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
        <outline text="法的实施" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
          <outline text="法的实施与法的实现" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%E4%B8%8E%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%3C/span%3E" _note="" _mubu_note="">
            <outline text="法的实施的概念" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%E7%9A%84%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
              <outline text="从抽象变具体、从应然变实然" _mubu_text="%3Cspan%3E%E4%BB%8E%E6%8A%BD%E8%B1%A1%E5%8F%98%E5%85%B7%E4%BD%93%E3%80%81%E4%BB%8E%E5%BA%94%E7%84%B6%E5%8F%98%E5%AE%9E%E7%84%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律的遵守（守法）" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E9%81%B5%E5%AE%88%EF%BC%88%E5%AE%88%E6%B3%95%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律的执行（执法）" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E6%89%A7%E8%A1%8C%EF%BC%88%E6%89%A7%E6%B3%95%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律的适用（司法）" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E9%80%82%E7%94%A8%EF%BC%88%E5%8F%B8%E6%B3%95%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法律实现" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%AE%9E%E7%8E%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="法律实现的概念" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%AE%9E%E7%8E%B0%E7%9A%84%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
                <outline text="实现法律规范所设定的权义，达到合目的性的结果" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%8E%B0%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E6%89%80%E8%AE%BE%E5%AE%9A%E7%9A%84%E6%9D%83%E4%B9%89%EF%BC%8C%E8%BE%BE%E5%88%B0%E5%90%88%E7%9B%AE%E7%9A%84%E6%80%A7%E7%9A%84%E7%BB%93%E6%9E%9C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法的实现与法的实施的区别" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%E4%B8%8E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%E7%9A%84%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
                <outline text="法的实施强调过程，法的实现强调结果" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%E5%BC%BA%E8%B0%83%E8%BF%87%E7%A8%8B%EF%BC%8C%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%E5%BC%BA%E8%B0%83%E7%BB%93%E6%9E%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法的实施是手段，法的实现是目的" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E6%96%BD%E6%98%AF%E6%89%8B%E6%AE%B5%EF%BC%8C%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%E6%98%AF%E7%9B%AE%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法的实现与法的效果的区别" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%E4%B8%8E%E6%B3%95%E7%9A%84%E6%95%88%E6%9E%9C%E7%9A%84%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
                <outline text="指涉对象" _mubu_text="%3Cspan%3E%E6%8C%87%E6%B6%89%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法的实现：法律所规定的权义转化为现实" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%AE%9E%E7%8E%B0%EF%BC%9A%E6%B3%95%E5%BE%8B%E6%89%80%E8%A7%84%E5%AE%9A%E7%9A%84%E6%9D%83%E4%B9%89%E8%BD%AC%E5%8C%96%E4%B8%BA%E7%8E%B0%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="法的效果：法律在现实生活中实际被人们遵守和实施的状态" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E6%95%88%E6%9E%9C%EF%BC%9A%E6%B3%95%E5%BE%8B%E5%9C%A8%E7%8E%B0%E5%AE%9E%E7%94%9F%E6%B4%BB%E4%B8%AD%E5%AE%9E%E9%99%85%E8%A2%AB%E4%BA%BA%E4%BB%AC%E9%81%B5%E5%AE%88%E5%92%8C%E5%AE%9E%E6%96%BD%E7%9A%84%E7%8A%B6%E6%80%81%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="评价后果" _mubu_text="%3Cspan%3E%E8%AF%84%E4%BB%B7%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="法律的实现一定是正面效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E5%AE%9E%E7%8E%B0%E4%B8%80%E5%AE%9A%E6%98%AF%E6%AD%A3%E9%9D%A2%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="法律的实效则可好可坏" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E5%AE%9E%E6%95%88%E5%88%99%E5%8F%AF%E5%A5%BD%E5%8F%AF%E5%9D%8F%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="执法" _mubu_text="%3Cspan%3E%E6%89%A7%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
            <outline text="含义" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="狭义的执法仅指行政机关执行法律的活动" _mubu_text="%3Cspan%3E%E7%8B%AD%E4%B9%89%E7%9A%84%E6%89%A7%E6%B3%95%E4%BB%85%E6%8C%87%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E6%89%A7%E8%A1%8C%E6%B3%95%E5%BE%8B%E7%9A%84%E6%B4%BB%E5%8A%A8%3C/span%3E" _note="即行政执法" _mubu_note="%3Cspan%3E%E5%8D%B3%E8%A1%8C%E6%94%BF%E6%89%A7%E6%B3%95%3C/span%3E"/>
            </outline>
            <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="国家权威性" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%9D%83%E5%A8%81%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="以国家的名义对社会进行全面管理" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%9B%BD%E5%AE%B6%E7%9A%84%E5%90%8D%E4%B9%89%E5%AF%B9%E7%A4%BE%E4%BC%9A%E8%BF%9B%E8%A1%8C%E5%85%A8%E9%9D%A2%E7%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="主体的特定性" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E7%9A%84%E7%89%B9%E5%AE%9A%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="主体是国家行政机关及其公职人员" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E6%98%AF%E5%9B%BD%E5%AE%B6%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E5%8F%8A%E5%85%B6%E5%85%AC%E8%81%8C%E4%BA%BA%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="国家强制性" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="以国家强制力为后盾" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E5%8A%9B%E4%B8%BA%E5%90%8E%E7%9B%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="主动性和单方面性" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E6%80%A7%E5%92%8C%E5%8D%95%E6%96%B9%E9%9D%A2%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="执法权可以主动行使，无需行政相对人的申请和同意" _mubu_text="%3Cspan%3E%E6%89%A7%E6%B3%95%E6%9D%83%E5%8F%AF%E4%BB%A5%E4%B8%BB%E5%8A%A8%E8%A1%8C%E4%BD%BF%EF%BC%8C%E6%97%A0%E9%9C%80%E8%A1%8C%E6%94%BF%E7%9B%B8%E5%AF%B9%E4%BA%BA%E7%9A%84%E7%94%B3%E8%AF%B7%E5%92%8C%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="依法行政原则(最基本原则)" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E8%A1%8C%E6%94%BF%E5%8E%9F%E5%88%99(%E6%9C%80%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99)%3C/span%3E" _note="主体、内容、程序合法" _mubu_note="%3Cspan%3E%E4%B8%BB%E4%BD%93%E3%80%81%E5%86%85%E5%AE%B9%E3%80%81%E7%A8%8B%E5%BA%8F%E5%90%88%E6%B3%95%3C/span%3E"/>
              <outline text="讲求效率原则" _mubu_text="%3Cspan%3E%E8%AE%B2%E6%B1%82%E6%95%88%E7%8E%87%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="公平合理原则" _mubu_text="%3Cspan%3E%E5%85%AC%E5%B9%B3%E5%90%88%E7%90%86%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="司法" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8F%B8%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
            <outline text="司法的含义" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%9A%84%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="法的适用" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
                <outline text="司法机关在中国专指人民法院和人民检察院" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E5%9C%A8%E4%B8%AD%E5%9B%BD%E4%B8%93%E6%8C%87%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E5%92%8C%E4%BA%BA%E6%B0%91%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="我国的司法权包括审判权和检察权" _mubu_text="%3Cspan%3E%E6%88%91%E5%9B%BD%E7%9A%84%E5%8F%B8%E6%B3%95%E6%9D%83%E5%8C%85%E6%8B%AC%E5%AE%A1%E5%88%A4%E6%9D%83%E5%92%8C%E6%A3%80%E5%AF%9F%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="司法的特征" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%9A%84%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="国家权威性" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%9D%83%E5%A8%81%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="国家强制性" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="严格的程序性及合法性" _mubu_text="%3Cspan%3E%E4%B8%A5%E6%A0%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%E6%80%A7%E5%8F%8A%E5%90%88%E6%B3%95%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
