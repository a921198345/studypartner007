            </outline>
            <outline text="党的领导原则" _mubu_text="%3Cspan%3E%E5%85%9A%E7%9A%84%E9%A2%86%E5%AF%BC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="党按照总揽全局协调各方的原则" _mubu_text="%3Cspan%3E%E5%85%9A%E6%8C%89%E7%85%A7%E6%80%BB%E6%8F%BD%E5%85%A8%E5%B1%80%E5%8D%8F%E8%B0%83%E5%90%84%E6%96%B9%E7%9A%84%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="在同级各种组织、在中央和地方国家机关中的领导作用" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%90%8C%E7%BA%A7%E5%90%84%E7%A7%8D%E7%BB%84%E7%BB%87%E3%80%81%E5%9C%A8%E4%B8%AD%E5%A4%AE%E5%92%8C%E5%9C%B0%E6%96%B9%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E4%B8%AD%E7%9A%84%E9%A2%86%E5%AF%BC%E4%BD%9C%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="把党的领导贯彻到党和国家机构履行职责全过程" _mubu_text="%3Cspan%3E%E6%8A%8A%E5%85%9A%E7%9A%84%E9%A2%86%E5%AF%BC%E8%B4%AF%E5%BD%BB%E5%88%B0%E5%85%9A%E5%92%8C%E5%9B%BD%E5%AE%B6%E6%9C%BA%E6%9E%84%E5%B1%A5%E8%A1%8C%E8%81%8C%E8%B4%A3%E5%85%A8%E8%BF%87%E7%A8%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法治原则" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="依法设立、依法办事、违法必究" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E8%AE%BE%E7%AB%8B%E3%80%81%E4%BE%9D%E6%B3%95%E5%8A%9E%E4%BA%8B%E3%80%81%E8%BF%9D%E6%B3%95%E5%BF%85%E7%A9%B6%3C/span%3E" _note="" _mubu_note=""/>
