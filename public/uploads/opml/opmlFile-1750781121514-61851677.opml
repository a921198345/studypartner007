                <outline text="司法机关与行政机关" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E4%B8%8E%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
                <outline text="司法活动的对象仅限于案件" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E7%9A%84%E5%AF%B9%E8%B1%A1%E4%BB%85%E9%99%90%E4%BA%8E%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="执法活动的对象远比司法广泛" _mubu_text="%3Cspan%3E%E6%89%A7%E6%B3%95%E6%B4%BB%E5%8A%A8%E7%9A%84%E5%AF%B9%E8%B1%A1%E8%BF%9C%E6%AF%94%E5%8F%B8%E6%B3%95%E5%B9%BF%E6%B3%9B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="程序性要求" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E6%80%A7%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="司法活动有严格的程序性要求" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E6%9C%89%E4%B8%A5%E6%A0%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%E6%80%A7%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="执法活动没有司法活动那样严格和细致" _mubu_text="%3Cspan%3E%E6%89%A7%E6%B3%95%E6%B4%BB%E5%8A%A8%E6%B2%A1%E6%9C%89%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E9%82%A3%E6%A0%B7%E4%B8%A5%E6%A0%BC%E5%92%8C%E7%BB%86%E8%87%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="主动性" _mubu_text="%3Cspan%3E%E4%B8%BB%E5%8A%A8%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="司法活动具有被动性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E5%85%B7%E6%9C%89%E8%A2%AB%E5%8A%A8%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="执法活动具有较强的主动性" _mubu_text="%3Cspan%3E%E6%89%A7%E6%B3%95%E6%B4%BB%E5%8A%A8%E5%85%B7%E6%9C%89%E8%BE%83%E5%BC%BA%E7%9A%84%E4%B8%BB%E5%8A%A8%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="司法的原则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%9A%84%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="司法法治原则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B3%95%E6%B2%BB%E5%8E%9F%E5%88%99%3C/span%3E" _note="即以事实为依据，以法律为准绳" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%BB%A5%E4%BA%8B%E5%AE%9E%E4%B8%BA%E4%BE%9D%E6%8D%AE%EF%BC%8C%E4%BB%A5%E6%B3%95%E5%BE%8B%E4%B8%BA%E5%87%86%E7%BB%B3%3C/span%3E"/>
              <outline text="司法平等原则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E5%B9%B3%E7%AD%89%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
                <outline text="平等地享受权利、履行义务、承担责任" _mubu_text="%3Cspan%3E%E5%B9%B3%E7%AD%89%E5%9C%B0%E4%BA%AB%E5%8F%97%E6%9D%83%E5%88%A9%E3%80%81%E5%B1%A5%E8%A1%8C%E4%B9%89%E5%8A%A1%E3%80%81%E6%89%BF%E6%8B%85%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="司法机关独立行使职权原则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%E7%8B%AC%E7%AB%8B%E8%A1%8C%E4%BD%BF%E8%81%8C%E6%9D%83%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
