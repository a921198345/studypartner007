                <outline text="除有关外交、国防和其他按基本法规定不属于自治范围的" _mubu_text="%3Cspan%3E%E9%99%A4%E6%9C%89%E5%85%B3%E5%A4%96%E4%BA%A4%E3%80%81%E5%9B%BD%E9%98%B2%E5%92%8C%E5%85%B6%E4%BB%96%E6%8C%89%E5%9F%BA%E6%9C%AC%E6%B3%95%E8%A7%84%E5%AE%9A%E4%B8%8D%E5%B1%9E%E4%BA%8E%E8%87%AA%E6%B2%BB%E8%8C%83%E5%9B%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="独立的司法权和终审权" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E7%9A%84%E5%8F%B8%E6%B3%95%E6%9D%83%E5%92%8C%E7%BB%88%E5%AE%A1%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
