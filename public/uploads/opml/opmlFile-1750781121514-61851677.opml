              <outline text="效力相当于法律" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%E7%9B%B8%E5%BD%93%E4%BA%8E%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="特别行政区的法律" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E8%A1%8C%E6%94%BF%E5%8C%BA%E7%9A%84%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="须报全人常备案" _mubu_text="%3Cspan%3E%E9%A1%BB%E6%8A%A5%E5%85%A8%E4%BA%BA%E5%B8%B8%E5%A4%87%E6%A1%88%3C/span%3E" _note="备案不影响生效" _mubu_note="%3Cspan%3E%E5%A4%87%E6%A1%88%E4%B8%8D%E5%BD%B1%E5%93%8D%E7%94%9F%E6%95%88%3C/span%3E"/>
            </outline>
            <outline text="国际条约、国际惯例" _mubu_text="%3Cspan%3E%E5%9B%BD%E9%99%85%E6%9D%A1%E7%BA%A6%E3%80%81%E5%9B%BD%E9%99%85%E6%83%AF%E4%BE%8B%3C/span%3E" _note="" _mubu_note="">
