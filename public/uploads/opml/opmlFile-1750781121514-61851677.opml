                  <outline text="2/3以上的组成人员参加方可召开" _mubu_text="%3Cspan%3E2/3%E4%BB%A5%E4%B8%8A%E7%9A%84%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%98%E5%8F%82%E5%8A%A0%E6%96%B9%E5%8F%AF%E5%8F%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="所作决定经到会人员的过半数同意" _mubu_text="%3Cspan%3E%E6%89%80%E4%BD%9C%E5%86%B3%E5%AE%9A%E7%BB%8F%E5%88%B0%E4%BC%9A%E4%BA%BA%E5%91%98%E7%9A%84%E8%BF%87%E5%8D%8A%E6%95%B0%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="村规民约" _mubu_text="%3Cspan%3E%E6%9D%91%E8%A7%84%E6%B0%91%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="村民会议制定" _mubu_text="%3Cspan%3E%E6%9D%91%E6%B0%91%E4%BC%9A%E8%AE%AE%E5%88%B6%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="报乡镇政府备案" _mubu_text="%3Cspan%3E%E6%8A%A5%E4%B9%A1%E9%95%87%E6%94%BF%E5%BA%9C%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
