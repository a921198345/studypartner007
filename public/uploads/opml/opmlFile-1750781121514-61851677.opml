            </outline>
          </outline>
          <outline text="守法" _mubu_text="%3Cspan%3E%E5%AE%88%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
            <outline text="守法的含义" _mubu_text="%3Cspan%3E%E5%AE%88%E6%B3%95%E7%9A%84%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="包括积极守法和消极守法" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E7%A7%AF%E6%9E%81%E5%AE%88%E6%B3%95%E5%92%8C%E6%B6%88%E6%9E%81%E5%AE%88%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="守法的构成要素" _mubu_text="%3Cspan%3E%E5%AE%88%E6%B3%95%E7%9A%84%E6%9E%84%E6%88%90%E8%A6%81%E7%B4%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="主体：一切国家机关、社会组织和个人" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E4%B8%80%E5%88%87%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%E3%80%81%E7%A4%BE%E4%BC%9A%E7%BB%84%E7%BB%87%E5%92%8C%E4%B8%AA%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="范围：一切规范性法律文件与非规范性法律文件" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%EF%BC%9A%E4%B8%80%E5%88%87%E8%A7%84%E8%8C%83%E6%80%A7%E6%B3%95%E5%BE%8B%E6%96%87%E4%BB%B6%E4%B8%8E%E9%9D%9E%E8%A7%84%E8%8C%83%E6%80%A7%E6%B3%95%E5%BE%8B%E6%96%87%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="内容：正确行使法律权利，切实履行法律义务" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%EF%BC%9A%E6%AD%A3%E7%A1%AE%E8%A1%8C%E4%BD%BF%E6%B3%95%E5%BE%8B%E6%9D%83%E5%88%A9%EF%BC%8C%E5%88%87%E5%AE%9E%E5%B1%A5%E8%A1%8C%E6%B3%95%E5%BE%8B%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="法律监督" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note="">
