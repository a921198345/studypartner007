                </outline>
              </outline>
              <outline text="法律不针对思想" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E4%B8%8D%E9%92%88%E5%AF%B9%E6%80%9D%E6%83%B3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只调整人们的涉他行为" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%B0%83%E6%95%B4%E4%BA%BA%E4%BB%AC%E7%9A%84%E6%B6%89%E4%BB%96%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="关系行为" _mubu_note="%3Cspan%3E%E5%85%B3%E7%B3%BB%E8%A1%8C%E4%B8%BA%3C/span%3E">
                <outline text="自我指向的行动法律在原则上并不会针对" _mubu_text="%3Cspan%3E%E8%87%AA%E6%88%91%E6%8C%87%E5%90%91%E7%9A%84%E8%A1%8C%E5%8A%A8%E6%B3%95%E5%BE%8B%E5%9C%A8%E5%8E%9F%E5%88%99%E4%B8%8A%E5%B9%B6%E4%B8%8D%E4%BC%9A%E9%92%88%E5%AF%B9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="公权力机构制定或认可的具有特定形式的" _mubu_text="%3Cspan%3E%E5%85%AC%E6%9D%83%E5%8A%9B%E6%9C%BA%E6%9E%84%E5%88%B6%E5%AE%9A%E6%88%96%E8%AE%A4%E5%8F%AF%E7%9A%84%E5%85%B7%E6%9C%89%E7%89%B9%E5%AE%9A%E5%BD%A2%E5%BC%8F%E7%9A%84%3C/span%3E" _note="国家意志性" _mubu_note="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%84%8F%E5%BF%97%E6%80%A7%3C/span%3E">
              <outline text="国家意志体现的两种通道" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%84%8F%E5%BF%97%E4%BD%93%E7%8E%B0%E7%9A%84%E4%B8%A4%E7%A7%8D%E9%80%9A%E9%81%93%3C/span%3E" _note="" _mubu_note="">
                <outline text="制定" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="认可" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%8F%AF%3C/span%3E" _note="" _mubu_note="">
                  <outline text="认可的两种途径" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%8F%AF%E7%9A%84%E4%B8%A4%E7%A7%8D%E9%80%94%E5%BE%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="明示" _mubu_text="%3Cspan%3E%E6%98%8E%E7%A4%BA%3C/span%3E" _note="" _mubu_note="">
                      <outline text="立法机关" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%B3%95%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="具普遍约束力" _mubu_text="%3Cspan%3E%E5%85%B7%E6%99%AE%E9%81%8D%E7%BA%A6%E6%9D%9F%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="默示" _mubu_text="%3Cspan%3E%E9%BB%98%E7%A4%BA%3C/span%3E" _note="" _mubu_note="">
                      <outline text="司法机关" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="不具普遍约束力" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%B7%E6%99%AE%E9%81%8D%E7%BA%A6%E6%9D%9F%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                </outline>
              </outline>
            </outline>
            <outline text="具有普遍性的" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%99%AE%E9%81%8D%E6%80%A7%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="前提：在一国主权管辖范围内" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%EF%BC%9A%E5%9C%A8%E4%B8%80%E5%9B%BD%E4%B8%BB%E6%9D%83%E7%AE%A1%E8%BE%96%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note="">
                <outline text="具有民族性、地域性" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%B0%91%E6%97%8F%E6%80%A7%E3%80%81%E5%9C%B0%E5%9F%9F%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="蕴含了法律面前人人平等的内涵" _mubu_text="%3Cspan%3E%E8%95%B4%E5%90%AB%E4%BA%86%E6%B3%95%E5%BE%8B%E9%9D%A2%E5%89%8D%E4%BA%BA%E4%BA%BA%E5%B9%B3%E7%AD%89%E7%9A%84%E5%86%85%E6%B6%B5%3C/span%3E" _note="近代以来" _mubu_note="%3Cspan%3E%E8%BF%91%E4%BB%A3%E4%BB%A5%E6%9D%A5%3C/span%3E"/>
              <outline text="与人类的普遍要求相一致的趋向" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%BA%BA%E7%B1%BB%E7%9A%84%E6%99%AE%E9%81%8D%E8%A6%81%E6%B1%82%E7%9B%B8%E4%B8%80%E8%87%B4%E7%9A%84%E8%B6%8B%E5%90%91%3C/span%3E" _note="近代以来" _mubu_note="%3Cspan%3E%E8%BF%91%E4%BB%A3%E4%BB%A5%E6%9D%A5%3C/span%3E"/>
            </outline>
            <outline text="以权利义务为内容的" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%86%85%E5%AE%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="自然规范和技术规范不以权利和义务为内容" _mubu_text="%3Cspan%3E%E8%87%AA%E7%84%B6%E8%A7%84%E8%8C%83%E5%92%8C%E6%8A%80%E6%9C%AF%E8%A7%84%E8%8C%83%E4%B8%8D%E4%BB%A5%E6%9D%83%E5%88%A9%E5%92%8C%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律之外的社会规范通常只以义务为内容" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E4%B9%8B%E5%A4%96%E7%9A%84%E7%A4%BE%E4%BC%9A%E8%A7%84%E8%8C%83%E9%80%9A%E5%B8%B8%E5%8F%AA%E4%BB%A5%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法律的内容主要表现为权利和义务" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%9A%84%E5%86%85%E5%AE%B9%E4%B8%BB%E8%A6%81%E8%A1%A8%E7%8E%B0%E4%B8%BA%E6%9D%83%E5%88%A9%E5%92%8C%E4%B9%89%E5%8A%A1%3C/span%3E" _note="现代社会主要秉持权利本位" _mubu_note="%3Cspan%3E%E7%8E%B0%E4%BB%A3%E7%A4%BE%E4%BC%9A%E4%B8%BB%E8%A6%81%E7%A7%89%E6%8C%81%E6%9D%83%E5%88%A9%E6%9C%AC%E4%BD%8D%3C/span%3E"/>
            </outline>
            <outline text="以国家强制力为后盾，通过法律程序保证实现" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E5%8A%9B%E4%B8%BA%E5%90%8E%E7%9B%BE%EF%BC%8C%E9%80%9A%E8%BF%87%E6%B3%95%E5%BE%8B%E7%A8%8B%E5%BA%8F%E4%BF%9D%E8%AF%81%E5%AE%9E%E7%8E%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="一切社会规范都具有强制性" _mubu_text="%3Cspan%3E%E4%B8%80%E5%88%87%E7%A4%BE%E4%BC%9A%E8%A7%84%E8%8C%83%E9%83%BD%E5%85%B7%E6%9C%89%E5%BC%BA%E5%88%B6%E6%80%A7%3C/span%3E" _note="道德、习惯，宗教等" _mubu_note="%3Cspan%3E%E9%81%93%E5%BE%B7%E3%80%81%E4%B9%A0%E6%83%AF%EF%BC%8C%E5%AE%97%E6%95%99%E7%AD%89%3C/span%3E"/>
              <outline text="唯有法律具备国家强制性" _mubu_text="%3Cspan%3E%E5%94%AF%E6%9C%89%E6%B3%95%E5%BE%8B%E5%85%B7%E5%A4%87%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E6%80%A7%3C/span%3E" _note="即以国家暴力保证实施" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%BB%A5%E5%9B%BD%E5%AE%B6%E6%9A%B4%E5%8A%9B%E4%BF%9D%E8%AF%81%E5%AE%9E%E6%96%BD%3C/span%3E">
                <outline text="相比其他社会规范，法律具有最强的强制力" _mubu_text="%3Cspan%3E%E7%9B%B8%E6%AF%94%E5%85%B6%E4%BB%96%E7%A4%BE%E4%BC%9A%E8%A7%84%E8%8C%83%EF%BC%8C%E6%B3%95%E5%BE%8B%E5%85%B7%E6%9C%89%E6%9C%80%E5%BC%BA%E7%9A%84%E5%BC%BA%E5%88%B6%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="为最终的保障力量存在" _mubu_text="%3Cspan%3E%E4%B8%BA%E6%9C%80%E7%BB%88%E7%9A%84%E4%BF%9D%E9%9A%9C%E5%8A%9B%E9%87%8F%E5%AD%98%E5%9C%A8%3C/span%3E" _note="被违反之后才会显现" _mubu_note="%3Cspan%3E%E8%A2%AB%E8%BF%9D%E5%8F%8D%E4%B9%8B%E5%90%8E%E6%89%8D%E4%BC%9A%E6%98%BE%E7%8E%B0%3C/span%3E"/>
              </outline>
              <outline text="原因" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="需要通过国家强制力强迫人们遵行" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E9%80%9A%E8%BF%87%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E5%8A%9B%E5%BC%BA%E8%BF%AB%E4%BA%BA%E4%BB%AC%E9%81%B5%E8%A1%8C%3C/span%3E" _note="无法自愿遵守" _mubu_note="%3Cspan%3E%E6%97%A0%E6%B3%95%E8%87%AA%E6%84%BF%E9%81%B5%E5%AE%88%3C/span%3E"/>
                <outline text="需要国家专门机关予以运用" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E5%9B%BD%E5%AE%B6%E4%B8%93%E9%97%A8%E6%9C%BA%E5%85%B3%E4%BA%88%E4%BB%A5%E8%BF%90%E7%94%A8%3C/span%3E" _note="不能自行实施" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E8%87%AA%E8%A1%8C%E5%AE%9E%E6%96%BD%3C/span%3E"/>
              </outline>
              <outline text="国家强制性需要具备合法性与合理性基础" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E6%80%A7%E9%9C%80%E8%A6%81%E5%85%B7%E5%A4%87%E5%90%88%E6%B3%95%E6%80%A7%E4%B8%8E%E5%90%88%E7%90%86%E6%80%A7%E5%9F%BA%E7%A1%80%3C/span%3E" _note="并非纯粹的暴力" _mubu_note="%3Cspan%3E%E5%B9%B6%E9%9D%9E%E7%BA%AF%E7%B2%B9%E7%9A%84%E6%9A%B4%E5%8A%9B%3C/span%3E"/>
            </outline>
            <outline text="正当程序性" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BD%93%E7%A8%8B%E5%BA%8F%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="天然地强调程序的价值" _mubu_text="%3Cspan%3E%E5%A4%A9%E7%84%B6%E5%9C%B0%E5%BC%BA%E8%B0%83%E7%A8%8B%E5%BA%8F%E7%9A%84%E4%BB%B7%E5%80%BC%3C/span%3E" _note="" _mubu_note="">
                <outline text="程序对于效率的保障" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E5%AF%B9%E4%BA%8E%E6%95%88%E7%8E%87%E7%9A%84%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="程序对于法律权威的保障" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E5%AF%B9%E4%BA%8E%E6%B3%95%E5%BE%8B%E6%9D%83%E5%A8%81%E7%9A%84%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="程序对于人权的保障" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E5%AF%B9%E4%BA%8E%E4%BA%BA%E6%9D%83%E7%9A%84%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="程序对于正义的保障" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E5%AF%B9%E4%BA%8E%E6%AD%A3%E4%B9%89%E7%9A%84%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="同时包括实体正义与程序正义" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%8C%85%E6%8B%AC%E5%AE%9E%E4%BD%93%E6%AD%A3%E4%B9%89%E4%B8%8E%E7%A8%8B%E5%BA%8F%E6%AD%A3%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="同等重要，如车之两轮，鸟之两翼，相辅相成，不可偏废" _mubu_text="%3Cspan%3E%E5%90%8C%E7%AD%89%E9%87%8D%E8%A6%81%EF%BC%8C%E5%A6%82%E8%BD%A6%E4%B9%8B%E4%B8%A4%E8%BD%AE%EF%BC%8C%E9%B8%9F%E4%B9%8B%E4%B8%A4%E7%BF%BC%EF%BC%8C%E7%9B%B8%E8%BE%85%E7%9B%B8%E6%88%90%EF%BC%8C%E4%B8%8D%E5%8F%AF%E5%81%8F%E5%BA%9F%3C/span%3E" _note="分别都有独立性价值" _mubu_note="%3Cspan%3E%E5%88%86%E5%88%AB%E9%83%BD%E6%9C%89%E7%8B%AC%E7%AB%8B%E6%80%A7%E4%BB%B7%E5%80%BC%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
            <outline text="可诉性" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%AF%89%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="在诉讼中被人们作为相应行动依据使用" _mubu_text="%3Cspan%3E%E5%9C%A8%E8%AF%89%E8%AE%BC%E4%B8%AD%E8%A2%AB%E4%BA%BA%E4%BB%AC%E4%BD%9C%E4%B8%BA%E7%9B%B8%E5%BA%94%E8%A1%8C%E5%8A%A8%E4%BE%9D%E6%8D%AE%E4%BD%BF%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可争讼性" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BA%89%E8%AE%BC%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="当事人将法律作为起诉或应诉的依据" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%B0%86%E6%B3%95%E5%BE%8B%E4%BD%9C%E4%B8%BA%E8%B5%B7%E8%AF%89%E6%88%96%E5%BA%94%E8%AF%89%E7%9A%84%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="可裁判性" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%A3%81%E5%88%A4%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院或法官将法律作为案件裁判的依据" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%88%96%E6%B3%95%E5%AE%98%E5%B0%86%E6%B3%95%E5%BE%8B%E4%BD%9C%E4%B8%BA%E6%A1%88%E4%BB%B6%E8%A3%81%E5%88%A4%E7%9A%84%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="反映法律被实际运用的可能性，也是其是否具备活力的标志" _mubu_text="%3Cspan%3E%E5%8F%8D%E6%98%A0%E6%B3%95%E5%BE%8B%E8%A2%AB%E5%AE%9E%E9%99%85%E8%BF%90%E7%94%A8%E7%9A%84%E5%8F%AF%E8%83%BD%E6%80%A7%EF%BC%8C%E4%B9%9F%E6%98%AF%E5%85%B6%E6%98%AF%E5%90%A6%E5%85%B7%E5%A4%87%E6%B4%BB%E5%8A%9B%E7%9A%84%E6%A0%87%E5%BF%97%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宪法因未被司法化，因此不具备可诉性" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E5%9B%A0%E6%9C%AA%E8%A2%AB%E5%8F%B8%E6%B3%95%E5%8C%96%EF%BC%8C%E5%9B%A0%E6%AD%A4%E4%B8%8D%E5%85%B7%E5%A4%87%E5%8F%AF%E8%AF%89%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="法的作用" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E4%BD%9C%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
