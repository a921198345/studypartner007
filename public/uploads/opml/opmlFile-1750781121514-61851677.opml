          <outline text="实质特征" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="是公民权利的保障书" _mubu_text="%3Cspan%3E%E6%98%AF%E5%85%AC%E6%B0%91%E6%9D%83%E5%88%A9%E7%9A%84%E4%BF%9D%E9%9A%9C%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="是民主制度法律化的基本形式" _mubu_text="%3Cspan%3E%E6%98%AF%E6%B0%91%E4%B8%BB%E5%88%B6%E5%BA%A6%E6%B3%95%E5%BE%8B%E5%8C%96%E7%9A%84%E5%9F%BA%E6%9C%AC%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="规定了代议制和普选制" _mubu_text="%3Cspan%3E%E8%A7%84%E5%AE%9A%E4%BA%86%E4%BB%A3%E8%AE%AE%E5%88%B6%E5%92%8C%E6%99%AE%E9%80%89%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="赋予人民广泛的权利" _mubu_text="%3Cspan%3E%E8%B5%8B%E4%BA%88%E4%BA%BA%E6%B0%91%E5%B9%BF%E6%B3%9B%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="具体规范了国家机构的职权和行使程序" _mubu_text="%3Cspan%3E%E5%85%B7%E4%BD%93%E8%A7%84%E8%8C%83%E4%BA%86%E5%9B%BD%E5%AE%B6%E6%9C%BA%E6%9E%84%E7%9A%84%E8%81%8C%E6%9D%83%E5%92%8C%E8%A1%8C%E4%BD%BF%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="是各种政治力量对比关系的集中体现" _mubu_text="%3Cspan%3E%E6%98%AF%E5%90%84%E7%A7%8D%E6%94%BF%E6%B2%BB%E5%8A%9B%E9%87%8F%E5%AF%B9%E6%AF%94%E5%85%B3%E7%B3%BB%E7%9A%84%E9%9B%86%E4%B8%AD%E4%BD%93%E7%8E%B0%3C/span%3E" _note="" _mubu_note="">
              <outline text="是统治阶级巩固自身优势地位的法律武器" _mubu_text="%3Cspan%3E%E6%98%AF%E7%BB%9F%E6%B2%BB%E9%98%B6%E7%BA%A7%E5%B7%A9%E5%9B%BA%E8%87%AA%E8%BA%AB%E4%BC%98%E5%8A%BF%E5%9C%B0%E4%BD%8D%E7%9A%84%E6%B3%95%E5%BE%8B%E6%AD%A6%E5%99%A8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="反映了社会各阶级的社会地位和力量对比关系" _mubu_text="%3Cspan%3E%E5%8F%8D%E6%98%A0%E4%BA%86%E7%A4%BE%E4%BC%9A%E5%90%84%E9%98%B6%E7%BA%A7%E7%9A%84%E7%A4%BE%E4%BC%9A%E5%9C%B0%E4%BD%8D%E5%92%8C%E5%8A%9B%E9%87%8F%E5%AF%B9%E6%AF%94%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="随社会各阶级社会地位和力量对比关系的改变而变化" _mubu_text="%3Cspan%3E%E9%9A%8F%E7%A4%BE%E4%BC%9A%E5%90%84%E9%98%B6%E7%BA%A7%E7%A4%BE%E4%BC%9A%E5%9C%B0%E4%BD%8D%E5%92%8C%E5%8A%9B%E9%87%8F%E5%AF%B9%E6%AF%94%E5%85%B3%E7%B3%BB%E7%9A%84%E6%94%B9%E5%8F%98%E8%80%8C%E5%8F%98%E5%8C%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="宪法的制定" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E7%9A%84%E5%88%B6%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="制宪主体" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%AA%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="人民有权制定宪法" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%B0%91%E6%9C%89%E6%9D%83%E5%88%B6%E5%AE%9A%E5%AE%AA%E6%B3%95%3C/span%3E" _note="法国西耶士提出" _mubu_note="%3Cspan%3E%E6%B3%95%E5%9B%BD%E8%A5%BF%E8%80%B6%E5%A3%AB%E6%8F%90%E5%87%BA%3C/span%3E"/>
            <outline text="制宪权是原生性的创制元规则的权" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%AA%E6%9D%83%E6%98%AF%E5%8E%9F%E7%94%9F%E6%80%A7%E7%9A%84%E5%88%9B%E5%88%B6%E5%85%83%E8%A7%84%E5%88%99%E7%9A%84%E6%9D%83%3C/span%3E" _note="几乎不受限" _mubu_note="%3Cspan%3E%E5%87%A0%E4%B9%8E%E4%B8%8D%E5%8F%97%E9%99%90%3C/span%3E"/>
          </outline>
          <outline text="修宪主体" _mubu_text="%3Cspan%3E%E4%BF%AE%E5%AE%AA%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="全人大" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%3C/span%3E" _note="54宪法规定" _mubu_note="%3Cspan%3E54%E5%AE%AA%E6%B3%95%E8%A7%84%E5%AE%9A%3C/span%3E"/>
            <outline text="修宪权是依制宪权而产生的一种派生性的权力" _mubu_text="%3Cspan%3E%E4%BF%AE%E5%AE%AA%E6%9D%83%E6%98%AF%E4%BE%9D%E5%88%B6%E5%AE%AA%E6%9D%83%E8%80%8C%E4%BA%A7%E7%94%9F%E7%9A%84%E4%B8%80%E7%A7%8D%E6%B4%BE%E7%94%9F%E6%80%A7%E7%9A%84%E6%9D%83%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="释宪主体" _mubu_text="%3Cspan%3E%E9%87%8A%E5%AE%AA%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="全人常" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%3C/span%3E" _note="78宪法规定" _mubu_note="%3Cspan%3E78%E5%AE%AA%E6%B3%95%E8%A7%84%E5%AE%9A%3C/span%3E"/>
          </outline>
          <outline text="监督宪法实施主体" _mubu_text="%3Cspan%3E%E7%9B%91%E7%9D%A3%E5%AE%AA%E6%B3%95%E5%AE%9E%E6%96%BD%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="全人大及常" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%A4%A7%E5%8F%8A%E5%B8%B8%3C/span%3E" _note="82宪法规定" _mubu_note="%3Cspan%3E82%E5%AE%AA%E6%B3%95%E8%A7%84%E5%AE%9A%3C/span%3E"/>
          </outline>
          <outline text="制宪程序" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%AA%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="设立制宪机关" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%AB%8B%E5%88%B6%E5%AE%AA%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
