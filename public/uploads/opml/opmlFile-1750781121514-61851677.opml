          </outline>
          <outline text="交涉性" _mubu_text="%3Cspan%3E%E4%BA%A4%E6%B6%89%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="多方当事人的诉讼参与" _mubu_text="%3Cspan%3E%E5%A4%9A%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E8%AF%89%E8%AE%BC%E5%8F%82%E4%B8%8E%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="程序性" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="普遍性" _mubu_text="%3Cspan%3E%E6%99%AE%E9%81%8D%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="司法是法律与社会生活的纽带" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%98%AF%E6%B3%95%E5%BE%8B%E4%B8%8E%E7%A4%BE%E4%BC%9A%E7%94%9F%E6%B4%BB%E7%9A%84%E7%BA%BD%E5%B8%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="终极性" _mubu_text="%3Cspan%3E%E7%BB%88%E6%9E%81%E6%80%A7%3C/span%3E" _note="终局性" _mubu_note="%3Cspan%3E%E7%BB%88%E5%B1%80%E6%80%A7%3C/span%3E">
            <outline text="法律适用结果是最终性的决定" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E9%80%82%E7%94%A8%E7%BB%93%E6%9E%9C%E6%98%AF%E6%9C%80%E7%BB%88%E6%80%A7%E7%9A%84%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="功能" _mubu_text="%3Cspan%3E%E5%8A%9F%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
          <outline text="一般全选" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E5%85%A8%E9%80%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="制度" _mubu_text="%3Cspan%3E%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="广义的司法制度除了审判、检察制度外，还包括律师、公证制度等" _mubu_text="%3Cspan%3E%E5%B9%BF%E4%B9%89%E7%9A%84%E5%8F%B8%E6%B3%95%E5%88%B6%E5%BA%A6%E9%99%A4%E4%BA%86%E5%AE%A1%E5%88%A4%E3%80%81%E6%A3%80%E5%AF%9F%E5%88%B6%E5%BA%A6%E5%A4%96%EF%BC%8C%E8%BF%98%E5%8C%85%E6%8B%AC%E5%BE%8B%E5%B8%88%E3%80%81%E5%85%AC%E8%AF%81%E5%88%B6%E5%BA%A6%E7%AD%89%3C/span%3E" _note="默认广义" _mubu_note="%3Cspan%3E%E9%BB%98%E8%AE%A4%E5%B9%BF%E4%B9%89%3C/span%3E"/>
