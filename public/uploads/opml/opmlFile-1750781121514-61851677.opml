              <outline text="坚持中特社义制度" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E5%88%B6%E5%BA%A6%3C/span%3E" _note="根本制度保证" _mubu_note="%3Cspan%3E%E6%A0%B9%E6%9C%AC%E5%88%B6%E5%BA%A6%E4%BF%9D%E8%AF%81%3C/span%3E"/>
              <outline text="坚持社义法治理论" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E7%A4%BE%E4%B9%89%E6%B3%95%E6%B2%BB%E7%90%86%E8%AE%BA%3C/span%3E" _note="理论指导和学理支撑" _mubu_note="%3Cspan%3E%E7%90%86%E8%AE%BA%E6%8C%87%E5%AF%BC%E5%92%8C%E5%AD%A6%E7%90%86%E6%94%AF%E6%92%91%3C/span%3E"/>
            </outline>
          </outline>
