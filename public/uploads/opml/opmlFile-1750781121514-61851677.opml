                </outline>
                <outline text="地方性法规与部门规章不一致" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%80%A7%E6%B3%95%E8%A7%84%E4%B8%8E%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%E4%B8%8D%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note="">
                  <outline text="交由国务院提出意见" _mubu_text="%3Cspan%3E%E4%BA%A4%E7%94%B1%E5%9B%BD%E5%8A%A1%E9%99%A2%E6%8F%90%E5%87%BA%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
                    <outline text="其认为应当适用地方性法规的" _mubu_text="%3Cspan%3E%E5%85%B6%E8%AE%A4%E4%B8%BA%E5%BA%94%E5%BD%93%E9%80%82%E7%94%A8%E5%9C%B0%E6%96%B9%E6%80%A7%E6%B3%95%E8%A7%84%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                      <outline text="适用地方性法规" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E5%9C%B0%E6%96%B9%E6%80%A7%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                    <outline text="其认为应当适用部门规章的" _mubu_text="%3Cspan%3E%E5%85%B6%E8%AE%A4%E4%B8%BA%E5%BA%94%E5%BD%93%E9%80%82%E7%94%A8%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                      <outline text="提请全人常裁决" _mubu_text="%3Cspan%3E%E6%8F%90%E8%AF%B7%E5%85%A8%E4%BA%BA%E5%B8%B8%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                </outline>
                <outline text="部门规章和地方政府规章之间" _mubu_text="%3Cspan%3E%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%E5%92%8C%E5%9C%B0%E6%96%B9%E6%94%BF%E5%BA%9C%E8%A7%84%E7%AB%A0%E4%B9%8B%E9%97%B4%3C/span%3E" _note="包括部门规章之间" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%E4%B9%8B%E9%97%B4%3C/span%3E">
                  <outline text="统一由国务院裁决" _mubu_text="%3Cspan%3E%E7%BB%9F%E4%B8%80%E7%94%B1%E5%9B%BD%E5%8A%A1%E9%99%A2%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
