                <outline text="行使职权的独立性" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E8%81%8C%E6%9D%83%E7%9A%84%E7%8B%AC%E7%AB%8B%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="行使职权的合法性" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E8%81%8C%E6%9D%83%E7%9A%84%E5%90%88%E6%B3%95%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="司法责任原则" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E8%B4%A3%E4%BB%BB%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
