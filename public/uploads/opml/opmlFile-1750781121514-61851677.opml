                <outline text="代表组成" _mubu_text="%3Cspan%3E%E4%BB%A3%E8%A1%A8%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                  <outline text="村民代表应占村代会议组成人员的4/5以上" _mubu_text="%3Cspan%3E%E6%9D%91%E6%B0%91%E4%BB%A3%E8%A1%A8%E5%BA%94%E5%8D%A0%E6%9D%91%E4%BB%A3%E4%BC%9A%E8%AE%AE%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%98%E7%9A%844/5%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="妇女代表应占村代会议组成人员的1/3以上" _mubu_text="%3Cspan%3E%E5%A6%87%E5%A5%B3%E4%BB%A3%E8%A1%A8%E5%BA%94%E5%8D%A0%E6%9D%91%E4%BB%A3%E4%BC%9A%E8%AE%AE%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%98%E7%9A%841/3%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="每季度召开一次" _mubu_text="%3Cspan%3E%E6%AF%8F%E5%AD%A3%E5%BA%A6%E5%8F%AC%E5%BC%80%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note="">
