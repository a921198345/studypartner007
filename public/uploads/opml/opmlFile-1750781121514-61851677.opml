              <outline text="宪法起草机关" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E8%B5%B7%E8%8D%89%E6%9C%BA%E5%85%B3%3C/span%3E" _note="任命方式" _mubu_note="%3Cspan%3E%E4%BB%BB%E5%91%BD%E6%96%B9%E5%BC%8F%3C/span%3E"/>
              <outline text="宪法通过机关" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E9%80%9A%E8%BF%87%E6%9C%BA%E5%85%B3%3C/span%3E" _note="选举方式" _mubu_note="%3Cspan%3E%E9%80%89%E4%B8%BE%E6%96%B9%E5%BC%8F%3C/span%3E"/>
            </outline>
            <outline text="提出宪法草案" _mubu_text="%3Cspan%3E%E6%8F%90%E5%87%BA%E5%AE%AA%E6%B3%95%E8%8D%89%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="应具备广泛的代表性和科学性" _mubu_text="%3Cspan%3E%E5%BA%94%E5%85%B7%E5%A4%87%E5%B9%BF%E6%B3%9B%E7%9A%84%E4%BB%A3%E8%A1%A8%E6%80%A7%E5%92%8C%E7%A7%91%E5%AD%A6%E6%80%A7%3C/span%3E" _note="54宪法采用了全民讨论的办法" _mubu_note="%3Cspan%3E54%E5%AE%AA%E6%B3%95%E9%87%87%E7%94%A8%E4%BA%86%E5%85%A8%E6%B0%91%E8%AE%A8%E8%AE%BA%E7%9A%84%E5%8A%9E%E6%B3%95%3C/span%3E"/>
            </outline>
            <outline text="通过宪法草案" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E5%AE%AA%E6%B3%95%E8%8D%89%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="宪法通过机关的绝对多数" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E9%80%9A%E8%BF%87%E6%9C%BA%E5%85%B3%E7%9A%84%E7%BB%9D%E5%AF%B9%E5%A4%9A%E6%95%B0%3C/span%3E" _note="绝对多数是理念要求，≠2/3" _mubu_note="%3Cspan%3E%E7%BB%9D%E5%AF%B9%E5%A4%9A%E6%95%B0%E6%98%AF%E7%90%86%E5%BF%B5%E8%A6%81%E6%B1%82%EF%BC%8C%E2%89%A02/3%3C/span%3E"/>
              <outline text="或全民公决" _mubu_text="%3Cspan%3E%E6%88%96%E5%85%A8%E6%B0%91%E5%85%AC%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="公布宪法" _mubu_text="%3Cspan%3E%E5%85%AC%E5%B8%83%E5%AE%AA%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
