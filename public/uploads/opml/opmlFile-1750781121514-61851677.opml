                <outline text="国家权力机关的监督" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E6%9D%83%E5%8A%9B%E6%9C%BA%E5%85%B3%E7%9A%84%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note="">
                  <outline text="处于核心和主导地位" _mubu_text="%3Cspan%3E%E5%A4%84%E4%BA%8E%E6%A0%B8%E5%BF%83%E5%92%8C%E4%B8%BB%E5%AF%BC%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
