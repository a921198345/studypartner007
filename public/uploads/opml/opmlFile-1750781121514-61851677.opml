                  <outline text="1/5以上的户、" _mubu_text="%3Cspan%3E1/5%E4%BB%A5%E4%B8%8A%E7%9A%84%E6%88%B7%E3%80%81%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或1/3以上的居民小组提议，应当召集" _mubu_text="%3Cspan%3E%E6%88%961/3%E4%BB%A5%E4%B8%8A%E7%9A%84%E5%B1%85%E6%B0%91%E5%B0%8F%E7%BB%84%E6%8F%90%E8%AE%AE%EF%BC%8C%E5%BA%94%E5%BD%93%E5%8F%AC%E9%9B%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="会议有效" _mubu_text="%3Cspan%3E%E4%BC%9A%E8%AE%AE%E6%9C%89%E6%95%88%3C/span%3E" _note="人数条件" _mubu_note="%3Cspan%3E%E4%BA%BA%E6%95%B0%E6%9D%A1%E4%BB%B6%3C/span%3E">
                  <outline text="过半数的年满18周岁居民参加" _mubu_text="%3Cspan%3E%E8%BF%87%E5%8D%8A%E6%95%B0%E7%9A%84%E5%B9%B4%E6%BB%A118%E5%91%A8%E5%B2%81%E5%B1%85%E6%B0%91%E5%8F%82%E5%8A%A0%3C/span%3E" _note="未规定未被剥夺政治权利" _mubu_note="%3Cspan%3E%E6%9C%AA%E8%A7%84%E5%AE%9A%E6%9C%AA%E8%A2%AB%E5%89%A5%E5%A4%BA%E6%94%BF%E6%B2%BB%E6%9D%83%E5%88%A9%3C/span%3E"/>
                  <outline text="或过半数居民代表参加" _mubu_text="%3Cspan%3E%E6%88%96%E8%BF%87%E5%8D%8A%E6%95%B0%E5%B1%85%E6%B0%91%E4%BB%A3%E8%A1%A8%E5%8F%82%E5%8A%A0%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="所作决定应经到会人员过半数通过" _mubu_text="%3Cspan%3E%E6%89%80%E4%BD%9C%E5%86%B3%E5%AE%9A%E5%BA%94%E7%BB%8F%E5%88%B0%E4%BC%9A%E4%BA%BA%E5%91%98%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E" _note="与村委会是唯二到会过半数通过" _mubu_note="%3Cspan%3E%E4%B8%8E%E6%9D%91%E5%A7%94%E4%BC%9A%E6%98%AF%E5%94%AF%E4%BA%8C%E5%88%B0%E4%BC%9A%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E"/>
                <outline text="居民会议有权撤换和补选居委会成员" _mubu_text="%3Cspan%3E%E5%B1%85%E6%B0%91%E4%BC%9A%E8%AE%AE%E6%9C%89%E6%9D%83%E6%92%A4%E6%8D%A2%E5%92%8C%E8%A1%A5%E9%80%89%E5%B1%85%E5%A7%94%E4%BC%9A%E6%88%90%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="居民公约" _mubu_text="%3Cspan%3E%E5%B1%85%E6%B0%91%E5%85%AC%E7%BA%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="居民会议制定" _mubu_text="%3Cspan%3E%E5%B1%85%E6%B0%91%E4%BC%9A%E8%AE%AE%E5%88%B6%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="备案" _mubu_text="%3Cspan%3E%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
                  <outline text="报地级市或市辖区政府" _mubu_text="%3Cspan%3E%E6%8A%A5%E5%9C%B0%E7%BA%A7%E5%B8%82%E6%88%96%E5%B8%82%E8%BE%96%E5%8C%BA%E6%94%BF%E5%BA%9C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或它的派出机关" _mubu_text="%3Cspan%3E%E6%88%96%E5%AE%83%E7%9A%84%E6%B4%BE%E5%87%BA%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
        </outline>
