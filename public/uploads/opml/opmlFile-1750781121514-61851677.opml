        <outline text="特别行政区制度" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%89%B9%E5%88%AB%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="中央与特区的权力分配体系" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%A4%AE%E4%B8%8E%E7%89%B9%E5%8C%BA%E7%9A%84%E6%9D%83%E5%8A%9B%E5%88%86%E9%85%8D%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="中央" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%A4%AE%3C/span%3E" _note="“国防外交基本法，人事任命不正常”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%9B%BD%E9%98%B2%E5%A4%96%E4%BA%A4%E5%9F%BA%E6%9C%AC%E6%B3%95%EF%BC%8C%E4%BA%BA%E4%BA%8B%E4%BB%BB%E5%91%BD%E4%B8%8D%E6%AD%A3%E5%B8%B8%E2%80%9D%3C/span%3E">
              <outline text="负责管理与特区有关的外交事务" _mubu_text="%3Cspan%3E%E8%B4%9F%E8%B4%A3%E7%AE%A1%E7%90%86%E4%B8%8E%E7%89%B9%E5%8C%BA%E6%9C%89%E5%85%B3%E7%9A%84%E5%A4%96%E4%BA%A4%E4%BA%8B%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="负责特区的防务" _mubu_text="%3Cspan%3E%E8%B4%9F%E8%B4%A3%E7%89%B9%E5%8C%BA%E7%9A%84%E9%98%B2%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="任命特区的行政长官和行政机关的主要行政官员(澳门检察长)" _mubu_text="%3Cspan%3E%E4%BB%BB%E5%91%BD%E7%89%B9%E5%8C%BA%E7%9A%84%E8%A1%8C%E6%94%BF%E9%95%BF%E5%AE%98%E5%92%8C%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E7%9A%84%E4%B8%BB%E8%A6%81%E8%A1%8C%E6%94%BF%E5%AE%98%E5%91%98(%E6%BE%B3%E9%97%A8%E6%A3%80%E5%AF%9F%E9%95%BF)%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="全人常决定特区进入紧急状态" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E5%86%B3%E5%AE%9A%E7%89%B9%E5%8C%BA%E8%BF%9B%E5%85%A5%E7%B4%A7%E6%80%A5%E7%8A%B6%E6%80%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="全人常宣布战争状态或决定特区进入紧急状态" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E5%AE%A3%E5%B8%83%E6%88%98%E4%BA%89%E7%8A%B6%E6%80%81%E6%88%96%E5%86%B3%E5%AE%9A%E7%89%B9%E5%8C%BA%E8%BF%9B%E5%85%A5%E7%B4%A7%E6%80%A5%E7%8A%B6%E6%80%81%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国务院可发布命令将有关全国性法律在特区实施" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%AF%E5%8F%91%E5%B8%83%E5%91%BD%E4%BB%A4%E5%B0%86%E6%9C%89%E5%85%B3%E5%85%A8%E5%9B%BD%E6%80%A7%E6%B3%95%E5%BE%8B%E5%9C%A8%E7%89%B9%E5%8C%BA%E5%AE%9E%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="全人常解释、修改基本法" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BA%BA%E5%B8%B8%E8%A7%A3%E9%87%8A%E3%80%81%E4%BF%AE%E6%94%B9%E5%9F%BA%E6%9C%AC%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="特区" _mubu_text="%3Cspan%3E%E7%89%B9%E5%8C%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="行政管理权" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E7%AE%A1%E7%90%86%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="特区财政独立，中央不在特区征税" _mubu_text="%3Cspan%3E%E7%89%B9%E5%8C%BA%E8%B4%A2%E6%94%BF%E7%8B%AC%E7%AB%8B%EF%BC%8C%E4%B8%AD%E5%A4%AE%E4%B8%8D%E5%9C%A8%E7%89%B9%E5%8C%BA%E5%BE%81%E7%A8%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="特区的土地和自然资源属国家所有，由特区政府管理使用开发支配" _mubu_text="%3Cspan%3E%E7%89%B9%E5%8C%BA%E7%9A%84%E5%9C%9F%E5%9C%B0%E5%92%8C%E8%87%AA%E7%84%B6%E8%B5%84%E6%BA%90%E5%B1%9E%E5%9B%BD%E5%AE%B6%E6%89%80%E6%9C%89%EF%BC%8C%E7%94%B1%E7%89%B9%E5%8C%BA%E6%94%BF%E5%BA%9C%E7%AE%A1%E7%90%86%E4%BD%BF%E7%94%A8%E5%BC%80%E5%8F%91%E6%94%AF%E9%85%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
