            <outline text="一般必然有一方主体是国家机关" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E5%BF%85%E7%84%B6%E6%9C%89%E4%B8%80%E6%96%B9%E4%B8%BB%E4%BD%93%E6%98%AF%E5%9B%BD%E5%AE%B6%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
