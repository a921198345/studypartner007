            <outline text="司法组织体系" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%BB%84%E7%BB%87%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="司法制度体系" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E5%88%B6%E5%BA%A6%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="司法人员管理体系" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E4%BA%BA%E5%91%98%E7%AE%A1%E7%90%86%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="公正" _mubu_text="%3Cspan%3E%E5%85%AC%E6%AD%A3%3C/span%3E" _note="" _mubu_note="">
          <outline text="实体公正、程序公正" _mubu_text="%3Cspan%3E%E5%AE%9E%E4%BD%93%E5%85%AC%E6%AD%A3%E3%80%81%E7%A8%8B%E5%BA%8F%E5%85%AC%E6%AD%A3%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="司法活动的合法性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E7%9A%84%E5%90%88%E6%B3%95%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="司法人员的中立性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E4%BA%BA%E5%91%98%E7%9A%84%E4%B8%AD%E7%AB%8B%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="当事人地位的平等性" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%B0%E4%BD%8D%E7%9A%84%E5%B9%B3%E7%AD%89%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="司法程序的参与性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%A8%8B%E5%BA%8F%E7%9A%84%E5%8F%82%E4%B8%8E%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="司法结果的正确性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E7%BB%93%E6%9E%9C%E7%9A%84%E6%AD%A3%E7%A1%AE%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="司法活动的公开性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E6%B4%BB%E5%8A%A8%E7%9A%84%E5%85%AC%E5%BC%80%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁判文书公开" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%E6%96%87%E4%B9%A6%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="“国密隐私未成年，调解结案不公开”" _mubu_text="%3Cspan%3E%E2%80%9C%E5%9B%BD%E5%AF%86%E9%9A%90%E7%A7%81%E6%9C%AA%E6%88%90%E5%B9%B4%EF%BC%8C%E8%B0%83%E8%A7%A3%E7%BB%93%E6%A1%88%E4%B8%8D%E5%85%AC%E5%BC%80%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="检务公开" _mubu_text="%3Cspan%3E%E6%A3%80%E5%8A%A1%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="“程序信息相关人，重要案件社会人”" _mubu_text="%3Cspan%3E%E2%80%9C%E7%A8%8B%E5%BA%8F%E4%BF%A1%E6%81%AF%E7%9B%B8%E5%85%B3%E4%BA%BA%EF%BC%8C%E9%87%8D%E8%A6%81%E6%A1%88%E4%BB%B6%E7%A4%BE%E4%BC%9A%E4%BA%BA%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="司法人员的廉洁性" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E4%BA%BA%E5%91%98%E7%9A%84%E5%BB%89%E6%B4%81%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="只要能引发对廉洁公正合理怀疑，就一律可定性为不当交往" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%A6%81%E8%83%BD%E5%BC%95%E5%8F%91%E5%AF%B9%E5%BB%89%E6%B4%81%E5%85%AC%E6%AD%A3%E5%90%88%E7%90%86%E6%80%80%E7%96%91%EF%BC%8C%E5%B0%B1%E4%B8%80%E5%BE%8B%E5%8F%AF%E5%AE%9A%E6%80%A7%E4%B8%BA%E4%B8%8D%E5%BD%93%E4%BA%A4%E5%BE%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="司法人员与其他人员正常接触的判断标准" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E4%BA%BA%E5%91%98%E4%B8%8E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%91%98%E6%AD%A3%E5%B8%B8%E6%8E%A5%E8%A7%A6%E7%9A%84%E5%88%A4%E6%96%AD%E6%A0%87%E5%87%86%3C/span%3E" _note="“三工”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%B8%89%E5%B7%A5%E2%80%9D%3C/span%3E">
              <outline text="工作需要、工作时间、工作地点" _mubu_text="%3Cspan%3E%E5%B7%A5%E4%BD%9C%E9%9C%80%E8%A6%81%E3%80%81%E5%B7%A5%E4%BD%9C%E6%97%B6%E9%97%B4%E3%80%81%E5%B7%A5%E4%BD%9C%E5%9C%B0%E7%82%B9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="基于工作需要的非工作时间和地点的接触" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E5%B7%A5%E4%BD%9C%E9%9C%80%E8%A6%81%E7%9A%84%E9%9D%9E%E5%B7%A5%E4%BD%9C%E6%97%B6%E9%97%B4%E5%92%8C%E5%9C%B0%E7%82%B9%E7%9A%84%E6%8E%A5%E8%A7%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则上报批，来不及的3日内报备" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E6%8A%A5%E6%89%B9%EF%BC%8C%E6%9D%A5%E4%B8%8D%E5%8F%8A%E7%9A%843%E6%97%A5%E5%86%85%E6%8A%A5%E5%A4%87%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="效率" _mubu_text="%3Cspan%3E%E6%95%88%E7%8E%87%3C/span%3E" _note="" _mubu_note="">
          <outline text="包括" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%3C/span%3E" _note="" _mubu_note="">
            <outline text="时间效率、资源利用效率、成本效率" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%E6%95%88%E7%8E%87%E3%80%81%E8%B5%84%E6%BA%90%E5%88%A9%E7%94%A8%E6%95%88%E7%8E%87%E3%80%81%E6%88%90%E6%9C%AC%E6%95%88%E7%8E%87%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="公正优先，兼顾效率" _mubu_text="%3Cspan%3E%E5%85%AC%E6%AD%A3%E4%BC%98%E5%85%88%EF%BC%8C%E5%85%BC%E9%A1%BE%E6%95%88%E7%8E%87%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="独立" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="审判独立与检察独立" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%8B%AC%E7%AB%8B%E4%B8%8E%E6%A3%80%E5%AF%9F%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
            <outline text="审判独立不仅包括法官的个体独立还应包括法院整体上的独立" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%8B%AC%E7%AB%8B%E4%B8%8D%E4%BB%85%E5%8C%85%E6%8B%AC%E6%B3%95%E5%AE%98%E7%9A%84%E4%B8%AA%E4%BD%93%E7%8B%AC%E7%AB%8B%E8%BF%98%E5%BA%94%E5%8C%85%E6%8B%AC%E6%B3%95%E9%99%A2%E6%95%B4%E4%BD%93%E4%B8%8A%E7%9A%84%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="检察独立指的是检察系统整体的对外独立" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E7%8B%AC%E7%AB%8B%E6%8C%87%E7%9A%84%E6%98%AF%E6%A3%80%E5%AF%9F%E7%B3%BB%E7%BB%9F%E6%95%B4%E4%BD%93%E7%9A%84%E5%AF%B9%E5%A4%96%E7%8B%AC%E7%AB%8B%3C/span%3E" _note="系统内部遵循上命下从的检察一体原则" _mubu_note="%3Cspan%3E%E7%B3%BB%E7%BB%9F%E5%86%85%E9%83%A8%E9%81%B5%E5%BE%AA%E4%B8%8A%E5%91%BD%E4%B8%8B%E4%BB%8E%E7%9A%84%E6%A3%80%E5%AF%9F%E4%B8%80%E4%BD%93%E5%8E%9F%E5%88%99%3C/span%3E"/>
          </outline>
          <outline text="各级机关干部支持法检依法独立公正行使职权" _mubu_text="%3Cspan%3E%E5%90%84%E7%BA%A7%E6%9C%BA%E5%85%B3%E5%B9%B2%E9%83%A8%E6%94%AF%E6%8C%81%E6%B3%95%E6%A3%80%E4%BE%9D%E6%B3%95%E7%8B%AC%E7%AB%8B%E5%85%AC%E6%AD%A3%E8%A1%8C%E4%BD%BF%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="健全维护司法权威的法律制度" _mubu_text="%3Cspan%3E%E5%81%A5%E5%85%A8%E7%BB%B4%E6%8A%A4%E5%8F%B8%E6%B3%95%E6%9D%83%E5%A8%81%E7%9A%84%E6%B3%95%E5%BE%8B%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="建立健全司法人员履行法定职责保护机制" _mubu_text="%3Cspan%3E%E5%BB%BA%E7%AB%8B%E5%81%A5%E5%85%A8%E5%8F%B8%E6%B3%95%E4%BA%BA%E5%91%98%E5%B1%A5%E8%A1%8C%E6%B3%95%E5%AE%9A%E8%81%8C%E8%B4%A3%E4%BF%9D%E6%8A%A4%E6%9C%BA%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="改革" _mubu_text="%3Cspan%3E%E6%94%B9%E9%9D%A9%3C/span%3E" _note="" _mubu_note="">
          <outline text="涉及各方面一切问题" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%90%84%E6%96%B9%E9%9D%A2%E4%B8%80%E5%88%87%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="法律职业道德概述" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%81%8C%E4%B8%9A%E9%81%93%E5%BE%B7%E6%A6%82%E8%BF%B0%3C/span%3E" _note="" _mubu_note="">
        <outline text="法律职业" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%81%8C%E4%B8%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="有训练、有技能、有伦理" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AE%AD%E7%BB%83%E3%80%81%E6%9C%89%E6%8A%80%E8%83%BD%E3%80%81%E6%9C%89%E4%BC%A6%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="政治、法律、行业、专业属性" _mubu_text="%3Cspan%3E%E6%94%BF%E6%B2%BB%E3%80%81%E6%B3%95%E5%BE%8B%E3%80%81%E8%A1%8C%E4%B8%9A%E3%80%81%E4%B8%93%E4%B8%9A%E5%B1%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="法律职业道德" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%81%8C%E4%B8%9A%E9%81%93%E5%BE%B7%3C/span%3E" _note="" _mubu_note="">
          <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="职业、实践、正式、更高性" _mubu_text="%3Cspan%3E%E8%81%8C%E4%B8%9A%E3%80%81%E5%AE%9E%E8%B7%B5%E3%80%81%E6%AD%A3%E5%BC%8F%E3%80%81%E6%9B%B4%E9%AB%98%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="基本原则" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="忠于党、忠于国家、忠于人民、忠于法律" _mubu_text="%3Cspan%3E%E5%BF%A0%E4%BA%8E%E5%85%9A%E3%80%81%E5%BF%A0%E4%BA%8E%E5%9B%BD%E5%AE%B6%E3%80%81%E5%BF%A0%E4%BA%8E%E4%BA%BA%E6%B0%91%E3%80%81%E5%BF%A0%E4%BA%8E%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="以事实为根据，以法律为准绳" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%BA%8B%E5%AE%9E%E4%B8%BA%E6%A0%B9%E6%8D%AE%EF%BC%8C%E4%BB%A5%E6%B3%95%E5%BE%8B%E4%B8%BA%E5%87%86%E7%BB%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="严明纪律，保守秘密" _mubu_text="%3Cspan%3E%E4%B8%A5%E6%98%8E%E7%BA%AA%E5%BE%8B%EF%BC%8C%E4%BF%9D%E5%AE%88%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="互相尊重，互相配合" _mubu_text="%3Cspan%3E%E4%BA%92%E7%9B%B8%E5%B0%8A%E9%87%8D%EF%BC%8C%E4%BA%92%E7%9B%B8%E9%85%8D%E5%90%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="恪尽职守，勤勉尽责" _mubu_text="%3Cspan%3E%E6%81%AA%E5%B0%BD%E8%81%8C%E5%AE%88%EF%BC%8C%E5%8B%A4%E5%8B%89%E5%B0%BD%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="清正廉洁，遵纪守法" _mubu_text="%3Cspan%3E%E6%B8%85%E6%AD%A3%E5%BB%89%E6%B4%81%EF%BC%8C%E9%81%B5%E7%BA%AA%E5%AE%88%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="审判制度和法官职业道德" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E5%88%B6%E5%BA%A6%E5%92%8C%E6%B3%95%E5%AE%98%E8%81%8C%E4%B8%9A%E9%81%93%E5%BE%B7%3C/span%3E" _note="" _mubu_note="">
        <outline text="审判制度" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院由国家权力机关产生并受其监督" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E7%94%B1%E5%9B%BD%E5%AE%B6%E6%9D%83%E5%8A%9B%E6%9C%BA%E5%85%B3%E4%BA%A7%E7%94%9F%E5%B9%B6%E5%8F%97%E5%85%B6%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法院统一设立并独立行使审判权" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E7%BB%9F%E4%B8%80%E8%AE%BE%E7%AB%8B%E5%B9%B6%E7%8B%AC%E7%AB%8B%E8%A1%8C%E4%BD%BF%E5%AE%A1%E5%88%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审判活动实行某些特有的原则和制度" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E6%B4%BB%E5%8A%A8%E5%AE%9E%E8%A1%8C%E6%9F%90%E4%BA%9B%E7%89%B9%E6%9C%89%E7%9A%84%E5%8E%9F%E5%88%99%E5%92%8C%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="审判独立原则" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%8B%AC%E7%AB%8B%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不告不理原则" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%91%8A%E4%B8%8D%E7%90%86%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="直接言词原则" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%A8%80%E8%AF%8D%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审判及时原则" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E5%8F%8A%E6%97%B6%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="制度" _mubu_text="%3Cspan%3E%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="两审终审制度" _mubu_text="%3Cspan%3E%E4%B8%A4%E5%AE%A1%E7%BB%88%E5%AE%A1%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审判公开制度" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E5%85%AC%E5%BC%80%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="人民陪审制度" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%B0%91%E9%99%AA%E5%AE%A1%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审判监督制度" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%9B%91%E7%9D%A3%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法官条件" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%98%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="同检察官条件" _mubu_note="%3Cspan%3E%E5%90%8C%E6%A3%80%E5%AF%9F%E5%AE%98%E6%9D%A1%E4%BB%B6%3C/span%3E">
            <outline text="学历条件" _mubu_text="%3Cspan%3E%E5%AD%A6%E5%8E%86%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="本科" _mubu_text="%3Cspan%3E%E6%9C%AC%E7%A7%91%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="年限条件" _mubu_text="%3Cspan%3E%E5%B9%B4%E9%99%90%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="学士：5年法律工作经验" _mubu_text="%3Cspan%3E%E5%AD%A6%E5%A3%AB%EF%BC%9A5%E5%B9%B4%E6%B3%95%E5%BE%8B%E5%B7%A5%E4%BD%9C%E7%BB%8F%E9%AA%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="硕士：4年法律工作经验" _mubu_text="%3Cspan%3E%E7%A1%95%E5%A3%AB%EF%BC%9A4%E5%B9%B4%E6%B3%95%E5%BE%8B%E5%B7%A5%E4%BD%9C%E7%BB%8F%E9%AA%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="博士：3年法律工作经验" _mubu_text="%3Cspan%3E%E5%8D%9A%E5%A3%AB%EF%BC%9A3%E5%B9%B4%E6%B3%95%E5%BE%8B%E5%B7%A5%E4%BD%9C%E7%BB%8F%E9%AA%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="禁止性条件" _mubu_text="%3Cspan%3E%E7%A6%81%E6%AD%A2%E6%80%A7%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="“开除吊证，犯罪除名”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%BC%80%E9%99%A4%E5%90%8A%E8%AF%81%EF%BC%8C%E7%8A%AF%E7%BD%AA%E9%99%A4%E5%90%8D%E2%80%9D%3C/span%3E">
              <outline text="因犯罪受过刑事处罚的" _mubu_text="%3Cspan%3E%E5%9B%A0%E7%8A%AF%E7%BD%AA%E5%8F%97%E8%BF%87%E5%88%91%E4%BA%8B%E5%A4%84%E7%BD%9A%E7%9A%84%3C/span%3E" _note="包括过失" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E8%BF%87%E5%A4%B1%3C/span%3E"/>
              <outline text="被开除公职的" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%BC%80%E9%99%A4%E5%85%AC%E8%81%8C%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="被吊销律师、公证员执业证书或者被仲裁委员会除名的" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%90%8A%E9%94%80%E5%BE%8B%E5%B8%88%E3%80%81%E5%85%AC%E8%AF%81%E5%91%98%E6%89%A7%E4%B8%9A%E8%AF%81%E4%B9%A6%E6%88%96%E8%80%85%E8%A2%AB%E4%BB%B2%E8%A3%81%E5%A7%94%E5%91%98%E4%BC%9A%E9%99%A4%E5%90%8D%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="限制条件" _mubu_text="%3Cspan%3E%E9%99%90%E5%88%B6%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得兼任其他职务，除学术型兼任" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%85%BC%E4%BB%BB%E5%85%B6%E4%BB%96%E8%81%8C%E5%8A%A1%EF%BC%8C%E9%99%A4%E5%AD%A6%E6%9C%AF%E5%9E%8B%E5%85%BC%E4%BB%BB%3C/span%3E" _note="不得兼任人常组成人员，但可以担任人大代表" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%85%BC%E4%BB%BB%E4%BA%BA%E5%B8%B8%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%98%EF%BC%8C%E4%BD%86%E5%8F%AF%E4%BB%A5%E6%8B%85%E4%BB%BB%E4%BA%BA%E5%A4%A7%E4%BB%A3%E8%A1%A8%3C/span%3E"/>
              <outline text="从法院离任后2年内，不得以律师身份担任诉代或辩护人" _mubu_text="%3Cspan%3E%E4%BB%8E%E6%B3%95%E9%99%A2%E7%A6%BB%E4%BB%BB%E5%90%8E2%E5%B9%B4%E5%86%85%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BB%A5%E5%BE%8B%E5%B8%88%E8%BA%AB%E4%BB%BD%E6%8B%85%E4%BB%BB%E8%AF%89%E4%BB%A3%E6%88%96%E8%BE%A9%E6%8A%A4%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="从法院离任后，不得担任原任职法院案件的诉代或辩护人" _mubu_text="%3Cspan%3E%E4%BB%8E%E6%B3%95%E9%99%A2%E7%A6%BB%E4%BB%BB%E5%90%8E%EF%BC%8C%E4%B8%8D%E5%BE%97%E6%8B%85%E4%BB%BB%E5%8E%9F%E4%BB%BB%E8%81%8C%E6%B3%95%E9%99%A2%E6%A1%88%E4%BB%B6%E7%9A%84%E8%AF%89%E4%BB%A3%E6%88%96%E8%BE%A9%E6%8A%A4%E4%BA%BA%3C/span%3E" _note="但作为当事人的监护人或近亲属代理诉讼或者进行辩护的除外" _mubu_note="%3Cspan%3E%E4%BD%86%E4%BD%9C%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%88%96%E8%BF%91%E4%BA%B2%E5%B1%9E%E4%BB%A3%E7%90%86%E8%AF%89%E8%AE%BC%E6%88%96%E8%80%85%E8%BF%9B%E8%A1%8C%E8%BE%A9%E6%8A%A4%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              <outline text="被开除后，不得担任诉代或辩护人" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%BC%80%E9%99%A4%E5%90%8E%EF%BC%8C%E4%B8%8D%E5%BE%97%E6%8B%85%E4%BB%BB%E8%AF%89%E4%BB%A3%E6%88%96%E8%BE%A9%E6%8A%A4%E4%BA%BA%3C/span%3E" _note="但作为当事人的监护人或近亲属代理诉讼或辩护的除外" _mubu_note="%3Cspan%3E%E4%BD%86%E4%BD%9C%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E7%9B%91%E6%8A%A4%E4%BA%BA%E6%88%96%E8%BF%91%E4%BA%B2%E5%B1%9E%E4%BB%A3%E7%90%86%E8%AF%89%E8%AE%BC%E6%88%96%E8%BE%A9%E6%8A%A4%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="任职回避" _mubu_text="%3Cspan%3E%E4%BB%BB%E8%81%8C%E5%9B%9E%E9%81%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="法官之间有夫妻、直系/三代以内旁系血亲及近姻亲关系的" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%98%E4%B9%8B%E9%97%B4%E6%9C%89%E5%A4%AB%E5%A6%BB%E3%80%81%E7%9B%B4%E7%B3%BB/%E4%B8%89%E4%BB%A3%E4%BB%A5%E5%86%85%E6%97%81%E7%B3%BB%E8%A1%80%E4%BA%B2%E5%8F%8A%E8%BF%91%E5%A7%BB%E4%BA%B2%E5%85%B3%E7%B3%BB%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得同院都领导" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%90%8C%E9%99%A2%E9%83%BD%E9%A2%86%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得同院正副领导和被领导" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%90%8C%E9%99%A2%E6%AD%A3%E5%89%AF%E9%A2%86%E5%AF%BC%E5%92%8C%E8%A2%AB%E9%A2%86%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得同院同部门" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%90%8C%E9%99%A2%E5%90%8C%E9%83%A8%E9%97%A8%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不得上下两级正副领导" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%B8%8A%E4%B8%8B%E4%B8%A4%E7%BA%A7%E6%AD%A3%E5%89%AF%E9%A2%86%E5%AF%BC%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="配偶、父母和子女导致的法官任职回避" _mubu_text="%3Cspan%3E%E9%85%8D%E5%81%B6%E3%80%81%E7%88%B6%E6%AF%8D%E5%92%8C%E5%AD%90%E5%A5%B3%E5%AF%BC%E8%87%B4%E7%9A%84%E6%B3%95%E5%AE%98%E4%BB%BB%E8%81%8C%E5%9B%9E%E9%81%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="不得在法官所在法院辖区内当律师挣钱" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%9C%A8%E6%B3%95%E5%AE%98%E6%89%80%E5%9C%A8%E6%B3%95%E9%99%A2%E8%BE%96%E5%8C%BA%E5%86%85%E5%BD%93%E5%BE%8B%E5%B8%88%E6%8C%A3%E9%92%B1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="法官管理" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%98%E7%AE%A1%E7%90%86%3C/span%3E" _note="同检察官管理" _mubu_note="%3Cspan%3E%E5%90%8C%E6%A3%80%E5%AF%9F%E5%AE%98%E7%AE%A1%E7%90%86%3C/span%3E">
            <outline text="遴选" _mubu_text="%3Cspan%3E%E9%81%B4%E9%80%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="最高法和高院设置法官遴选委员会" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%B3%95%E5%92%8C%E9%AB%98%E9%99%A2%E8%AE%BE%E7%BD%AE%E6%B3%95%E5%AE%98%E9%81%B4%E9%80%89%E5%A7%94%E5%91%98%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="省级法官遴选委员会的组成人员中法官代表不少于1/3" _mubu_text="%3Cspan%3E%E7%9C%81%E7%BA%A7%E6%B3%95%E5%AE%98%E9%81%B4%E9%80%89%E5%A7%94%E5%91%98%E4%BC%9A%E7%9A%84%E7%BB%84%E6%88%90%E4%BA%BA%E5%91%98%E4%B8%AD%E6%B3%95%E5%AE%98%E4%BB%A3%E8%A1%A8%E4%B8%8D%E5%B0%91%E4%BA%8E1/3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="初任法官一般到基层法任职" _mubu_text="%3Cspan%3E%E5%88%9D%E4%BB%BB%E6%B3%95%E5%AE%98%E4%B8%80%E8%88%AC%E5%88%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E4%BB%BB%E8%81%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="上级法院法官一般逐级遴选" _mubu_text="%3Cspan%3E%E4%B8%8A%E7%BA%A7%E6%B3%95%E9%99%A2%E6%B3%95%E5%AE%98%E4%B8%80%E8%88%AC%E9%80%90%E7%BA%A7%E9%81%B4%E9%80%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="最高法和高法可从下两级法院遴选" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%B3%95%E5%92%8C%E9%AB%98%E6%B3%95%E5%8F%AF%E4%BB%8E%E4%B8%8B%E4%B8%A4%E7%BA%A7%E6%B3%95%E9%99%A2%E9%81%B4%E9%80%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法院可根据需要，从律师或法学教研人员等中公开选拔法官" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E6%A0%B9%E6%8D%AE%E9%9C%80%E8%A6%81%EF%BC%8C%E4%BB%8E%E5%BE%8B%E5%B8%88%E6%88%96%E6%B3%95%E5%AD%A6%E6%95%99%E7%A0%94%E4%BA%BA%E5%91%98%E7%AD%89%E4%B8%AD%E5%85%AC%E5%BC%80%E9%80%89%E6%8B%94%E6%B3%95%E5%AE%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="律师应实际执业≥5年" _mubu_text="%3Cspan%3E%E5%BE%8B%E5%B8%88%E5%BA%94%E5%AE%9E%E9%99%85%E6%89%A7%E4%B8%9A%E2%89%A55%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法学教研人员应具有中级以上职称，从事教研工作≥5年" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AD%A6%E6%95%99%E7%A0%94%E4%BA%BA%E5%91%98%E5%BA%94%E5%85%B7%E6%9C%89%E4%B8%AD%E7%BA%A7%E4%BB%A5%E4%B8%8A%E8%81%8C%E7%A7%B0%EF%BC%8C%E4%BB%8E%E4%BA%8B%E6%95%99%E7%A0%94%E5%B7%A5%E4%BD%9C%E2%89%A55%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="辞职与辞退" _mubu_text="%3Cspan%3E%E8%BE%9E%E8%81%8C%E4%B8%8E%E8%BE%9E%E9%80%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="辞职" _mubu_text="%3Cspan%3E%E8%BE%9E%E8%81%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="辞去在本单位担任的领导职务" _mubu_text="%3Cspan%3E%E8%BE%9E%E5%8E%BB%E5%9C%A8%E6%9C%AC%E5%8D%95%E4%BD%8D%E6%8B%85%E4%BB%BB%E7%9A%84%E9%A2%86%E5%AF%BC%E8%81%8C%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="辞去公职" _mubu_text="%3Cspan%3E%E8%BE%9E%E5%8E%BB%E5%85%AC%E8%81%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="辞退" _mubu_text="%3Cspan%3E%E8%BE%9E%E9%80%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="态度和能力问题" _mubu_text="%3Cspan%3E%E6%80%81%E5%BA%A6%E5%92%8C%E8%83%BD%E5%8A%9B%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="对辞退处理不服" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%BE%9E%E9%80%80%E5%A4%84%E7%90%86%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="自收到决定起30日内向原处理机关申请复议，并可向其上级机关申诉" _mubu_text="%3Cspan%3E%E8%87%AA%E6%94%B6%E5%88%B0%E5%86%B3%E5%AE%9A%E8%B5%B730%E6%97%A5%E5%86%85%E5%90%91%E5%8E%9F%E5%A4%84%E7%90%86%E6%9C%BA%E5%85%B3%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%EF%BC%8C%E5%B9%B6%E5%8F%AF%E5%90%91%E5%85%B6%E4%B8%8A%E7%BA%A7%E6%9C%BA%E5%85%B3%E7%94%B3%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="受理申诉机关必须按规定处理" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E7%94%B3%E8%AF%89%E6%9C%BA%E5%85%B3%E5%BF%85%E9%A1%BB%E6%8C%89%E8%A7%84%E5%AE%9A%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
