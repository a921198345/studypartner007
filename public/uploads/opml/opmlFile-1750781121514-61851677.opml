        <outline text="工作布局" _mubu_text="%3Cspan%3E%E5%B7%A5%E4%BD%9C%E5%B8%83%E5%B1%80%3C/span%3E" _note="" _mubu_note="">
          <outline text="坚持在法治轨道上推进国家治理体系和能力现代化" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E5%9C%A8%E6%B3%95%E6%B2%BB%E8%BD%A8%E9%81%93%E4%B8%8A%E6%8E%A8%E8%BF%9B%E5%9B%BD%E5%AE%B6%E6%B2%BB%E7%90%86%E4%BD%93%E7%B3%BB%E5%92%8C%E8%83%BD%E5%8A%9B%E7%8E%B0%E4%BB%A3%E5%8C%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="是国家治理领域一场广阔而深刻的革命" _mubu_text="%3Cspan%3E%E6%98%AF%E5%9B%BD%E5%AE%B6%E6%B2%BB%E7%90%86%E9%A2%86%E5%9F%9F%E4%B8%80%E5%9C%BA%E5%B9%BF%E9%98%94%E8%80%8C%E6%B7%B1%E5%88%BB%E7%9A%84%E9%9D%A9%E5%91%BD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法治是国家治理体系和治理能力的重要依托" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E6%98%AF%E5%9B%BD%E5%AE%B6%E6%B2%BB%E7%90%86%E4%BD%93%E7%B3%BB%E5%92%8C%E6%B2%BB%E7%90%86%E8%83%BD%E5%8A%9B%E7%9A%84%E9%87%8D%E8%A6%81%E4%BE%9D%E6%89%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="更好发挥法治固根本、稳预期、利长远的保障作用" _mubu_text="%3Cspan%3E%E6%9B%B4%E5%A5%BD%E5%8F%91%E6%8C%A5%E6%B3%95%E6%B2%BB%E5%9B%BA%E6%A0%B9%E6%9C%AC%E3%80%81%E7%A8%B3%E9%A2%84%E6%9C%9F%E3%80%81%E5%88%A9%E9%95%BF%E8%BF%9C%E7%9A%84%E4%BF%9D%E9%9A%9C%E4%BD%9C%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="坚持依法治军，从严治军" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%86%9B%EF%BC%8C%E4%BB%8E%E4%B8%A5%E6%B2%BB%E5%86%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="坚持依法保障&quot;一国两制&quot;实践与推进祖国统一" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E4%BF%9D%E9%9A%9C%22%E4%B8%80%E5%9B%BD%E4%B8%A4%E5%88%B6%22%E5%AE%9E%E8%B7%B5%E4%B8%8E%E6%8E%A8%E8%BF%9B%E7%A5%96%E5%9B%BD%E7%BB%9F%E4%B8%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="坚持依法治网" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E6%B2%BB%E7%BD%91%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="坚持建设中特社义法治体系" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E5%BB%BA%E8%AE%BE%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E6%B3%95%E6%B2%BB%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="是推进全面依法治国的总目标和总抓手" _mubu_text="%3Cspan%3E%E6%98%AF%E6%8E%A8%E8%BF%9B%E5%85%A8%E9%9D%A2%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E7%9A%84%E6%80%BB%E7%9B%AE%E6%A0%87%E5%92%8C%E6%80%BB%E6%8A%93%E6%89%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="全面推进依法治国总目标" _mubu_text="%3Cspan%3E%E5%85%A8%E9%9D%A2%E6%8E%A8%E8%BF%9B%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E6%80%BB%E7%9B%AE%E6%A0%87%3C/span%3E" _note="" _mubu_note="">
                <outline text="建设中特社义法治体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E6%B3%95%E6%B2%BB%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="建设社义法治国家" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E7%A4%BE%E4%B9%89%E6%B3%95%E6%B2%BB%E5%9B%BD%E5%AE%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="在党领导下，坚持中特社义制度，贯彻中特社义法治理论，" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%85%9A%E9%A2%86%E5%AF%BC%E4%B8%8B%EF%BC%8C%E5%9D%9A%E6%8C%81%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E5%88%B6%E5%BA%A6%EF%BC%8C%E8%B4%AF%E5%BD%BB%E4%B8%AD%E7%89%B9%E7%A4%BE%E4%B9%89%E6%B3%95%E6%B2%BB%E7%90%86%E8%AE%BA%EF%BC%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="形成完备的法律规范体系、高效的法治实施体系、严密的法治监督体系、有力的法治保障体系、完善的党内法规体系，" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E5%AE%8C%E5%A4%87%E7%9A%84%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E4%BD%93%E7%B3%BB%E3%80%81%E9%AB%98%E6%95%88%E7%9A%84%E6%B3%95%E6%B2%BB%E5%AE%9E%E6%96%BD%E4%BD%93%E7%B3%BB%E3%80%81%E4%B8%A5%E5%AF%86%E7%9A%84%E6%B3%95%E6%B2%BB%E7%9B%91%E7%9D%A3%E4%BD%93%E7%B3%BB%E3%80%81%E6%9C%89%E5%8A%9B%E7%9A%84%E6%B3%95%E6%B2%BB%E4%BF%9D%E9%9A%9C%E4%BD%93%E7%B3%BB%E3%80%81%E5%AE%8C%E5%96%84%E7%9A%84%E5%85%9A%E5%86%85%E6%B3%95%E8%A7%84%E4%BD%93%E7%B3%BB%EF%BC%8C%3C/span%3E" _note="“五个体系”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%BA%94%E4%B8%AA%E4%BD%93%E7%B3%BB%E2%80%9D%3C/span%3E"/>
              <outline text="坚持依法治国、依法执政、依法行政共同推进，" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E3%80%81%E4%BE%9D%E6%B3%95%E6%89%A7%E6%94%BF%E3%80%81%E4%BE%9D%E6%B3%95%E8%A1%8C%E6%94%BF%E5%85%B1%E5%90%8C%E6%8E%A8%E8%BF%9B%EF%BC%8C%3C/span%3E" _note="“三依”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%B8%89%E4%BE%9D%E2%80%9D%3C/span%3E"/>
              <outline text="坚持法治国家、法治政府、法治社会一体建设，" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E6%B3%95%E6%B2%BB%E5%9B%BD%E5%AE%B6%E3%80%81%E6%B3%95%E6%B2%BB%E6%94%BF%E5%BA%9C%E3%80%81%E6%B3%95%E6%B2%BB%E7%A4%BE%E4%BC%9A%E4%B8%80%E4%BD%93%E5%BB%BA%E8%AE%BE%EF%BC%8C%3C/span%3E" _note="“三治”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%B8%89%E6%B2%BB%E2%80%9D%3C/span%3E"/>
              <outline text="实现科学立法、严格执法、公正司法、全民守法，" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%8E%B0%E7%A7%91%E5%AD%A6%E7%AB%8B%E6%B3%95%E3%80%81%E4%B8%A5%E6%A0%BC%E6%89%A7%E6%B3%95%E3%80%81%E5%85%AC%E6%AD%A3%E5%8F%B8%E6%B3%95%E3%80%81%E5%85%A8%E6%B0%91%E5%AE%88%E6%B3%95%EF%BC%8C%3C/span%3E" _note="“四个任务”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%9B%9B%E4%B8%AA%E4%BB%BB%E5%8A%A1%E2%80%9D%3C/span%3E"/>
              <outline text="促进国家治理体系和治理能力现代化" _mubu_text="%3Cspan%3E%E4%BF%83%E8%BF%9B%E5%9B%BD%E5%AE%B6%E6%B2%BB%E7%90%86%E4%BD%93%E7%B3%BB%E5%92%8C%E6%B2%BB%E7%90%86%E8%83%BD%E5%8A%9B%E7%8E%B0%E4%BB%A3%E5%8C%96%3C/span%3E" _note="“一个现代化”" _mubu_note="%3Cspan%3E%E2%80%9C%E4%B8%80%E4%B8%AA%E7%8E%B0%E4%BB%A3%E5%8C%96%E2%80%9D%3C/span%3E"/>
            </outline>
            <outline text="建设完备的法律规范体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E5%AE%8C%E5%A4%87%E7%9A%84%E6%B3%95%E5%BE%8B%E8%A7%84%E8%8C%83%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="建设高效的法治实施体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E9%AB%98%E6%95%88%E7%9A%84%E6%B3%95%E6%B2%BB%E5%AE%9E%E6%96%BD%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="建设严密的法治监督体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E4%B8%A5%E5%AF%86%E7%9A%84%E6%B3%95%E6%B2%BB%E7%9B%91%E7%9D%A3%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="建设有力的法治保障体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E6%9C%89%E5%8A%9B%E7%9A%84%E6%B3%95%E6%B2%BB%E4%BF%9D%E9%9A%9C%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="建设完善的党内法规体系" _mubu_text="%3Cspan%3E%E5%BB%BA%E8%AE%BE%E5%AE%8C%E5%96%84%E7%9A%84%E5%85%9A%E5%86%85%E6%B3%95%E8%A7%84%E4%BD%93%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="坚持“三依“共同推进、“三治”一体建设" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E2%80%9C%E4%B8%89%E4%BE%9D%E2%80%9C%E5%85%B1%E5%90%8C%E6%8E%A8%E8%BF%9B%E3%80%81%E2%80%9C%E4%B8%89%E6%B2%BB%E2%80%9D%E4%B8%80%E4%BD%93%E5%BB%BA%E8%AE%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="全面依法治国是一个系统性工程" _mubu_text="%3Cspan%3E%E5%85%A8%E9%9D%A2%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E6%98%AF%E4%B8%80%E4%B8%AA%E7%B3%BB%E7%BB%9F%E6%80%A7%E5%B7%A5%E7%A8%8B%3C/span%3E" _note="”三依三治“是全面依法治国的工作布局" _mubu_note="%3Cspan%3E%E2%80%9D%E4%B8%89%E4%BE%9D%E4%B8%89%E6%B2%BB%E2%80%9C%E6%98%AF%E5%85%A8%E9%9D%A2%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E7%9A%84%E5%B7%A5%E4%BD%9C%E5%B8%83%E5%B1%80%3C/span%3E"/>
            <outline text="法治国家是法治建设的目标" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E5%9B%BD%E5%AE%B6%E6%98%AF%E6%B3%95%E6%B2%BB%E5%BB%BA%E8%AE%BE%E7%9A%84%E7%9B%AE%E6%A0%87%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法治政府是建设法治国家的主体" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E6%94%BF%E5%BA%9C%E6%98%AF%E5%BB%BA%E8%AE%BE%E6%B3%95%E6%B2%BB%E5%9B%BD%E5%AE%B6%E7%9A%84%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法治社会是构筑法治国家的基础" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E7%A4%BE%E4%BC%9A%E6%98%AF%E6%9E%84%E7%AD%91%E6%B3%95%E6%B2%BB%E5%9B%BD%E5%AE%B6%E7%9A%84%E5%9F%BA%E7%A1%80%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="重点任务" _mubu_text="%3Cspan%3E%E9%87%8D%E7%82%B9%E4%BB%BB%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="首要任务" _mubu_text="%3Cspan%3E%E9%A6%96%E8%A6%81%E4%BB%BB%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="坚持依宪治国、依宪执政" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E5%AE%AA%E6%B2%BB%E5%9B%BD%E3%80%81%E4%BE%9D%E5%AE%AA%E6%89%A7%E6%94%BF%3C/span%3E" _note="" _mubu_note="">
              <outline text="坚持依法治国首先要坚持依宪治国，坚持依法执政首先要坚持依宪执政" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E6%B2%BB%E5%9B%BD%E9%A6%96%E5%85%88%E8%A6%81%E5%9D%9A%E6%8C%81%E4%BE%9D%E5%AE%AA%E6%B2%BB%E5%9B%BD%EF%BC%8C%E5%9D%9A%E6%8C%81%E4%BE%9D%E6%B3%95%E6%89%A7%E6%94%BF%E9%A6%96%E5%85%88%E8%A6%81%E5%9D%9A%E6%8C%81%E4%BE%9D%E5%AE%AA%E6%89%A7%E6%94%BF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宪法是治国理政的总章程" _mubu_text="%3Cspan%3E%E5%AE%AA%E6%B3%95%E6%98%AF%E6%B2%BB%E5%9B%BD%E7%90%86%E6%94%BF%E7%9A%84%E6%80%BB%E7%AB%A0%E7%A8%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="推进合宪性审查工作" _mubu_text="%3Cspan%3E%E6%8E%A8%E8%BF%9B%E5%90%88%E5%AE%AA%E6%80%A7%E5%AE%A1%E6%9F%A5%E5%B7%A5%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="深入开展宪法宣传教育" _mubu_text="%3Cspan%3E%E6%B7%B1%E5%85%A5%E5%BC%80%E5%B1%95%E5%AE%AA%E6%B3%95%E5%AE%A3%E4%BC%A0%E6%95%99%E8%82%B2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
