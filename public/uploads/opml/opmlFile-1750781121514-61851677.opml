                <outline text="作为人类文明成果" _mubu_text="%3Cspan%3E%E4%BD%9C%E4%B8%BA%E4%BA%BA%E7%B1%BB%E6%96%87%E6%98%8E%E6%88%90%E6%9E%9C%3C/span%3E" _note="随文明一同延续" _mubu_note="%3Cspan%3E%E9%9A%8F%E6%96%87%E6%98%8E%E4%B8%80%E5%90%8C%E5%BB%B6%E7%BB%AD%3C/span%3E"/>
                <outline text="法的发展的历史事实验证了" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E5%8F%91%E5%B1%95%E7%9A%84%E5%8E%86%E5%8F%B2%E4%BA%8B%E5%AE%9E%E9%AA%8C%E8%AF%81%E4%BA%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="法的移植" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E7%A7%BB%E6%A4%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="含义" _mubu_text="%3Cspan%3E%E5%90%AB%E4%B9%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="对同时代其他国家和地区法律制度的借鉴和学习" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%90%8C%E6%97%B6%E4%BB%A3%E5%85%B6%E4%BB%96%E5%9B%BD%E5%AE%B6%E5%92%8C%E5%9C%B0%E5%8C%BA%E6%B3%95%E5%BE%8B%E5%88%B6%E5%BA%A6%E7%9A%84%E5%80%9F%E9%89%B4%E5%92%8C%E5%AD%A6%E4%B9%A0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="前提" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="受同一规律的支配，互不排斥，可互相吸纳" _mubu_text="%3Cspan%3E%E5%8F%97%E5%90%8C%E4%B8%80%E8%A7%84%E5%BE%8B%E7%9A%84%E6%94%AF%E9%85%8D%EF%BC%8C%E4%BA%92%E4%B8%8D%E6%8E%92%E6%96%A5%EF%BC%8C%E5%8F%AF%E4%BA%92%E7%9B%B8%E5%90%B8%E7%BA%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="必然性和必要性" _mubu_text="%3Cspan%3E%E5%BF%85%E7%84%B6%E6%80%A7%E5%92%8C%E5%BF%85%E8%A6%81%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="社会发展和法的发展的不平衡性" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E5%8F%91%E5%B1%95%E5%92%8C%E6%B3%95%E7%9A%84%E5%8F%91%E5%B1%95%E7%9A%84%E4%B8%8D%E5%B9%B3%E8%A1%A1%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="市场经济的客观规律和根本特征" _mubu_text="%3Cspan%3E%E5%B8%82%E5%9C%BA%E7%BB%8F%E6%B5%8E%E7%9A%84%E5%AE%A2%E8%A7%82%E8%A7%84%E5%BE%8B%E5%92%8C%E6%A0%B9%E6%9C%AC%E7%89%B9%E5%BE%81%3C/span%3E" _note="要求冲破一切地域的限制，使国内市场和国际市场接轨" _mubu_note="%3Cspan%3E%E8%A6%81%E6%B1%82%E5%86%B2%E7%A0%B4%E4%B8%80%E5%88%87%E5%9C%B0%E5%9F%9F%E7%9A%84%E9%99%90%E5%88%B6%EF%BC%8C%E4%BD%BF%E5%9B%BD%E5%86%85%E5%B8%82%E5%9C%BA%E5%92%8C%E5%9B%BD%E9%99%85%E5%B8%82%E5%9C%BA%E6%8E%A5%E8%BD%A8%3C/span%3E"/>
                <outline text="法治现代化是社会现代化的基本内容和动力" _mubu_text="%3Cspan%3E%E6%B3%95%E6%B2%BB%E7%8E%B0%E4%BB%A3%E5%8C%96%E6%98%AF%E7%A4%BE%E4%BC%9A%E7%8E%B0%E4%BB%A3%E5%8C%96%E7%9A%84%E5%9F%BA%E6%9C%AC%E5%86%85%E5%AE%B9%E5%92%8C%E5%8A%A8%E5%8A%9B%3C/span%3E" _note="法的移植是法治现代化的一个过程和途径" _mubu_note="%3Cspan%3E%E6%B3%95%E7%9A%84%E7%A7%BB%E6%A4%8D%E6%98%AF%E6%B3%95%E6%B2%BB%E7%8E%B0%E4%BB%A3%E5%8C%96%E7%9A%84%E4%B8%80%E4%B8%AA%E8%BF%87%E7%A8%8B%E5%92%8C%E9%80%94%E5%BE%84%3C/span%3E"/>
                <outline text="是对外开放的应有内容" _mubu_text="%3Cspan%3E%E6%98%AF%E5%AF%B9%E5%A4%96%E5%BC%80%E6%94%BE%E7%9A%84%E5%BA%94%E6%9C%89%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="核心区别" _mubu_text="%3Cspan%3E%E6%A0%B8%E5%BF%83%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
              <outline text="不在空间，而体现为时间" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%9C%A8%E7%A9%BA%E9%97%B4%EF%BC%8C%E8%80%8C%E4%BD%93%E7%8E%B0%E4%B8%BA%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="继承强调跨不同历史类型，移植强调同时代" _mubu_text="%3Cspan%3E%E7%BB%A7%E6%89%BF%E5%BC%BA%E8%B0%83%E8%B7%A8%E4%B8%8D%E5%90%8C%E5%8E%86%E5%8F%B2%E7%B1%BB%E5%9E%8B%EF%BC%8C%E7%A7%BB%E6%A4%8D%E5%BC%BA%E8%B0%83%E5%90%8C%E6%97%B6%E4%BB%A3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="法的传统" _mubu_text="%3Cspan%3E%E6%B3%95%E7%9A%84%E4%BC%A0%E7%BB%9F%3C/span%3E" _note="" _mubu_note="">
          <outline text="法系" _mubu_text="%3Cspan%3E%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="法系的概念" _mubu_text="%3Cspan%3E%E6%B3%95%E7%B3%BB%E7%9A%84%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
              <outline text="具有某种共性或共同历史传统的法律总称" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%9F%90%E7%A7%8D%E5%85%B1%E6%80%A7%E6%88%96%E5%85%B1%E5%90%8C%E5%8E%86%E5%8F%B2%E4%BC%A0%E7%BB%9F%E7%9A%84%E6%B3%95%E5%BE%8B%E6%80%BB%E7%A7%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="英美法系与大陆法系" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%E6%B3%95%E7%B3%BB%E4%B8%8E%E5%A4%A7%E9%99%86%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="英美法系" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="普通法法系、判例法系、不成文法系、海洋法系" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E6%B3%95%E6%B3%95%E7%B3%BB%E3%80%81%E5%88%A4%E4%BE%8B%E6%B3%95%E7%B3%BB%E3%80%81%E4%B8%8D%E6%88%90%E6%96%87%E6%B3%95%E7%B3%BB%E3%80%81%E6%B5%B7%E6%B4%8B%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="判例法（普通法）为基础发展形成" _mubu_text="%3Cspan%3E%E5%88%A4%E4%BE%8B%E6%B3%95%EF%BC%88%E6%99%AE%E9%80%9A%E6%B3%95%EF%BC%89%E4%B8%BA%E5%9F%BA%E7%A1%80%E5%8F%91%E5%B1%95%E5%BD%A2%E6%88%90%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="大陆法系" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="民法法系、罗马法系、日耳曼法系、法典法系、成文法系" _mubu_text="%3Cspan%3E%E6%B0%91%E6%B3%95%E6%B3%95%E7%B3%BB%E3%80%81%E7%BD%97%E9%A9%AC%E6%B3%95%E7%B3%BB%E3%80%81%E6%97%A5%E8%80%B3%E6%9B%BC%E6%B3%95%E7%B3%BB%E3%80%81%E6%B3%95%E5%85%B8%E6%B3%95%E7%B3%BB%E3%80%81%E6%88%90%E6%96%87%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="以罗马法为基础发展起来" _mubu_text="%3Cspan%3E%E4%BB%A5%E7%BD%97%E9%A9%AC%E6%B3%95%E4%B8%BA%E5%9F%BA%E7%A1%80%E5%8F%91%E5%B1%95%E8%B5%B7%E6%9D%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法国、德国为代表" _mubu_text="%3Cspan%3E%E6%B3%95%E5%9B%BD%E3%80%81%E5%BE%B7%E5%9B%BD%E4%B8%BA%E4%BB%A3%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="看似英美法系，实际大陆法系" _mubu_text="%3Cspan%3E%E7%9C%8B%E4%BC%BC%E8%8B%B1%E7%BE%8E%E6%B3%95%E7%B3%BB%EF%BC%8C%E5%AE%9E%E9%99%85%E5%A4%A7%E9%99%86%E6%B3%95%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
                <outline text="英国的苏格兰、加拿大的魁北克、美国的路易斯安那、南非、非律宾" _mubu_text="%3Cspan%3E%E8%8B%B1%E5%9B%BD%E7%9A%84%E8%8B%8F%E6%A0%BC%E5%85%B0%E3%80%81%E5%8A%A0%E6%8B%BF%E5%A4%A7%E7%9A%84%E9%AD%81%E5%8C%97%E5%85%8B%E3%80%81%E7%BE%8E%E5%9B%BD%E7%9A%84%E8%B7%AF%E6%98%93%E6%96%AF%E5%AE%89%E9%82%A3%E3%80%81%E5%8D%97%E9%9D%9E%E3%80%81%E9%9D%9E%E5%BE%8B%E5%AE%BE%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="两大法系的区别" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%A4%E5%A4%A7%E6%B3%95%E7%B3%BB%E7%9A%84%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
                <outline text="法律渊源不同" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%B8%8A%E6%BA%90%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="大陆" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="正式的法的渊源主要是指制定法" _mubu_text="%3Cspan%3E%E6%AD%A3%E5%BC%8F%E7%9A%84%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%E4%B8%BB%E8%A6%81%E6%98%AF%E6%8C%87%E5%88%B6%E5%AE%9A%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="英美" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="制定法和判例法都是正式的法的渊源" _mubu_text="%3Cspan%3E%E5%88%B6%E5%AE%9A%E6%B3%95%E5%92%8C%E5%88%A4%E4%BE%8B%E6%B3%95%E9%83%BD%E6%98%AF%E6%AD%A3%E5%BC%8F%E7%9A%84%E6%B3%95%E7%9A%84%E6%B8%8A%E6%BA%90%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="法律分类不同" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%88%86%E7%B1%BB%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="大陆" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="公法和私法" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B3%95%E5%92%8C%E7%A7%81%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="英美" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="普通法和衡平体" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E6%B3%95%E5%92%8C%E8%A1%A1%E5%B9%B3%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="法典编纂不同" _mubu_text="%3Cspan%3E%E6%B3%95%E5%85%B8%E7%BC%96%E7%BA%82%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="大陆" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="基本法律一般采用系统的法典形式" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E6%B3%95%E5%BE%8B%E4%B8%80%E8%88%AC%E9%87%87%E7%94%A8%E7%B3%BB%E7%BB%9F%E7%9A%84%E6%B3%95%E5%85%B8%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="英美" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="往往是单行的法律、法规" _mubu_text="%3Cspan%3E%E5%BE%80%E5%BE%80%E6%98%AF%E5%8D%95%E8%A1%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E3%80%81%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
                <outline text="诉讼程序和判决程式不同" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%E5%92%8C%E5%88%A4%E5%86%B3%E7%A8%8B%E5%BC%8F%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="大陆" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="以法官为重心，奉行职权主义" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%B3%95%E5%AE%98%E4%B8%BA%E9%87%8D%E5%BF%83%EF%BC%8C%E5%A5%89%E8%A1%8C%E8%81%8C%E6%9D%83%E4%B8%BB%E4%B9%89%3C/span%3E" _note="具在纠问程序的特点" _mubu_note="%3Cspan%3E%E5%85%B7%E5%9C%A8%E7%BA%A0%E9%97%AE%E7%A8%8B%E5%BA%8F%E7%9A%84%E7%89%B9%E7%82%B9%3C/span%3E"/>
                  </outline>
                  <outline text="英美" _mubu_text="%3Cspan%3E%E8%8B%B1%E7%BE%8E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="奉行当事人主义" _mubu_text="%3Cspan%3E%E5%A5%89%E8%A1%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BB%E4%B9%89%3C/span%3E" _note="法官消极、中立" _mubu_note="%3Cspan%3E%E6%B3%95%E5%AE%98%E6%B6%88%E6%9E%81%E3%80%81%E4%B8%AD%E7%AB%8B%3C/span%3E"/>
                  </outline>
                </outline>
                <outline text="法律思维方式不同" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%80%9D%E7%BB%B4%E6%96%B9%E5%BC%8F%E4%B8%8D%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="大陆" _mubu_text="%3Cspan%3E%E5%A4%A7%E9%99%86%3C/span%3E" _note="" _mubu_note="">
                    <outline text="演绎性思维" _mubu_text="%3Cspan%3E%E6%BC%94%E7%BB%8E%E6%80%A7%E6%80%9D%E7%BB%B4%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
