                  <outline text="检察长不同意检委会多数人决定" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%95%BF%E4%B8%8D%E5%90%8C%E6%84%8F%E6%A3%80%E5%A7%94%E4%BC%9A%E5%A4%9A%E6%95%B0%E4%BA%BA%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                    <outline text="属于在办案件的，报上一级检决定" _mubu_text="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E5%9C%A8%E5%8A%9E%E6%A1%88%E4%BB%B6%E7%9A%84%EF%BC%8C%E6%8A%A5%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%A3%80%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="其他重大事项，报上一级检或同级人常决定" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%EF%BC%8C%E6%8A%A5%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%A3%80%E6%88%96%E5%90%8C%E7%BA%A7%E4%BA%BA%E5%B8%B8%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="原则" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="检察权统一行使原则" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E6%9D%83%E7%BB%9F%E4%B8%80%E8%A1%8C%E4%BD%BF%E5%8E%9F%E5%88%99%3C/span%3E" _note="即检察一体化原则" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%A3%80%E5%AF%9F%E4%B8%80%E4%BD%93%E5%8C%96%E5%8E%9F%E5%88%99%3C/span%3E">
              <outline text="各级检察院、检察官依法构成统一的整体" _mubu_text="%3Cspan%3E%E5%90%84%E7%BA%A7%E6%A3%80%E5%AF%9F%E9%99%A2%E3%80%81%E6%A3%80%E5%AF%9F%E5%AE%98%E4%BE%9D%E6%B3%95%E6%9E%84%E6%88%90%E7%BB%9F%E4%B8%80%E7%9A%84%E6%95%B4%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="上下级检察机关和检察官之间存在着上命下从的领导关系" _mubu_text="%3Cspan%3E%E4%B8%8A%E4%B8%8B%E7%BA%A7%E6%A3%80%E5%AF%9F%E6%9C%BA%E5%85%B3%E5%92%8C%E6%A3%80%E5%AF%9F%E5%AE%98%E4%B9%8B%E9%97%B4%E5%AD%98%E5%9C%A8%E7%9D%80%E4%B8%8A%E5%91%BD%E4%B8%8B%E4%BB%8E%E7%9A%84%E9%A2%86%E5%AF%BC%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="各地、各级检察机关之间具有职能协助的义务" _mubu_text="%3Cspan%3E%E5%90%84%E5%9C%B0%E3%80%81%E5%90%84%E7%BA%A7%E6%A3%80%E5%AF%9F%E6%9C%BA%E5%85%B3%E4%B9%8B%E9%97%B4%E5%85%B7%E6%9C%89%E8%81%8C%E8%83%BD%E5%8D%8F%E5%8A%A9%E7%9A%84%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="检察官之间和人民检察院之间职务上可相互承继、移转和代理" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E5%AE%98%E4%B9%8B%E9%97%B4%E5%92%8C%E4%BA%BA%E6%B0%91%E6%A3%80%E5%AF%9F%E9%99%A2%E4%B9%8B%E9%97%B4%E8%81%8C%E5%8A%A1%E4%B8%8A%E5%8F%AF%E7%9B%B8%E4%BA%92%E6%89%BF%E7%BB%A7%E3%80%81%E7%A7%BB%E8%BD%AC%E5%92%8C%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="检察权独立行使原则" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E6%9D%83%E7%8B%AC%E7%AB%8B%E8%A1%8C%E4%BD%BF%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对诉讼活动实行法律监督原则" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%AF%89%E8%AE%BC%E6%B4%BB%E5%8A%A8%E5%AE%9E%E8%A1%8C%E6%B3%95%E5%BE%8B%E7%9B%91%E7%9D%A3%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="主要制度" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="检务公开制度" _mubu_text="%3Cspan%3E%E6%A3%80%E5%8A%A1%E5%85%AC%E5%BC%80%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="人民监督员制度" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%B0%91%E7%9B%91%E7%9D%A3%E5%91%98%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="其他" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="检察官职业道德" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E5%AE%98%E8%81%8C%E4%B8%9A%E9%81%93%E5%BE%B7%3C/span%3E" _note="“法官讲象，检察官有担当”" _mubu_note="%3Cspan%3E%E2%80%9C%E6%B3%95%E5%AE%98%E8%AE%B2%E8%B1%A1%EF%BC%8C%E6%A3%80%E5%AF%9F%E5%AE%98%E6%9C%89%E6%8B%85%E5%BD%93%E2%80%9D%3C/span%3E">
          <outline text="忠诚" _mubu_text="%3Cspan%3E%E5%BF%A0%E8%AF%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="公正" _mubu_text="%3Cspan%3E%E5%85%AC%E6%AD%A3%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="廉洁" _mubu_text="%3Cspan%3E%E5%BB%89%E6%B4%81%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="为民" _mubu_text="%3Cspan%3E%E4%B8%BA%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="担当" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%8B%85%E5%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="坚持担当精神，强化法律监督" _mubu_text="%3Cspan%3E%E5%9D%9A%E6%8C%81%E6%8B%85%E5%BD%93%E7%B2%BE%E7%A5%9E%EF%BC%8C%E5%BC%BA%E5%8C%96%E6%B3%95%E5%BE%8B%E7%9B%91%E7%9D%A3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="要坚决打击损害群众利益的各类犯罪，增强群众安全感和满意度" _mubu_text="%3Cspan%3E%E8%A6%81%E5%9D%9A%E5%86%B3%E6%89%93%E5%87%BB%E6%8D%9F%E5%AE%B3%E7%BE%A4%E4%BC%97%E5%88%A9%E7%9B%8A%E7%9A%84%E5%90%84%E7%B1%BB%E7%8A%AF%E7%BD%AA%EF%BC%8C%E5%A2%9E%E5%BC%BA%E7%BE%A4%E4%BC%97%E5%AE%89%E5%85%A8%E6%84%9F%E5%92%8C%E6%BB%A1%E6%84%8F%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="要坚守良知、公正执法、执法公开，自觉接受人民群众和社会的监督" _mubu_text="%3Cspan%3E%E8%A6%81%E5%9D%9A%E5%AE%88%E8%89%AF%E7%9F%A5%E3%80%81%E5%85%AC%E6%AD%A3%E6%89%A7%E6%B3%95%E3%80%81%E6%89%A7%E6%B3%95%E5%85%AC%E5%BC%80%EF%BC%8C%E8%87%AA%E8%A7%89%E6%8E%A5%E5%8F%97%E4%BA%BA%E6%B0%91%E7%BE%A4%E4%BC%97%E5%92%8C%E7%A4%BE%E4%BC%9A%E7%9A%84%E7%9B%91%E7%9D%A3%3C/span%3E" _note="以公开促公正" _mubu_note="%3Cspan%3E%E4%BB%A5%E5%85%AC%E5%BC%80%E4%BF%83%E5%85%AC%E6%AD%A3%3C/span%3E"/>
            <outline text="要直面矛盾，正视问题" _mubu_text="%3Cspan%3E%E8%A6%81%E7%9B%B4%E9%9D%A2%E7%9F%9B%E7%9B%BE%EF%BC%8C%E6%AD%A3%E8%A7%86%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="检察官(法官)惩戒委员会" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E5%AE%98(%E6%B3%95%E5%AE%98)%E6%83%A9%E6%88%92%E5%A7%94%E5%91%98%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="设置" _mubu_text="%3Cspan%3E%E8%AE%BE%E7%BD%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="最高和省级法检设立" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E5%92%8C%E7%9C%81%E7%BA%A7%E6%B3%95%E6%A3%80%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="组成" _mubu_text="%3Cspan%3E%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="法官、检察官代表≥全体委员50%" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%98%E3%80%81%E6%A3%80%E5%AF%9F%E5%AE%98%E4%BB%A3%E8%A1%A8%E2%89%A5%E5%85%A8%E4%BD%93%E5%A7%94%E5%91%9850%25%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="职责" _mubu_text="%3Cspan%3E%E8%81%8C%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="不直接受理对法官、检察官的举报、投诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%9B%B4%E6%8E%A5%E5%8F%97%E7%90%86%E5%AF%B9%E6%B3%95%E5%AE%98%E3%80%81%E6%A3%80%E5%AF%9F%E5%AE%98%E7%9A%84%E4%B8%BE%E6%8A%A5%E3%80%81%E6%8A%95%E8%AF%89%3C/span%3E" _note="如收到举报投诉材料，应转交有关部门处理" _mubu_note="%3Cspan%3E%E5%A6%82%E6%94%B6%E5%88%B0%E4%B8%BE%E6%8A%A5%E6%8A%95%E8%AF%89%E6%9D%90%E6%96%99%EF%BC%8C%E5%BA%94%E8%BD%AC%E4%BA%A4%E6%9C%89%E5%85%B3%E9%83%A8%E9%97%A8%E5%A4%84%E7%90%86%3C/span%3E"/>
          </outline>
          <outline text="案件来源" _mubu_text="%3Cspan%3E%E6%A1%88%E4%BB%B6%E6%9D%A5%E6%BA%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="法检认为需要认定的，应在查明事实的基础上，提请惩戒委员会审议" _mubu_text="%3Cspan%3E%E6%B3%95%E6%A3%80%E8%AE%A4%E4%B8%BA%E9%9C%80%E8%A6%81%E8%AE%A4%E5%AE%9A%E7%9A%84%EF%BC%8C%E5%BA%94%E5%9C%A8%E6%9F%A5%E6%98%8E%E4%BA%8B%E5%AE%9E%E7%9A%84%E5%9F%BA%E7%A1%80%E4%B8%8A%EF%BC%8C%E6%8F%90%E8%AF%B7%E6%83%A9%E6%88%92%E5%A7%94%E5%91%98%E4%BC%9A%E5%AE%A1%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="决定通过" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E9%80%9A%E8%BF%87%3C/span%3E" _note="" _mubu_note="">
            <outline text="全体委员的2/3以上的多数通过，提出审查意见" _mubu_text="%3Cspan%3E%E5%85%A8%E4%BD%93%E5%A7%94%E5%91%98%E7%9A%842/3%E4%BB%A5%E4%B8%8A%E7%9A%84%E5%A4%9A%E6%95%B0%E9%80%9A%E8%BF%87%EF%BC%8C%E6%8F%90%E5%87%BA%E5%AE%A1%E6%9F%A5%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="异议" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="向委员会提出" _mubu_text="%3Cspan%3E%E5%90%91%E5%A7%94%E5%91%98%E4%BC%9A%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="决定处理" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="法检作出决定，并给予相应处理" _mubu_text="%3Cspan%3E%E6%B3%95%E6%A3%80%E4%BD%9C%E5%87%BA%E5%86%B3%E5%AE%9A%EF%BC%8C%E5%B9%B6%E7%BB%99%E4%BA%88%E7%9B%B8%E5%BA%94%E5%A4%84%E7%90%86%3C/span%3E" _note="原单位作出" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%8D%95%E4%BD%8D%E4%BD%9C%E5%87%BA%3C/span%3E"/>
          </outline>
          <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="向作出决定的法检申请复议" _mubu_text="%3Cspan%3E%E5%90%91%E4%BD%9C%E5%87%BA%E5%86%B3%E5%AE%9A%E7%9A%84%E6%B3%95%E6%A3%80%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可向上一级法检申诉" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%B3%95%E6%A3%80%E7%94%B3%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
