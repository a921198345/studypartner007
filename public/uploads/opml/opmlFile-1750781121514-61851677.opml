                    <outline text="并经投票村民的过半数通过" _mubu_text="%3Cspan%3E%E5%B9%B6%E7%BB%8F%E6%8A%95%E7%A5%A8%E6%9D%91%E6%B0%91%E7%9A%84%E8%BF%87%E5%8D%8A%E6%95%B0%E9%80%9A%E8%BF%87%3C/span%3E" _note="只有此处双过半，其他罢免都全体过半" _mubu_note="%3Cspan%3E%E5%8F%AA%E6%9C%89%E6%AD%A4%E5%A4%84%E5%8F%8C%E8%BF%87%E5%8D%8A%EF%BC%8C%E5%85%B6%E4%BB%96%E7%BD%A2%E5%85%8D%E9%83%BD%E5%85%A8%E4%BD%93%E8%BF%87%E5%8D%8A%3C/span%3E"/>
                  </outline>
                </outline>
              </outline>
              <outline text="下设委员会" _mubu_text="%3Cspan%3E%E4%B8%8B%E8%AE%BE%E5%A7%94%E5%91%98%E4%BC%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="根据需要设立" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E9%9C%80%E8%A6%81%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="村委会成员分工负责" _mubu_text="%3Cspan%3E%E6%9D%91%E5%A7%94%E4%BC%9A%E6%88%90%E5%91%98%E5%88%86%E5%B7%A5%E8%B4%9F%E8%B4%A3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="村务公开" _mubu_text="%3Cspan%3E%E6%9D%91%E5%8A%A1%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
                <outline text="周期" _mubu_text="%3Cspan%3E%E5%91%A8%E6%9C%9F%3C/span%3E" _note="" _mubu_note="">
                  <outline text="一般事项至少每季度公布一次" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%BA%8B%E9%A1%B9%E8%87%B3%E5%B0%91%E6%AF%8F%E5%AD%A3%E5%BA%A6%E5%85%AC%E5%B8%83%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="集体财务往来较多的，财务收支情况应每月公布一次" _mubu_text="%3Cspan%3E%E9%9B%86%E4%BD%93%E8%B4%A2%E5%8A%A1%E5%BE%80%E6%9D%A5%E8%BE%83%E5%A4%9A%E7%9A%84%EF%BC%8C%E8%B4%A2%E5%8A%A1%E6%94%B6%E6%94%AF%E6%83%85%E5%86%B5%E5%BA%94%E6%AF%8F%E6%9C%88%E5%85%AC%E5%B8%83%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="涉及村民利益的重大事项应随时公布" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E6%9D%91%E6%B0%91%E5%88%A9%E7%9B%8A%E7%9A%84%E9%87%8D%E5%A4%A7%E4%BA%8B%E9%A1%B9%E5%BA%94%E9%9A%8F%E6%97%B6%E5%85%AC%E5%B8%83%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="村委会成员的任期和离任经济责任审计" _mubu_text="%3Cspan%3E%E6%9D%91%E5%A7%94%E4%BC%9A%E6%88%90%E5%91%98%E7%9A%84%E4%BB%BB%E6%9C%9F%E5%92%8C%E7%A6%BB%E4%BB%BB%E7%BB%8F%E6%B5%8E%E8%B4%A3%E4%BB%BB%E5%AE%A1%E8%AE%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="由县政府农业、财政部门或乡镇政府负责组织" _mubu_text="%3Cspan%3E%E7%94%B1%E5%8E%BF%E6%94%BF%E5%BA%9C%E5%86%9C%E4%B8%9A%E3%80%81%E8%B4%A2%E6%94%BF%E9%83%A8%E9%97%A8%E6%88%96%E4%B9%A1%E9%95%87%E6%94%BF%E5%BA%9C%E8%B4%9F%E8%B4%A3%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="应建立村务监督机构" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BB%BA%E7%AB%8B%E6%9D%91%E5%8A%A1%E7%9B%91%E7%9D%A3%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="村民会议" _mubu_text="%3Cspan%3E%E6%9D%91%E6%B0%91%E4%BC%9A%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
