                <outline text="中国人" _mubu_text="%3Cspan%3E%E4%B8%AD%E5%9B%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="25~65周岁" _mubu_text="%3Cspan%3E25~65%E5%91%A8%E5%B2%81%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="过法考" _mubu_text="%3Cspan%3E%E8%BF%87%E6%B3%95%E8%80%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="经过实习" _mubu_text="%3Cspan%3E%E7%BB%8F%E8%BF%87%E5%AE%9E%E4%B9%A0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="在公证机构实习2年以上" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%85%AC%E8%AF%81%E6%9C%BA%E6%9E%84%E5%AE%9E%E4%B9%A02%E5%B9%B4%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="或具有3年以上其他法律职业经历+在公证机构实习1年以上" _mubu_text="%3Cspan%3E%E6%88%96%E5%85%B7%E6%9C%893%E5%B9%B4%E4%BB%A5%E4%B8%8A%E5%85%B6%E4%BB%96%E6%B3%95%E5%BE%8B%E8%81%8C%E4%B8%9A%E7%BB%8F%E5%8E%86+%E5%9C%A8%E5%85%AC%E8%AF%81%E6%9C%BA%E6%9E%84%E5%AE%9E%E4%B9%A01%E5%B9%B4%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="消极" _mubu_text="%3Cspan%3E%E6%B6%88%E6%9E%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="无/限行能" _mubu_text="%3Cspan%3E%E6%97%A0/%E9%99%90%E8%A1%8C%E8%83%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="故意/职务过失犯罪" _mubu_text="%3Cspan%3E%E6%95%85%E6%84%8F/%E8%81%8C%E5%8A%A1%E8%BF%87%E5%A4%B1%E7%8A%AF%E7%BD%AA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="开除公职、吊证" _mubu_text="%3Cspan%3E%E5%BC%80%E9%99%A4%E5%85%AC%E8%81%8C%E3%80%81%E5%90%8A%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="申请程序" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="公证机构推荐→所在地司法局报省级审核→报国务院司法行政部门任命" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E6%9C%BA%E6%9E%84%E6%8E%A8%E8%8D%90%E2%86%92%E6%89%80%E5%9C%A8%E5%9C%B0%E5%8F%B8%E6%B3%95%E5%B1%80%E6%8A%A5%E7%9C%81%E7%BA%A7%E5%AE%A1%E6%A0%B8%E2%86%92%E6%8A%A5%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8F%B8%E6%B3%95%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%E4%BB%BB%E5%91%BD%3C/span%3E" _note="由省级司法行政部门颁发公证员执业证书" _mubu_note="%3Cspan%3E%E7%94%B1%E7%9C%81%E7%BA%A7%E5%8F%B8%E6%B3%95%E8%A1%8C%E6%94%BF%E9%83%A8%E9%97%A8%E9%A2%81%E5%8F%91%E5%85%AC%E8%AF%81%E5%91%98%E6%89%A7%E4%B8%9A%E8%AF%81%E4%B9%A6%3C/span%3E"/>
            </outline>
            <outline text="义务" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="不得办理与本人及近亲属有利害关系的公证" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%8A%9E%E7%90%86%E4%B8%8E%E6%9C%AC%E4%BA%BA%E5%8F%8A%E8%BF%91%E4%BA%B2%E5%B1%9E%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E7%9A%84%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="公证机构" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="不按行政区划层层设立" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%8C%89%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%88%92%E5%B1%82%E5%B1%82%E8%AE%BE%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="最低到县" _mubu_text="%3Cspan%3E%E6%9C%80%E4%BD%8E%E5%88%B0%E5%8E%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="市以上名称中可有字号" _mubu_text="%3Cspan%3E%E5%B8%82%E4%BB%A5%E4%B8%8A%E5%90%8D%E7%A7%B0%E4%B8%AD%E5%8F%AF%E6%9C%89%E5%AD%97%E5%8F%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="公证员≥2" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E5%91%98%E2%89%A52%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="不予办理公证" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8A%9E%E7%90%86%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="“不合法、不真实、不给钱”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%8D%E5%90%88%E6%B3%95%E3%80%81%E4%B8%8D%E7%9C%9F%E5%AE%9E%E3%80%81%E4%B8%8D%E7%BB%99%E9%92%B1%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="应当终止公证" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E7%BB%88%E6%AD%A2%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="当事人原因" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="超期、撤回、阻挠、死" _mubu_note="%3Cspan%3E%E8%B6%85%E6%9C%9F%E3%80%81%E6%92%A4%E5%9B%9E%E3%80%81%E9%98%BB%E6%8C%A0%E3%80%81%E6%AD%BB%3C/span%3E"/>
          </outline>
          <outline text="公证救济" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="复查" _mubu_text="%3Cspan%3E%E5%A4%8D%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="(应)知道起1年/作出起20年" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E8%B5%B71%E5%B9%B4/%E4%BD%9C%E5%87%BA%E8%B5%B720%E5%B9%B4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="维持、更正、撤销、自始无效" _mubu_text="%3Cspan%3E%E7%BB%B4%E6%8C%81%E3%80%81%E6%9B%B4%E6%AD%A3%E3%80%81%E6%92%A4%E9%94%80%E3%80%81%E8%87%AA%E5%A7%8B%E6%97%A0%E6%95%88%3C/span%3E" _note="" _mubu_note="">
                <outline text="撤销、补正的报地方协会备案+录入系统" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E3%80%81%E8%A1%A5%E6%AD%A3%E7%9A%84%E6%8A%A5%E5%9C%B0%E6%96%B9%E5%8D%8F%E4%BC%9A%E5%A4%87%E6%A1%88+%E5%BD%95%E5%85%A5%E7%B3%BB%E7%BB%9F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="撤销后费用按过错责任退还" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E5%90%8E%E8%B4%B9%E7%94%A8%E6%8C%89%E8%BF%87%E9%94%99%E8%B4%A3%E4%BB%BB%E9%80%80%E8%BF%98%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="诉讼" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="民诉" _mubu_note="%3Cspan%3E%E6%B0%91%E8%AF%89%3C/span%3E"/>
          </outline>
          <outline text="公证程序的特别规定" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%85%AC%E8%AF%81%E7%A8%8B%E5%BA%8F%E7%9A%84%E7%89%B9%E5%88%AB%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="公证代理" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E4%BB%A3%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="可委托他人代理" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%A7%94%E6%89%98%E4%BB%96%E4%BA%BA%E4%BB%A3%E7%90%86%3C/span%3E" _note="遗嘱、生存、收养关系等除外" _mubu_note="%3Cspan%3E%E9%81%97%E5%98%B1%E3%80%81%E7%94%9F%E5%AD%98%E3%80%81%E6%94%B6%E5%85%BB%E5%85%B3%E7%B3%BB%E7%AD%89%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="现场监督类公证" _mubu_text="%3Cspan%3E%E7%8E%B0%E5%9C%BA%E7%9B%91%E7%9D%A3%E7%B1%BB%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="2人共同办理" _mubu_text="%3Cspan%3E2%E4%BA%BA%E5%85%B1%E5%90%8C%E5%8A%9E%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="遗嘱公证" _mubu_text="%3Cspan%3E%E9%81%97%E5%98%B1%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="2人共同办理" _mubu_text="%3Cspan%3E2%E4%BA%BA%E5%85%B1%E5%90%8C%E5%8A%9E%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只能由1名" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E7%94%B11%E5%90%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="应请1名见证人在场" _mubu_text="%3Cspan%3E%E5%BA%94%E8%AF%B71%E5%90%8D%E8%A7%81%E8%AF%81%E4%BA%BA%E5%9C%A8%E5%9C%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="保全证据公证" _mubu_text="%3Cspan%3E%E4%BF%9D%E5%85%A8%E8%AF%81%E6%8D%AE%E5%85%AC%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="2人承办，亲自外出办理" _mubu_text="%3Cspan%3E2%E4%BA%BA%E6%89%BF%E5%8A%9E%EF%BC%8C%E4%BA%B2%E8%87%AA%E5%A4%96%E5%87%BA%E5%8A%9E%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="违法手段取得证据的，不予办理" _mubu_text="%3Cspan%3E%E8%BF%9D%E6%B3%95%E6%89%8B%E6%AE%B5%E5%8F%96%E5%BE%97%E8%AF%81%E6%8D%AE%E7%9A%84%EF%BC%8C%E4%B8%8D%E4%BA%88%E5%8A%9E%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="职业责任" _mubu_text="%3Cspan%3E%E8%81%8C%E4%B8%9A%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="公证员" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E5%91%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="纪律处分" _mubu_text="%3Cspan%3E%E7%BA%AA%E5%BE%8B%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="警告、严重警告、罚款、记过、暂停会员资格、取消会员资格" _mubu_text="%3Cspan%3E%E8%AD%A6%E5%91%8A%E3%80%81%E4%B8%A5%E9%87%8D%E8%AD%A6%E5%91%8A%E3%80%81%E7%BD%9A%E6%AC%BE%E3%80%81%E8%AE%B0%E8%BF%87%E3%80%81%E6%9A%82%E5%81%9C%E4%BC%9A%E5%91%98%E8%B5%84%E6%A0%BC%E3%80%81%E5%8F%96%E6%B6%88%E4%BC%9A%E5%91%98%E8%B5%84%E6%A0%BC%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="行政处分" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%A4%84%E5%88%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="警告、罚款、停止执业、没收违法所得、吊证" _mubu_text="%3Cspan%3E%E8%AD%A6%E5%91%8A%E3%80%81%E7%BD%9A%E6%AC%BE%E3%80%81%E5%81%9C%E6%AD%A2%E6%89%A7%E4%B8%9A%E3%80%81%E6%B2%A1%E6%94%B6%E8%BF%9D%E6%B3%95%E6%89%80%E5%BE%97%E3%80%81%E5%90%8A%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="公证机构" _mubu_text="%3Cspan%3E%E5%85%AC%E8%AF%81%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="警告、罚款、没收违法所得、停业整顿" _mubu_text="%3Cspan%3E%E8%AD%A6%E5%91%8A%E3%80%81%E7%BD%9A%E6%AC%BE%E3%80%81%E6%B2%A1%E6%94%B6%E8%BF%9D%E6%B3%95%E6%89%80%E5%BE%97%E3%80%81%E5%81%9C%E4%B8%9A%E6%95%B4%E9%A1%BF%3C/span%3E" _note="" _mubu_note="">
                <outline text="无吊证" _mubu_text="%3Cspan%3E%E6%97%A0%E5%90%8A%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="民责顺序先机构再公证员" _mubu_text="%3Cspan%3E%E6%B0%91%E8%B4%A3%E9%A1%BA%E5%BA%8F%E5%85%88%E6%9C%BA%E6%9E%84%E5%86%8D%E5%85%AC%E8%AF%81%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
