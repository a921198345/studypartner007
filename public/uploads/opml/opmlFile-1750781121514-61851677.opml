              <outline text="责任自负" _mubu_text="%3Cspan%3E%E8%B4%A3%E4%BB%BB%E8%87%AA%E8%B4%9F%3C/span%3E" _note="也会存在责任转承" _mubu_note="%3Cspan%3E%E4%B9%9F%E4%BC%9A%E5%AD%98%E5%9C%A8%E8%B4%A3%E4%BB%BB%E8%BD%AC%E6%89%BF%3C/span%3E"/>
            </outline>
            <outline text="免责条件" _mubu_text="%3Cspan%3E%E5%85%8D%E8%B4%A3%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
