        <outline text="法律责任" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="概念" _mubu_text="%3Cspan%3E%E6%A6%82%E5%BF%B5%3C/span%3E" _note="" _mubu_note="">
            <outline text="产生原因" _mubu_text="%3Cspan%3E%E4%BA%A7%E7%94%9F%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
              <outline text="由于违法行为、违约行为或由于法律特别规定" _mubu_text="%3Cspan%3E%E7%94%B1%E4%BA%8E%E8%BF%9D%E6%B3%95%E8%A1%8C%E4%B8%BA%E3%80%81%E8%BF%9D%E7%BA%A6%E8%A1%8C%E4%B8%BA%E6%88%96%E7%94%B1%E4%BA%8E%E6%B3%95%E5%BE%8B%E7%89%B9%E5%88%AB%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="以法律规定或法律义务为前提" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E6%88%96%E6%B3%95%E5%BE%8B%E4%B9%89%E5%8A%A1%E4%B8%BA%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="由国家强制力实施" _mubu_text="%3Cspan%3E%E7%94%B1%E5%9B%BD%E5%AE%B6%E5%BC%BA%E5%88%B6%E5%8A%9B%E5%AE%9E%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="法律责任的竞合" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%E7%9A%84%E7%AB%9E%E5%90%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="同一法律主体" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E6%B3%95%E5%BE%8B%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="实施了一个行为" _mubu_text="%3Cspan%3E%E5%AE%9E%E6%96%BD%E4%BA%86%E4%B8%80%E4%B8%AA%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="该行为符合两个或两个以上的法律责任构成要件" _mubu_text="%3Cspan%3E%E8%AF%A5%E8%A1%8C%E4%B8%BA%E7%AC%A6%E5%90%88%E4%B8%A4%E4%B8%AA%E6%88%96%E4%B8%A4%E4%B8%AA%E4%BB%A5%E4%B8%8A%E7%9A%84%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%E6%9E%84%E6%88%90%E8%A6%81%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="数个法律责任之间相互冲突" _mubu_text="%3Cspan%3E%E6%95%B0%E4%B8%AA%E6%B3%95%E5%BE%8B%E8%B4%A3%E4%BB%BB%E4%B9%8B%E9%97%B4%E7%9B%B8%E4%BA%92%E5%86%B2%E7%AA%81%3C/span%3E" _note="不能被其中之一吸收，也不能并存" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E8%A2%AB%E5%85%B6%E4%B8%AD%E4%B9%8B%E4%B8%80%E5%90%B8%E6%94%B6%EF%BC%8C%E4%B9%9F%E4%B8%8D%E8%83%BD%E5%B9%B6%E5%AD%98%3C/span%3E"/>
            <outline text="解决办法" _mubu_text="%3Cspan%3E%E8%A7%A3%E5%86%B3%E5%8A%9E%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="从一重" _mubu_text="%3Cspan%3E%E4%BB%8E%E4%B8%80%E9%87%8D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="或当事人自主选择" _mubu_text="%3Cspan%3E%E6%88%96%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%87%AA%E4%B8%BB%E9%80%89%E6%8B%A9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="常考：定金违约金、想象竞合犯、侵权与违约" _mubu_text="%3Cspan%3E%E5%B8%B8%E8%80%83%EF%BC%9A%E5%AE%9A%E9%87%91%E8%BF%9D%E7%BA%A6%E9%87%91%E3%80%81%E6%83%B3%E8%B1%A1%E7%AB%9E%E5%90%88%E7%8A%AF%E3%80%81%E4%BE%B5%E6%9D%83%E4%B8%8E%E8%BF%9D%E7%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
