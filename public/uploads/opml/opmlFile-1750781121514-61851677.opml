                <outline text="许可制" _mubu_text="%3Cspan%3E%E8%AE%B8%E5%8F%AF%E5%88%B6%3C/span%3E" _note="我国" _mubu_note="%3Cspan%3E%E6%88%91%E5%9B%BD%3C/span%3E">
                  <outline text="向举行地的市、县公安局、城市公安分局" _mubu_text="%3Cspan%3E%E5%90%91%E4%B8%BE%E8%A1%8C%E5%9C%B0%E7%9A%84%E5%B8%82%E3%80%81%E5%8E%BF%E5%85%AC%E5%AE%89%E5%B1%80%E3%80%81%E5%9F%8E%E5%B8%82%E5%85%AC%E5%AE%89%E5%88%86%E5%B1%80%3C/span%3E" _note="" _mubu_note=""/>
