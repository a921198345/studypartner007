                <outline text="以内容的正确性为定义要素" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%86%85%E5%AE%B9%E7%9A%84%E6%AD%A3%E7%A1%AE%E6%80%A7%E4%B8%BA%E5%AE%9A%E4%B9%89%E8%A6%81%E7%B4%A0%3C/span%3E" _note="道德正确" _mubu_note="%3Cspan%3E%E9%81%93%E5%BE%B7%E6%AD%A3%E7%A1%AE%3C/span%3E"/>
              </outline>
              <outline text="第三条道路" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E6%9D%A1%E9%81%93%E8%B7%AF%3C/span%3E" _note="综合法学派" _mubu_note="%3Cspan%3E%E7%BB%BC%E5%90%88%E6%B3%95%E5%AD%A6%E6%B4%BE%3C/span%3E">
