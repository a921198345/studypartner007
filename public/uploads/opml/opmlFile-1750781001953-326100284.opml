          <outline text="视为送达" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="送达人、见证人签名后" _mubu_text="%3Cspan%3E%E9%80%81%E8%BE%BE%E4%BA%BA%E3%80%81%E8%A7%81%E8%AF%81%E4%BA%BA%E7%AD%BE%E5%90%8D%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="用拍照、录像的方式记录" _mubu_text="%3Cspan%3E%E7%94%A8%E6%8B%8D%E7%85%A7%E3%80%81%E5%BD%95%E5%83%8F%E7%9A%84%E6%96%B9%E5%BC%8F%E8%AE%B0%E5%BD%95%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="调解书不能留置送达" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E4%B8%8D%E8%83%BD%E7%95%99%E7%BD%AE%E9%80%81%E8%BE%BE%3C/span%3E" _note="当事人不签收，调解书不生效" _mubu_note="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8D%E7%AD%BE%E6%94%B6%EF%BC%8C%E8%B0%83%E8%A7%A3%E4%B9%A6%E4%B8%8D%E7%94%9F%E6%95%88%3C/span%3E"/>
        </outline>
        <outline text="电子送达" _mubu_text="%3Cspan%3E%E7%94%B5%E5%AD%90%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
          <outline text="必须要经过受送达人同意。且确认其能收悉" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E8%A6%81%E7%BB%8F%E8%BF%87%E5%8F%97%E9%80%81%E8%BE%BE%E4%BA%BA%E5%90%8C%E6%84%8F%E3%80%82%E4%B8%94%E7%A1%AE%E8%AE%A4%E5%85%B6%E8%83%BD%E6%94%B6%E6%82%89%3C/span%3E" _note="" _mubu_note=""/>
