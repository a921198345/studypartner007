          <outline text="申请证人出庭" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请法庭通知有专门知识的人出庭" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%B3%95%E5%BA%AD%E9%80%9A%E7%9F%A5%E6%9C%89%E4%B8%93%E9%97%A8%E7%9F%A5%E8%AF%86%E7%9A%84%E4%BA%BA%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请文书提出命令（责令对方提交书证）" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%96%87%E4%B9%A6%E6%8F%90%E5%87%BA%E5%91%BD%E4%BB%A4%EF%BC%88%E8%B4%A3%E4%BB%A4%E5%AF%B9%E6%96%B9%E6%8F%90%E4%BA%A4%E4%B9%A6%E8%AF%81%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="法庭辩论终结前" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%E7%BB%88%E7%BB%93%E5%89%8D%3C/span%3E" _note="" _mubu_note="">
