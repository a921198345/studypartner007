        <outline text="直接证据or间接证据" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%AF%81%E6%8D%AEor%E9%97%B4%E6%8E%A5%E8%AF%81%E6%8D%AE%3C/span%3E" _note="能否完整证明待征事实" _mubu_note="%3Cspan%3E%E8%83%BD%E5%90%A6%E5%AE%8C%E6%95%B4%E8%AF%81%E6%98%8E%E5%BE%85%E5%BE%81%E4%BA%8B%E5%AE%9E%3C/span%3E"/>
        <outline text="本征or反证" _mubu_text="%3Cspan%3E%E6%9C%AC%E5%BE%81or%E5%8F%8D%E8%AF%81%3C/span%3E" _note="承担证明责任的一方提出由于证明自己主张的为本证" _mubu_note="%3Cspan%3E%E6%89%BF%E6%8B%85%E8%AF%81%E6%98%8E%E8%B4%A3%E4%BB%BB%E7%9A%84%E4%B8%80%E6%96%B9%E6%8F%90%E5%87%BA%E7%94%B1%E4%BA%8E%E8%AF%81%E6%98%8E%E8%87%AA%E5%B7%B1%E4%B8%BB%E5%BC%A0%E7%9A%84%E4%B8%BA%E6%9C%AC%E8%AF%81%3C/span%3E"/>
      </outline>
      <outline text="法定分类" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
        <outline text="物证" _mubu_text="%3Cspan%3E%E7%89%A9%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="书证" _mubu_text="%3Cspan%3E%E4%B9%A6%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="通过原始记录方式，内容很直观" _mubu_text="%3Cspan%3E%E9%80%9A%E8%BF%87%E5%8E%9F%E5%A7%8B%E8%AE%B0%E5%BD%95%E6%96%B9%E5%BC%8F%EF%BC%8C%E5%86%85%E5%AE%B9%E5%BE%88%E7%9B%B4%E8%A7%82%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="书证的几个重要证据规则" _mubu_text="%3Cspan%3E%E4%B9%A6%E8%AF%81%E7%9A%84%E5%87%A0%E4%B8%AA%E9%87%8D%E8%A6%81%E8%AF%81%E6%8D%AE%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="同样适用视听资料、电子数据" _mubu_text="%3Cspan%3E%E5%90%8C%E6%A0%B7%E9%80%82%E7%94%A8%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%E3%80%81%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="都是用内容证明案件事实" _mubu_note="%3Cspan%3E%E9%83%BD%E6%98%AF%E7%94%A8%E5%86%85%E5%AE%B9%E8%AF%81%E6%98%8E%E6%A1%88%E4%BB%B6%E4%BA%8B%E5%AE%9E%3C/span%3E"/>
            <outline text="最佳证据规则" _mubu_text="%3Cspan%3E%E6%9C%80%E4%BD%B3%E8%AF%81%E6%8D%AE%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="应提交原件" _mubu_text="%3Cspan%3E%E5%BA%94%E6%8F%90%E4%BA%A4%E5%8E%9F%E4%BB%B6%3C/span%3E" _note="提交确有困难经准许的除外" _mubu_note="%3Cspan%3E%E6%8F%90%E4%BA%A4%E7%A1%AE%E6%9C%89%E5%9B%B0%E9%9A%BE%E7%BB%8F%E5%87%86%E8%AE%B8%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="公文真实性规则" _mubu_text="%3Cspan%3E%E5%85%AC%E6%96%87%E7%9C%9F%E5%AE%9E%E6%80%A7%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="公文书证" _mubu_text="%3Cspan%3E%E5%85%AC%E6%96%87%E4%B9%A6%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="国家公权力机关" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E5%85%AC%E6%9D%83%E5%8A%9B%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="职权范围内" _mubu_text="%3Cspan%3E%E8%81%8C%E6%9D%83%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="推定为真实，有相反足以推翻除外" _mubu_text="%3Cspan%3E%E6%8E%A8%E5%AE%9A%E4%B8%BA%E7%9C%9F%E5%AE%9E%EF%BC%8C%E6%9C%89%E7%9B%B8%E5%8F%8D%E8%B6%B3%E4%BB%A5%E6%8E%A8%E7%BF%BB%E9%99%A4%E5%A4%96%3C/span%3E" _note="仅针对我国公文书" _mubu_note="%3Cspan%3E%E4%BB%85%E9%92%88%E5%AF%B9%E6%88%91%E5%9B%BD%E5%85%AC%E6%96%87%E4%B9%A6%3C/span%3E"/>
              </outline>
              <outline text="私文书证" _mubu_text="%3Cspan%3E%E7%A7%81%E6%96%87%E4%B9%A6%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="形式真实性" _mubu_text="%3Cspan%3E%E5%BD%A2%E5%BC%8F%E7%9C%9F%E5%AE%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                  <outline text="谁主张谁证明" _mubu_text="%3Cspan%3E%E8%B0%81%E4%B8%BB%E5%BC%A0%E8%B0%81%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="内容真实性" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%E7%9C%9F%E5%AE%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
                  <outline text="具备形式真实性后推定内容真实性" _mubu_text="%3Cspan%3E%E5%85%B7%E5%A4%87%E5%BD%A2%E5%BC%8F%E7%9C%9F%E5%AE%9E%E6%80%A7%E5%90%8E%E6%8E%A8%E5%AE%9A%E5%86%85%E5%AE%B9%E7%9C%9F%E5%AE%9E%E6%80%A7%3C/span%3E" _note="由制作者或者其代理人签名、盖章或者捺印的，推定为真实" _mubu_note="%3Cspan%3E%E7%94%B1%E5%88%B6%E4%BD%9C%E8%80%85%E6%88%96%E8%80%85%E5%85%B6%E4%BB%A3%E7%90%86%E4%BA%BA%E7%AD%BE%E5%90%8D%E3%80%81%E7%9B%96%E7%AB%A0%E6%88%96%E8%80%85%E6%8D%BA%E5%8D%B0%E7%9A%84%EF%BC%8C%E6%8E%A8%E5%AE%9A%E4%B8%BA%E7%9C%9F%E5%AE%9E%3C/span%3E"/>
                  <outline text="反对的一方承担证明责任" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%AF%B9%E7%9A%84%E4%B8%80%E6%96%B9%E6%89%BF%E6%8B%85%E8%AF%81%E6%98%8E%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="文书提出命令规则" _mubu_text="%3Cspan%3E%E6%96%87%E4%B9%A6%E6%8F%90%E5%87%BA%E5%91%BD%E4%BB%A4%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
              <outline text="控制书证的当事人无正当理由拒不提交" _mubu_text="%3Cspan%3E%E6%8E%A7%E5%88%B6%E4%B9%A6%E8%AF%81%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%97%A0%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E6%8B%92%E4%B8%8D%E6%8F%90%E4%BA%A4%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院可认定申请人所主张的书证内容为真实" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E8%AE%A4%E5%AE%9A%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%89%80%E4%B8%BB%E5%BC%A0%E7%9A%84%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B9%A6%E8%AF%81%E5%86%85%E5%AE%B9%3C/span%3E%3Cspan%3E%E4%B8%BA%E7%9C%9F%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="持有书证的当事人故意毁灭或其他致使书证不能使用的" _mubu_text="%3Cspan%3E%E6%8C%81%E6%9C%89%E4%B9%A6%E8%AF%81%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%95%85%E6%84%8F%E6%AF%81%E7%81%AD%E6%88%96%E5%85%B6%E4%BB%96%E8%87%B4%E4%BD%BF%E4%B9%A6%E8%AF%81%E4%B8%8D%E8%83%BD%E4%BD%BF%E7%94%A8%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院可认定申请人所主张以该书证证明事实为真实" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E8%AE%A4%E5%AE%9A%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%89%80%E4%B8%BB%E5%BC%A0%E4%BB%A5%E8%AF%A5%E4%B9%A6%E8%AF%81%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%AF%81%E6%98%8E%E4%BA%8B%E5%AE%9E%3C/span%3E%3Cspan%3E%E4%B8%BA%E7%9C%9F%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="同时可以追究其妨碍诉讼的责任" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%8F%AF%E4%BB%A5%E8%BF%BD%E7%A9%B6%E5%85%B6%E5%A6%A8%E7%A2%8D%E8%AF%89%E8%AE%BC%E7%9A%84%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="罚款、拘留" _mubu_note="%3Cspan%3E%E7%BD%9A%E6%AC%BE%E3%80%81%E6%8B%98%E7%95%99%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="域外书证认证" _mubu_text="%3Cspan%3E%E5%9F%9F%E5%A4%96%E4%B9%A6%E8%AF%81%E8%AE%A4%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="域外公文书证" _mubu_text="%3Cspan%3E%E5%9F%9F%E5%A4%96%E5%85%AC%E6%96%87%E4%B9%A6%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
                <outline text="该国公证机关证明" _mubu_text="%3Cspan%3E%E8%AF%A5%E5%9B%BD%E5%85%AC%E8%AF%81%E6%9C%BA%E5%85%B3%E8%AF%81%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="域外涉及身份关系的证据" _mubu_text="%3Cspan%3E%E5%9F%9F%E5%A4%96%E6%B6%89%E5%8F%8A%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%E7%9A%84%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="该国公证机关证明＋我国驻该国使领馆认证" _mubu_text="%3Cspan%3E%E8%AF%A5%E5%9B%BD%E5%85%AC%E8%AF%81%E6%9C%BA%E5%85%B3%E8%AF%81%E6%98%8E%EF%BC%8B%E6%88%91%E5%9B%BD%E9%A9%BB%E8%AF%A5%E5%9B%BD%E4%BD%BF%E9%A2%86%E9%A6%86%E8%AE%A4%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="视听资料" _mubu_text="%3Cspan%3E%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="电子数据" _mubu_text="%3Cspan%3E%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="存储在电子介质中的录音影像为电子数据" _mubu_text="%3Cspan%3E%E5%AD%98%E5%82%A8%E5%9C%A8%E7%94%B5%E5%AD%90%E4%BB%8B%E8%B4%A8%E4%B8%AD%E7%9A%84%E5%BD%95%E9%9F%B3%E5%BD%B1%E5%83%8F%E4%B8%BA%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="电子数据相较于视听资料" _mubu_text="%3Cspan%3E%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%E7%9B%B8%E8%BE%83%E4%BA%8E%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="可以实现精确复制" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E5%AE%9E%E7%8E%B0%E7%B2%BE%E7%A1%AE%E5%A4%8D%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可以在虚拟空间内快速传播" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E5%9C%A8%E8%99%9A%E6%8B%9F%E7%A9%BA%E9%97%B4%E5%86%85%E5%BF%AB%E9%80%9F%E4%BC%A0%E6%92%AD%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="极易实现剪辑、修改" _mubu_text="%3Cspan%3E%E6%9E%81%E6%98%93%E5%AE%9E%E7%8E%B0%E5%89%AA%E8%BE%91%E3%80%81%E4%BF%AE%E6%94%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="证人证言" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note="">
          <outline text="证人资格" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%B5%84%E6%A0%BC%3C/span%3E" _note="" _mubu_note="">
            <outline text="凡是知道案件情况的单位和个人都有义务出庭作证" _mubu_text="%3Cspan%3E%E5%87%A1%E6%98%AF%E7%9F%A5%E9%81%93%E6%A1%88%E4%BB%B6%E6%83%85%E5%86%B5%E7%9A%84%E5%8D%95%E4%BD%8D%E5%92%8C%E4%B8%AA%E4%BA%BA%E9%83%BD%E6%9C%89%E4%B9%89%E5%8A%A1%E5%87%BA%E5%BA%AD%E4%BD%9C%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不能正确表达意思的人，不能作为证人" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%AD%A3%E7%A1%AE%E8%A1%A8%E8%BE%BE%E6%84%8F%E6%80%9D%E7%9A%84%E4%BA%BA%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%BD%9C%E4%B8%BA%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="未成年人、精神病人做证人的问题" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%88%90%E5%B9%B4%E4%BA%BA%E3%80%81%E7%B2%BE%E7%A5%9E%E7%97%85%E4%BA%BA%E5%81%9A%E8%AF%81%E4%BA%BA%E7%9A%84%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="待证事实与其年龄、智力、精神健康状况相适应的可以" _mubu_text="%3Cspan%3E%E5%BE%85%E8%AF%81%E4%BA%8B%E5%AE%9E%E4%B8%8E%E5%85%B6%E5%B9%B4%E9%BE%84%E3%80%81%E6%99%BA%E5%8A%9B%E3%80%81%E7%B2%BE%E7%A5%9E%E5%81%A5%E5%BA%B7%E7%8A%B6%E5%86%B5%E7%9B%B8%E9%80%82%E5%BA%94%E7%9A%84%E5%8F%AF%E4%BB%A5%3C/span%3E" _note="附条件" _mubu_note="%3Cspan%3E%E9%99%84%E6%9D%A1%E4%BB%B6%3C/span%3E"/>
              <outline text="证明力" _mubu_text="%3Cspan%3E%E8%AF%81%E6%98%8E%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
                <outline text="与其年龄、智力、精神健康状况不相当的证言不能单独作为" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%85%B6%E5%B9%B4%E9%BE%84%E3%80%81%E6%99%BA%E5%8A%9B%E3%80%81%E7%B2%BE%E7%A5%9E%E5%81%A5%E5%BA%B7%E7%8A%B6%E5%86%B5%E4%B8%8D%E7%9B%B8%E5%BD%93%E7%9A%84%E8%AF%81%E8%A8%80%E4%B8%8D%E8%83%BD%E5%8D%95%E7%8B%AC%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="即证明力较小，需补强" _mubu_note="%3Cspan%3E%E5%8D%B3%E8%AF%81%E6%98%8E%E5%8A%9B%E8%BE%83%E5%B0%8F%EF%BC%8C%E9%9C%80%E8%A1%A5%E5%BC%BA%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="证人不适用回避制度" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E4%B8%8D%E9%80%82%E7%94%A8%E5%9B%9E%E9%81%BF%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="与案件有利害关系的人可以作为证人" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%A1%88%E4%BB%B6%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E7%9A%84%E4%BA%BA%E5%8F%AF%E4%BB%A5%E4%BD%9C%E4%B8%BA%E8%AF%81%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="证明力" _mubu_text="%3Cspan%3E%E8%AF%81%E6%98%8E%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
                <outline text="不能单独作为" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E5%8D%95%E7%8B%AC%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="证人出庭" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：依申请" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%BE%9D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外：法院依职权" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="证言属于法院依职权调查收集的证据的" _mubu_text="%3Cspan%3E%E8%AF%81%E8%A8%80%E5%B1%9E%E4%BA%8E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E8%B0%83%E6%9F%A5%E6%94%B6%E9%9B%86%E7%9A%84%E8%AF%81%E6%8D%AE%E7%9A%84%3C/span%3E" _note="涉及国家、社会、第三人利益，身份关系，程序性事实" _mubu_note="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E5%AE%B6%E3%80%81%E7%A4%BE%E4%BC%9A%E3%80%81%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%88%A9%E7%9B%8A%EF%BC%8C%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%EF%BC%8C%E7%A8%8B%E5%BA%8F%E6%80%A7%E4%BA%8B%E5%AE%9E%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="原则：证人应当出庭作证" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E8%AF%81%E4%BA%BA%E5%BA%94%E5%BD%93%E5%87%BA%E5%BA%AD%E4%BD%9C%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="无正当理由未出庭的证人以书面形式提供的证言，不得作为定案根据" _mubu_text="%3Cspan%3E%E6%97%A0%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E6%9C%AA%E5%87%BA%E5%BA%AD%E7%9A%84%E8%AF%81%E4%BA%BA%E4%BB%A5%E4%B9%A6%E9%9D%A2%E5%BD%A2%E5%BC%8F%E6%8F%90%E4%BE%9B%E7%9A%84%E8%AF%81%E8%A8%80%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BD%9C%E4%B8%BA%E5%AE%9A%E6%A1%88%E6%A0%B9%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="视为出庭" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="证人在审理前准备阶段或者调查、询问等双方当事人在场时陈述证言的" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E5%9C%A8%E5%AE%A1%E7%90%86%E5%89%8D%E5%87%86%E5%A4%87%E9%98%B6%E6%AE%B5%E6%88%96%E8%80%85%E8%B0%83%E6%9F%A5%E3%80%81%E8%AF%A2%E9%97%AE%E7%AD%89%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E5%9C%BA%E6%97%B6%E9%99%88%E8%BF%B0%E8%AF%81%E8%A8%80%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="可以不出庭" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E5%87%BA%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="法定原因" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="健康原因、交通原因、不可抗力等" _mubu_text="%3Cspan%3E%E5%81%A5%E5%BA%B7%E5%8E%9F%E5%9B%A0%E3%80%81%E4%BA%A4%E9%80%9A%E5%8E%9F%E5%9B%A0%E3%80%81%E4%B8%8D%E5%8F%AF%E6%8A%97%E5%8A%9B%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="意定原因" _mubu_text="%3Cspan%3E%E6%84%8F%E5%AE%9A%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="虽然无法定原因，但当事人均同意，且经法院准许的" _mubu_text="%3Cspan%3E%E8%99%BD%E7%84%B6%E6%97%A0%E6%B3%95%E5%AE%9A%E5%8E%9F%E5%9B%A0%EF%BC%8C%E4%BD%86%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9D%87%E5%90%8C%E6%84%8F%EF%BC%8C%E4%B8%94%E7%BB%8F%E6%B3%95%E9%99%A2%E5%87%86%E8%AE%B8%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="费用补助" _mubu_text="%3Cspan%3E%E8%B4%B9%E7%94%A8%E8%A1%A5%E5%8A%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="败诉方承担" _mubu_text="%3Cspan%3E%E8%B4%A5%E8%AF%89%E6%96%B9%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="申请人/法院先行垫付" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA/%E6%B3%95%E9%99%A2%E5%85%88%E8%A1%8C%E5%9E%AB%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="证人作证" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E4%BD%9C%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="如实作证保证书问题" _mubu_text="%3Cspan%3E%E5%A6%82%E5%AE%9E%E4%BD%9C%E8%AF%81%E4%BF%9D%E8%AF%81%E4%B9%A6%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="完民行能人" _mubu_text="%3Cspan%3E%E5%AE%8C%E6%B0%91%E8%A1%8C%E8%83%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="应当签署并宣读保证书，拒绝签的，不能作证" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E7%AD%BE%E7%BD%B2%E5%B9%B6%E5%AE%A3%E8%AF%BB%E4%BF%9D%E8%AF%81%E4%B9%A6%EF%BC%8C%E6%8B%92%E7%BB%9D%E7%AD%BE%E7%9A%84%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%BD%9C%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="无/限民行能人" _mubu_text="%3Cspan%3E%E6%97%A0/%E9%99%90%E6%B0%91%E8%A1%8C%E8%83%BD%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                <outline text="不需签，只要能够正确表达意思，不签也能作证" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%9C%80%E7%AD%BE%EF%BC%8C%E5%8F%AA%E8%A6%81%E8%83%BD%E5%A4%9F%E6%AD%A3%E7%A1%AE%E8%A1%A8%E8%BE%BE%E6%84%8F%E6%80%9D%EF%BC%8C%E4%B8%8D%E7%AD%BE%E4%B9%9F%E8%83%BD%E4%BD%9C%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="证人陈述证言" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E9%99%88%E8%BF%B0%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="应客观陈述其亲身感受的事实，不得使用猜测、推断或者评论性语言" _mubu_text="%3Cspan%3E%E5%BA%94%E5%AE%A2%E8%A7%82%E9%99%88%E8%BF%B0%E5%85%B6%E4%BA%B2%E8%BA%AB%E6%84%9F%E5%8F%97%E7%9A%84%E4%BA%8B%E5%AE%9E%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BD%BF%E7%94%A8%E7%8C%9C%E6%B5%8B%E3%80%81%E6%8E%A8%E6%96%AD%E6%88%96%E8%80%85%E8%AF%84%E8%AE%BA%E6%80%A7%E8%AF%AD%E8%A8%80%3C/span%3E" _note="意见证据规则" _mubu_note="%3Cspan%3E%E6%84%8F%E8%A7%81%E8%AF%81%E6%8D%AE%E8%A7%84%E5%88%99%3C/span%3E"/>
              <outline text="作证前不得旁听庭审" _mubu_text="%3Cspan%3E%E4%BD%9C%E8%AF%81%E5%89%8D%E4%B8%8D%E5%BE%97%E6%97%81%E5%90%AC%E5%BA%AD%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不得以宣读事先准备的书面材料的方式陈述证言" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%BB%A5%E5%AE%A3%E8%AF%BB%E4%BA%8B%E5%85%88%E5%87%86%E5%A4%87%E7%9A%84%E4%B9%A6%E9%9D%A2%E6%9D%90%E6%96%99%E7%9A%84%E6%96%B9%E5%BC%8F%E9%99%88%E8%BF%B0%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="鉴定意见" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动程序" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：依申请" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E4%BE%9D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="谁承担证明责任谁申请" _mubu_note="%3Cspan%3E%E8%B0%81%E6%89%BF%E6%8B%85%E8%AF%81%E6%98%8E%E8%B4%A3%E4%BB%BB%E8%B0%81%E7%94%B3%E8%AF%B7%3C/span%3E"/>
            <outline text="例外：法院依职权" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
              <outline text="属于法院依职权调查收集的证据的" _mubu_text="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E8%B0%83%E6%9F%A5%E6%94%B6%E9%9B%86%E7%9A%84%E8%AF%81%E6%8D%AE%E7%9A%84%3C/span%3E" _note="涉及国家、社会、第三人利益，身份关系，程序性事实" _mubu_note="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E5%AE%B6%E3%80%81%E7%A4%BE%E4%BC%9A%E3%80%81%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%88%A9%E7%9B%8A%EF%BC%8C%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%EF%BC%8C%E7%A8%8B%E5%BA%8F%E6%80%A7%E4%BA%8B%E5%AE%9E%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="鉴定前的准备" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E5%89%8D%E7%9A%84%E5%87%86%E5%A4%87%3C/span%3E" _note="" _mubu_note="">
            <outline text="鉴定人签署承诺书" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E7%AD%BE%E7%BD%B2%E6%89%BF%E8%AF%BA%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对鉴定材料的质证" _mubu_text="%3Cspan%3E%E5%AF%B9%E9%89%B4%E5%AE%9A%E6%9D%90%E6%96%99%E7%9A%84%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="组织当事人对鉴定材料进行质证" _mubu_text="%3Cspan%3E%E7%BB%84%E7%BB%87%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%AF%B9%E9%89%B4%E5%AE%9A%E6%9D%90%E6%96%99%E8%BF%9B%E8%A1%8C%E8%B4%A8%E8%AF%81%3C/span%3E" _note="未经质证的材料，不得作为鉴定的根据" _mubu_note="%3Cspan%3E%E6%9C%AA%E7%BB%8F%E8%B4%A8%E8%AF%81%E7%9A%84%E6%9D%90%E6%96%99%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BD%9C%E4%B8%BA%E9%89%B4%E5%AE%9A%E7%9A%84%E6%A0%B9%E6%8D%AE%3C/span%3E"/>
            </outline>
            <outline text="鉴定人的确定" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E7%9A%84%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="申请鉴定的" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E9%89%B4%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="当事人协商确定" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8D%8F%E5%95%86%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="协商不成，法院指定" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%95%86%E4%B8%8D%E6%88%90%EF%BC%8C%E6%B3%95%E9%99%A2%E6%8C%87%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="法院依职权" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院可询问当事人意见后指定" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E8%AF%A2%E9%97%AE%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%84%8F%E8%A7%81%E5%90%8E%E6%8C%87%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="鉴定意见" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="鉴定人有权了解进行鉴定所需要的案件材料" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E4%BA%BA%E6%9C%89%E6%9D%83%E4%BA%86%E8%A7%A3%E8%BF%9B%E8%A1%8C%E9%89%B4%E5%AE%9A%E6%89%80%E9%9C%80%E8%A6%81%E7%9A%84%E6%A1%88%E4%BB%B6%E6%9D%90%E6%96%99%3C/span%3E" _note="在鉴定过程中可以询问当事人、证人" _mubu_note="%3Cspan%3E%E5%9C%A8%E9%89%B4%E5%AE%9A%E8%BF%87%E7%A8%8B%E4%B8%AD%E5%8F%AF%E4%BB%A5%E8%AF%A2%E9%97%AE%E5%BD%93%E4%BA%8B%E4%BA%BA%E3%80%81%E8%AF%81%E4%BA%BA%3C/span%3E"/>
            <outline text="按时完成鉴定" _mubu_text="%3Cspan%3E%E6%8C%89%E6%97%B6%E5%AE%8C%E6%88%90%E9%89%B4%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="须鉴定人签名、盖章" _mubu_text="%3Cspan%3E%E9%A1%BB%E9%89%B4%E5%AE%9A%E4%BA%BA%E7%AD%BE%E5%90%8D%E3%80%81%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
