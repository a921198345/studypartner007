        <outline text="案外人对执行标的的异议" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E5%AF%B9%E6%89%A7%E8%A1%8C%E6%A0%87%E7%9A%84%E7%9A%84%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="书面异议" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="异议审查" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="收到书面异议起15日内" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E4%B9%A6%E9%9D%A2%E5%BC%82%E8%AE%AE%E8%B5%B715%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="成立，中止；不成立，驳回" _mubu_text="%3Cspan%3E%E6%88%90%E7%AB%8B%EF%BC%8C%E4%B8%AD%E6%AD%A2%EF%BC%9B%E4%B8%8D%E6%88%90%E7%AB%8B%EF%BC%8C%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="后续处理" _mubu_text="%3Cspan%3E%E5%90%8E%E7%BB%AD%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="认为原判裁确有错误" _mubu_text="%3Cspan%3E%E8%AE%A4%E4%B8%BA%E5%8E%9F%E5%88%A4%E8%A3%81%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%3C/span%3E" _note="判的和执行的一样" _mubu_note="%3Cspan%3E%E5%88%A4%E7%9A%84%E5%92%8C%E6%89%A7%E8%A1%8C%E7%9A%84%E4%B8%80%E6%A0%B7%3C/span%3E">
              <outline text="中止/驳回后，再审" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2/%E9%A9%B3%E5%9B%9E%E5%90%8E%EF%BC%8C%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="与原判裁无关" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%8E%9F%E5%88%A4%E8%A3%81%E6%97%A0%E5%85%B3%3C/span%3E" _note="判的和执行的不一样" _mubu_note="%3Cspan%3E%E5%88%A4%E7%9A%84%E5%92%8C%E6%89%A7%E8%A1%8C%E7%9A%84%E4%B8%8D%E4%B8%80%E6%A0%B7%3C/span%3E">
              <outline text="自裁定送达之日起15日内另诉" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A3%81%E5%AE%9A%E9%80%81%E8%BE%BE%E4%B9%8B%E6%97%A5%E8%B5%B715%E6%97%A5%E5%86%85%E5%8F%A6%E8%AF%89%3C/span%3E" _note="执行异议之诉" _mubu_note="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%BC%82%E8%AE%AE%E4%B9%8B%E8%AF%89%3C/span%3E">
                <outline text="中止" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="申请人起诉" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E8%B5%B7%E8%AF%89%3C/span%3E" _note="许可执行之诉" _mubu_note="%3Cspan%3E%E8%AE%B8%E5%8F%AF%E6%89%A7%E8%A1%8C%E4%B9%8B%E8%AF%89%3C/span%3E">
                    <outline text="消极确认之诉" _mubu_text="%3Cspan%3E%E6%B6%88%E6%9E%81%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="案外人为被告" _mubu_text="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="被执行人" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                      <outline text="反对的，列为共同被告" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%AF%B9%E7%9A%84%EF%BC%8C%E5%88%97%E4%B8%BA%E5%85%B1%E5%90%8C%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="不反对的，列为第三人" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%8D%E5%AF%B9%E7%9A%84%EF%BC%8C%E5%88%97%E4%B8%BA%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                </outline>
                <outline text="驳回" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note="">
                  <outline text="案外人起诉" _mubu_text="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E8%B5%B7%E8%AF%89%3C/span%3E" _note="案外人异议之诉" _mubu_note="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E5%BC%82%E8%AE%AE%E4%B9%8B%E8%AF%89%3C/span%3E">
                    <outline text="积极确认之诉" _mubu_text="%3Cspan%3E%E7%A7%AF%E6%9E%81%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="申请人为被告" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                    <outline text="被执行人" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                      <outline text="反对的，列为共同被告" _mubu_text="%3Cspan%3E%E5%8F%8D%E5%AF%B9%E7%9A%84%EF%BC%8C%E5%88%97%E4%B8%BA%E5%85%B1%E5%90%8C%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                      <outline text="不反对的，列为第三人" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%8D%E5%AF%B9%E7%9A%84%EF%BC%8C%E5%88%97%E4%B8%BA%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                    </outline>
                  </outline>
                </outline>
                <outline text="谁主张谁举证" _mubu_text="%3Cspan%3E%E8%B0%81%E4%B8%BB%E5%BC%A0%E8%B0%81%E4%B8%BE%E8%AF%81%3C/span%3E" _note="一般为案外人承担证明责任" _mubu_note="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%B8%BA%E6%A1%88%E5%A4%96%E4%BA%BA%E6%89%BF%E6%8B%85%E8%AF%81%E6%98%8E%E8%B4%A3%E4%BB%BB%3C/span%3E"/>
                <outline text="另诉是为了通过诉讼程序解决执行标的的权利归属争议" _mubu_text="%3Cspan%3E%E5%8F%A6%E8%AF%89%E6%98%AF%E4%B8%BA%E4%BA%86%E9%80%9A%E8%BF%87%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%E8%A7%A3%E5%86%B3%E6%89%A7%E8%A1%8C%E6%A0%87%E7%9A%84%E7%9A%84%E6%9D%83%E5%88%A9%E5%BD%92%E5%B1%9E%E4%BA%89%E8%AE%AE%3C/span%3E" _note="并不是对执行法院作出的中止或驳回裁定的异议或救济" _mubu_note="%3Cspan%3E%E5%B9%B6%E4%B8%8D%E6%98%AF%E5%AF%B9%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%E4%BD%9C%E5%87%BA%E7%9A%84%E4%B8%AD%E6%AD%A2%E6%88%96%E9%A9%B3%E5%9B%9E%E8%A3%81%E5%AE%9A%E7%9A%84%E5%BC%82%E8%AE%AE%E6%88%96%E6%95%91%E6%B5%8E%3C/span%3E"/>
                <outline text="其本质上是一种确认之诉" _mubu_text="%3Cspan%3E%E5%85%B6%E6%9C%AC%E8%B4%A8%E4%B8%8A%E6%98%AF%E4%B8%80%E7%A7%8D%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                  <outline text="对案外人对执行标的是否享有足以排除执行的权利进行确认" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%A1%88%E5%A4%96%E4%BA%BA%E5%AF%B9%E6%89%A7%E8%A1%8C%E6%A0%87%E7%9A%84%E6%98%AF%E5%90%A6%E4%BA%AB%E6%9C%89%E8%B6%B3%E4%BB%A5%E6%8E%92%E9%99%A4%E6%89%A7%E8%A1%8C%E7%9A%84%E6%9D%83%E5%88%A9%E8%BF%9B%E8%A1%8C%E7%A1%AE%E8%AE%A4%3C/span%3E" _note="案外人：积极确认之诉  ； 申请人：消极确认之诉" _mubu_note="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%EF%BC%9A%E7%A7%AF%E6%9E%81%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%20%20%EF%BC%9B%20%E7%94%B3%E8%AF%B7%E4%BA%BA%EF%BC%9A%E6%B6%88%E6%9E%81%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="三撤or案外人异议" _mubu_text="%3Cspan%3E%E4%B8%89%E6%92%A4or%E6%A1%88%E5%A4%96%E4%BA%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="三撤：生效裁判侵犯案外人权利" _mubu_text="%3Cspan%3E%E4%B8%89%E6%92%A4%EF%BC%9A%E7%94%9F%E6%95%88%E8%A3%81%E5%88%A4%E4%BE%B5%E7%8A%AF%E6%A1%88%E5%A4%96%E4%BA%BA%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="案外人异议：执行侵犯案外人权利" _mubu_text="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E5%BC%82%E8%AE%AE%EF%BC%9A%E6%89%A7%E8%A1%8C%E4%BE%B5%E7%8A%AF%E6%A1%88%E5%A4%96%E4%BA%BA%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="解题思路" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%A2%98%E6%80%9D%E8%B7%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="判≠执" _mubu_text="%3Cspan%3E%E5%88%A4%E2%89%A0%E6%89%A7%3C/span%3E" _note="" _mubu_note="">
                <outline text="只能案外人异议" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E6%A1%88%E5%A4%96%E4%BA%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="与三撤和再审均无关" _mubu_note="%3Cspan%3E%E4%B8%8E%E4%B8%89%E6%92%A4%E5%92%8C%E5%86%8D%E5%AE%A1%E5%9D%87%E6%97%A0%E5%85%B3%3C/span%3E">
                  <outline text="中止" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
                    <outline text="申请人起诉" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E8%B5%B7%E8%AF%89%3C/span%3E" _note="许可执行之诉" _mubu_note="%3Cspan%3E%E8%AE%B8%E5%8F%AF%E6%89%A7%E8%A1%8C%E4%B9%8B%E8%AF%89%3C/span%3E"/>
                  </outline>
                  <outline text="驳回" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note="">
                    <outline text="案外人起诉" _mubu_text="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E8%B5%B7%E8%AF%89%3C/span%3E" _note="案外人异议之诉" _mubu_note="%3Cspan%3E%E6%A1%88%E5%A4%96%E4%BA%BA%E5%BC%82%E8%AE%AE%E4%B9%8B%E8%AF%89%3C/span%3E"/>
                  </outline>
                </outline>
              </outline>
