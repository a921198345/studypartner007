            <outline text="人际关系" _mubu_text="%3Cspan%3E%E4%BA%BA%E9%99%85%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
              <outline text="婚姻家庭和继承纠纷" _mubu_text="%3Cspan%3E%E5%A9%9A%E5%A7%BB%E5%AE%B6%E5%BA%AD%E5%92%8C%E7%BB%A7%E6%89%BF%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="宅基地和相邻关系纠纷" _mubu_text="%3Cspan%3E%E5%AE%85%E5%9F%BA%E5%9C%B0%E5%92%8C%E7%9B%B8%E9%82%BB%E5%85%B3%E7%B3%BB%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="合伙合同纠纷" _mubu_text="%3Cspan%3E%E5%90%88%E4%BC%99%E5%90%88%E5%90%8C%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="简单明确" _mubu_text="%3Cspan%3E%E7%AE%80%E5%8D%95%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳务合同纠纷" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E5%90%88%E5%90%8C%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
