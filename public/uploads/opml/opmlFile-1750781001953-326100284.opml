                <outline text="拒签送达回证的" _mubu_text="%3Cspan%3E%E6%8B%92%E7%AD%BE%E9%80%81%E8%BE%BE%E5%9B%9E%E8%AF%81%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="拍照录像方式记录" _mubu_text="%3Cspan%3E%E6%8B%8D%E7%85%A7%E5%BD%95%E5%83%8F%E6%96%B9%E5%BC%8F%E8%AE%B0%E5%BD%95%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="审判人员书记员在回证注明情况并签字" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E4%B9%A6%E8%AE%B0%E5%91%98%E5%9C%A8%E5%9B%9E%E8%AF%81%E6%B3%A8%E6%98%8E%E6%83%85%E5%86%B5%E5%B9%B6%E7%AD%BE%E5%AD%97%3C/span%3E" _note="" _mubu_note=""/>
