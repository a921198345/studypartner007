    <outline text="简易程序" _mubu_text="%3Cspan%3E%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
        <outline text="积极条件" _mubu_text="%3Cspan%3E%E7%A7%AF%E6%9E%81%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="基层法院" _mubu_text="%3Cspan%3E%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="及其派出法庭" _mubu_note="%3Cspan%3E%E5%8F%8A%E5%85%B6%E6%B4%BE%E5%87%BA%E6%B3%95%E5%BA%AD%3C/span%3E"/>
          <outline text="一审民事" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E6%B0%91%E4%BA%8B%3C/span%3E" _note="≠一审程序审理的" _mubu_note="%3Cspan%3E%E2%89%A0%E4%B8%80%E5%AE%A1%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%E7%9A%84%3C/span%3E"/>
        </outline>
        <outline text="消极条件" _mubu_text="%3Cspan%3E%E6%B6%88%E6%9E%81%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="“利益众多提三撤，下落不明重再审”" _mubu_note="%3Cspan%3E%E2%80%9C%E5%88%A9%E7%9B%8A%E4%BC%97%E5%A4%9A%E6%8F%90%E4%B8%89%E6%92%A4%EF%BC%8C%E4%B8%8B%E8%90%BD%E4%B8%8D%E6%98%8E%E9%87%8D%E5%86%8D%E5%AE%A1%E2%80%9D%3C/span%3E">
          <outline text="起诉时被告下落不明" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E6%97%B6%E8%A2%AB%E5%91%8A%E4%B8%8B%E8%90%BD%E4%B8%8D%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="发回重审的、适用审判监督程序的" _mubu_text="%3Cspan%3E%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%E7%9A%84%E3%80%81%E9%80%82%E7%94%A8%E5%AE%A1%E5%88%A4%E7%9B%91%E7%9D%A3%E7%A8%8B%E5%BA%8F%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="当事人一方人数众多的" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%80%E6%96%B9%E4%BA%BA%E6%95%B0%E4%BC%97%E5%A4%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="涉及国家利益、社会公共利益的" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E5%AE%B6%E5%88%A9%E7%9B%8A%E3%80%81%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="三撤之诉" _mubu_text="%3Cspan%3E%E4%B8%89%E6%92%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="适用程序" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="依职权" _mubu_text="%3Cspan%3E%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="依协议" _mubu_text="%3Cspan%3E%E4%BE%9D%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="不得约定简易变为普通" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E7%BA%A6%E5%AE%9A%E7%AE%80%E6%98%93%E5%8F%98%E4%B8%BA%E6%99%AE%E9%80%9A%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="特点" _mubu_text="%3Cspan%3E%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
        <outline text="程序的简化" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E7%9A%84%E7%AE%80%E5%8C%96%3C/span%3E" _note="" _mubu_note="">
          <outline text="受理、送法、审理简化" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E3%80%81%E9%80%81%E6%B3%95%E3%80%81%E5%AE%A1%E7%90%86%E7%AE%80%E5%8C%96%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="可用简便方式传唤、送达文书(判裁调)" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%A8%E7%AE%80%E4%BE%BF%E6%96%B9%E5%BC%8F%E4%BC%A0%E5%94%A4%E3%80%81%E9%80%81%E8%BE%BE%E6%96%87%E4%B9%A6(%E5%88%A4%E8%A3%81%E8%B0%83)%3C/span%3E" _note="电话、传真、电子邮件等；不能公告送达" _mubu_note="%3Cspan%3E%E7%94%B5%E8%AF%9D%E3%80%81%E4%BC%A0%E7%9C%9F%E3%80%81%E7%94%B5%E5%AD%90%E9%82%AE%E4%BB%B6%E7%AD%89%EF%BC%9B%E4%B8%8D%E8%83%BD%E5%85%AC%E5%91%8A%E9%80%81%E8%BE%BE%3C/span%3E">
            <outline text="无需经当事人同意" _mubu_text="%3Cspan%3E%E6%97%A0%E9%9C%80%E7%BB%8F%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="但应保障当事人陈述意见权利" _mubu_text="%3Cspan%3E%E4%BD%86%E5%BA%94%E4%BF%9D%E9%9A%9C%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%99%88%E8%BF%B0%E6%84%8F%E8%A7%81%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="简便方式送达开庭通知的" _mubu_text="%3Cspan%3E%E7%AE%80%E4%BE%BF%E6%96%B9%E5%BC%8F%E9%80%81%E8%BE%BE%E5%BC%80%E5%BA%AD%E9%80%9A%E7%9F%A5%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="需确保当事人收悉" _mubu_text="%3Cspan%3E%E9%9C%80%E7%A1%AE%E4%BF%9D%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%94%B6%E6%82%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="否则不允许缺席判决" _mubu_text="%3Cspan%3E%E5%90%A6%E5%88%99%E4%B8%8D%E5%85%81%E8%AE%B8%E7%BC%BA%E5%B8%AD%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="无法通过原告提供的被告地址送达或通知应诉的" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E9%80%9A%E8%BF%87%E5%8E%9F%E5%91%8A%E6%8F%90%E4%BE%9B%E7%9A%84%E8%A2%AB%E5%91%8A%E5%9C%B0%E5%9D%80%E9%80%81%E8%BE%BE%E6%88%96%E9%80%9A%E7%9F%A5%E5%BA%94%E8%AF%89%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="原告提供了被告准确的送达地址" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E6%8F%90%E4%BE%9B%E4%BA%86%E8%A2%AB%E5%91%8A%E5%87%86%E7%A1%AE%E7%9A%84%E9%80%81%E8%BE%BE%E5%9C%B0%E5%9D%80%3C/span%3E" _note="但无法直接或留置送达应诉通知书的" _mubu_note="%3Cspan%3E%E4%BD%86%E6%97%A0%E6%B3%95%E7%9B%B4%E6%8E%A5%E6%88%96%E7%95%99%E7%BD%AE%E9%80%81%E8%BE%BE%E5%BA%94%E8%AF%89%E9%80%9A%E7%9F%A5%E4%B9%A6%E7%9A%84%3C/span%3E">
                <outline text="应当将案件转入普通程序审理" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%B0%86%E6%A1%88%E4%BB%B6%E8%BD%AC%E5%85%A5%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E" _note="需公告送达，但简易程序不能公告" _mubu_note="%3Cspan%3E%E9%9C%80%E5%85%AC%E5%91%8A%E9%80%81%E8%BE%BE%EF%BC%8C%E4%BD%86%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%E4%B8%8D%E8%83%BD%E5%85%AC%E5%91%8A%3C/span%3E"/>
              </outline>
              <outline text="原告不能提供，法院经查证后仍不能确定" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E4%B8%8D%E8%83%BD%E6%8F%90%E4%BE%9B%EF%BC%8C%E6%B3%95%E9%99%A2%E7%BB%8F%E6%9F%A5%E8%AF%81%E5%90%8E%E4%BB%8D%E4%B8%8D%E8%83%BD%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="可以被告不明确为由裁定驳回原告起诉" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E8%A2%AB%E5%91%8A%E4%B8%8D%E6%98%8E%E7%A1%AE%E4%B8%BA%E7%94%B1%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%E5%8E%9F%E5%91%8A%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="独任审判" _mubu_text="%3Cspan%3E%E7%8B%AC%E4%BB%BB%E5%AE%A1%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
