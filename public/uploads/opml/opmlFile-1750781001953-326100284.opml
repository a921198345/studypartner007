        <outline text="诉讼调解" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
          <outline text="公力救济" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8A%9B%E6%95%91%E6%B5%8E%3C/span%3E" _note="仍然是法院行使审判权" _mubu_note="%3Cspan%3E%E4%BB%8D%E7%84%B6%E6%98%AF%E6%B3%95%E9%99%A2%E8%A1%8C%E4%BD%BF%E5%AE%A1%E5%88%A4%E6%9D%83%3C/span%3E"/>
          <outline text="制作的调解书可以强制执行" _mubu_text="%3Cspan%3E%E5%88%B6%E4%BD%9C%E7%9A%84%E8%B0%83%E8%A7%A3%E4%B9%A6%E5%8F%AF%E4%BB%A5%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
