<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <head>
    <title>民事诉讼法</title>
  </head>
  <body>
    <outline text="民事诉讼与民事诉讼法" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E4%B8%8E%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E6%B3%95%3C/span%3E" _note="" _mubu_note="">
      <outline text="民诉实质" _mubu_text="%3Cspan%3E%E6%B0%91%E8%AF%89%E5%AE%9E%E8%B4%A8%3C/span%3E" _note="" _mubu_note="">
        <outline text="利用国家审判权" _mubu_text="%3Cspan%3E%E5%88%A9%E7%94%A8%E5%9B%BD%E5%AE%B6%E5%AE%A1%E5%88%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="解决平等主体之间" _mubu_text="%3Cspan%3E%E8%A7%A3%E5%86%B3%E5%B9%B3%E7%AD%89%E4%B8%BB%E4%BD%93%E4%B9%8B%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="人身、财产权利义务纠纷" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E3%80%81%E8%B4%A2%E4%BA%A7%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="民事审判程序" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E5%AE%A1%E5%88%A4%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="诉讼程序" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="解决民事权利义务纠纷" _mubu_text="%3Cspan%3E%E8%A7%A3%E5%86%B3%E6%B0%91%E4%BA%8B%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="一、二、再审" _mubu_note="%3Cspan%3E%E4%B8%80%E3%80%81%E4%BA%8C%E3%80%81%E5%86%8D%E5%AE%A1%3C/span%3E"/>
        </outline>
        <outline text="非讼程序" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="不解决民事权利义务纠纷" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A7%A3%E5%86%B3%E6%B0%91%E4%BA%8B%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="特别程序(选民资格案件除外)、督促程序、公示催告程序等" _mubu_note="%3Cspan%3E%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F(%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%E9%99%A4%E5%A4%96)%E3%80%81%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%E7%AD%89%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="民事纠纷解决途径" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E7%BA%A0%E7%BA%B7%E8%A7%A3%E5%86%B3%E9%80%94%E5%BE%84%3C/span%3E" _note="" _mubu_note="">
        <outline text="私力救济" _mubu_text="%3Cspan%3E%E7%A7%81%E5%8A%9B%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="和解：自行协商并达成协议" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%EF%BC%9A%E8%87%AA%E8%A1%8C%E5%8D%8F%E5%95%86%E5%B9%B6%E8%BE%BE%E6%88%90%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="社会救济" _mubu_text="%3Cspan%3E%E7%A4%BE%E4%BC%9A%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="调解：第三方对双方进行调停、说和" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%EF%BC%9A%E7%AC%AC%E4%B8%89%E6%96%B9%E5%AF%B9%E5%8F%8C%E6%96%B9%E8%BF%9B%E8%A1%8C%E8%B0%83%E5%81%9C%E3%80%81%E8%AF%B4%E5%92%8C%3C/span%3E" _note="此处调解仅指诉讼外调解，不包括法院调解" _mubu_note="%3Cspan%3E%E6%AD%A4%E5%A4%84%E8%B0%83%E8%A7%A3%E4%BB%85%E6%8C%87%E8%AF%89%E8%AE%BC%E5%A4%96%E8%B0%83%E8%A7%A3%EF%BC%8C%E4%B8%8D%E5%8C%85%E6%8B%AC%E6%B3%95%E9%99%A2%E8%B0%83%E8%A7%A3%3C/span%3E">
            <outline text="不具强制执行力，但具法律效力" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%B7%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E5%8A%9B%EF%BC%8C%E4%BD%86%E5%85%B7%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="效力类似合同" _mubu_note="%3Cspan%3E%E6%95%88%E5%8A%9B%E7%B1%BB%E4%BC%BC%E5%90%88%E5%90%8C%3C/span%3E"/>
          </outline>
          <outline text="仲裁" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="仲裁委裁决" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%A7%94%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
              <outline text="仲裁机构性质为民间组织" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E6%9C%BA%E6%9E%84%E6%80%A7%E8%B4%A8%E4%B8%BA%E6%B0%91%E9%97%B4%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="具有法律约束力和强制执行力" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%B3%95%E5%BE%8B%E7%BA%A6%E6%9D%9F%E5%8A%9B%E5%92%8C%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="公力救济" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8A%9B%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="民事诉讼" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="民诉法的属性" _mubu_text="%3Cspan%3E%E6%B0%91%E8%AF%89%E6%B3%95%E7%9A%84%E5%B1%9E%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
        <outline text="法律体系：基本法" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E4%BD%93%E7%B3%BB%EF%BC%9A%E5%9F%BA%E6%9C%AC%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="调整社会关系：部门法" _mubu_text="%3Cspan%3E%E8%B0%83%E6%95%B4%E7%A4%BE%E4%BC%9A%E5%85%B3%E7%B3%BB%EF%BC%9A%E9%83%A8%E9%97%A8%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="内容：程序法" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%EF%BC%9A%E7%A8%8B%E5%BA%8F%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="公私：公法" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A7%81%EF%BC%9A%E5%85%AC%E6%B3%95%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="民诉法的效力" _mubu_text="%3Cspan%3E%E6%B0%91%E8%AF%89%E6%B3%95%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
        <outline text="凡是在中国参加民事诉讼，必须遵守中国民事诉讼法" _mubu_text="%3Cspan%3E%E5%87%A1%E6%98%AF%E5%9C%A8%E4%B8%AD%E5%9B%BD%E5%8F%82%E5%8A%A0%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%EF%BC%8C%E5%BF%85%E9%A1%BB%E9%81%B5%E5%AE%88%E4%B8%AD%E5%9B%BD%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E6%B3%95%3C/span%3E" _note="即在中国进行民事诉讼不存在适用外国民事诉讼法的问题" _mubu_note="%3Cspan%3E%E5%8D%B3%E5%9C%A8%E4%B8%AD%E5%9B%BD%E8%BF%9B%E8%A1%8C%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E4%B8%8D%E5%AD%98%E5%9C%A8%E9%80%82%E7%94%A8%E5%A4%96%E5%9B%BD%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E6%B3%95%E7%9A%84%E9%97%AE%E9%A2%98%3C/span%3E">
          <outline text="涉及一国审判权的行使和司法主权" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E4%B8%80%E5%9B%BD%E5%AE%A1%E5%88%A4%E6%9D%83%E7%9A%84%E8%A1%8C%E4%BD%BF%E5%92%8C%E5%8F%B8%E6%B3%95%E4%B8%BB%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="仲裁规则和民法可以适用他国的" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E8%A7%84%E5%88%99%E5%92%8C%E6%B0%91%E6%B3%95%E5%8F%AF%E4%BB%A5%E9%80%82%E7%94%A8%E4%BB%96%E5%9B%BD%E7%9A%84%3C/span%3E" _note="分别属于社会救济和私法" _mubu_note="%3Cspan%3E%E5%88%86%E5%88%AB%E5%B1%9E%E4%BA%8E%E7%A4%BE%E4%BC%9A%E6%95%91%E6%B5%8E%E5%92%8C%E7%A7%81%E6%B3%95%3C/span%3E"/>
        </outline>
      </outline>
    </outline>
    <outline text="诉的基本理论" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E5%9F%BA%E6%9C%AC%E7%90%86%E8%AE%BA%3C/span%3E" _note="" _mubu_note="">
      <outline text="诉的要素" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E8%A6%81%E7%B4%A0%3C/span%3E" _note="" _mubu_note="">
        <outline text="主体，即当事人" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%8C%E5%8D%B3%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="诉讼标的" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
          <outline text="当事人发生争议并请求法院作出裁判的民事实体法律关系" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%91%E7%94%9F%E4%BA%89%E8%AE%AE%E5%B9%B6%E8%AF%B7%E6%B1%82%E6%B3%95%E9%99%A2%E4%BD%9C%E5%87%BA%E8%A3%81%E5%88%A4%E7%9A%84%E6%B0%91%E4%BA%8B%E5%AE%9E%E4%BD%93%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="诉讼标的or诉讼请求" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84or%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
            <outline text=" 诉讼标的：民事实体法律关系" _mubu_text="%3Cspan%3E%20%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%EF%BC%9A%E6%B0%91%E4%BA%8B%E5%AE%9E%E4%BD%93%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="诉讼请求：当事人基于实体法律关系(诉讼标的)请求法院所作特定裁判" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%EF%BC%9A%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9F%BA%E4%BA%8E%E5%AE%9E%E4%BD%93%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB(%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84)%E8%AF%B7%E6%B1%82%E6%B3%95%E9%99%A2%E6%89%80%E4%BD%9C%E7%89%B9%E5%AE%9A%E8%A3%81%E5%88%A4%3C/span%3E" _note="请求可有多个" _mubu_note="%3Cspan%3E%E8%AF%B7%E6%B1%82%E5%8F%AF%E6%9C%89%E5%A4%9A%E4%B8%AA%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="诉的理由：当事人所依据的事实和法律" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E7%90%86%E7%94%B1%EF%BC%9A%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%89%80%E4%BE%9D%E6%8D%AE%E7%9A%84%E4%BA%8B%E5%AE%9E%E5%92%8C%E6%B3%95%E5%BE%8B%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="诉的分类" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AF%89%E7%9A%84%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
        <outline text="前提是“诉”，非讼程序谈不上诉的分类" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%E6%98%AF%E2%80%9C%E8%AF%89%E2%80%9D%EF%BC%8C%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%E8%B0%88%E4%B8%8D%E4%B8%8A%E8%AF%89%E7%9A%84%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="诉的分类唯一依据是原告的诉讼请求" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E5%88%86%E7%B1%BB%E5%94%AF%E4%B8%80%E4%BE%9D%E6%8D%AE%E6%98%AF%E5%8E%9F%E5%91%8A%E7%9A%84%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="与被告的答辩意见无关" _mubu_note="%3Cspan%3E%E4%B8%8E%E8%A2%AB%E5%91%8A%E7%9A%84%E7%AD%94%E8%BE%A9%E6%84%8F%E8%A7%81%E6%97%A0%E5%85%B3%3C/span%3E"/>
        </outline>
        <outline text="确认之诉" _mubu_text="%3Cspan%3E%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="确认是否存在某种法律关系" _mubu_text="%3Cspan%3E%E7%A1%AE%E8%AE%A4%E6%98%AF%E5%90%A6%E5%AD%98%E5%9C%A8%E6%9F%90%E7%A7%8D%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="积极/消极确认之诉" _mubu_note="%3Cspan%3E%E7%A7%AF%E6%9E%81/%E6%B6%88%E6%9E%81%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E"/>
        </outline>
        <outline text="给付之诉" _mubu_text="%3Cspan%3E%E7%BB%99%E4%BB%98%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="请求被告履行一定义务" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E8%A2%AB%E5%91%8A%E5%B1%A5%E8%A1%8C%E4%B8%80%E5%AE%9A%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="财物的给付" _mubu_text="%3Cspan%3E%E8%B4%A2%E7%89%A9%E7%9A%84%E7%BB%99%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="行为的给付" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%B8%BA%E7%9A%84%E7%BB%99%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
              <outline text="积极的作为" _mubu_text="%3Cspan%3E%E7%A7%AF%E6%9E%81%E7%9A%84%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="消极的不作为" _mubu_text="%3Cspan%3E%E6%B6%88%E6%9E%81%E7%9A%84%E4%B8%8D%E4%BD%9C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="形成之诉" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="消灭或变更某种既存的法律关系" _mubu_text="%3Cspan%3E%E6%B6%88%E7%81%AD%E6%88%96%E5%8F%98%E6%9B%B4%E6%9F%90%E7%A7%8D%E6%97%A2%E5%AD%98%E7%9A%84%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="法律关系：主体、客体、内容" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%EF%BC%9A%E4%B8%BB%E4%BD%93%E3%80%81%E5%AE%A2%E4%BD%93%E3%80%81%E5%86%85%E5%AE%B9%3C/span%3E" _note="请求对其中一项变更即变更之诉" _mubu_note="%3Cspan%3E%E8%AF%B7%E6%B1%82%E5%AF%B9%E5%85%B6%E4%B8%AD%E4%B8%80%E9%A1%B9%E5%8F%98%E6%9B%B4%E5%8D%B3%E5%8F%98%E6%9B%B4%E4%B9%8B%E8%AF%89%3C/span%3E"/>
            <outline text="给付之诉、形成之诉中往往包含确认的情求" _mubu_text="%3Cspan%3E%E7%BB%99%E4%BB%98%E4%B9%8B%E8%AF%89%E3%80%81%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%E4%B8%AD%E5%BE%80%E5%BE%80%E5%8C%85%E5%90%AB%E7%A1%AE%E8%AE%A4%E7%9A%84%E6%83%85%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="消极的确认之诉or形成之诉" _mubu_text="%3Cspan%3E%E6%B6%88%E6%9E%81%E7%9A%84%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89or%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="确认之诉中法律系是否存在、有效是双方当事人正在争议的问题" _mubu_text="%3Cspan%3E%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%E4%B8%AD%E6%B3%95%E5%BE%8B%E7%B3%BB%E6%98%AF%E5%90%A6%E5%AD%98%E5%9C%A8%E3%80%81%E6%9C%89%E6%95%88%E6%98%AF%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%AD%A3%E5%9C%A8%E4%BA%89%E8%AE%AE%E7%9A%84%E9%97%AE%E9%A2%98%3C/span%3E" _note="如：确认婚姻关系无效" _mubu_note="%3Cspan%3E%E5%A6%82%EF%BC%9A%E7%A1%AE%E8%AE%A4%E5%A9%9A%E5%A7%BB%E5%85%B3%E7%B3%BB%E6%97%A0%E6%95%88%3C/span%3E"/>
              <outline text="形成之诉中待变更、消灭的法律关系一定是既存而有效的" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%E4%B8%AD%E5%BE%85%E5%8F%98%E6%9B%B4%E3%80%81%E6%B6%88%E7%81%AD%E7%9A%84%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E4%B8%80%E5%AE%9A%E6%98%AF%E6%97%A2%E5%AD%98%E8%80%8C%E6%9C%89%E6%95%88%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="解除合同为确认之诉" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%E4%B8%BA%E7%A1%AE%E8%AE%A4%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="解除合同采用通知到达主义，非判决生效时解除" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E5%90%88%E5%90%8C%E9%87%87%E7%94%A8%E9%80%9A%E7%9F%A5%E5%88%B0%E8%BE%BE%E4%B8%BB%E4%B9%89%EF%BC%8C%E9%9D%9E%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%E6%97%B6%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="后续诉讼只是对合同是否解除进行确认" _mubu_text="%3Cspan%3E%E5%90%8E%E7%BB%AD%E8%AF%89%E8%AE%BC%E5%8F%AA%E6%98%AF%E5%AF%B9%E5%90%88%E5%90%8C%E6%98%AF%E5%90%A6%E8%A7%A3%E9%99%A4%E8%BF%9B%E8%A1%8C%E7%A1%AE%E8%AE%A4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对撤销权提起诉讼为形成之诉" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%92%A4%E9%94%80%E6%9D%83%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%E4%B8%BA%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="形成之诉胜诉判决具有形成力，判决生效即法律关系消灭、变更" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%E8%83%9C%E8%AF%89%E5%88%A4%E5%86%B3%E5%85%B7%E6%9C%89%E5%BD%A2%E6%88%90%E5%8A%9B%EF%BC%8C%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%E5%8D%B3%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%B6%88%E7%81%AD%E3%80%81%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="诉讼请求、诉讼标的、诉讼的分类三者之间的关系" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E3%80%81%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E3%80%81%E8%AF%89%E8%AE%BC%E7%9A%84%E5%88%86%E7%B1%BB%E4%B8%89%E8%80%85%E4%B9%8B%E9%97%B4%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="基于同一诉讼标的的原告可以选择提出不同诉讼请求" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E5%90%8C%E4%B8%80%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E7%9A%84%E5%8E%9F%E5%91%8A%E5%8F%AF%E4%BB%A5%E9%80%89%E6%8B%A9%E6%8F%90%E5%87%BA%E4%B8%8D%E5%90%8C%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="诉的分类以诉讼请求为依据" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E5%88%86%E7%B1%BB%E4%BB%A5%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E4%B8%BA%E4%BE%9D%E6%8D%AE%3C/span%3E" _note="诉讼请求不同可能导致诉的分类不同" _mubu_note="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E4%B8%8D%E5%90%8C%E5%8F%AF%E8%83%BD%E5%AF%BC%E8%87%B4%E8%AF%89%E7%9A%84%E5%88%86%E7%B1%BB%E4%B8%8D%E5%90%8C%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="诉的合并" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
        <outline text="主体的合并" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E7%9A%84%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="必要共同诉讼" _mubu_text="%3Cspan%3E%E5%BF%85%E8%A6%81%E5%85%B1%E5%90%8C%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="普通共同诉讼" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E5%85%B1%E5%90%8C%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="当事人在诉讼中死亡后，数个继承人承受诉讼" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E8%AF%89%E8%AE%BC%E4%B8%AD%E6%AD%BB%E4%BA%A1%E5%90%8E%EF%BC%8C%E6%95%B0%E4%B8%AA%E7%BB%A7%E6%89%BF%E4%BA%BA%E6%89%BF%E5%8F%97%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="客体的合并" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%E7%9A%84%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="主体单一" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E5%8D%95%E4%B8%80%3C/span%3E" _note="同一原告和被告之间" _mubu_note="%3Cspan%3E%E5%90%8C%E4%B8%80%E5%8E%9F%E5%91%8A%E5%92%8C%E8%A2%AB%E5%91%8A%E4%B9%8B%E9%97%B4%3C/span%3E"/>
            <outline text="客体多个" _mubu_text="%3Cspan%3E%E5%AE%A2%E4%BD%93%E5%A4%9A%E4%B8%AA%3C/span%3E" _note="存在多个法律关系" _mubu_note="%3Cspan%3E%E5%AD%98%E5%9C%A8%E5%A4%9A%E4%B8%AA%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E"/>
          </outline>
          <outline text="数个诉之间无牵连" _mubu_text="%3Cspan%3E%E6%95%B0%E4%B8%AA%E8%AF%89%E4%B9%8B%E9%97%B4%E6%97%A0%E7%89%B5%E8%BF%9E%3C/span%3E" _note="" _mubu_note="">
            <outline text="单纯的合并" _mubu_text="%3Cspan%3E%E5%8D%95%E7%BA%AF%E7%9A%84%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="同一原告对同一被告提起若干个之间不存在牵连关系的诉" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E5%8E%9F%E5%91%8A%E5%AF%B9%E5%90%8C%E4%B8%80%E8%A2%AB%E5%91%8A%E6%8F%90%E8%B5%B7%E8%8B%A5%E5%B9%B2%E4%B8%AA%E4%B9%8B%E9%97%B4%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%89%B5%E8%BF%9E%E5%85%B3%E7%B3%BB%E7%9A%84%E8%AF%89%3C/span%3E" _note="单纯为提高诉讼效率" _mubu_note="%3Cspan%3E%E5%8D%95%E7%BA%AF%E4%B8%BA%E6%8F%90%E9%AB%98%E8%AF%89%E8%AE%BC%E6%95%88%E7%8E%87%3C/span%3E"/>
              <outline text="须分别审理，分别判决" _mubu_text="%3Cspan%3E%E9%A1%BB%E5%88%86%E5%88%AB%E5%AE%A1%E7%90%86%EF%BC%8C%E5%88%86%E5%88%AB%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="数个诉之间有牵连" _mubu_text="%3Cspan%3E%E6%95%B0%E4%B8%AA%E8%AF%89%E4%B9%8B%E9%97%B4%E6%9C%89%E7%89%B5%E8%BF%9E%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉的预备合并" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E9%A2%84%E5%A4%87%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="原告同时提出两个具有先后顺位的请求" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%90%8C%E6%97%B6%E6%8F%90%E5%87%BA%E4%B8%A4%E4%B8%AA%E5%85%B7%E6%9C%89%E5%85%88%E5%90%8E%E9%A1%BA%E4%BD%8D%E7%9A%84%E8%AF%B7%E6%B1%82%3C/span%3E" _note="主诉和预备之诉" _mubu_note="%3Cspan%3E%E4%B8%BB%E8%AF%89%E5%92%8C%E9%A2%84%E5%A4%87%E4%B9%8B%E8%AF%89%3C/span%3E">
                <outline text="在主诉请求无法得到满足时，法院将对预备请求进行审理与裁判" _mubu_text="%3Cspan%3E%E5%9C%A8%E4%B8%BB%E8%AF%89%E8%AF%B7%E6%B1%82%E6%97%A0%E6%B3%95%E5%BE%97%E5%88%B0%E6%BB%A1%E8%B6%B3%E6%97%B6%EF%BC%8C%E6%B3%95%E9%99%A2%E5%B0%86%E5%AF%B9%E9%A2%84%E5%A4%87%E8%AF%B7%E6%B1%82%E8%BF%9B%E8%A1%8C%E5%AE%A1%E7%90%86%E4%B8%8E%E8%A3%81%E5%88%A4%3C/span%3E" _note="原告希望尽力争取主诉请求得到支持，但同时提出预备请求保底" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%B8%8C%E6%9C%9B%E5%B0%BD%E5%8A%9B%E4%BA%89%E5%8F%96%E4%B8%BB%E8%AF%89%E8%AF%B7%E6%B1%82%E5%BE%97%E5%88%B0%E6%94%AF%E6%8C%81%EF%BC%8C%E4%BD%86%E5%90%8C%E6%97%B6%E6%8F%90%E5%87%BA%E9%A2%84%E5%A4%87%E8%AF%B7%E6%B1%82%E4%BF%9D%E5%BA%95%3C/span%3E"/>
              </outline>
              <outline text="其中一诉是其他诉的先决问题" _mubu_text="%3Cspan%3E%E5%85%B6%E4%B8%AD%E4%B8%80%E8%AF%89%E6%98%AF%E5%85%B6%E4%BB%96%E8%AF%89%E7%9A%84%E5%85%88%E5%86%B3%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="应先审理该诉，才有可能进一步审理其他各诉" _mubu_text="%3Cspan%3E%E5%BA%94%E5%85%88%E5%AE%A1%E7%90%86%E8%AF%A5%E8%AF%89%EF%BC%8C%E6%89%8D%E6%9C%89%E5%8F%AF%E8%83%BD%E8%BF%9B%E4%B8%80%E6%AD%A5%E5%AE%A1%E7%90%86%E5%85%B6%E4%BB%96%E5%90%84%E8%AF%89%3C/span%3E" _note="如离婚诉讼与分割共同财产" _mubu_note="%3Cspan%3E%E5%A6%82%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%E4%B8%8E%E5%88%86%E5%89%B2%E5%85%B1%E5%90%8C%E8%B4%A2%E4%BA%A7%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="诉的重叠合并" _mubu_text="%3Cspan%3E%E8%AF%89%E7%9A%84%E9%87%8D%E5%8F%A0%E5%90%88%E5%B9%B6%3C/span%3E" _note="诉的选择合并" _mubu_note="%3Cspan%3E%E8%AF%89%E7%9A%84%E9%80%89%E6%8B%A9%E5%90%88%E5%B9%B6%3C/span%3E">
              <outline text="原告基于若干不同诉讼标的提出同一诉讼请求" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%9F%BA%E4%BA%8E%E8%8B%A5%E5%B9%B2%E4%B8%8D%E5%90%8C%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E6%8F%90%E5%87%BA%E5%90%8C%E4%B8%80%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="即不论哪一个诉成立都将产生相同的法律效果" _mubu_note="%3Cspan%3E%E5%8D%B3%E4%B8%8D%E8%AE%BA%E5%93%AA%E4%B8%80%E4%B8%AA%E8%AF%89%E6%88%90%E7%AB%8B%E9%83%BD%E5%B0%86%E4%BA%A7%E7%94%9F%E7%9B%B8%E5%90%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E">
                <outline text="法院根据当事人主张，对诉讼标的择一审理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%A0%B9%E6%8D%AE%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BB%E5%BC%A0%EF%BC%8C%E5%AF%B9%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E6%8B%A9%E4%B8%80%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="反诉" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
        <outline text="诉讼进行中，本诉被告对本诉原告向法院提出独立的反请求" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%BF%9B%E8%A1%8C%E4%B8%AD%EF%BC%8C%E6%9C%AC%E8%AF%89%E8%A2%AB%E5%91%8A%E5%AF%B9%E6%9C%AC%E8%AF%89%E5%8E%9F%E5%91%8A%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E7%8B%AC%E7%AB%8B%E7%9A%84%E5%8F%8D%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="构成" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
          <outline text="主体同一性：被告向原告提出" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E5%90%8C%E4%B8%80%E6%80%A7%EF%BC%9A%E8%A2%AB%E5%91%8A%E5%90%91%E5%8E%9F%E5%91%8A%E6%8F%90%E5%87%BA%3C/span%3E" _note="本、反诉中当事人诉讼地位正好相反" _mubu_note="%3Cspan%3E%E6%9C%AC%E3%80%81%E5%8F%8D%E8%AF%89%E4%B8%AD%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%AF%89%E8%AE%BC%E5%9C%B0%E4%BD%8D%E6%AD%A3%E5%A5%BD%E7%9B%B8%E5%8F%8D%3C/span%3E"/>
          <outline text="本、反诉间有牵连关系" _mubu_text="%3Cspan%3E%E6%9C%AC%E3%80%81%E5%8F%8D%E8%AF%89%E9%97%B4%E6%9C%89%E7%89%B5%E8%BF%9E%E5%85%B3%E7%B3%BB%3C/span%3E" _note="不必基于同一法律关系" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BF%85%E5%9F%BA%E4%BA%8E%E5%90%8C%E4%B8%80%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E">
            <outline text="基于相同法律关系" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E7%9B%B8%E5%90%8C%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="具有因果关系" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E5%9B%A0%E6%9E%9C%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="基于相同事实" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E7%9B%B8%E5%90%8C%E4%BA%8B%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="在一审法庭辩论终结前提出" _mubu_text="%3Cspan%3E%E5%9C%A8%E4%B8%80%E5%AE%A1%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%E7%BB%88%E7%BB%93%E5%89%8D%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="一审辩论终结前提出" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E8%BE%A9%E8%AE%BA%E7%BB%88%E7%BB%93%E5%89%8D%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="二审提出的" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E6%8F%90%E5%87%BA%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="可以调解" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="调解不成，告知另行起诉" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B8%8D%E6%88%90%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%A1%8C%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="但双方当事人同意的，可一并审理" _mubu_text="%3Cspan%3E%E4%BD%86%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%90%8C%E6%84%8F%E7%9A%84%EF%BC%8C%E5%8F%AF%E4%B8%80%E5%B9%B6%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="管辖：向受理本次的法院提出" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%EF%BC%9A%E5%90%91%E5%8F%97%E7%90%86%E6%9C%AC%E6%AC%A1%E7%9A%84%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%3C/span%3E" _note="违背专属管辖的除外" _mubu_note="%3Cspan%3E%E8%BF%9D%E8%83%8C%E4%B8%93%E5%B1%9E%E7%AE%A1%E8%BE%96%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E">
            <outline text="牵连管辖" _mubu_text="%3Cspan%3E%E7%89%B5%E8%BF%9E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="对反诉不一定有管辖权" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%8F%8D%E8%AF%89%E4%B8%8D%E4%B8%80%E5%AE%9A%E6%9C%89%E7%AE%A1%E8%BE%96%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="基于本、反诉之间的牵连关系，当然取得" _mubu_text="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E6%9C%AC%E3%80%81%E5%8F%8D%E8%AF%89%E4%B9%8B%E9%97%B4%E7%9A%84%E7%89%B5%E8%BF%9E%E5%85%B3%E7%B3%BB%EF%BC%8C%E5%BD%93%E7%84%B6%E5%8F%96%E5%BE%97%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只要不违背专属管辖" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%A6%81%E4%B8%8D%E8%BF%9D%E8%83%8C%E4%B8%93%E5%B1%9E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="程序：本、反诉应适用同一程序审理" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%EF%BC%9A%E6%9C%AC%E3%80%81%E5%8F%8D%E8%AF%89%E5%BA%94%E9%80%82%E7%94%A8%E5%90%8C%E4%B8%80%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="反诉or反驳" _mubu_text="%3Cspan%3E%E5%8F%8D%E8%AF%89or%E5%8F%8D%E9%A9%B3%3C/span%3E" _note="" _mubu_note="">
          <outline text="反诉为独立的诉" _mubu_text="%3Cspan%3E%E5%8F%8D%E8%AF%89%E4%B8%BA%E7%8B%AC%E7%AB%8B%E7%9A%84%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="即去掉原告的主张，反诉也可单独直接向法院起诉" _mubu_text="%3Cspan%3E%E5%8D%B3%E5%8E%BB%E6%8E%89%E5%8E%9F%E5%91%8A%E7%9A%84%E4%B8%BB%E5%BC%A0%EF%BC%8C%E5%8F%8D%E8%AF%89%E4%B9%9F%E5%8F%AF%E5%8D%95%E7%8B%AC%E7%9B%B4%E6%8E%A5%E5%90%91%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="本诉的撤销并不影响反诉" _mubu_text="%3Cspan%3E%E6%9C%AC%E8%AF%89%E7%9A%84%E6%92%A4%E9%94%80%E5%B9%B6%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="否则仅为反驳" _mubu_text="%3Cspan%3E%E5%90%A6%E5%88%99%E4%BB%85%E4%B8%BA%E5%8F%8D%E9%A9%B3%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="基本原则" _mubu_text="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
      <outline text="平等原则" _mubu_text="%3Cspan%3E%E5%B9%B3%E7%AD%89%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="权利义务相同" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="权利义务相对应" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E7%9B%B8%E5%AF%B9%E5%BA%94%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="同等、对等原则" _mubu_text="%3Cspan%3E%E5%90%8C%E7%AD%89%E3%80%81%E5%AF%B9%E7%AD%89%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="仅针对外国人、无国籍人" _mubu_text="%3Cspan%3E%E4%BB%85%E9%92%88%E5%AF%B9%E5%A4%96%E5%9B%BD%E4%BA%BA%E3%80%81%E6%97%A0%E5%9B%BD%E7%B1%8D%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="同等：中国人和外国人一视同仁" _mubu_text="%3Cspan%3E%E5%90%8C%E7%AD%89%EF%BC%9A%E4%B8%AD%E5%9B%BD%E4%BA%BA%E5%92%8C%E5%A4%96%E5%9B%BD%E4%BA%BA%E4%B8%80%E8%A7%86%E5%90%8C%E4%BB%81%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="对等：加以限制的同等限制" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%AD%89%EF%BC%9A%E5%8A%A0%E4%BB%A5%E9%99%90%E5%88%B6%E7%9A%84%E5%90%8C%E7%AD%89%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="处分原则" _mubu_text="%3Cspan%3E%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="当事人有权在法律规定的范围内处分自己的权利" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%89%E6%9D%83%E5%9C%A8%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E7%9A%84%E8%8C%83%E5%9B%B4%E5%86%85%E5%A4%84%E5%88%86%E8%87%AA%E5%B7%B1%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="处分范围" _mubu_text="%3Cspan%3E%E5%A4%84%E5%88%86%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="实体权利和诉讼权利" _mubu_text="%3Cspan%3E%E5%AE%9E%E4%BD%93%E6%9D%83%E5%88%A9%E5%92%8C%E8%AF%89%E8%AE%BC%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="行使必须在法律规定范围内" _mubu_text="%3Cspan%3E%E8%A1%8C%E4%BD%BF%E5%BF%85%E9%A1%BB%E5%9C%A8%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="违反处分原则的判断" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%E7%9A%84%E5%88%A4%E6%96%AD%3C/span%3E" _note="" _mubu_note="">
          <outline text="判决超出了原告的诉讼请求即为违反处分原则" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E8%B6%85%E5%87%BA%E4%BA%86%E5%8E%9F%E5%91%8A%E7%9A%84%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E5%8D%B3%E4%B8%BA%E8%BF%9D%E5%8F%8D%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="辩论原则" _mubu_text="%3Cspan%3E%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="审理案件时，当事人有权进行辩论" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E6%A1%88%E4%BB%B6%E6%97%B6%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%89%E6%9D%83%E8%BF%9B%E8%A1%8C%E8%BE%A9%E8%AE%BA%3C/span%3E" _note="证人没有辩论权" _mubu_note="%3Cspan%3E%E8%AF%81%E4%BA%BA%E6%B2%A1%E6%9C%89%E8%BE%A9%E8%AE%BA%E6%9D%83%3C/span%3E"/>
        <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="当事人" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="时间" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="整个诉讼过程中" _mubu_text="%3Cspan%3E%E6%95%B4%E4%B8%AA%E8%AF%89%E8%AE%BC%E8%BF%87%E7%A8%8B%E4%B8%AD%3C/span%3E" _note="不仅仅限于法庭辩论阶段，包括整个一审、二审、再审" _mubu_note="%3Cspan%3E%E4%B8%8D%E4%BB%85%E4%BB%85%E9%99%90%E4%BA%8E%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%E9%98%B6%E6%AE%B5%EF%BC%8C%E5%8C%85%E6%8B%AC%E6%95%B4%E4%B8%AA%E4%B8%80%E5%AE%A1%E3%80%81%E4%BA%8C%E5%AE%A1%E3%80%81%E5%86%8D%E5%AE%A1%3C/span%3E"/>
          <outline text="其他程序不适用" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E7%A8%8B%E5%BA%8F%E4%B8%8D%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="形式" _mubu_text="%3Cspan%3E%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="书面和口头" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E5%92%8C%E5%8F%A3%E5%A4%B4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="内容" _mubu_text="%3Cspan%3E%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note="">
          <outline text="实体和程序" _mubu_text="%3Cspan%3E%E5%AE%9E%E4%BD%93%E5%92%8C%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="约束性辩论原则" _mubu_text="%3Cspan%3E%E7%BA%A6%E6%9D%9F%E6%80%A7%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="具体要求" _mubu_text="%3Cspan%3E%E5%85%B7%E4%BD%93%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
            <outline text="只有当事人提出并加以主张的事实，法院才能予以认定" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%9C%89%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8F%90%E5%87%BA%E5%B9%B6%E5%8A%A0%E4%BB%A5%E4%B8%BB%E5%BC%A0%E7%9A%84%E4%BA%8B%E5%AE%9E%EF%BC%8C%E6%B3%95%E9%99%A2%E6%89%8D%E8%83%BD%E4%BA%88%E4%BB%A5%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对双方当事人都没有争议的事实，法院应当予以认定" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%83%BD%E6%B2%A1%E6%9C%89%E4%BA%89%E8%AE%AE%E7%9A%84%E4%BA%8B%E5%AE%9E%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%E4%BA%88%E4%BB%A5%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="即法院应当受当事人自认之事实的约束" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%E5%8F%97%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%87%AA%E8%AE%A4%E4%B9%8B%E4%BA%8B%E5%AE%9E%E7%9A%84%E7%BA%A6%E6%9D%9F%3C/span%3E"/>
            <outline text="法院对证据的调查，原则上仅限于当事人提出的证据" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%AF%B9%E8%AF%81%E6%8D%AE%E7%9A%84%E8%B0%83%E6%9F%A5%EF%BC%8C%E5%8E%9F%E5%88%99%E4%B8%8A%E4%BB%85%E9%99%90%E4%BA%8E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8F%90%E5%87%BA%E7%9A%84%E8%AF%81%E6%8D%AE%3C/span%3E" _note="不允许法院依职权主动调查证据" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%85%81%E8%AE%B8%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E4%B8%BB%E5%8A%A8%E8%B0%83%E6%9F%A5%E8%AF%81%E6%8D%AE%3C/span%3E"/>
          </outline>
          <outline text="处分原则or约束性辩论原则" _mubu_text="%3Cspan%3E%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99or%E7%BA%A6%E6%9D%9F%E6%80%A7%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="都强调法院的裁判要受当事人主张的约束" _mubu_text="%3Cspan%3E%E9%83%BD%E5%BC%BA%E8%B0%83%E6%B3%95%E9%99%A2%E7%9A%84%E8%A3%81%E5%88%A4%E8%A6%81%E5%8F%97%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BB%E5%BC%A0%E7%9A%84%E7%BA%A6%E6%9D%9F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="核心区别" _mubu_text="%3Cspan%3E%E6%A0%B8%E5%BF%83%E5%8C%BA%E5%88%AB%3C/span%3E" _note="" _mubu_note="">
              <outline text="处分原则主要针对的是诉讼请求层面" _mubu_text="%3Cspan%3E%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%E4%B8%BB%E8%A6%81%E9%92%88%E5%AF%B9%E7%9A%84%E6%98%AF%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E5%B1%82%E9%9D%A2%3C/span%3E" _note="强调法院的判决不能超出当事人的诉讼请求，否则违反处分原则" _mubu_note="%3Cspan%3E%E5%BC%BA%E8%B0%83%E6%B3%95%E9%99%A2%E7%9A%84%E5%88%A4%E5%86%B3%E4%B8%8D%E8%83%BD%E8%B6%85%E5%87%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%EF%BC%8C%E5%90%A6%E5%88%99%E8%BF%9D%E5%8F%8D%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E"/>
              <outline text="辩论原则主要针对的是事实、证据层面" _mubu_text="%3Cspan%3E%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%E4%B8%BB%E8%A6%81%E9%92%88%E5%AF%B9%E7%9A%84%E6%98%AF%E4%BA%8B%E5%AE%9E%E3%80%81%E8%AF%81%E6%8D%AE%E5%B1%82%E9%9D%A2%3C/span%3E" _note="强调法院的判决不能超出当事人在事实、证据层面的主张，否则违反辩论原则" _mubu_note="%3Cspan%3E%E5%BC%BA%E8%B0%83%E6%B3%95%E9%99%A2%E7%9A%84%E5%88%A4%E5%86%B3%E4%B8%8D%E8%83%BD%E8%B6%85%E5%87%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E4%BA%8B%E5%AE%9E%E3%80%81%E8%AF%81%E6%8D%AE%E5%B1%82%E9%9D%A2%E7%9A%84%E4%B8%BB%E5%BC%A0%EF%BC%8C%E5%90%A6%E5%88%99%E8%BF%9D%E5%8F%8D%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E"/>
              <outline text="主要是判断是超事实还是超请求" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E6%98%AF%E5%88%A4%E6%96%AD%E6%98%AF%E8%B6%85%E4%BA%8B%E5%AE%9E%E8%BF%98%E6%98%AF%E8%B6%85%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="裁判超事实，违反辩论原则" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%E8%B6%85%E4%BA%8B%E5%AE%9E%EF%BC%8C%E8%BF%9D%E5%8F%8D%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="裁判超请求，违反处分原则" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%E8%B6%85%E8%AF%B7%E6%B1%82%EF%BC%8C%E8%BF%9D%E5%8F%8D%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="既超事实，又超请求，两者都违反" _mubu_text="%3Cspan%3E%E6%97%A2%E8%B6%85%E4%BA%8B%E5%AE%9E%EF%BC%8C%E5%8F%88%E8%B6%85%E8%AF%B7%E6%B1%82%EF%BC%8C%E4%B8%A4%E8%80%85%E9%83%BD%E8%BF%9D%E5%8F%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
