          <outline text="裁定转" _mubu_text="%3Cspan%20class=%22bold%22%3E%E8%A3%81%E5%AE%9A%3C/span%3E%3Cspan%3E%E8%BD%AC%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院认为需要转" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%AE%A4%E4%B8%BA%E9%9C%80%E8%A6%81%E8%BD%AC%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="当事人就案件适用简易程序提出异议" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%B0%B1%E6%A1%88%E4%BB%B6%E9%80%82%E7%94%A8%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="可以不再进行举证、质证" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E5%86%8D%E8%BF%9B%E8%A1%8C%E4%B8%BE%E8%AF%81%E3%80%81%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="普通程序的审限自立案之日起计算" _mubu_text="%3Cspan%3E%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%E7%9A%84%E5%AE%A1%E9%99%90%E8%87%AA%E7%AB%8B%E6%A1%88%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%AE%A1%E7%AE%97%3C/span%3E" _note="不重算" _mubu_note="%3Cspan%3E%E4%B8%8D%E9%87%8D%E7%AE%97%3C/span%3E">
            <outline text="刑诉需重算" _mubu_text="%3Cspan%3E%E5%88%91%E8%AF%89%E9%9C%80%E9%87%8D%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
