        </outline>
        <outline text="审理" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="可一次开庭审结并且当庭宣判" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%80%E6%AC%A1%E5%BC%80%E5%BA%AD%E5%AE%A1%E7%BB%93%E5%B9%B6%E4%B8%94%E5%BD%93%E5%BA%AD%E5%AE%A3%E5%88%A4%3C/span%3E" _note="非应当" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%BA%94%E5%BD%93%3C/span%3E"/>
        </outline>
        <outline text="审限" _mubu_text="%3Cspan%3E%E5%AE%A1%E9%99%90%3C/span%3E" _note="" _mubu_note="">
          <outline text="(2+1)个月" _mubu_text="%3Cspan%3E(2+1)%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="一审终审" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="小额中所有判决裁定均为一审终审" _mubu_text="%3Cspan%3E%E5%B0%8F%E9%A2%9D%E4%B8%AD%E6%89%80%E6%9C%89%E5%88%A4%E5%86%B3%E8%A3%81%E5%AE%9A%E5%9D%87%E4%B8%BA%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="包括驳回起诉和管辖权异议裁定" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E5%92%8C%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E8%A3%81%E5%AE%9A%3C/span%3E">
            <outline text="但可申请再审" _mubu_text="%3Cspan%3E%E4%BD%86%E5%8F%AF%E7%94%B3%E8%AF%B7%E5%86%8D%E5%AE%A1%3C/span%3E" _note="所作判决为一审判决" _mubu_note="%3Cspan%3E%E6%89%80%E4%BD%9C%E5%88%A4%E5%86%B3%E4%B8%BA%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%3C/span%3E">
              <outline text="应向原法院提出" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E5%8E%9F%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="再审应组合议庭" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E5%BA%94%E7%BB%84%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="当事人对适用小额诉讼的程序审理有异议而再审" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%AF%B9%E9%80%82%E7%94%A8%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%E6%9C%89%E5%BC%82%E8%AE%AE%E8%80%8C%E5%86%8D%E5%AE%A1%3C/span%3E" _note="不再适用一审终审的规定，所作判决可以上诉" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%86%8D%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%E7%9A%84%E8%A7%84%E5%AE%9A%EF%BC%8C%E6%89%80%E4%BD%9C%E5%88%A4%E5%86%B3%E5%8F%AF%E4%BB%A5%E4%B8%8A%E8%AF%89%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="一审终审情形汇总" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%E6%83%85%E5%BD%A2%E6%B1%87%E6%80%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="最高人民法院的一审判决、裁定" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E7%9A%84%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="调解书" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一般的裁定书" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%9A%84%E8%A3%81%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="不予受理、驳回起诉、管辖权异议除外" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E3%80%81%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text=" 特别程序所作判决一审终审" _mubu_text="%3Cspan%3E%20%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%E6%89%80%E4%BD%9C%E5%88%A4%E5%86%B3%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="不得申请再审" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BE%97%E7%94%B3%E8%AF%B7%E5%86%8D%E5%AE%A1%3C/span%3E"/>
            <outline text="小额诉讼的程序" _mubu_text="%3Cspan%3E%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="程序转换" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E8%BD%AC%E6%8D%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="转简易或普通" _mubu_text="%3Cspan%3E%E8%BD%AC%E7%AE%80%E6%98%93%E6%88%96%E6%99%AE%E9%80%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="法院认为需要转" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%AE%A4%E4%B8%BA%E9%9C%80%E8%A6%81%E8%BD%AC%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="当事人就案件适用简易程序提出异议" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%B0%B1%E6%A1%88%E4%BB%B6%E9%80%82%E7%94%A8%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="裁判文书简化" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%E6%96%87%E4%B9%A6%E7%AE%80%E5%8C%96%3C/span%3E" _note="" _mubu_note="">
          <outline text="对认定事实或裁判理由部分可省略" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E6%88%96%E8%A3%81%E5%88%A4%E7%90%86%E7%94%B1%E9%83%A8%E5%88%86%E5%8F%AF%E7%9C%81%E7%95%A5%3C/span%3E" _note="简易程序可简化不可省略" _mubu_note="%3Cspan%3E%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%E5%8F%AF%E7%AE%80%E5%8C%96%E4%B8%8D%E5%8F%AF%E7%9C%81%E7%95%A5%3C/span%3E"/>
        </outline>
      </outline>
    </outline>
    <outline text="公益诉讼程序" _mubu_text="%3Cspan%3E%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="起诉" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
        <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="法律规定的机关和有关组织可向法院起诉" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E7%9A%84%E6%9C%BA%E5%85%B3%E5%92%8C%E6%9C%89%E5%85%B3%E7%BB%84%E7%BB%87%E5%8F%AF%E5%90%91%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="相较于一般" _mubu_note="%3Cspan%3E%E7%9B%B8%E8%BE%83%E4%BA%8E%E4%B8%80%E8%88%AC%3C/span%3E">
          <outline text="减少了对原告适格的要求" _mubu_text="%3Cspan%3E%E5%87%8F%E5%B0%91%E4%BA%86%E5%AF%B9%E5%8E%9F%E5%91%8A%E9%80%82%E6%A0%BC%E7%9A%84%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="增加了对公共利益受损害的初步证据要求" _mubu_text="%3Cspan%3E%E5%A2%9E%E5%8A%A0%E4%BA%86%E5%AF%B9%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E5%8F%97%E6%8D%9F%E5%AE%B3%E7%9A%84%E5%88%9D%E6%AD%A5%E8%AF%81%E6%8D%AE%E8%A6%81%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
        <outline text="侵权行为地/被告住所地" _mubu_text="%3Cspan%3E%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E5%9C%B0/%E8%A2%AB%E5%91%8A%E4%BD%8F%E6%89%80%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="中院" _mubu_text="%3Cspan%3E%E4%B8%AD%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="告知程序" _mubu_text="%3Cspan%3E%E5%91%8A%E7%9F%A5%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="受理后10日内书面告知相关行政主管部门" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E10%E6%97%A5%E5%86%85%E4%B9%A6%E9%9D%A2%E5%91%8A%E7%9F%A5%E7%9B%B8%E5%85%B3%E8%A1%8C%E6%94%BF%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%3C/span%3E" _note="行政主管部门履职使原告诉请全部实现的，原告可撤诉" _mubu_note="%3Cspan%3E%E8%A1%8C%E6%94%BF%E4%B8%BB%E7%AE%A1%E9%83%A8%E9%97%A8%E5%B1%A5%E8%81%8C%E4%BD%BF%E5%8E%9F%E5%91%8A%E8%AF%89%E8%AF%B7%E5%85%A8%E9%83%A8%E5%AE%9E%E7%8E%B0%E7%9A%84%EF%BC%8C%E5%8E%9F%E5%91%8A%E5%8F%AF%E6%92%A4%E8%AF%89%3C/span%3E"/>
      </outline>
      <outline text="法院释明" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E9%87%8A%E6%98%8E%3C/span%3E" _note="" _mubu_note="">
        <outline text="法院认为原告诉请不足以保护公共利益" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%AE%A4%E4%B8%BA%E5%8E%9F%E5%91%8A%E8%AF%89%E8%AF%B7%E4%B8%8D%E8%B6%B3%E4%BB%A5%E4%BF%9D%E6%8A%A4%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="可向其释明变更或增加诉请" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E5%85%B6%E9%87%8A%E6%98%8E%E5%8F%98%E6%9B%B4%E6%88%96%E5%A2%9E%E5%8A%A0%E8%AF%89%E8%AF%B7%3C/span%3E" _note="停止侵害、修复生态环境等" _mubu_note="%3Cspan%3E%E5%81%9C%E6%AD%A2%E4%BE%B5%E5%AE%B3%E3%80%81%E4%BF%AE%E5%A4%8D%E7%94%9F%E6%80%81%E7%8E%AF%E5%A2%83%E7%AD%89%3C/span%3E">
          <outline text="是否变更增加仍由原告决定" _mubu_text="%3Cspan%3E%E6%98%AF%E5%90%A6%E5%8F%98%E6%9B%B4%E5%A2%9E%E5%8A%A0%E4%BB%8D%E7%94%B1%E5%8E%9F%E5%91%8A%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="其他机关、组织参与诉讼" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%9C%BA%E5%85%B3%E3%80%81%E7%BB%84%E7%BB%87%E5%8F%82%E4%B8%8E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
        <outline text="可在开庭前向法院申请，准许后列为共同原告" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%9C%A8%E5%BC%80%E5%BA%AD%E5%89%8D%E5%90%91%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%EF%BC%8C%E5%87%86%E8%AE%B8%E5%90%8E%E5%88%97%E4%B8%BA%E5%85%B1%E5%90%8C%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="没有申请作，裁判生效后不可另行提起公益诉讼，另规除外" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%E7%94%B3%E8%AF%B7%E4%BD%9C%EF%BC%8C%E8%A3%81%E5%88%A4%E7%94%9F%E6%95%88%E5%90%8E%E4%B8%8D%E5%8F%AF%E5%8F%A6%E8%A1%8C%E6%8F%90%E8%B5%B7%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%EF%BC%8C%E5%8F%A6%E8%A7%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="和/调解" _mubu_text="%3Cspan%3E%E5%92%8C/%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
        <outline text="法院应将和/调解协议进行公告" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E5%B0%86%E5%92%8C/%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E8%BF%9B%E8%A1%8C%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
          <outline text="不能以达成和解协议为由撤诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E4%BB%A5%E8%BE%BE%E6%88%90%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E4%B8%BA%E7%94%B1%E6%92%A4%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="公告期间" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="≥30日" _mubu_text="%3Cspan%3E%E2%89%A530%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="期满后" _mubu_text="%3Cspan%3E%E6%9C%9F%E6%BB%A1%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="协议不违反社会公共利益的，应当出具调解书" _mubu_text="%3Cspan%3E%E5%8D%8F%E8%AE%AE%E4%B8%8D%E8%BF%9D%E5%8F%8D%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%87%BA%E5%85%B7%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="违反社会公共利益的，不出调解书，继续对案件审理裁判" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%87%BA%E8%B0%83%E8%A7%A3%E4%B9%A6%EF%BC%8C%E7%BB%A7%E7%BB%AD%E5%AF%B9%E6%A1%88%E4%BB%B6%E5%AE%A1%E7%90%86%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="公益诉讼不允许提出反诉" _mubu_text="%3Cspan%3E%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E4%B8%8D%E5%85%81%E8%AE%B8%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
      <outline text="公益诉讼与普通民事诉讼的关系" _mubu_text="%3Cspan%3E%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E4%B8%8E%E6%99%AE%E9%80%9A%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E7%9A%84%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
        <outline text="受理公益诉讼案件，不影响同一侵权行为的受害人依法向法院提起诉讼" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E6%A1%88%E4%BB%B6%EF%BC%8C%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%90%8C%E4%B8%80%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%8F%97%E5%AE%B3%E4%BA%BA%E4%BE%9D%E6%B3%95%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="同一侵权行为的受害人不能对公益诉讼案件生效裁判提起第三人撤销之诉" _mubu_note="%3Cspan%3E%E5%90%8C%E4%B8%80%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%8F%97%E5%AE%B3%E4%BA%BA%E4%B8%8D%E8%83%BD%E5%AF%B9%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E6%A1%88%E4%BB%B6%E7%94%9F%E6%95%88%E8%A3%81%E5%88%A4%E6%8F%90%E8%B5%B7%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%92%A4%E9%94%80%E4%B9%8B%E8%AF%89%3C/span%3E"/>
      </outline>
      <outline text="检察机关提起公益诉讼" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E6%9C%BA%E5%85%B3%E6%8F%90%E8%B5%B7%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
        <outline text="前提是没有法律规定的机关和组织，或其没有提起诉讼" _mubu_text="%3Cspan%3E%E5%89%8D%E6%8F%90%E6%98%AF%E6%B2%A1%E6%9C%89%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E7%9A%84%E6%9C%BA%E5%85%B3%E5%92%8C%E7%BB%84%E7%BB%87%EF%BC%8C%E6%88%96%E5%85%B6%E6%B2%A1%E6%9C%89%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="即检察院提起公益诉讼应当具有补充性" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%A3%80%E5%AF%9F%E9%99%A2%E6%8F%90%E8%B5%B7%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E5%BA%94%E5%BD%93%E5%85%B7%E6%9C%89%E8%A1%A5%E5%85%85%E6%80%A7%3C/span%3E"/>
        <outline text="诉前公告" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8D%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
          <outline text="公告期间为30日" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E6%9C%9F%E9%97%B4%E4%B8%BA30%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="公告期满后检可提起诉讼" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E6%9C%9F%E6%BB%A1%E5%90%8E%E6%A3%80%E5%8F%AF%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
            <outline text="侵害英雄烈士的也可以直接征询英雄烈士近亲属的意见" _mubu_text="%3Cspan%3E%E4%BE%B5%E5%AE%B3%E8%8B%B1%E9%9B%84%E7%83%88%E5%A3%AB%E7%9A%84%E4%B9%9F%E5%8F%AF%E4%BB%A5%E7%9B%B4%E6%8E%A5%E5%BE%81%E8%AF%A2%E8%8B%B1%E9%9B%84%E7%83%88%E5%A3%AB%E8%BF%91%E4%BA%B2%E5%B1%9E%E7%9A%84%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="起诉主体" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="市级检察院" _mubu_text="%3Cspan%3E%E5%B8%82%E7%BA%A7%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="以公益诉讼起诉人身份" _mubu_text="%3Cspan%3E%E4%BB%A5%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E8%B5%B7%E8%AF%89%E4%BA%BA%E8%BA%AB%E4%BB%BD%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼中类似于原告身份" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%E7%B1%BB%E4%BC%BC%E4%BA%8E%E5%8E%9F%E5%91%8A%E8%BA%AB%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="管辖法院" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="由被告住所地、侵权行为地中院管辖" _mubu_text="%3Cspan%3E%E7%94%B1%E8%A2%AB%E5%91%8A%E4%BD%8F%E6%89%80%E5%9C%B0%E3%80%81%E4%BE%B5%E6%9D%83%E8%A1%8C%E4%B8%BA%E5%9C%B0%E4%B8%AD%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="上诉与二审" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%AF%89%E4%B8%8E%E4%BA%8C%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="检不服，可向上一级法院提起上诉" _mubu_text="%3Cspan%3E%E6%A3%80%E4%B8%8D%E6%9C%8D%EF%BC%8C%E5%8F%AF%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%B3%95%E9%99%A2%E6%8F%90%E8%B5%B7%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="二审中，由提起公益诉讼的检派员出庭" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E4%B8%AD%EF%BC%8C%E7%94%B1%E6%8F%90%E8%B5%B7%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E7%9A%84%E6%A3%80%E6%B4%BE%E5%91%98%E5%87%BA%E5%BA%AD%3C/span%3E" _note="上一级检也可以派员参加" _mubu_note="%3Cspan%3E%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%A3%80%E4%B9%9F%E5%8F%AF%E4%BB%A5%E6%B4%BE%E5%91%98%E5%8F%82%E5%8A%A0%3C/span%3E"/>
        </outline>
      </outline>
    </outline>
    <outline text="第三人撤销之诉" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%92%A4%E9%94%80%E4%B9%8B%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
      <outline text="起诉与受理" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E4%B8%8E%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
        <outline text="起诉" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="因不能归责于本人的事由未参加诉讼" _mubu_text="%3Cspan%3E%E5%9B%A0%E4%B8%8D%E8%83%BD%E5%BD%92%E8%B4%A3%E4%BA%8E%E6%9C%AC%E4%BA%BA%E7%9A%84%E4%BA%8B%E7%94%B1%E6%9C%AA%E5%8F%82%E5%8A%A0%E8%AF%89%E8%AE%BC%3C/span%3E" _note="主体一定是本应作为有独三、无独三的人" _mubu_note="%3Cspan%3E%E4%B8%BB%E4%BD%93%E4%B8%80%E5%AE%9A%E6%98%AF%E6%9C%AC%E5%BA%94%E4%BD%9C%E4%B8%BA%E6%9C%89%E7%8B%AC%E4%B8%89%E3%80%81%E6%97%A0%E7%8B%AC%E4%B8%89%E7%9A%84%E4%BA%BA%3C/span%3E"/>
          <outline text="有证据证明发生法律效力的裁判部分/全部内容错误，损害其权益" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E5%8F%91%E7%94%9F%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%E7%9A%84%E8%A3%81%E5%88%A4%E9%83%A8%E5%88%86/%E5%85%A8%E9%83%A8%E5%86%85%E5%AE%B9%E9%94%99%E8%AF%AF%EF%BC%8C%E6%8D%9F%E5%AE%B3%E5%85%B6%E6%9D%83%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="自(应)知道其权益受到损害之日起6个月内" _mubu_text="%3Cspan%3E%E8%87%AA(%E5%BA%94)%E7%9F%A5%E9%81%93%E5%85%B6%E6%9D%83%E7%9B%8A%E5%8F%97%E5%88%B0%E6%8D%9F%E5%AE%B3%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="向作出法院起诉" _mubu_text="%3Cspan%3E%E5%90%91%E4%BD%9C%E5%87%BA%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="审查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
          <outline text="法院在收到起诉状和证据材料之日起5日内送交对方当事人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%9C%A8%E6%94%B6%E5%88%B0%E8%B5%B7%E8%AF%89%E7%8A%B6%E5%92%8C%E8%AF%81%E6%8D%AE%E6%9D%90%E6%96%99%E4%B9%8B%E6%97%A5%E8%B5%B75%E6%97%A5%E5%86%85%E9%80%81%E4%BA%A4%E5%AF%B9%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="即原审双方当事人" _mubu_note="%3Cspan%3E%E5%8D%B3%E5%8E%9F%E5%AE%A1%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E"/>
          <outline text="对方当事人可以自收到起诉状之日起10日内提出书面意见" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%AF%E4%BB%A5%E8%87%AA%E6%94%B6%E5%88%B0%E8%B5%B7%E8%AF%89%E7%8A%B6%E4%B9%8B%E6%97%A5%E8%B5%B710%E6%97%A5%E5%86%85%E6%8F%90%E5%87%BA%E4%B9%A6%E9%9D%A2%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="法院应对起诉材料和书面意见进行审查" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E5%AF%B9%E8%B5%B7%E8%AF%89%E6%9D%90%E6%96%99%E5%92%8C%E4%B9%A6%E9%9D%A2%E6%84%8F%E8%A7%81%E8%BF%9B%E8%A1%8C%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="必要时可询问双方当事人" _mubu_note="%3Cspan%3E%E5%BF%85%E8%A6%81%E6%97%B6%E5%8F%AF%E8%AF%A2%E9%97%AE%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E"/>
        </outline>
        <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="法院应在收到起诉状之日起30日内立案/裁定不予受理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E5%9C%A8%E6%94%B6%E5%88%B0%E8%B5%B7%E8%AF%89%E7%8A%B6%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%E7%AB%8B%E6%A1%88/%E8%A3%81%E5%AE%9A%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E" _note="普通诉讼为7日" _mubu_note="%3Cspan%3E%E6%99%AE%E9%80%9A%E8%AF%89%E8%AE%BC%E4%B8%BA7%E6%97%A5%3C/span%3E"/>
        </outline>
        <outline text="不予受理情形" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用非讼程序处理的" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%E5%A4%84%E7%90%86%E7%9A%84%3C/span%3E" _note="特别、督促、公示催告、破产程序等" _mubu_note="%3Cspan%3E%E7%89%B9%E5%88%AB%E3%80%81%E7%9D%A3%E4%BF%83%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E3%80%81%E7%A0%B4%E4%BA%A7%E7%A8%8B%E5%BA%8F%E7%AD%89%3C/span%3E"/>
          <outline text="婚姻无效、撤销或者解除等判裁调中涉及身份关系的内容" _mubu_text="%3Cspan%3E%E5%A9%9A%E5%A7%BB%E6%97%A0%E6%95%88%E3%80%81%E6%92%A4%E9%94%80%E6%88%96%E8%80%85%E8%A7%A3%E9%99%A4%E7%AD%89%E5%88%A4%E8%A3%81%E8%B0%83%E4%B8%AD%E6%B6%89%E5%8F%8A%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%E7%9A%84%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="未参加登记的权利人对代表人诉讼案件的生效裁判" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%8F%82%E5%8A%A0%E7%99%BB%E8%AE%B0%E7%9A%84%E6%9D%83%E5%88%A9%E4%BA%BA%E5%AF%B9%E4%BB%A3%E8%A1%A8%E4%BA%BA%E8%AF%89%E8%AE%BC%E6%A1%88%E4%BB%B6%E7%9A%84%E7%94%9F%E6%95%88%E8%A3%81%E5%88%A4%3C/span%3E" _note="可另诉" _mubu_note="%3Cspan%3E%E5%8F%AF%E5%8F%A6%E8%AF%89%3C/span%3E"/>
          <outline text="损害社会公共利益行为的受害人对公益诉讼案件的生效裁判" _mubu_text="%3Cspan%3E%E6%8D%9F%E5%AE%B3%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%8F%97%E5%AE%B3%E4%BA%BA%E5%AF%B9%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E6%A1%88%E4%BB%B6%E7%9A%84%E7%94%9F%E6%95%88%E8%A3%81%E5%88%A4%3C/span%3E" _note="可另诉" _mubu_note="%3Cspan%3E%E5%8F%AF%E5%8F%A6%E8%AF%89%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="审理方式" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="开庭审理" _mubu_text="%3Cspan%3E%E5%BC%80%E5%BA%AD%E5%AE%A1%E7%90%86%3C/span%3E" _note="不得书面审理" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BE%97%E4%B9%A6%E9%9D%A2%E5%AE%A1%E7%90%86%3C/span%3E"/>
        <outline text="合议庭审理" _mubu_text="%3Cspan%3E%E5%90%88%E8%AE%AE%E5%BA%AD%E5%AE%A1%E7%90%86%3C/span%3E" _note="普通程序审理" _mubu_note="%3Cspan%3E%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E"/>
        <outline text="诉讼地位" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="第三人为原告" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%E4%B8%BA%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="原双方当事人为被告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="其他第三人" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="有独三" _mubu_text="%3Cspan%3E%E6%9C%89%E7%8B%AC%E4%B8%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="无独三" _mubu_text="%3Cspan%3E%E6%97%A0%E7%8B%AC%E4%B8%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="担责" _mubu_text="%3Cspan%3E%E6%8B%85%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="不担责" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%8B%85%E8%B4%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="第三人" _mubu_text="%3Cspan%3E%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="中止执行" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
        <outline text="向执行法院提出案外人对执行标的的异议" _mubu_text="%3Cspan%3E%E5%90%91%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E6%A1%88%E5%A4%96%E4%BA%BA%E5%AF%B9%E6%89%A7%E8%A1%8C%E6%A0%87%E7%9A%84%E7%9A%84%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="向法院提供担保，请求中止执行" _mubu_text="%3Cspan%3E%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%EF%BC%8C%E8%AF%B7%E6%B1%82%E4%B8%AD%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="判决" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
        <outline text="请求成立且主张全部/部分成立" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E6%88%90%E7%AB%8B%E4%B8%94%E4%B8%BB%E5%BC%A0%E5%85%A8%E9%83%A8/%E9%83%A8%E5%88%86%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
          <outline text="改变原判裁调内容的错误部分" _mubu_text="%3Cspan%3E%E6%94%B9%E5%8F%98%E5%8E%9F%E5%88%A4%E8%A3%81%E8%B0%83%E5%86%85%E5%AE%B9%E7%9A%84%E9%94%99%E8%AF%AF%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="请求成立但主张全部/部分不成立/无主张" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E6%88%90%E7%AB%8B%E4%BD%86%E4%B8%BB%E5%BC%A0%E5%85%A8%E9%83%A8/%E9%83%A8%E5%88%86%E4%B8%8D%E6%88%90%E7%AB%8B/%E6%97%A0%E4%B8%BB%E5%BC%A0%3C/span%3E" _note="" _mubu_note="">
          <outline text="撤销原判裁调内容的错误部分" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%E8%A3%81%E8%B0%83%E5%86%85%E5%AE%B9%E7%9A%84%E9%94%99%E8%AF%AF%E9%83%A8%E5%88%86%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="请求不成立的" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E4%B8%8D%E6%88%90%E7%AB%8B%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
          <outline text="驳回诉请" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%E8%AF%89%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不服可上诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%9C%8D%E5%8F%AF%E4%B8%8A%E8%AF%89%3C/span%3E" _note="三撤适用一审普通程序" _mubu_note="%3Cspan%3E%E4%B8%89%E6%92%A4%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="三撤or再审" _mubu_text="%3Cspan%3E%E4%B8%89%E6%92%A4or%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
        <outline text="主体不一样" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%E4%B8%8D%E4%B8%80%E6%A0%B7%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="同时出现，只能择其一，不能同时进行" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%87%BA%E7%8E%B0%EF%BC%8C%E5%8F%AA%E8%83%BD%E6%8B%A9%E5%85%B6%E4%B8%80%EF%BC%8C%E4%B8%8D%E8%83%BD%E5%90%8C%E6%97%B6%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="原则：再审优先" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%86%8D%E5%AE%A1%E4%BC%98%E5%85%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="将第三人诉讼请求并入再审程序处理" _mubu_text="%3Cspan%3E%E5%B0%86%E7%AC%AC%E4%B8%89%E4%BA%BA%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E5%B9%B6%E5%85%A5%E5%86%8D%E5%AE%A1%E7%A8%8B%E5%BA%8F%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="先判断是否存在提审" _mubu_text="%3Cspan%3E%E5%85%88%E5%88%A4%E6%96%AD%E6%98%AF%E5%90%A6%E5%AD%98%E5%9C%A8%E6%8F%90%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="即原终审法院为基层，中院再审" _mubu_text="%3Cspan%3E%E5%8D%B3%E5%8E%9F%E7%BB%88%E5%AE%A1%E6%B3%95%E9%99%A2%E4%B8%BA%E5%9F%BA%E5%B1%82%EF%BC%8C%E4%B8%AD%E9%99%A2%E5%86%8D%E5%AE%A1%3C/span%3E" _note="检提起抗诉(向中院提)" _mubu_note="%3Cspan%3E%E6%A3%80%E6%8F%90%E8%B5%B7%E6%8A%97%E8%AF%89(%E5%90%91%E4%B8%AD%E9%99%A2%E6%8F%90)%3C/span%3E"/>
                <outline text="存在提审，一律适用二审" _mubu_text="%3Cspan%3E%E5%AD%98%E5%9C%A8%E6%8F%90%E5%AE%A1%EF%BC%8C%E4%B8%80%E5%BE%8B%E9%80%82%E7%94%A8%E4%BA%8C%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="适用一审的再审，直接并入再审即可" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E7%9A%84%E5%86%8D%E5%AE%A1%EF%BC%8C%E7%9B%B4%E6%8E%A5%E5%B9%B6%E5%85%A5%E5%86%8D%E5%AE%A1%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="适用二审的再审" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8C%E5%AE%A1%E7%9A%84%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="只能调解，调解不成，发回一审法院重审" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E8%B0%83%E8%A7%A3%EF%BC%8C%E8%B0%83%E8%A7%A3%E4%B8%8D%E6%88%90%EF%BC%8C%E5%8F%91%E5%9B%9E%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%E9%87%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="原审为有/无独三依然为有/无独三身份参加诉讼" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E4%B8%BA%E6%9C%89/%E6%97%A0%E7%8B%AC%E4%B8%89%E4%BE%9D%E7%84%B6%E4%B8%BA%E6%9C%89/%E6%97%A0%E7%8B%AC%E4%B8%89%E8%BA%AB%E4%BB%BD%E5%8F%82%E5%8A%A0%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="原审当事人恶意串通，损害第三人合法权益" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%81%B6%E6%84%8F%E4%B8%B2%E9%80%9A%EF%BC%8C%E6%8D%9F%E5%AE%B3%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="先行审理第三人撤销之诉，中止再审程序" _mubu_text="%3Cspan%3E%E5%85%88%E8%A1%8C%E5%AE%A1%E7%90%86%E7%AC%AC%E4%B8%89%E4%BA%BA%E6%92%A4%E9%94%80%E4%B9%8B%E8%AF%89%EF%BC%8C%E4%B8%AD%E6%AD%A2%E5%86%8D%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="二审程序" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="上诉" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
        <outline text="主体：本案当事人" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%EF%BC%9A%E6%9C%AC%E6%A1%88%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
          <outline text="原告、被告、有独三、判决承担义务的无独三有权上诉" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E3%80%81%E8%A2%AB%E5%91%8A%E3%80%81%E6%9C%89%E7%8B%AC%E4%B8%89%E3%80%81%E5%88%A4%E5%86%B3%E6%89%BF%E6%8B%85%E4%B9%89%E5%8A%A1%E7%9A%84%E6%97%A0%E7%8B%AC%E4%B8%89%E6%9C%89%E6%9D%83%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="无独三其他重要考点" _mubu_text="%3Cspan%3E%E6%97%A0%E7%8B%AC%E4%B8%89%E5%85%B6%E4%BB%96%E9%87%8D%E8%A6%81%E8%80%83%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
              <outline text="签收调解书" _mubu_text="%3Cspan%3E%E7%AD%BE%E6%94%B6%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
                <outline text="调解书需无独三担责，则需经无独三签收才生效" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E9%9C%80%E6%97%A0%E7%8B%AC%E4%B8%89%E6%8B%85%E8%B4%A3%EF%BC%8C%E5%88%99%E9%9C%80%E7%BB%8F%E6%97%A0%E7%8B%AC%E4%B8%89%E7%AD%BE%E6%94%B6%E6%89%8D%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不需无独三担责，则无需签收" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%9C%80%E6%97%A0%E7%8B%AC%E4%B8%89%E6%8B%85%E8%B4%A3%EF%BC%8C%E5%88%99%E6%97%A0%E9%9C%80%E7%AD%BE%E6%94%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="三撤之诉中诉讼地位" _mubu_text="%3Cspan%3E%E4%B8%89%E6%92%A4%E4%B9%8B%E8%AF%89%E4%B8%AD%E8%AF%89%E8%AE%BC%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
                <outline text="原审担责的无独三作为被告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E6%8B%85%E8%B4%A3%E7%9A%84%E6%97%A0%E7%8B%AC%E4%B8%89%E4%BD%9C%E4%B8%BA%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="原审不担责的无独三作为第三人" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E4%B8%8D%E6%8B%85%E8%B4%A3%E7%9A%84%E6%97%A0%E7%8B%AC%E4%B8%89%E4%BD%9C%E4%B8%BA%E7%AC%AC%E4%B8%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="委托代理人需要特别授权" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BB%A3%E7%90%86%E4%BA%BA%E9%9C%80%E8%A6%81%E7%89%B9%E5%88%AB%E6%8E%88%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="检察院不能，其只能抗诉启动再审" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E4%B8%8D%E8%83%BD%EF%BC%8C%E5%85%B6%E5%8F%AA%E8%83%BD%E6%8A%97%E8%AF%89%E5%90%AF%E5%8A%A8%E5%86%8D%E5%AE%A1%3C/span%3E" _note="与刑诉不同" _mubu_note="%3Cspan%3E%E4%B8%8E%E5%88%91%E8%AF%89%E4%B8%8D%E5%90%8C%3C/span%3E"/>
        </outline>
        <outline text="对象" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="一审判决" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="一审终审情形" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="最高法的一审判决、裁定" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%B3%95%E7%9A%84%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="调解书" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="一般的裁定书" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%9A%84%E8%A3%81%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="除不予受理、驳回起诉、管辖权异议" _mubu_note="%3Cspan%3E%E9%99%A4%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E3%80%81%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%3C/span%3E"/>
              <outline text="特别、督促、公示催告" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E3%80%81%E7%9D%A3%E4%BF%83%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="小额诉讼程序" _mubu_text="%3Cspan%3E%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="三类裁定" _mubu_text="%3Cspan%3E%E4%B8%89%E7%B1%BB%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="不予受理、驳回起诉、管辖权异议" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E3%80%81%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="上诉期" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%AF%89%E6%9C%9F%3C/span%3E" _note="" _mubu_note="">
          <outline text="判决15日，裁定10日" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B315%E6%97%A5%EF%BC%8C%E8%A3%81%E5%AE%9A10%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="自一审判决送达之日起计算" _mubu_text="%3Cspan%3E%E8%87%AA%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E9%80%81%E8%BE%BE%E4%B9%8B%E6%97%A5%E8%B5%B7%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="必须书面" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E4%B9%A6%E9%9D%A2%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="应通过一审法院提交上诉状" _mubu_text="%3Cspan%3E%E5%BA%94%E9%80%9A%E8%BF%87%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%E6%8F%90%E4%BA%A4%E4%B8%8A%E8%AF%89%E7%8A%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="直接向二审法院提交的" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E5%90%91%E4%BA%8C%E5%AE%A1%E6%B3%95%E9%99%A2%E6%8F%90%E4%BA%A4%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="二审法院在5日内将其移交一审法院" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E6%B3%95%E9%99%A2%E5%9C%A85%E6%97%A5%E5%86%85%E5%B0%86%E5%85%B6%E7%A7%BB%E4%BA%A4%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="一审法院收到后，应在5日内将副本送达对方当事人" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%E6%94%B6%E5%88%B0%E5%90%8E%EF%BC%8C%E5%BA%94%E5%9C%A85%E6%97%A5%E5%86%85%E5%B0%86%E5%89%AF%E6%9C%AC%E9%80%81%E8%BE%BE%E5%AF%B9%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="对方在15日内提交答辩状" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%96%B9%E5%9C%A815%E6%97%A5%E5%86%85%E6%8F%90%E4%BA%A4%E7%AD%94%E8%BE%A9%E7%8A%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="收到答辩状后5日内将副本送达上诉人" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E7%AD%94%E8%BE%A9%E7%8A%B6%E5%90%8E5%E6%97%A5%E5%86%85%E5%B0%86%E5%89%AF%E6%9C%AC%E9%80%81%E8%BE%BE%E4%B8%8A%E8%AF%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="同时将上诉状、答辩状连同全部案卷和证据报送二审法院" _mubu_text="%3Cspan%3E%E5%90%8C%E6%97%B6%E5%B0%86%E4%B8%8A%E8%AF%89%E7%8A%B6%E3%80%81%E7%AD%94%E8%BE%A9%E7%8A%B6%E8%BF%9E%E5%90%8C%E5%85%A8%E9%83%A8%E6%A1%88%E5%8D%B7%E5%92%8C%E8%AF%81%E6%8D%AE%E6%8A%A5%E9%80%81%E4%BA%8C%E5%AE%A1%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="诉讼地位" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E5%9C%B0%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="上诉人" _mubu_text="%3Cspan%3E%E4%B8%8A%E8%AF%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="谁提谁是上诉人；都提都是上诉人" _mubu_text="%3Cspan%3E%E8%B0%81%E6%8F%90%E8%B0%81%E6%98%AF%E4%B8%8A%E8%AF%89%E4%BA%BA%EF%BC%9B%E9%83%BD%E6%8F%90%E9%83%BD%E6%98%AF%E4%B8%8A%E8%AF%89%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="被上诉人判断步骤" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%B8%8A%E8%AF%89%E4%BA%BA%E5%88%A4%E6%96%AD%E6%AD%A5%E9%AA%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="1、逐项划出一审判决" _mubu_text="%3Cspan%3E1%E3%80%81%E9%80%90%E9%A1%B9%E5%88%92%E5%87%BA%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="2、逐项划出上诉人的上诉请求" _mubu_text="%3Cspan%3E2%E3%80%81%E9%80%90%E9%A1%B9%E5%88%92%E5%87%BA%E4%B8%8A%E8%AF%89%E4%BA%BA%E7%9A%84%E4%B8%8A%E8%AF%89%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="3、将上诉请求和一审判决对比，假设上诉人的上诉请求完全成立" _mubu_text="%3Cspan%3E3%E3%80%81%E5%B0%86%E4%B8%8A%E8%AF%89%E8%AF%B7%E6%B1%82%E5%92%8C%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E5%AF%B9%E6%AF%94%EF%BC%8C%E5%81%87%E8%AE%BE%E4%B8%8A%E8%AF%89%E4%BA%BA%E7%9A%84%E4%B8%8A%E8%AF%89%E8%AF%B7%E6%B1%82%E5%AE%8C%E5%85%A8%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note="">
              <outline text="相较于一审判决而言，谁会受到损失，谁就是被上诉人" _mubu_text="%3Cspan%3E%E7%9B%B8%E8%BE%83%E4%BA%8E%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E8%80%8C%E8%A8%80%EF%BC%8C%E8%B0%81%E4%BC%9A%E5%8F%97%E5%88%B0%E6%8D%9F%E5%A4%B1%EF%BC%8C%E8%B0%81%E5%B0%B1%E6%98%AF%E8%A2%AB%E4%B8%8A%E8%AF%89%E4%BA%BA%3C/span%3E" _note="包括权利的减少或者义务的增加" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E6%9D%83%E5%88%A9%E7%9A%84%E5%87%8F%E5%B0%91%E6%88%96%E8%80%85%E4%B9%89%E5%8A%A1%E7%9A%84%E5%A2%9E%E5%8A%A0%3C/span%3E"/>
              <outline text="不会受到损失的人按照原审地位列明即可" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BC%9A%E5%8F%97%E5%88%B0%E6%8D%9F%E5%A4%B1%E7%9A%84%E4%BA%BA%E6%8C%89%E7%85%A7%E5%8E%9F%E5%AE%A1%E5%9C%B0%E4%BD%8D%E5%88%97%E6%98%8E%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="原则上由审判员组成合议庭" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E7%94%B1%E5%AE%A1%E5%88%A4%E5%91%98%E7%BB%84%E6%88%90%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
          <outline text="符合条件可独任" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E6%9D%A1%E4%BB%B6%E5%8F%AF%E7%8B%AC%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="审理范围" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="应当围绕当事人的上诉请求" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%9B%B4%E7%BB%95%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E4%B8%8A%E8%AF%89%E8%AF%B7%E6%B1%82%3C/span%3E" _note="基于处分原则" _mubu_note="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E">
            <outline text="当事人没有提出请求的，不予审理" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%B2%A1%E6%9C%89%E6%8F%90%E5%87%BA%E8%AF%B7%E6%B1%82%E7%9A%84%EF%BC%8C%E4%B8%8D%E4%BA%88%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="但一审判决违法，或者损害国家、社会公共、他人合法权益的除外" _mubu_text="%3Cspan%3E%E4%BD%86%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E8%BF%9D%E6%B3%95%EF%BC%8C%E6%88%96%E8%80%85%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E3%80%81%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E3%80%81%E4%BB%96%E4%BA%BA%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="与刑诉不同" _mubu_text="%3Cspan%3E%E4%B8%8E%E5%88%91%E8%AF%89%E4%B8%8D%E5%90%8C%3C/span%3E" _note="刑诉要全面审查" _mubu_note="%3Cspan%3E%E5%88%91%E8%AF%89%E8%A6%81%E5%85%A8%E9%9D%A2%E5%AE%A1%E6%9F%A5%3C/span%3E"/>
          </outline>
          <outline text="限于一审诉请和审理范围内" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E4%B8%80%E5%AE%A1%E8%AF%89%E8%AF%B7%E5%92%8C%E5%AE%A1%E7%90%86%E8%8C%83%E5%9B%B4%E5%86%85%3C/span%3E" _note="基于两审终审" _mubu_note="%3Cspan%3E%E5%9F%BA%E4%BA%8E%E4%B8%A4%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E">
            <outline text="另约除外(图)" _mubu_text="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
              <outline text="一审法院存在一定过错的，发回重审；不存在过错的，告知另行起诉" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%E5%AD%98%E5%9C%A8%E4%B8%80%E5%AE%9A%E8%BF%87%E9%94%99%E7%9A%84%EF%BC%8C%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%EF%BC%9B%E4%B8%8D%E5%AD%98%E5%9C%A8%E8%BF%87%E9%94%99%E7%9A%84%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%A1%8C%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22FTMvtHcMgw%22%2C%22oh%22%3A418%2C%22ow%22%3A1208%2C%22uri%22%3A%22document_image%2Fc396d62c-4448-4176-9e1e-46144ee7988c-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
          </outline>
        </outline>
        <outline text="审理形式" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="原则：开庭" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%BC%80%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="认为不需开庭" _mubu_note="%3Cspan%3E%E8%AE%A4%E4%B8%BA%E4%B8%8D%E9%9C%80%E5%BC%80%E5%BA%AD%3C/span%3E">
            <outline text="不服不予受理、管辖权异议和驳回起诉裁定的" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%9C%8D%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E5%92%8C%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E8%A3%81%E5%AE%9A%E7%9A%84%3C/span%3E" _note="即对裁定上诉的" _mubu_note="%3Cspan%3E%E5%8D%B3%E5%AF%B9%E8%A3%81%E5%AE%9A%E4%B8%8A%E8%AF%89%E7%9A%84%3C/span%3E"/>
            <outline text="当事人提出的上诉请求明显不能成立的" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8F%90%E5%87%BA%E7%9A%84%E4%B8%8A%E8%AF%89%E8%AF%B7%E6%B1%82%E6%98%8E%E6%98%BE%E4%B8%8D%E8%83%BD%E6%88%90%E7%AB%8B%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="原判裁认定事实清楚，但适用法律错误的" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E8%A3%81%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E6%B8%85%E6%A5%9A%EF%BC%8C%E4%BD%86%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E9%94%99%E8%AF%AF%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="原判决严重违反法定程序，需要发回重审的" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E5%86%B3%E4%B8%A5%E9%87%8D%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%AE%9A%E7%A8%8B%E5%BA%8F%EF%BC%8C%E9%9C%80%E8%A6%81%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="结案" _mubu_text="%3Cspan%3E%E7%BB%93%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
        <outline text="裁判" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
          <outline text="维持原判决、裁定" _mubu_text="%3Cspan%3E%E7%BB%B4%E6%8C%81%E5%8E%9F%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="用判决、裁定方式改判、撤销、变更" _mubu_text="%3Cspan%3E%E7%94%A8%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E6%96%B9%E5%BC%8F%E6%94%B9%E5%88%A4%E3%80%81%E6%92%A4%E9%94%80%E3%80%81%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="认定事实或者适用法律错误" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E6%88%96%E8%80%85%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note="">
              <outline text="认为案件不符合起诉条件的" _mubu_text="%3Cspan%3E%E8%AE%A4%E4%B8%BA%E6%A1%88%E4%BB%B6%E4%B8%8D%E7%AC%A6%E5%90%88%E8%B5%B7%E8%AF%89%E6%9D%A1%E4%BB%B6%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可撤销原判，驳回起诉" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%EF%BC%8C%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="可改可回" _mubu_text="%3Cspan%3E%E5%8F%AF%E6%94%B9%E5%8F%AF%E5%9B%9E%3C/span%3E" _note="" _mubu_note="">
            <outline text="认定基本事实不清" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E5%9F%BA%E6%9C%AC%E4%BA%8B%E5%AE%9E%E4%B8%8D%E6%B8%85%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="撤销原判，发回重审" _mubu_text="%3Cspan%3E%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%EF%BC%8C%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="严重违反法定程序" _mubu_text="%3Cspan%3E%E4%B8%A5%E9%87%8D%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%AE%9A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="重审应另组合议庭，适用一审，可上诉" _mubu_text="%3Cspan%3E%E9%87%8D%E5%AE%A1%E5%BA%94%E5%8F%A6%E7%BB%84%E5%90%88%E8%AE%AE%E5%BA%AD%EF%BC%8C%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%EF%BC%8C%E5%8F%AF%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="发回只能发一次，不区分任何情形" _mubu_text="%3Cspan%3E%E5%8F%91%E5%9B%9E%E5%8F%AA%E8%83%BD%E5%8F%91%E4%B8%80%E6%AC%A1%EF%BC%8C%E4%B8%8D%E5%8C%BA%E5%88%86%E4%BB%BB%E4%BD%95%E6%83%85%E5%BD%A2%3C/span%3E" _note="反复发回不利于定纷止争" _mubu_note="%3Cspan%3E%E5%8F%8D%E5%A4%8D%E5%8F%91%E5%9B%9E%E4%B8%8D%E5%88%A9%E4%BA%8E%E5%AE%9A%E7%BA%B7%E6%AD%A2%E4%BA%89%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="调解" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
          <outline text="应作调解书" _mubu_text="%3Cspan%3E%E5%BA%94%E4%BD%9C%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="符合法定情形可不作" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E6%B3%95%E5%AE%9A%E6%83%85%E5%BD%A2%E5%8F%AF%E4%B8%8D%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="原判视为撤销" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E8%A7%86%E4%B8%BA%E6%92%A4%E9%94%80%3C/span%3E" _note="明确一审判决效力" _mubu_note="%3Cspan%3E%E6%98%8E%E7%A1%AE%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E6%95%88%E5%8A%9B%3C/span%3E"/>
        </outline>
        <outline text="撤诉" _mubu_text="%3Cspan%3E%E6%92%A4%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="撤回起诉" _mubu_text="%3Cspan%3E%E6%92%A4%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="应一并裁定撤销一审判决" _mubu_text="%3Cspan%3E%E5%BA%94%E4%B8%80%E5%B9%B6%E8%A3%81%E5%AE%9A%E6%92%A4%E9%94%80%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不得再次起诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="一审中撤回起诉后当事人可再次起诉" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E4%B8%AD%E6%92%A4%E5%9B%9E%E8%B5%B7%E8%AF%89%E5%90%8E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%AF%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="二审、再审中撤回起诉后，原告不能再次起诉" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E3%80%81%E5%86%8D%E5%AE%A1%E4%B8%AD%E6%92%A4%E5%9B%9E%E8%B5%B7%E8%AF%89%E5%90%8E%EF%BC%8C%E5%8E%9F%E5%91%8A%E4%B8%8D%E8%83%BD%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="撤回上诉" _mubu_text="%3Cspan%3E%E6%92%A4%E5%9B%9E%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁定准许撤回上诉之日，一审判决生效" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E5%87%86%E8%AE%B8%E6%92%A4%E5%9B%9E%E4%B8%8A%E8%AF%89%E4%B9%8B%E6%97%A5%EF%BC%8C%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="和解后不履行义务的，申请强制执行一审判决" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%E5%90%8E%E4%B8%8D%E5%B1%A5%E8%A1%8C%E4%B9%89%E5%8A%A1%E7%9A%84%EF%BC%8C%E7%94%B3%E8%AF%B7%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%3C/span%3E" _note="存在生效判决和和解协议的，一般不能就和解协议起诉，执行和解协议例外" _mubu_note="%3Cspan%3E%E5%AD%98%E5%9C%A8%E7%94%9F%E6%95%88%E5%88%A4%E5%86%B3%E5%92%8C%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E7%9A%84%EF%BC%8C%E4%B8%80%E8%88%AC%E4%B8%8D%E8%83%BD%E5%B0%B1%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E8%B5%B7%E8%AF%89%EF%BC%8C%E6%89%A7%E8%A1%8C%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E4%BE%8B%E5%A4%96%3C/span%3E"/>
            </outline>
            <outline text="不应准许情形" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BA%94%E5%87%86%E8%AE%B8%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="一审判决确有错误" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E5%88%A4%E5%86%B3%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="双方当事人串通损害国家、集体、社会公共、他人合法权益的" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%B2%E9%80%9A%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E3%80%81%E9%9B%86%E4%BD%93%E3%80%81%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E3%80%81%E4%BB%96%E4%BA%BA%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="审判监督程序" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%9B%91%E7%9D%A3%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="再审对象" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
        <outline text="判决书" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="不得再审的例外" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%86%8D%E5%AE%A1%E7%9A%84%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="特别、非讼程序的判决书不适用再审" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E3%80%81%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%E7%9A%84%E5%88%A4%E5%86%B3%E4%B9%A6%E4%B8%8D%E9%80%82%E7%94%A8%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="解除婚姻关系的判裁调不得申请再审" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%E5%A9%9A%E5%A7%BB%E5%85%B3%E7%B3%BB%E7%9A%84%E5%88%A4%E8%A3%81%E8%B0%83%E4%B8%8D%E5%BE%97%E7%94%B3%E8%AF%B7%E5%86%8D%E5%AE%A1%3C/span%3E" _note="≠离婚诉讼不得再审" _mubu_note="%3Cspan%3E%E2%89%A0%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%E4%B8%8D%E5%BE%97%E5%86%8D%E5%AE%A1%3C/span%3E">
              <outline text="就财产分割申请再审" _mubu_text="%3Cspan%3E%E5%B0%B1%E8%B4%A2%E4%BA%A7%E5%88%86%E5%89%B2%E7%94%B3%E8%AF%B7%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
                <outline text="判决中已经分割的财产，可再审" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B8%AD%E5%B7%B2%E7%BB%8F%E5%88%86%E5%89%B2%E7%9A%84%E8%B4%A2%E4%BA%A7%EF%BC%8C%E5%8F%AF%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="判决书中未作处理的夫妻共同财产，告知另诉" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B9%A6%E4%B8%AD%E6%9C%AA%E4%BD%9C%E5%A4%84%E7%90%86%E7%9A%84%E5%A4%AB%E5%A6%BB%E5%85%B1%E5%90%8C%E8%B4%A2%E4%BA%A7%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="裁定书" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="不予受理" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="驳回起诉" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="调解书" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="法院启动" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="调解书确有错误" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="当事人申请" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="调解书违反自愿、合法原则" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E8%BF%9D%E5%8F%8D%E8%87%AA%E6%84%BF%E3%80%81%E5%90%88%E6%B3%95%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="检抗诉或提检察建议" _mubu_text="%3Cspan%3E%E6%A3%80%E6%8A%97%E8%AF%89%E6%88%96%E6%8F%90%E6%A3%80%E5%AF%9F%E5%BB%BA%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="调解书损害国家或社会公共利益" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E6%88%96%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
        <outline text="法院启动" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="本院启动" _mubu_text="%3Cspan%3E%E6%9C%AC%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="院长认为本院已生效判裁调确有错误，需要再审的" _mubu_text="%3Cspan%3E%E9%99%A2%E9%95%BF%E8%AE%A4%E4%B8%BA%E6%9C%AC%E9%99%A2%E5%B7%B2%E7%94%9F%E6%95%88%E5%88%A4%E8%A3%81%E8%B0%83%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%EF%BC%8C%E9%9C%80%E8%A6%81%E5%86%8D%E5%AE%A1%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="提交审判委员会讨论决定" _mubu_text="%3Cspan%3E%E6%8F%90%E4%BA%A4%E5%AE%A1%E5%88%A4%E5%A7%94%E5%91%98%E4%BC%9A%E8%AE%A8%E8%AE%BA%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="上级法院启动" _mubu_text="%3Cspan%3E%E4%B8%8A%E7%BA%A7%E6%B3%95%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="最高院对各级，上级对下级" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E9%99%A2%E5%AF%B9%E5%90%84%E7%BA%A7%EF%BC%8C%E4%B8%8A%E7%BA%A7%E5%AF%B9%E4%B8%8B%E7%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="认为确有错误，有权启动再审" _mubu_text="%3Cspan%3E%E8%AE%A4%E4%B8%BA%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%EF%BC%8C%E6%9C%89%E6%9D%83%E5%90%AF%E5%8A%A8%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="检察院启动" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="对生效判、裁" _mubu_text="%3Cspan%3E%E5%AF%B9%E7%94%9F%E6%95%88%E5%88%A4%E3%80%81%E8%A3%81%3C/span%3E" _note="" _mubu_note="">
              <outline text="证据问题" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="适用法律问题" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="程序问题" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
                <outline text="管辖权错误不是再审法定理由" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E6%9D%83%E9%94%99%E8%AF%AF%E4%B8%8D%E6%98%AF%E5%86%8D%E5%AE%A1%E6%B3%95%E5%AE%9A%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="审判人员问题" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="以上也是当事人申请再审法定情形" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%B8%8A%E4%B9%9F%E6%98%AF%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%E5%86%8D%E5%AE%A1%E6%B3%95%E5%AE%9A%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="对调解书" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
              <outline text="损害国家社会利益的" _mubu_text="%3Cspan%3E%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E7%A4%BE%E4%BC%9A%E5%88%A9%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="启动方式" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="抗诉" _mubu_text="%3Cspan%3E%E6%8A%97%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="“上对下，同级抗”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%B8%8A%E5%AF%B9%E4%B8%8B%EF%BC%8C%E5%90%8C%E7%BA%A7%E6%8A%97%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法院收到后30日内作出再审裁定" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%94%B6%E5%88%B0%E5%90%8E30%E6%97%A5%E5%86%85%E4%BD%9C%E5%87%BA%E5%86%8D%E5%AE%A1%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
                <outline text="必须直接作出" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E7%9B%B4%E6%8E%A5%E4%BD%9C%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不能实质审查，也不能不再审" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E5%AE%9E%E8%B4%A8%E5%AE%A1%E6%9F%A5%EF%BC%8C%E4%B9%9F%E4%B8%8D%E8%83%BD%E4%B8%8D%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="检察建议" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E5%BB%BA%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="“同级提，上备案”" _mubu_text="%3Cspan%3E%E2%80%9C%E5%90%8C%E7%BA%A7%E6%8F%90%EF%BC%8C%E4%B8%8A%E5%A4%87%E6%A1%88%E2%80%9D%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法院收到后应组成合议庭3个月内审查" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%94%B6%E5%88%B0%E5%90%8E%E5%BA%94%E7%BB%84%E6%88%90%E5%90%88%E8%AE%AE%E5%BA%AD3%E4%B8%AA%E6%9C%88%E5%86%85%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="决定不予再审的，应书面回复检" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E4%B8%8D%E4%BA%88%E5%86%8D%E5%AE%A1%E7%9A%84%EF%BC%8C%E5%BA%94%E4%B9%A6%E9%9D%A2%E5%9B%9E%E5%A4%8D%E6%A3%80%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="当事人申请" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
          <outline text="向法院" _mubu_text="%3Cspan%3E%E5%90%91%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="同检抗诉" _mubu_text="%3Cspan%3E%E5%90%8C%E6%A3%80%E6%8A%97%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="+生效调解书违背自愿或合法原则" _mubu_text="%3Cspan%3E+%E7%94%9F%E6%95%88%E8%B0%83%E8%A7%A3%E4%B9%A6%E8%BF%9D%E8%83%8C%E8%87%AA%E6%84%BF%E6%88%96%E5%90%88%E6%B3%95%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不受理情形" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%97%E7%90%86%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="法院已经处理过的" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%B7%B2%E7%BB%8F%E5%A4%84%E7%90%86%E8%BF%87%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="检察院处理过的" _mubu_text="%3Cspan%3E%E6%A3%80%E5%AF%9F%E9%99%A2%E5%A4%84%E7%90%86%E8%BF%87%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：向上一级法院申请" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="向原审法院申请" _mubu_text="%3Cspan%3E%E5%90%91%E5%8E%9F%E5%AE%A1%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                  <outline text="当事人一方人数众多" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%80%E6%96%B9%E4%BA%BA%E6%95%B0%E4%BC%97%E5%A4%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="双方当事人都是公民" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%83%BD%E6%98%AF%E5%85%AC%E6%B0%91%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="一方向上一级，一方向原审" _mubu_text="%3Cspan%3E%E4%B8%80%E6%96%B9%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%EF%BC%8C%E4%B8%80%E6%96%B9%E5%90%91%E5%8E%9F%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="无法协商，原审受理" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E5%8D%8F%E5%95%86%EF%BC%8C%E5%8E%9F%E5%AE%A1%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="期限" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
              <outline text="原则：生效之日起6个月" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E7%94%9F%E6%95%88%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="(应)知道之日起6个月" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note="">
                  <outline text="有新证据，足以推翻原判决、裁定的" _mubu_text="%3Cspan%3E%E6%9C%89%E6%96%B0%E8%AF%81%E6%8D%AE%EF%BC%8C%E8%B6%B3%E4%BB%A5%E6%8E%A8%E7%BF%BB%E5%8E%9F%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="原判裁认定事实主要证据是伪造的" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E8%A3%81%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E4%B8%BB%E8%A6%81%E8%AF%81%E6%8D%AE%E6%98%AF%E4%BC%AA%E9%80%A0%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="据以作出原判决的判决、裁定的法律文书被撤销或者变更的" _mubu_text="%3Cspan%3E%E6%8D%AE%E4%BB%A5%E4%BD%9C%E5%87%BA%E5%8E%9F%E5%88%A4%E5%86%B3%E7%9A%84%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E7%9A%84%E6%B3%95%E5%BE%8B%E6%96%87%E4%B9%A6%E8%A2%AB%E6%92%A4%E9%94%80%E6%88%96%E8%80%85%E5%8F%98%E6%9B%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="审判人员审理该案有贪污受贿，徇私舞弊，枉法裁判行为的" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E5%AE%A1%E7%90%86%E8%AF%A5%E6%A1%88%E6%9C%89%E8%B4%AA%E6%B1%A1%E5%8F%97%E8%B4%BF%EF%BC%8C%E5%BE%87%E7%A7%81%E8%88%9E%E5%BC%8A%EF%BC%8C%E6%9E%89%E6%B3%95%E8%A3%81%E5%88%A4%E8%A1%8C%E4%B8%BA%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="效果" _mubu_text="%3Cspan%3E%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
              <outline text="原判裁调" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E8%A3%81%E8%B0%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="不停止执行" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%81%9C%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="审查期限" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="收到申请书3个月" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E7%94%B3%E8%AF%B7%E4%B9%A63%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="特殊情况，院长批准延长" _mubu_text="%3Cspan%3E%E7%89%B9%E6%AE%8A%E6%83%85%E5%86%B5%EF%BC%8C%E9%99%A2%E9%95%BF%E6%89%B9%E5%87%86%E5%BB%B6%E9%95%BF%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="向检察院" _mubu_text="%3Cspan%3E%E5%90%91%E6%A3%80%E5%AF%9F%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="向法院提出再审申请后对其处理结果不满意" _mubu_text="%3Cspan%3E%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E5%86%8D%E5%AE%A1%E7%94%B3%E8%AF%B7%E5%90%8E%E5%AF%B9%E5%85%B6%E5%A4%84%E7%90%86%E7%BB%93%E6%9E%9C%E4%B8%8D%E6%BB%A1%E6%84%8F%3C/span%3E" _note="具有断后性" _mubu_note="%3Cspan%3E%E5%85%B7%E6%9C%89%E6%96%AD%E5%90%8E%E6%80%A7%3C/span%3E"/>
            </outline>
            <outline text="审查与处理" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E4%B8%8E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="检应在3个月内审查" _mubu_text="%3Cspan%3E%E6%A3%80%E5%BA%94%E5%9C%A83%E4%B8%AA%E6%9C%88%E5%86%85%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="当事人不得再次向检申请" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%86%8D%E6%AC%A1%E5%90%91%E6%A3%80%E7%94%B3%E8%AF%B7%3C/span%3E" _note="具有终局性" _mubu_note="%3Cspan%3E%E5%85%B7%E6%9C%89%E7%BB%88%E5%B1%80%E6%80%A7%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="审理程序" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="审理法院" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="法院启动" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="本院启动，本院审理" _mubu_text="%3Cspan%3E%E6%9C%AC%E9%99%A2%E5%90%AF%E5%8A%A8%EF%BC%8C%E6%9C%AC%E9%99%A2%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="上级法启动，上级法提审" _mubu_text="%3Cspan%3E%E4%B8%8A%E7%BA%A7%E6%B3%95%E5%90%AF%E5%8A%A8%EF%BC%8C%E4%B8%8A%E7%BA%A7%E6%B3%95%E6%8F%90%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="检启动" _mubu_text="%3Cspan%3E%E6%A3%80%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：接受抗诉的法院提审" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E6%8E%A5%E5%8F%97%E6%8A%97%E8%AF%89%E7%9A%84%E6%B3%95%E9%99%A2%E6%8F%90%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="有证据问题" _mubu_text="%3Cspan%3E%E6%9C%89%E8%AF%81%E6%8D%AE%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="可交由下一级法院审理" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BA%A4%E7%94%B1%E4%B8%8B%E4%B8%80%E7%BA%A7%E6%B3%95%E9%99%A2%E5%AE%A1%E7%90%86%3C/span%3E" _note="但经下一级法院再审审理过的除外" _mubu_note="%3Cspan%3E%E4%BD%86%E7%BB%8F%E4%B8%8B%E4%B8%80%E7%BA%A7%E6%B3%95%E9%99%A2%E5%86%8D%E5%AE%A1%E5%AE%A1%E7%90%86%E8%BF%87%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="当事人申请" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="中级以上法院审理" _mubu_text="%3Cspan%3E%E4%B8%AD%E7%BA%A7%E4%BB%A5%E4%B8%8A%E6%B3%95%E9%99%A2%E5%AE%A1%E7%90%86%3C/span%3E" _note="当事人依法选择基层的除外" _mubu_note="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BE%9D%E6%B3%95%E9%80%89%E6%8B%A9%E5%9F%BA%E5%B1%82%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E">
              <outline text="是为提审，适用二审程序" _mubu_text="%3Cspan%3E%E6%98%AF%E4%B8%BA%E6%8F%90%E5%AE%A1%EF%BC%8C%E9%80%82%E7%94%A8%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="最高院、高院裁定再审的" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E9%99%A2%E3%80%81%E9%AB%98%E9%99%A2%E8%A3%81%E5%AE%9A%E5%86%8D%E5%AE%A1%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="本院" _mubu_text="%3Cspan%3E%E6%9C%AC%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="原审法院" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="其他法院" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
              <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22556W4DlBYD%22%2C%22oh%22%3A1176%2C%22ow%22%3A3212%2C%22uri%22%3A%22document_image%2F1682318270907a19f.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
          </outline>
        </outline>
        <outline text="再审程序" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="中止原判执行" _mubu_text="%3Cspan%3E%E4%B8%AD%E6%AD%A2%E5%8E%9F%E5%88%A4%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="决定再审后，应当裁定中止原判裁调的执行" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E5%86%8D%E5%AE%A1%E5%90%8E%EF%BC%8C%E5%BA%94%E5%BD%93%E8%A3%81%E5%AE%9A%E4%B8%AD%E6%AD%A2%E5%8E%9F%E5%88%A4%E8%A3%81%E8%B0%83%E7%9A%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可以不中止" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="追索赡养费、扶养费、抚养费、抚恤金、医疗费用、劳动报酬等" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E8%B5%A1%E5%85%BB%E8%B4%B9%E3%80%81%E6%89%B6%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E6%81%A4%E9%87%91%E3%80%81%E5%8C%BB%E7%96%97%E8%B4%B9%E7%94%A8%E3%80%81%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="重组合议庭" _mubu_text="%3Cspan%3E%E9%87%8D%E7%BB%84%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="审理程序" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="适用一二审程序" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%B8%80%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="按原生效判决程序" _mubu_note="%3Cspan%3E%E6%8C%89%E5%8E%9F%E7%94%9F%E6%95%88%E5%88%A4%E5%86%B3%E7%A8%8B%E5%BA%8F%3C/span%3E"/>
            <outline text="提审的，按二审程序" _mubu_text="%3Cspan%3E%E6%8F%90%E5%AE%A1%E7%9A%84%EF%BC%8C%E6%8C%89%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="所作裁判为终审裁判" _mubu_note="%3Cspan%3E%E6%89%80%E4%BD%9C%E8%A3%81%E5%88%A4%E4%B8%BA%E7%BB%88%E5%AE%A1%E8%A3%81%E5%88%A4%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="再审的范围" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E7%9A%84%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="超出原审诉请，不予受理(不予审查)，告知另诉" _mubu_text="%3Cspan%3E%E8%B6%85%E5%87%BA%E5%8E%9F%E5%AE%A1%E8%AF%89%E8%AF%B7%EF%BC%8C%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86(%E4%B8%8D%E4%BA%88%E5%AE%A1%E6%9F%A5)%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="不调" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%B0%83%3C/span%3E">
            <outline text="即当事人增加变更诉请，被告提出反诉的" _mubu_text="%3Cspan%3E%E5%8D%B3%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%A2%9E%E5%8A%A0%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AF%B7%EF%BC%8C%E8%A2%AB%E5%91%8A%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="一审中" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="应在法庭辩论终结前" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%E7%BB%88%E7%BB%93%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可合并审理" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%88%E5%B9%B6%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="二审中" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
                <outline text="考虑两审终审制度" _mubu_text="%3Cspan%3E%E8%80%83%E8%99%91%E4%B8%A4%E5%AE%A1%E7%BB%88%E5%AE%A1%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="调解不成的，告知另诉" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B8%8D%E6%88%90%E7%9A%84%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="保护当事人上诉权" _mubu_note="%3Cspan%3E%E4%BF%9D%E6%8A%A4%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8A%E8%AF%89%E6%9D%83%3C/span%3E">
                  <outline text="但双方同意一并审理的除外" _mubu_text="%3Cspan%3E%E4%BD%86%E5%8F%8C%E6%96%B9%E5%90%8C%E6%84%8F%E4%B8%80%E5%B9%B6%E5%AE%A1%E7%90%86%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="二审发回重审的案件" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
                  <outline text="当事人增加变更诉请，被告提出反诉的，可合并审理" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%A2%9E%E5%8A%A0%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AF%B7%EF%BC%8C%E8%A2%AB%E5%91%8A%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%E7%9A%84%EF%BC%8C%E5%8F%AF%E5%90%88%E5%B9%B6%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
          </outline>
          <outline text="已生效判裁损害国家社会他人利益，应一并审理" _mubu_text="%3Cspan%3E%E5%B7%B2%E7%94%9F%E6%95%88%E5%88%A4%E8%A3%81%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E7%A4%BE%E4%BC%9A%E4%BB%96%E4%BA%BA%E5%88%A9%E7%9B%8A%EF%BC%8C%E5%BA%94%E4%B8%80%E5%B9%B6%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="对于再审发回的案件" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BA%8E%E5%86%8D%E5%AE%A1%E5%8F%91%E5%9B%9E%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="发回重审依然属于再审程序，适用范围有限原则" _mubu_note="%3Cspan%3E%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%E4%BE%9D%E7%84%B6%E5%B1%9E%E4%BA%8E%E5%86%8D%E5%AE%A1%E7%A8%8B%E5%BA%8F%EF%BC%8C%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%E6%9C%89%E9%99%90%E5%8E%9F%E5%88%99%3C/span%3E">
              <outline text="原审未合法传唤缺席判决" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E6%9C%AA%E5%90%88%E6%B3%95%E4%BC%A0%E5%94%A4%E7%BC%BA%E5%B8%AD%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="追加新的当事人" _mubu_text="%3Cspan%3E%E8%BF%BD%E5%8A%A0%E6%96%B0%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="诉讼标的物的灭失变化致使原诉请无法实现" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E7%89%A9%E7%9A%84%E7%81%AD%E5%A4%B1%E5%8F%98%E5%8C%96%E8%87%B4%E4%BD%BF%E5%8E%9F%E8%AF%89%E8%AF%B7%E6%97%A0%E6%B3%95%E5%AE%9E%E7%8E%B0%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="新增变更诉请无法通过另诉解决" _mubu_text="%3Cspan%3E%E6%96%B0%E5%A2%9E%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AF%B7%E6%97%A0%E6%B3%95%E9%80%9A%E8%BF%87%E5%8F%A6%E8%AF%89%E8%A7%A3%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="再审的结案" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E7%9A%84%E7%BB%93%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
          <outline text="裁判" _mubu_text="%3Cspan%3E%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="维持原判" _mubu_text="%3Cspan%3E%E7%BB%B4%E6%8C%81%E5%8E%9F%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="改判、撤销、变更" _mubu_text="%3Cspan%3E%E6%94%B9%E5%88%A4%E3%80%81%E6%92%A4%E9%94%80%E3%80%81%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="认定事实、适用法律错误" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E3%80%81%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="只能在经过重新审理程序后，在新的判、裁中明确是否予以撤销" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E5%9C%A8%E7%BB%8F%E8%BF%87%E9%87%8D%E6%96%B0%E5%AE%A1%E7%90%86%E7%A8%8B%E5%BA%8F%E5%90%8E%EF%BC%8C%E5%9C%A8%E6%96%B0%E7%9A%84%E5%88%A4%E3%80%81%E8%A3%81%E4%B8%AD%E6%98%8E%E7%A1%AE%E6%98%AF%E5%90%A6%E4%BA%88%E4%BB%A5%E6%92%A4%E9%94%80%3C/span%3E" _note="不能在裁定再审时予以撤销" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E5%9C%A8%E8%A3%81%E5%AE%9A%E5%86%8D%E5%AE%A1%E6%97%B6%E4%BA%88%E4%BB%A5%E6%92%A4%E9%94%80%3C/span%3E">
                <outline text="裁定再审时原则上应当裁定中止执行" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E5%86%8D%E5%AE%A1%E6%97%B6%E5%8E%9F%E5%88%99%E4%B8%8A%E5%BA%94%E5%BD%93%E8%A3%81%E5%AE%9A%E4%B8%AD%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="发回重审" _mubu_text="%3Cspan%3E%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="仅存在于适用二审的再审" _mubu_text="%3Cspan%3E%E4%BB%85%E5%AD%98%E5%9C%A8%E4%BA%8E%E9%80%82%E7%94%A8%E4%BA%8C%E5%AE%A1%E7%9A%84%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="未对案件基本事实予以审理" _mubu_text="%3Cspan%3E%E6%9C%AA%E5%AF%B9%E6%A1%88%E4%BB%B6%E5%9F%BA%E6%9C%AC%E4%BA%8B%E5%AE%9E%E4%BA%88%E4%BB%A5%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="程序错误，可能影响公正审理" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E9%94%99%E8%AF%AF%EF%BC%8C%E5%8F%AF%E8%83%BD%E5%BD%B1%E5%93%8D%E5%85%AC%E6%AD%A3%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="遗漏当事人诉讼请求、必须参加诉讼当事人" _mubu_text="%3Cspan%3E%E9%81%97%E6%BC%8F%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E3%80%81%E5%BF%85%E9%A1%BB%E5%8F%82%E5%8A%A0%E8%AF%89%E8%AE%BC%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
                  <outline text="调解不成，发回重审" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B8%8D%E6%88%90%EF%BC%8C%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="另组合议庭，一审普通程序" _mubu_text="%3Cspan%3E%E5%8F%A6%E7%BB%84%E5%90%88%E8%AE%AE%E5%BA%AD%EF%BC%8C%E4%B8%80%E5%AE%A1%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="驳回起诉" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="适用二审程序的再审" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%E7%9A%84%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="案件不符合起诉条件" _mubu_text="%3Cspan%3E%E6%A1%88%E4%BB%B6%E4%B8%8D%E7%AC%A6%E5%90%88%E8%B5%B7%E8%AF%89%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="属于不予受理的情形" _mubu_text="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E7%9A%84%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="调解" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="应制作调解书" _mubu_text="%3Cspan%3E%E5%BA%94%E5%88%B6%E4%BD%9C%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="原判、裁视为撤销" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%A4%E3%80%81%E8%A3%81%E8%A7%86%E4%B8%BA%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="撤诉" _mubu_text="%3Cspan%3E%E6%92%A4%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁定准许撤诉，应一并撤销原判决" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E5%87%86%E8%AE%B8%E6%92%A4%E8%AF%89%EF%BC%8C%E5%BA%94%E4%B8%80%E5%B9%B6%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="后不得重新起诉的" _mubu_text="%3Cspan%3E%E5%90%8E%E4%B8%8D%E5%BE%97%E9%87%8D%E6%96%B0%E8%B5%B7%E8%AF%89%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="终结" _mubu_text="%3Cspan%3E%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="审查再审申请期间，或再审审理期间" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E5%86%8D%E5%AE%A1%E7%94%B3%E8%AF%B7%E6%9C%9F%E9%97%B4%EF%BC%8C%E6%88%96%E5%86%8D%E5%AE%A1%E5%AE%A1%E7%90%86%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="达成和解协议，且已履行完毕" _mubu_text="%3Cspan%3E%E8%BE%BE%E6%88%90%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%EF%BC%8C%E4%B8%94%E5%B7%B2%E5%B1%A5%E8%A1%8C%E5%AE%8C%E6%AF%95%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="应裁定终结" _mubu_text="%3Cspan%3E%E5%BA%94%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%3C/span%3E" _note="和解协议中声明不放弃再审权利的除外" _mubu_note="%3Cspan%3E%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E4%B8%AD%E5%A3%B0%E6%98%8E%E4%B8%8D%E6%94%BE%E5%BC%83%E5%86%8D%E5%AE%A1%E6%9D%83%E5%88%A9%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E">
              <outline text="并不影响原生效判决的效力以及执行程序的继续进行" _mubu_text="%3Cspan%3E%E5%B9%B6%E4%B8%8D%E5%BD%B1%E5%93%8D%E5%8E%9F%E7%94%9F%E6%95%88%E5%88%A4%E5%86%B3%E7%9A%84%E6%95%88%E5%8A%9B%E4%BB%A5%E5%8F%8A%E6%89%A7%E8%A1%8C%E7%A8%8B%E5%BA%8F%E7%9A%84%E7%BB%A7%E7%BB%AD%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="再审中原判效力(图)" _mubu_text="%3Cspan%3E%E5%86%8D%E5%AE%A1%E4%B8%AD%E5%8E%9F%E5%88%A4%E6%95%88%E5%8A%9B(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22psyozJeLLW%22%2C%22oh%22%3A808%2C%22ow%22%3A2784%2C%22uri%22%3A%22document_image%2F8b8687a5-0df8-409f-9436-a4470f2e31d4-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="小额诉讼的再审" _mubu_text="%3Cspan%3E%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%9A%84%E5%86%8D%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="应向原审法院提出" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%91%E5%8E%9F%E5%AE%A1%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="应另行组成合议庭" _mubu_text="%3Cspan%3E%E5%BA%94%E5%8F%A6%E8%A1%8C%E7%BB%84%E6%88%90%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="应适用一审程序重新审理" _mubu_text="%3Cspan%3E%E5%BA%94%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E7%A8%8B%E5%BA%8F%E9%87%8D%E6%96%B0%E5%AE%A1%E7%90%86%3C/span%3E" _note="原来是一审终审" _mubu_note="%3Cspan%3E%E5%8E%9F%E6%9D%A5%E6%98%AF%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E">
            <outline text="能否上诉" _mubu_text="%3Cspan%3E%E8%83%BD%E5%90%A6%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
              <outline text="当事人对适用小额诉讼的程序审理无异议" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%AF%B9%E9%80%82%E7%94%A8%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%E6%97%A0%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="仍适用一审终审" _mubu_text="%3Cspan%3E%E4%BB%8D%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="有异议" _mubu_text="%3Cspan%3E%E6%9C%89%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="不适用一审终审，可上诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%EF%BC%8C%E5%8F%AF%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="特别程序" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="适用范围" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
        <outline text="不解决争议" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A7%A3%E5%86%B3%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="不是诉讼程序" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%98%AF%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="≠非讼程序" _mubu_text="%3Cspan%3E%E2%89%A0%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="选民资格案件，既不是诉讼也不是非诉" _mubu_note="%3Cspan%3E%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%EF%BC%8C%E6%97%A2%E4%B8%8D%E6%98%AF%E8%AF%89%E8%AE%BC%E4%B9%9F%E4%B8%8D%E6%98%AF%E9%9D%9E%E8%AF%89%3C/span%3E"/>
          </outline>
          <outline text="不能适用调解制度、辩论原则" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E9%80%82%E7%94%A8%E8%B0%83%E8%A7%A3%E5%88%B6%E5%BA%A6%E3%80%81%E8%BE%A9%E8%AE%BA%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不能有人民陪审员参加" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%83%BD%E6%9C%89%E4%BA%BA%E6%B0%91%E9%99%AA%E5%AE%A1%E5%91%98%E5%8F%82%E5%8A%A0%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="发现属于争议，应当裁定终结，告知另诉" _mubu_text="%3Cspan%3E%E5%8F%91%E7%8E%B0%E5%B1%9E%E4%BA%8E%E4%BA%89%E8%AE%AE%EF%BC%8C%E5%BA%94%E5%BD%93%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="共同特点" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
          <outline text="审判组织" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：独任制" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E7%8B%AC%E4%BB%BB%E5%88%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="合议制" _mubu_text="%3Cspan%3E%E5%90%88%E8%AE%AE%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="陪审员不能参加" _mubu_text="%3Cspan%3E%E9%99%AA%E5%AE%A1%E5%91%98%E4%B8%8D%E8%83%BD%E5%8F%82%E5%8A%A0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="选民资格案件" _mubu_text="%3Cspan%3E%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="重大、疑难案件" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7%E3%80%81%E7%96%91%E9%9A%BE%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="一审终审" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="且不适用审判监督、三撤" _mubu_text="%3Cspan%3E%E4%B8%94%E4%B8%8D%E9%80%82%E7%94%A8%E5%AE%A1%E5%88%A4%E7%9B%91%E7%9D%A3%E3%80%81%E4%B8%89%E6%92%A4%3C/span%3E" _note="为诉讼程序" _mubu_note="%3Cspan%3E%E4%B8%BA%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E"/>
            <outline text="当事人、利害关系人可提出异议" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E3%80%81%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E5%8F%AF%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="非复议" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%A4%8D%E8%AE%AE%3C/span%3E">
              <outline text="法院适用特别程序作出新的判裁，撤销或改变原判裁即可" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E9%80%82%E7%94%A8%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%E4%BD%9C%E5%87%BA%E6%96%B0%E7%9A%84%E5%88%A4%E8%A3%81%EF%BC%8C%E6%92%A4%E9%94%80%E6%88%96%E6%94%B9%E5%8F%98%E5%8E%9F%E5%88%A4%E8%A3%81%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="审限" _mubu_text="%3Cspan%3E%E5%AE%A1%E9%99%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="立案之日起30日内" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="或公告期满后30日内" _mubu_text="%3Cspan%3E%E6%88%96%E5%85%AC%E5%91%8A%E6%9C%9F%E6%BB%A1%E5%90%8E30%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="院长批准可以延长" _mubu_text="%3Cspan%3E%E9%99%A2%E9%95%BF%E6%89%B9%E5%87%86%E5%8F%AF%E4%BB%A5%E5%BB%B6%E9%95%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="选民资格案件必须在选举日前审结" _mubu_text="%3Cspan%3E%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%E5%BF%85%E9%A1%BB%E5%9C%A8%E9%80%89%E4%B8%BE%E6%97%A5%E5%89%8D%E5%AE%A1%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
        <outline text="选民资格案件" _mubu_text="%3Cspan%3E%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="申诉前置" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%89%E5%89%8D%E7%BD%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="公民不服选民名单应该先向选委会申诉" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E4%B8%8D%E6%9C%8D%E9%80%89%E6%B0%91%E5%90%8D%E5%8D%95%E5%BA%94%E8%AF%A5%E5%85%88%E5%90%91%E9%80%89%E5%A7%94%E4%BC%9A%E7%94%B3%E8%AF%89%3C/span%3E" _note="对申诉处理决定不服才能起诉" _mubu_note="%3Cspan%3E%E5%AF%B9%E7%94%B3%E8%AF%89%E5%A4%84%E7%90%86%E5%86%B3%E5%AE%9A%E4%B8%8D%E6%9C%8D%E6%89%8D%E8%83%BD%E8%B5%B7%E8%AF%89%3C/span%3E"/>
          </outline>
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="可在选举日的5日前向选区所在地基层法院起诉" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%9C%A8%E9%80%89%E4%B8%BE%E6%97%A5%E7%9A%845%E6%97%A5%E5%89%8D%E5%90%91%E9%80%89%E5%8C%BA%E6%89%80%E5%9C%A8%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="起诉人的范围" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E4%BA%BA%E7%9A%84%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="不要求起诉人是选民本人或者与案件有利害关系" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A6%81%E6%B1%82%E8%B5%B7%E8%AF%89%E4%BA%BA%E6%98%AF%E9%80%89%E6%B0%91%E6%9C%AC%E4%BA%BA%E6%88%96%E8%80%85%E4%B8%8E%E6%A1%88%E4%BB%B6%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="必须在选举日前审结" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E5%9C%A8%E9%80%89%E4%B8%BE%E6%97%A5%E5%89%8D%E5%AE%A1%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="审理时，起诉人、选举委员会代表和有关公民必须参加" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E6%97%B6%EF%BC%8C%E8%B5%B7%E8%AF%89%E4%BA%BA%E3%80%81%E9%80%89%E4%B8%BE%E5%A7%94%E5%91%98%E4%BC%9A%E4%BB%A3%E8%A1%A8%E5%92%8C%E6%9C%89%E5%85%B3%E5%85%AC%E6%B0%91%E5%BF%85%E9%A1%BB%E5%8F%82%E5%8A%A0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="判决书应在选举日前送达" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B9%A6%E5%BA%94%E5%9C%A8%E9%80%89%E4%B8%BE%E6%97%A5%E5%89%8D%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="宣告失踪、死亡案件" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%91%8A%E5%A4%B1%E8%B8%AA%E3%80%81%E6%AD%BB%E4%BA%A1%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="利害关系人申请" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="下落不明人住所地基层法院" _mubu_text="%3Cspan%3E%E4%B8%8B%E8%90%BD%E4%B8%8D%E6%98%8E%E4%BA%BA%E4%BD%8F%E6%89%80%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="宣告失踪的，应同时依法指定财产代管人" _mubu_text="%3Cspan%3E%E5%AE%A3%E5%91%8A%E5%A4%B1%E8%B8%AA%E7%9A%84%EF%BC%8C%E5%BA%94%E5%90%8C%E6%97%B6%E4%BE%9D%E6%B3%95%E6%8C%87%E5%AE%9A%E8%B4%A2%E4%BA%A7%E4%BB%A3%E7%AE%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="财产代管人的变更" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E4%BB%A3%E7%AE%A1%E4%BA%BA%E7%9A%84%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="代管人申请" _mubu_text="%3Cspan%3E%E4%BB%A3%E7%AE%A1%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="无纠纷" _mubu_note="%3Cspan%3E%E6%97%A0%E7%BA%A0%E7%BA%B7%3C/span%3E">
                  <outline text="成立的，裁定撤销，另行指定" _mubu_text="%3Cspan%3E%E6%88%90%E7%AB%8B%E7%9A%84%EF%BC%8C%E8%A3%81%E5%AE%9A%E6%92%A4%E9%94%80%EF%BC%8C%E5%8F%A6%E8%A1%8C%E6%8C%87%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="不成立的，裁定驳回申请" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%88%90%E7%AB%8B%E7%9A%84%EF%BC%8C%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="失踪人的其他利害关系人申请" _mubu_text="%3Cspan%3E%E5%A4%B1%E8%B8%AA%E4%BA%BA%E7%9A%84%E5%85%B6%E4%BB%96%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="有纠纷" _mubu_note="%3Cspan%3E%E6%9C%89%E7%BA%A0%E7%BA%B7%3C/span%3E">
                  <outline text="告知其以原指定的代管人为被告提起诉讼" _mubu_text="%3Cspan%3E%E5%91%8A%E7%9F%A5%E5%85%B6%E4%BB%A5%E5%8E%9F%E6%8C%87%E5%AE%9A%E7%9A%84%E4%BB%A3%E7%AE%A1%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="按照普通程序审理" _mubu_note="%3Cspan%3E%E6%8C%89%E7%85%A7%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E"/>
                </outline>
              </outline>
            </outline>
            <outline text="“亡者归来”" _mubu_text="%3Cspan%3E%E2%80%9C%E4%BA%A1%E8%80%85%E5%BD%92%E6%9D%A5%E2%80%9D%3C/span%3E" _note="" _mubu_note="">
              <outline text="经本人或利害关系人申请，作出新判，撤销原判" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%9C%AC%E4%BA%BA%E6%88%96%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E8%AF%B7%EF%BC%8C%E4%BD%9C%E5%87%BA%E6%96%B0%E5%88%A4%EF%BC%8C%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="认定公民无、限民行能案件" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E5%85%AC%E6%B0%91%E6%97%A0%E3%80%81%E9%99%90%E6%B0%91%E8%A1%8C%E8%83%BD%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="利害关系人或者有关组织提出申请" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E6%88%96%E8%80%85%E6%9C%89%E5%85%B3%E7%BB%84%E7%BB%87%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="该公民住所地基层法院" _mubu_text="%3Cspan%3E%E8%AF%A5%E5%85%AC%E6%B0%91%E4%BD%8F%E6%89%80%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="鉴定" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="必要时应鉴定" _mubu_text="%3Cspan%3E%E5%BF%85%E8%A6%81%E6%97%B6%E5%BA%94%E9%89%B4%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="申请人提供鉴定意见的，应当对鉴定意见进行审查" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%8F%90%E4%BE%9B%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E5%AF%B9%E9%89%B4%E5%AE%9A%E6%84%8F%E8%A7%81%E8%BF%9B%E8%A1%8C%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="选定代理人" _mubu_text="%3Cspan%3E%E9%80%89%E5%AE%9A%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="由该公民的近亲属为代理人" _mubu_text="%3Cspan%3E%E7%94%B1%E8%AF%A5%E5%85%AC%E6%B0%91%E7%9A%84%E8%BF%91%E4%BA%B2%E5%B1%9E%E4%B8%BA%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E" _note="申请人除外" _mubu_note="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E9%99%A4%E5%A4%96%3C/span%3E">
                <outline text="推诿的，由法院指定其中一人" _mubu_text="%3Cspan%3E%E6%8E%A8%E8%AF%BF%E7%9A%84%EF%BC%8C%E7%94%B1%E6%B3%95%E9%99%A2%E6%8C%87%E5%AE%9A%E5%85%B6%E4%B8%AD%E4%B8%80%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="健康状况允许的，应当询问本人意见" _mubu_text="%3Cspan%3E%E5%81%A5%E5%BA%B7%E7%8A%B6%E5%86%B5%E5%85%81%E8%AE%B8%E7%9A%84%EF%BC%8C%E5%BA%94%E5%BD%93%E8%AF%A2%E9%97%AE%E6%9C%AC%E4%BA%BA%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="判决的撤销" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E7%9A%84%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="本人、利害关系人或者有关组织申请" _mubu_text="%3Cspan%3E%E6%9C%AC%E4%BA%BA%E3%80%81%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E6%88%96%E8%80%85%E6%9C%89%E5%85%B3%E7%BB%84%E7%BB%87%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="原因已消除的，应作出新判，撤销原判" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%9B%A0%E5%B7%B2%E6%B6%88%E9%99%A4%E7%9A%84%EF%BC%8C%E5%BA%94%E4%BD%9C%E5%87%BA%E6%96%B0%E5%88%A4%EF%BC%8C%E6%92%A4%E9%94%80%E5%8E%9F%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="指定遗产管理人案件" _mubu_text="%3Cspan%3E%E6%8C%87%E5%AE%9A%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="利害关系人申请" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="申请书应当写明被继承人死亡的时间、申请事由和具体请求，并附有被继承人死亡的相关证据" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%B9%A6%E5%BA%94%E5%BD%93%E5%86%99%E6%98%8E%E8%A2%AB%E7%BB%A7%E6%89%BF%E4%BA%BA%E6%AD%BB%E4%BA%A1%E7%9A%84%E6%97%B6%E9%97%B4%E3%80%81%E7%94%B3%E8%AF%B7%E4%BA%8B%E7%94%B1%E5%92%8C%E5%85%B7%E4%BD%93%E8%AF%B7%E6%B1%82%EF%BC%8C%E5%B9%B6%E9%99%84%E6%9C%89%E8%A2%AB%E7%BB%A7%E6%89%BF%E4%BA%BA%E6%AD%BB%E4%BA%A1%E7%9A%84%E7%9B%B8%E5%85%B3%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="被继承人死亡时住所地或者主要遗产所在地基层法院" _mubu_text="%3Cspan%3E%E8%A2%AB%E7%BB%A7%E6%89%BF%E4%BA%BA%E6%AD%BB%E4%BA%A1%E6%97%B6%E4%BD%8F%E6%89%80%E5%9C%B0%E6%88%96%E8%80%85%E4%B8%BB%E8%A6%81%E9%81%97%E4%BA%A7%E6%89%80%E5%9C%A8%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院受理申请后，应当审查核实，并按照有利于遗产管理的原则，判决指定遗产管理人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%97%E7%90%86%E7%94%B3%E8%AF%B7%E5%90%8E%EF%BC%8C%E5%BA%94%E5%BD%93%E5%AE%A1%E6%9F%A5%E6%A0%B8%E5%AE%9E%EF%BC%8C%E5%B9%B6%E6%8C%89%E7%85%A7%E6%9C%89%E5%88%A9%E4%BA%8E%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E7%9A%84%E5%8E%9F%E5%88%99%EF%BC%8C%E5%88%A4%E5%86%B3%E6%8C%87%E5%AE%9A%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="另行指定遗产管理人" _mubu_text="%3Cspan%3E%E5%8F%A6%E8%A1%8C%E6%8C%87%E5%AE%9A%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="被指定的遗产管理人死亡、终止、丧失民事行为能力或者存在其他无法继续履行遗产管理职责情形的" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%8C%87%E5%AE%9A%E7%9A%84%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%E6%AD%BB%E4%BA%A1%E3%80%81%E7%BB%88%E6%AD%A2%E3%80%81%E4%B8%A7%E5%A4%B1%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E6%88%96%E8%80%85%E5%AD%98%E5%9C%A8%E5%85%B6%E4%BB%96%E6%97%A0%E6%B3%95%E7%BB%A7%E7%BB%AD%E5%B1%A5%E8%A1%8C%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E8%81%8C%E8%B4%A3%E6%83%85%E5%BD%A2%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="法院可以根据利害关系人或者本人的申请另行指定遗产管理人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BB%A5%E6%A0%B9%E6%8D%AE%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E6%88%96%E8%80%85%E6%9C%AC%E4%BA%BA%E7%9A%84%E7%94%B3%E8%AF%B7%E5%8F%A6%E8%A1%8C%E6%8C%87%E5%AE%9A%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="遗产管理人违反遗产管理职责，严重侵害继承人、受遗赠人或者债权人合法权益的" _mubu_text="%3Cspan%3E%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%E8%BF%9D%E5%8F%8D%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E8%81%8C%E8%B4%A3%EF%BC%8C%E4%B8%A5%E9%87%8D%E4%BE%B5%E5%AE%B3%E7%BB%A7%E6%89%BF%E4%BA%BA%E3%80%81%E5%8F%97%E9%81%97%E8%B5%A0%E4%BA%BA%E6%88%96%E8%80%85%E5%80%BA%E6%9D%83%E4%BA%BA%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="法院可以根据利害关系人的申请，撤销其遗产管理人资格，并依法指定新的遗产管理人" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BB%A5%E6%A0%B9%E6%8D%AE%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%9A%84%E7%94%B3%E8%AF%B7%EF%BC%8C%E6%92%A4%E9%94%80%E5%85%B6%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%E8%B5%84%E6%A0%BC%EF%BC%8C%E5%B9%B6%E4%BE%9D%E6%B3%95%E6%8C%87%E5%AE%9A%E6%96%B0%E7%9A%84%E9%81%97%E4%BA%A7%E7%AE%A1%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="认定财产无主案件" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E8%B4%A2%E4%BA%A7%E6%97%A0%E4%B8%BB%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="公民、法人或者其他组织申请" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E3%80%81%E6%B3%95%E4%BA%BA%E6%88%96%E8%80%85%E5%85%B6%E4%BB%96%E7%BB%84%E7%BB%87%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="财产所在地基层法院" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E6%89%80%E5%9C%A8%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="受理后，经审查核实，应发出财产认领公告" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%EF%BC%8C%E7%BB%8F%E5%AE%A1%E6%9F%A5%E6%A0%B8%E5%AE%9E%EF%BC%8C%E5%BA%94%E5%8F%91%E5%87%BA%E8%B4%A2%E4%BA%A7%E8%AE%A4%E9%A2%86%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="公告满1年无人认领" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E6%BB%A11%E5%B9%B4%E6%97%A0%E4%BA%BA%E8%AE%A4%E9%A2%86%3C/span%3E" _note="" _mubu_note="">
              <outline text="判决为财产无主，收归国家或者集体所有" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B8%BA%E8%B4%A2%E4%BA%A7%E6%97%A0%E4%B8%BB%EF%BC%8C%E6%94%B6%E5%BD%92%E5%9B%BD%E5%AE%B6%E6%88%96%E8%80%85%E9%9B%86%E4%BD%93%E6%89%80%E6%9C%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="公告期间，有人对该财产提出请求" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E6%9C%9F%E9%97%B4%EF%BC%8C%E6%9C%89%E4%BA%BA%E5%AF%B9%E8%AF%A5%E8%B4%A2%E4%BA%A7%E6%8F%90%E5%87%BA%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
              <outline text="应裁定终结特别程序，告知另诉" _mubu_text="%3Cspan%3E%E5%BA%94%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="有争议与纠纷；适用普通程序审理" _mubu_note="%3Cspan%3E%E6%9C%89%E4%BA%89%E8%AE%AE%E4%B8%8E%E7%BA%A0%E7%BA%B7%EF%BC%9B%E9%80%82%E7%94%A8%E6%99%AE%E9%80%9A%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E"/>
            </outline>
            <outline text="判决的撤销" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E7%9A%84%E6%92%A4%E9%94%80%3C/span%3E" _note="" _mubu_note="">
              <outline text="认定无主物后，原所有人或继承人出现" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E6%97%A0%E4%B8%BB%E7%89%A9%E5%90%8E%EF%BC%8C%E5%8E%9F%E6%89%80%E6%9C%89%E4%BA%BA%E6%88%96%E7%BB%A7%E6%89%BF%E4%BA%BA%E5%87%BA%E7%8E%B0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="确认调解协议效力案件" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%A1%AE%E8%AE%A4%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E6%95%88%E5%8A%9B%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="指人民调解委作的调解协议" _mubu_text="%3Cspan%3E%E6%8C%87%E4%BA%BA%E6%B0%91%E8%B0%83%E8%A7%A3%E5%A7%94%E4%BD%9C%E7%9A%84%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="即诉前的社会调解" _mubu_note="%3Cspan%3E%E5%8D%B3%E8%AF%89%E5%89%8D%E7%9A%84%E7%A4%BE%E4%BC%9A%E8%B0%83%E8%A7%A3%3C/span%3E">
            <outline text="受理后调解为诉中调解，可强执" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%E8%B0%83%E8%A7%A3%E4%B8%BA%E8%AF%89%E4%B8%AD%E8%B0%83%E8%A7%A3%EF%BC%8C%E5%8F%AF%E5%BC%BA%E6%89%A7%3C/span%3E" _note="不存在确认效力" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%A1%AE%E8%AE%A4%E6%95%88%E5%8A%9B%3C/span%3E"/>
          </outline>
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="自调解协议生效之日起30日内" _mubu_text="%3Cspan%3E%E8%87%AA%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E7%94%9F%E6%95%88%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="双方当事人共同申请" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%85%B1%E5%90%8C%E7%94%B3%E8%AF%B7%3C/span%3E" _note="书面/口头" _mubu_note="%3Cspan%3E%E4%B9%A6%E9%9D%A2/%E5%8F%A3%E5%A4%B4%3C/span%3E"/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院邀请调解组织开展先行调解的" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E9%82%80%E8%AF%B7%E8%B0%83%E8%A7%A3%E7%BB%84%E7%BB%87%E5%BC%80%E5%B1%95%E5%85%88%E8%A1%8C%E8%B0%83%E8%A7%A3%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="由作出邀请的法院" _mubu_text="%3Cspan%3E%E7%94%B1%E4%BD%9C%E5%87%BA%E9%82%80%E8%AF%B7%E7%9A%84%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="调解组织自行开展的" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E7%BB%84%E7%BB%87%E8%87%AA%E8%A1%8C%E5%BC%80%E5%B1%95%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="由基层" _mubu_text="%3Cspan%3E%E7%94%B1%E5%9F%BA%E5%B1%82%3C/span%3E" _note="" _mubu_note="">
                <outline text="向当事人住所地" _mubu_text="%3Cspan%3E%E5%90%91%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BD%8F%E6%89%80%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="标的物所在地" _mubu_text="%3Cspan%3E%E6%A0%87%E7%9A%84%E7%89%A9%E6%89%80%E5%9C%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="调解组织所在地" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E7%BB%84%E7%BB%87%E6%89%80%E5%9C%A8%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="由相应中院" _mubu_text="%3Cspan%3E%E7%94%B1%E7%9B%B8%E5%BA%94%E4%B8%AD%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
                <outline text="所涉纠纷应由中院管辖" _mubu_text="%3Cspan%3E%E6%89%80%E6%B6%89%E7%BA%A0%E7%BA%B7%E5%BA%94%E7%94%B1%E4%B8%AD%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="不予受理" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="不属于该法院受理/管辖范围" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%B1%9E%E4%BA%8E%E8%AF%A5%E6%B3%95%E9%99%A2%E5%8F%97%E7%90%86/%E7%AE%A1%E8%BE%96%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="申请确认身份关系无/有效或者解除" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E7%A1%AE%E8%AE%A4%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%E6%97%A0/%E6%9C%89%E6%95%88%E6%88%96%E8%80%85%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="涉及适用其他特别程序、公示催告、破产程序审理" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E9%80%82%E7%94%A8%E5%85%B6%E4%BB%96%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E3%80%81%E7%A0%B4%E4%BA%A7%E7%A8%8B%E5%BA%8F%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="调解协议内容涉及物权、知识产权确权" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%86%85%E5%AE%B9%E6%B6%89%E5%8F%8A%E7%89%A9%E6%9D%83%E3%80%81%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%E7%A1%AE%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="符合规定的" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E8%A7%84%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="裁定调解协议有效" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E6%9C%89%E6%95%88%3C/span%3E" _note="可申请强执" _mubu_note="%3Cspan%3E%E5%8F%AF%E7%94%B3%E8%AF%B7%E5%BC%BA%E6%89%A7%3C/span%3E"/>
              <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="当事人不服" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="收到裁定之日起15日内提出异议" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E8%A3%81%E5%AE%9A%E4%B9%8B%E6%97%A5%E8%B5%B715%E6%97%A5%E5%86%85%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="利害关系人不服" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E4%B8%8D%E6%9C%8D%3C/span%3E" _note="" _mubu_note="">
                  <outline text="(应)知道权益受损之日起6个月内提出异议" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E6%9D%83%E7%9B%8A%E5%8F%97%E6%8D%9F%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%E5%86%85%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
            </outline>
            <outline text="不符合规定的" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AC%A6%E5%90%88%E8%A7%84%E5%AE%9A%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="裁定驳回申请" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="此时只是说明就调解协议涉及的纠纷或者担保物权存在实体纠纷" _mubu_text="%3Cspan%3E%E6%AD%A4%E6%97%B6%E5%8F%AA%E6%98%AF%E8%AF%B4%E6%98%8E%E5%B0%B1%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E6%B6%89%E5%8F%8A%E7%9A%84%E7%BA%A0%E7%BA%B7%E6%88%96%E8%80%85%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E5%AD%98%E5%9C%A8%E5%AE%9E%E4%BD%93%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="并未明确当事人之间的权利义务关系" _mubu_note="%3Cspan%3E%E5%B9%B6%E6%9C%AA%E6%98%8E%E7%A1%AE%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B9%8B%E9%97%B4%E7%9A%84%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%3C/span%3E"/>
              </outline>
              <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
                <outline text="变更或重新达成调解协议" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%E6%88%96%E9%87%8D%E6%96%B0%E8%BE%BE%E6%88%90%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="向法院起诉" _mubu_text="%3Cspan%3E%E5%90%91%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="实现担保物权案件" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%AE%9E%E7%8E%B0%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="担保物权人以及其他有权请求实现担保物权的人申请" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E4%BA%BA%E4%BB%A5%E5%8F%8A%E5%85%B6%E4%BB%96%E6%9C%89%E6%9D%83%E8%AF%B7%E6%B1%82%E5%AE%9E%E7%8E%B0%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E7%9A%84%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="抵押人、出质人、财产被留置的债务人或者所有权人等" _mubu_note="%3Cspan%3E%E6%8A%B5%E6%8A%BC%E4%BA%BA%E3%80%81%E5%87%BA%E8%B4%A8%E4%BA%BA%E3%80%81%E8%B4%A2%E4%BA%A7%E8%A2%AB%E7%95%99%E7%BD%AE%E7%9A%84%E5%80%BA%E5%8A%A1%E4%BA%BA%E6%88%96%E8%80%85%E6%89%80%E6%9C%89%E6%9D%83%E4%BA%BA%E7%AD%89%3C/span%3E"/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="担保财产所在地基层法院" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E8%B4%A2%E4%BA%A7%E6%89%80%E5%9C%A8%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="担保物权登记地基层法院" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E7%99%BB%E8%AE%B0%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="专门法院管辖" _mubu_text="%3Cspan%3E%E4%B8%93%E9%97%A8%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="属于海事法院管辖等" _mubu_note="%3Cspan%3E%E5%B1%9E%E4%BA%8E%E6%B5%B7%E4%BA%8B%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%E7%AD%89%3C/span%3E"/>
          </outline>
          <outline text="审查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：可独任审查" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%8F%AF%E7%8B%AC%E4%BB%BB%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="担保财产标的额超过基层人民法院管辖范围的" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E8%B4%A2%E4%BA%A7%E6%A0%87%E7%9A%84%E9%A2%9D%E8%B6%85%E8%BF%87%E5%9F%BA%E5%B1%82%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%E8%8C%83%E5%9B%B4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应合议庭审查" _mubu_text="%3Cspan%3E%E5%BA%94%E5%90%88%E8%AE%AE%E5%BA%AD%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="依然由基层法院管辖" _mubu_note="%3Cspan%3E%E4%BE%9D%E7%84%B6%E7%94%B1%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院经形式审查" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E7%BB%8F%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无实质性争议且实现担保物权条件成就的" _mubu_text="%3Cspan%3E%E6%97%A0%E5%AE%9E%E8%B4%A8%E6%80%A7%E4%BA%89%E8%AE%AE%E4%B8%94%E5%AE%9E%E7%8E%B0%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E6%9D%A1%E4%BB%B6%E6%88%90%E5%B0%B1%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="裁定准许拍卖、变卖" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E5%87%86%E8%AE%B8%E6%8B%8D%E5%8D%96%E3%80%81%E5%8F%98%E5%8D%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="当事人申请执行" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%3C/span%3E" _note="有强制力，依申请，不得直接移交执行" _mubu_note="%3Cspan%3E%E6%9C%89%E5%BC%BA%E5%88%B6%E5%8A%9B%EF%BC%8C%E4%BE%9D%E7%94%B3%E8%AF%B7%EF%BC%8C%E4%B8%8D%E5%BE%97%E7%9B%B4%E6%8E%A5%E7%A7%BB%E4%BA%A4%E6%89%A7%E8%A1%8C%3C/span%3E"/>
            </outline>
            <outline text="有部分实质性争议的" _mubu_text="%3Cspan%3E%E6%9C%89%E9%83%A8%E5%88%86%E5%AE%9E%E8%B4%A8%E6%80%A7%E4%BA%89%E8%AE%AE%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="就无争议部分裁定准许拍卖、变卖" _mubu_text="%3Cspan%3E%E5%B0%B1%E6%97%A0%E4%BA%89%E8%AE%AE%E9%83%A8%E5%88%86%E8%A3%81%E5%AE%9A%E5%87%86%E8%AE%B8%E6%8B%8D%E5%8D%96%E3%80%81%E5%8F%98%E5%8D%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="有实质性争议" _mubu_text="%3Cspan%3E%E6%9C%89%E5%AE%9E%E8%B4%A8%E6%80%A7%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="裁定驳回，告知另诉" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%EF%BC%8C%E5%91%8A%E7%9F%A5%E5%8F%A6%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="准许拍卖、变卖" _mubu_text="%3Cspan%3E%E5%87%86%E8%AE%B8%E6%8B%8D%E5%8D%96%E3%80%81%E5%8F%98%E5%8D%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="当事人有异议" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%89%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="在15日内提出" _mubu_text="%3Cspan%3E%E5%9C%A815%E6%97%A5%E5%86%85%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="利害关系人有异议" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E6%9C%89%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
                <outline text="(应)知道权益受损之日起6个月内提出" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E6%9D%83%E7%9B%8A%E5%8F%97%E6%8D%9F%E4%B9%8B%E6%97%A5%E8%B5%B76%E4%B8%AA%E6%9C%88%E5%86%85%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="裁定驳回" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note="">
              <outline text="当事人通过诉讼等方式解决纠纷" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%80%9A%E8%BF%87%E8%AF%89%E8%AE%BC%E7%AD%89%E6%96%B9%E5%BC%8F%E8%A7%A3%E5%86%B3%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="非讼程序" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="督促程序" _mubu_text="%3Cspan%3E%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="不解决实体争议" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A7%A3%E5%86%B3%E5%AE%9E%E4%BD%93%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="实现没有争议的、关系明确的债权债务关系的便捷程序" _mubu_text="%3Cspan%3E%E5%AE%9E%E7%8E%B0%E6%B2%A1%E6%9C%89%E4%BA%89%E8%AE%AE%E7%9A%84%E3%80%81%E5%85%B3%E7%B3%BB%E6%98%8E%E7%A1%AE%E7%9A%84%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E5%85%B3%E7%B3%BB%E7%9A%84%E4%BE%BF%E6%8D%B7%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="由债权人申请，法院向债务人发出支付令" _mubu_text="%3Cspan%3E%E7%94%B1%E5%80%BA%E6%9D%83%E4%BA%BA%E7%94%B3%E8%AF%B7%EF%BC%8C%E6%B3%95%E9%99%A2%E5%90%91%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="申请支付令的条件" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="请求给付的标的为金钱或者有价证券" _mubu_text="%3Cspan%3E%E8%AF%B7%E6%B1%82%E7%BB%99%E4%BB%98%E7%9A%84%E6%A0%87%E7%9A%84%E4%B8%BA%E9%87%91%E9%92%B1%E6%88%96%E8%80%85%E6%9C%89%E4%BB%B7%E8%AF%81%E5%88%B8%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="已到期且数额确定" _mubu_text="%3Cspan%3E%E5%B7%B2%E5%88%B0%E6%9C%9F%E4%B8%94%E6%95%B0%E9%A2%9D%E7%A1%AE%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="债权人未给付" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E6%9C%AA%E7%BB%99%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="支付令必须能够送达给债务人" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E4%BB%A4%E5%BF%85%E9%A1%BB%E8%83%BD%E5%A4%9F%E9%80%81%E8%BE%BE%E7%BB%99%E5%80%BA%E5%8A%A1%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="必须能够通过直接送达、留置送达等" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E8%83%BD%E5%A4%9F%E9%80%9A%E8%BF%87%E7%9B%B4%E6%8E%A5%E9%80%81%E8%BE%BE%E3%80%81%E7%95%99%E7%BD%AE%E9%80%81%E8%BE%BE%E7%AD%89%3C/span%3E" _note="不能公告送达" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E5%85%AC%E5%91%8A%E9%80%81%E8%BE%BE%3C/span%3E"/>
          </outline>
          <outline text="债权人未申请诉前保全" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E6%9C%AA%E7%94%B3%E8%AF%B7%E8%AF%89%E5%89%8D%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="已经申请的，应在保全措施之日起30日内或诉或裁" _mubu_text="%3Cspan%3E%E5%B7%B2%E7%BB%8F%E7%94%B3%E8%AF%B7%E7%9A%84%EF%BC%8C%E5%BA%94%E5%9C%A8%E4%BF%9D%E5%85%A8%E6%8E%AA%E6%96%BD%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%E6%88%96%E8%AF%89%E6%88%96%E8%A3%81%3C/span%3E" _note="不能适用督促程序" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E9%80%82%E7%94%A8%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="与担保" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
          <outline text="支付令与担保人无关" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E4%BB%A4%E4%B8%8E%E6%8B%85%E4%BF%9D%E4%BA%BA%E6%97%A0%E5%85%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="对债务人发出的支付令对担保人没有约束效力" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%8F%91%E5%87%BA%E7%9A%84%E6%94%AF%E4%BB%98%E4%BB%A4%E5%AF%B9%E6%8B%85%E4%BF%9D%E4%BA%BA%E6%B2%A1%E6%9C%89%E7%BA%A6%E6%9D%9F%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="债权人不能申请向担保人发出支付令" _mubu_text="%3Cspan%3E%E5%80%BA%E6%9D%83%E4%BA%BA%E4%B8%8D%E8%83%BD%E7%94%B3%E8%AF%B7%E5%90%91%E6%8B%85%E4%BF%9D%E4%BA%BA%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
          <outline text="债务人住所地基层法院" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E4%BD%8F%E6%89%80%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="无论标的额大小等" _mubu_note="%3Cspan%3E%E6%97%A0%E8%AE%BA%E6%A0%87%E7%9A%84%E9%A2%9D%E5%A4%A7%E5%B0%8F%E7%AD%89%3C/span%3E"/>
        </outline>
        <outline text="审查" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
          <outline text="审查符合的，受理之日起15日内向债务人发出支付令" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%9F%A5%E7%AC%A6%E5%90%88%E7%9A%84%EF%BC%8C%E5%8F%97%E7%90%86%E4%B9%8B%E6%97%A5%E8%B5%B715%E6%97%A5%E5%86%85%E5%90%91%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="应裁定终结督促程序" _mubu_text="%3Cspan%3E%E5%BA%94%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="已发出支付令的，支付令失效" _mubu_note="%3Cspan%3E%E5%B7%B2%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%EF%BC%8C%E6%94%AF%E4%BB%98%E4%BB%A4%E5%A4%B1%E6%95%88%3C/span%3E">
            <outline text="受理支付令申请后，债权人就同一债权债务关系又提起诉讼的" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E6%94%AF%E4%BB%98%E4%BB%A4%E7%94%B3%E8%AF%B7%E5%90%8E%EF%BC%8C%E5%80%BA%E6%9D%83%E4%BA%BA%E5%B0%B1%E5%90%8C%E4%B8%80%E5%80%BA%E6%9D%83%E5%80%BA%E5%8A%A1%E5%85%B3%E7%B3%BB%E5%8F%88%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="发出支付令之日起30日内无法送达债务人的" _mubu_text="%3Cspan%3E%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%E4%B9%8B%E6%97%A5%E8%B5%B730%E6%97%A5%E5%86%85%E6%97%A0%E6%B3%95%E9%80%81%E8%BE%BE%E5%80%BA%E5%8A%A1%E4%BA%BA%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="债务人收到支付令前，债权人撤回申请的" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E6%94%B6%E5%88%B0%E6%94%AF%E4%BB%98%E4%BB%A4%E5%89%8D%EF%BC%8C%E5%80%BA%E6%9D%83%E4%BA%BA%E6%92%A4%E5%9B%9E%E7%94%B3%E8%AF%B7%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="支付令效力" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E4%BB%A4%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
          <outline text="自作出之日起发生法律效力" _mubu_text="%3Cspan%3E%E8%87%AA%E4%BD%9C%E5%87%BA%E4%B9%8B%E6%97%A5%E8%B5%B7%E5%8F%91%E7%94%9F%E6%B3%95%E5%BE%8B%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="债务人收到后" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E6%94%B6%E5%88%B0%E5%90%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="应在15日内清偿债务" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A815%E6%97%A5%E5%86%85%E6%B8%85%E5%81%BF%E5%80%BA%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="或向法院提出书面异议" _mubu_text="%3Cspan%3E%E6%88%96%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E4%B9%A6%E9%9D%A2%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="既不提出异议，又不履行义务，债权人可以向法院申请执行" _mubu_text="%3Cspan%3E%E6%97%A2%E4%B8%8D%E6%8F%90%E5%87%BA%E5%BC%82%E8%AE%AE%EF%BC%8C%E5%8F%88%E4%B8%8D%E5%B1%A5%E8%A1%8C%E4%B9%89%E5%8A%A1%EF%BC%8C%E5%80%BA%E6%9D%83%E4%BA%BA%E5%8F%AF%E4%BB%A5%E5%90%91%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="异议" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="异议成立" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E6%88%90%E7%AB%8B%3C/span%3E" _note="有纠纷" _mubu_note="%3Cspan%3E%E6%9C%89%E7%BA%A0%E7%BA%B7%3C/span%3E">
            <outline text="形式审查" _mubu_text="%3Cspan%3E%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="裁定终结督促程序，支付令自行失效" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%EF%BC%8C%E6%94%AF%E4%BB%98%E4%BB%A4%E8%87%AA%E8%A1%8C%E5%A4%B1%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="转入诉讼程序" _mubu_text="%3Cspan%3E%E8%BD%AC%E5%85%A5%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="申请支付令的一方当事人不同意起诉" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%E4%B8%80%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8D%E5%90%8C%E6%84%8F%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="应自收到终结裁定起7日内向法院提出" _mubu_text="%3Cspan%3E%E5%BA%94%E8%87%AA%E6%94%B6%E5%88%B0%E7%BB%88%E7%BB%93%E8%A3%81%E5%AE%9A%E8%B5%B77%E6%97%A5%E5%86%85%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="否则视为同意转入诉讼程序" _mubu_text="%3Cspan%3E%E5%90%A6%E5%88%99%E8%A7%86%E4%B8%BA%E5%90%8C%E6%84%8F%E8%BD%AC%E5%85%A5%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="申请支付令的时间视为起诉时间" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%E6%97%B6%E9%97%B4%E8%A7%86%E4%B8%BA%E8%B5%B7%E8%AF%89%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="关于异议" _mubu_text="%3Cspan%3E%E5%85%B3%E4%BA%8E%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="债务人对债务本身没有异议，不影响支付令的效力" _mubu_text="%3Cspan%3E%E5%80%BA%E5%8A%A1%E4%BA%BA%E5%AF%B9%E5%80%BA%E5%8A%A1%E6%9C%AC%E8%BA%AB%E6%B2%A1%E6%9C%89%E5%BC%82%E8%AE%AE%EF%BC%8C%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="只是提出缺乏清偿能力、延缓债务清偿期限、变更债务清偿方式等" _mubu_note="%3Cspan%3E%E5%8F%AA%E6%98%AF%E6%8F%90%E5%87%BA%E7%BC%BA%E4%B9%8F%E6%B8%85%E5%81%BF%E8%83%BD%E5%8A%9B%E3%80%81%E5%BB%B6%E7%BC%93%E5%80%BA%E5%8A%A1%E6%B8%85%E5%81%BF%E6%9C%9F%E9%99%90%E3%80%81%E5%8F%98%E6%9B%B4%E5%80%BA%E5%8A%A1%E6%B8%85%E5%81%BF%E6%96%B9%E5%BC%8F%E7%AD%89%3C/span%3E"/>
            <outline text="对支付令的异议须书面" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%94%AF%E4%BB%98%E4%BB%A4%E7%9A%84%E5%BC%82%E8%AE%AE%E9%A1%BB%E4%B9%A6%E9%9D%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="收到支付令后，向其他法院起诉的，不影响支付令效力" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E6%94%AF%E4%BB%98%E4%BB%A4%E5%90%8E%EF%BC%8C%E5%90%91%E5%85%B6%E4%BB%96%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%94%AF%E4%BB%98%E4%BB%A4%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="向发出支付令法院起诉，则会影响" _mubu_text="%3Cspan%3E%E5%90%91%E5%8F%91%E5%87%BA%E6%94%AF%E4%BB%98%E4%BB%A4%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%EF%BC%8C%E5%88%99%E4%BC%9A%E5%BD%B1%E5%93%8D%3C/span%3E" _note="视为书面异议" _mubu_note="%3Cspan%3E%E8%A7%86%E4%B8%BA%E4%B9%A6%E9%9D%A2%E5%BC%82%E8%AE%AE%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="公示催告程序" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="适用条件" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="按规定可以背书转让的票据被盗、遗失或者灭失" _mubu_text="%3Cspan%3E%E6%8C%89%E8%A7%84%E5%AE%9A%E5%8F%AF%E4%BB%A5%E8%83%8C%E4%B9%A6%E8%BD%AC%E8%AE%A9%E7%9A%84%E7%A5%A8%E6%8D%AE%E8%A2%AB%E7%9B%97%E3%80%81%E9%81%97%E5%A4%B1%E6%88%96%E8%80%85%E7%81%AD%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="依法可以申请公示催告的其他事项" _mubu_text="%3Cspan%3E%E4%BE%9D%E6%B3%95%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%9A%84%E5%85%B6%E4%BB%96%E4%BA%8B%E9%A1%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
            <outline text="票据最后持有人申请" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E6%9C%80%E5%90%8E%E6%8C%81%E6%9C%89%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
            <outline text="票据支付地基层法院" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E6%94%AF%E4%BB%98%E5%9C%B0%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="止付通知" _mubu_text="%3Cspan%3E%E6%AD%A2%E4%BB%98%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
          <outline text="决定受理，应同时通知支付人停止支付" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%E5%8F%97%E7%90%86%EF%BC%8C%E5%BA%94%E5%90%8C%E6%97%B6%E9%80%9A%E7%9F%A5%E6%94%AF%E4%BB%98%E4%BA%BA%E5%81%9C%E6%AD%A2%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="3日内公告" _mubu_text="%3Cspan%3E3%E6%97%A5%E5%86%85%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="催促利害关系人申报权利" _mubu_text="%3Cspan%3E%E5%82%AC%E4%BF%83%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%94%B3%E6%8A%A5%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="≥60日" _mubu_text="%3Cspan%3E%E2%89%A560%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="效力" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="支付人收到通知后应停止支付" _mubu_text="%3Cspan%3E%E6%94%AF%E4%BB%98%E4%BA%BA%E6%94%B6%E5%88%B0%E9%80%9A%E7%9F%A5%E5%90%8E%E5%BA%94%E5%81%9C%E6%AD%A2%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="公示催告期间转让票据行为无效" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E6%9C%9F%E9%97%B4%E8%BD%AC%E8%AE%A9%E7%A5%A8%E6%8D%AE%E8%A1%8C%E4%B8%BA%E6%97%A0%E6%95%88%3C/span%3E" _note="包括申请人自己" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E7%94%B3%E8%AF%B7%E4%BA%BA%E8%87%AA%E5%B7%B1%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="法律后果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
          <outline text="利害关系人的申报权利" _mubu_text="%3Cspan%3E%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%9A%84%E7%94%B3%E6%8A%A5%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="公示催告期间申报权利" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E6%9C%9F%E9%97%B4%E7%94%B3%E6%8A%A5%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="司法解释将该时间延展到除权判决作出前" _mubu_text="%3Cspan%3E%E5%8F%B8%E6%B3%95%E8%A7%A3%E9%87%8A%E5%B0%86%E8%AF%A5%E6%97%B6%E9%97%B4%E5%BB%B6%E5%B1%95%E5%88%B0%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E4%BD%9C%E5%87%BA%E5%89%8D%3C/span%3E" _note="即期间届满后暂未作出除权判决的，申报依然有效" _mubu_note="%3Cspan%3E%E5%8D%B3%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E5%90%8E%E6%9A%82%E6%9C%AA%E4%BD%9C%E5%87%BA%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E7%9A%84%EF%BC%8C%E7%94%B3%E6%8A%A5%E4%BE%9D%E7%84%B6%E6%9C%89%E6%95%88%3C/span%3E"/>
            </outline>
            <outline text="应通知权利人向法院出示票据" _mubu_text="%3Cspan%3E%E5%BA%94%E9%80%9A%E7%9F%A5%E6%9D%83%E5%88%A9%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E5%87%BA%E7%A4%BA%E7%A5%A8%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="并通知申请公示催告的人查看票据" _mubu_text="%3Cspan%3E%E5%B9%B6%E9%80%9A%E7%9F%A5%E7%94%B3%E8%AF%B7%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%9A%84%E4%BA%BA%E6%9F%A5%E7%9C%8B%E7%A5%A8%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="只进行形式审查" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%BF%9B%E8%A1%8C%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="票据一致" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="申报成立" _mubu_text="%3Cspan%3E%E7%94%B3%E6%8A%A5%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="裁定终结公示催告程序" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="申请人和申报人通过起诉方式解决纠纷" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%92%8C%E7%94%B3%E6%8A%A5%E4%BA%BA%E9%80%9A%E8%BF%87%E8%B5%B7%E8%AF%89%E6%96%B9%E5%BC%8F%E8%A7%A3%E5%86%B3%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="票据不一致" _mubu_text="%3Cspan%3E%E7%A5%A8%E6%8D%AE%E4%B8%8D%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="申报不成立" _mubu_text="%3Cspan%3E%E7%94%B3%E6%8A%A5%E4%B8%8D%E6%88%90%E7%AB%8B%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="法院裁定驳回申报" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%E7%94%B3%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="除权判决" _mubu_text="%3Cspan%3E%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="公示催告期间无人申报" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E6%9C%9F%E9%97%B4%E6%97%A0%E4%BA%BA%E7%94%B3%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="或申报被驳回" _mubu_text="%3Cspan%3E%E6%88%96%E7%94%B3%E6%8A%A5%E8%A2%AB%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="根据申请人申请，判决宣告票据无效" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E7%94%B3%E8%AF%B7%E4%BA%BA%E7%94%B3%E8%AF%B7%EF%BC%8C%E5%88%A4%E5%86%B3%E5%AE%A3%E5%91%8A%E7%A5%A8%E6%8D%AE%E6%97%A0%E6%95%88%3C/span%3E" _note="判决只能根据申请人的申请，不能依职权作出" _mubu_note="%3Cspan%3E%E5%88%A4%E5%86%B3%E5%8F%AA%E8%83%BD%E6%A0%B9%E6%8D%AE%E7%94%B3%E8%AF%B7%E4%BA%BA%E7%9A%84%E7%94%B3%E8%AF%B7%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%BE%9D%E8%81%8C%E6%9D%83%E4%BD%9C%E5%87%BA%3C/span%3E"/>
            <outline text="判决应公告" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E5%BA%94%E5%85%AC%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="自公告之日起，申请人有权向支付人请求支付" _mubu_text="%3Cspan%3E%E8%87%AA%E5%85%AC%E5%91%8A%E4%B9%8B%E6%97%A5%E8%B5%B7%EF%BC%8C%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%9C%89%E6%9D%83%E5%90%91%E6%94%AF%E4%BB%98%E4%BA%BA%E8%AF%B7%E6%B1%82%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="拒不支付的，以支付人为被告提起诉讼" _mubu_text="%3Cspan%3E%E6%8B%92%E4%B8%8D%E6%94%AF%E4%BB%98%E7%9A%84%EF%BC%8C%E4%BB%A5%E6%94%AF%E4%BB%98%E4%BA%BA%E4%B8%BA%E8%A2%AB%E5%91%8A%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="除权判决不具有强制执行效力，不能依其直接申请执行" _mubu_note="%3Cspan%3E%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E4%B8%8D%E5%85%B7%E6%9C%89%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E6%95%88%E5%8A%9B%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%BE%9D%E5%85%B6%E7%9B%B4%E6%8E%A5%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
        <outline text="审理组织" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note="">
          <outline text="公示催告阶段" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E9%98%B6%E6%AE%B5%3C/span%3E" _note="" _mubu_note="">
            <outline text="独任" _mubu_text="%3Cspan%3E%E7%8B%AC%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="除权判决阶段" _mubu_text="%3Cspan%3E%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E9%98%B6%E6%AE%B5%3C/span%3E" _note="" _mubu_note="">
            <outline text="合议" _mubu_text="%3Cspan%3E%E5%90%88%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="对利害关系人的救济" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E7%9A%84%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="因正当理由不能在判决前向法院申报的" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E4%B8%8D%E8%83%BD%E5%9C%A8%E5%88%A4%E5%86%B3%E5%89%8D%E5%90%91%E6%B3%95%E9%99%A2%E7%94%B3%E6%8A%A5%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="(应)知道判决公告之日起1年内" _mubu_text="%3Cspan%3E(%E5%BA%94)%E7%9F%A5%E9%81%93%E5%88%A4%E5%86%B3%E5%85%AC%E5%91%8A%E4%B9%8B%E6%97%A5%E8%B5%B71%E5%B9%B4%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="向作出除权判决的法院起诉" _mubu_text="%3Cspan%3E%E5%90%91%E4%BD%9C%E5%87%BA%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E7%9A%84%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="纠纷诉讼解决" _mubu_note="%3Cspan%3E%E7%BA%A0%E7%BA%B7%E8%AF%89%E8%AE%BC%E8%A7%A3%E5%86%B3%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="非讼程序规律总结" _mubu_text="%3Cspan%3E%E9%9D%9E%E8%AE%BC%E7%A8%8B%E5%BA%8F%E8%A7%84%E5%BE%8B%E6%80%BB%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
        <outline text="特别程序(选民资格除外)、督促程序、公示催告程序" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F(%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E9%99%A4%E5%A4%96)%E3%80%81%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="共同特点" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E7%89%B9%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
          <outline text="不解决纠纷" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%A7%A3%E5%86%B3%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="只要在表述中出现&quot;经调解&quot;&quot;组织双方当事人进行辩论&quot;&quot;对纠纷进行实质审查&quot;&quot;要求 XX 提供证据证明其对XX享有实体权利&quot;等实体解决纠纷的表述均为错误" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%A6%81%E5%9C%A8%E8%A1%A8%E8%BF%B0%E4%B8%AD%E5%87%BA%E7%8E%B0%22%E7%BB%8F%E8%B0%83%E8%A7%A3%22%22%E7%BB%84%E7%BB%87%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%BF%9B%E8%A1%8C%E8%BE%A9%E8%AE%BA%22%22%E5%AF%B9%E7%BA%A0%E7%BA%B7%E8%BF%9B%E8%A1%8C%E5%AE%9E%E8%B4%A8%E5%AE%A1%E6%9F%A5%22%22%E8%A6%81%E6%B1%82%20XX%20%E6%8F%90%E4%BE%9B%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E5%85%B6%E5%AF%B9XX%E4%BA%AB%E6%9C%89%E5%AE%9E%E4%BD%93%E6%9D%83%E5%88%A9%22%E7%AD%89%E5%AE%9E%E4%BD%93%E8%A7%A3%E5%86%B3%E7%BA%A0%E7%BA%B7%E7%9A%84%E8%A1%A8%E8%BF%B0%E5%9D%87%E4%B8%BA%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="程序设计上" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E8%AE%BE%E8%AE%A1%E4%B8%8A%3C/span%3E" _note="" _mubu_note="">
            <outline text="先由申请人向法院提出申请" _mubu_text="%3Cspan%3E%E5%85%88%E7%94%B1%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法院经过形式审查" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E7%BB%8F%E8%BF%87%E5%BD%A2%E5%BC%8F%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
              <outline text="不存在纠纷" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AD%98%E5%9C%A8%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="符合法定条件，则可强制执行" _mubu_text="%3Cspan%3E%E7%AC%A6%E5%90%88%E6%B3%95%E5%AE%9A%E6%9D%A1%E4%BB%B6%EF%BC%8C%E5%88%99%E5%8F%AF%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="公示催告为作出除权判决" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E4%B8%BA%E4%BD%9C%E5%87%BA%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="存在纠纷" _mubu_text="%3Cspan%3E%E5%AD%98%E5%9C%A8%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note="">
                <outline text="直接终结程序，当事人诉讼解决纠纷" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E7%BB%88%E7%BB%93%E7%A8%8B%E5%BA%8F%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%AF%89%E8%AE%BC%E8%A7%A3%E5%86%B3%E7%BA%A0%E7%BA%B7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="执行程序" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
      <outline text="执行开始" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%BC%80%E5%A7%8B%3C/span%3E" _note="" _mubu_note="">
        <outline text="根据" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="以生效法律文书为根据原则" _mubu_text="%3Cspan%3E%E4%BB%A5%E7%94%9F%E6%95%88%E6%B3%95%E5%BE%8B%E6%96%87%E4%B9%A6%E4%B8%BA%E6%A0%B9%E6%8D%AE%E5%8E%9F%E5%88%99%3C/span%3E" _note="民事判决、裁定、调解书与支付令，刑事裁判中的财产部分" _mubu_note="%3Cspan%3E%E6%B0%91%E4%BA%8B%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E3%80%81%E8%B0%83%E8%A7%A3%E4%B9%A6%E4%B8%8E%E6%94%AF%E4%BB%98%E4%BB%A4%EF%BC%8C%E5%88%91%E4%BA%8B%E8%A3%81%E5%88%A4%E4%B8%AD%E7%9A%84%E8%B4%A2%E4%BA%A7%E9%83%A8%E5%88%86%3C/span%3E"/>
          <outline text="其他机关制作的由法院执行的法律文书" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E6%9C%BA%E5%85%B3%E5%88%B6%E4%BD%9C%E7%9A%84%E7%94%B1%E6%B3%95%E9%99%A2%E6%89%A7%E8%A1%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E6%96%87%E4%B9%A6%3C/span%3E" _note="公证机关能强执的债权文书，仲裁机构能强执的仲裁裁决书" _mubu_note="%3Cspan%3E%E5%85%AC%E8%AF%81%E6%9C%BA%E5%85%B3%E8%83%BD%E5%BC%BA%E6%89%A7%E7%9A%84%E5%80%BA%E6%9D%83%E6%96%87%E4%B9%A6%EF%BC%8C%E4%BB%B2%E8%A3%81%E6%9C%BA%E6%9E%84%E8%83%BD%E5%BC%BA%E6%89%A7%E7%9A%84%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%E4%B9%A6%3C/span%3E"/>
          <outline text="法院承认并执行外国的判决、裁定" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%89%BF%E8%AE%A4%E5%B9%B6%E6%89%A7%E8%A1%8C%E5%A4%96%E5%9B%BD%E7%9A%84%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="审执分离" _mubu_text="%3Cspan%3E%E5%AE%A1%E6%89%A7%E5%88%86%E7%A6%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="审判不是执行的前提" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%B8%8D%E6%98%AF%E6%89%A7%E8%A1%8C%E7%9A%84%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="执行不是审判的继续" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%8D%E6%98%AF%E5%AE%A1%E5%88%A4%E7%9A%84%E7%BB%A7%E7%BB%AD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="启动" _mubu_text="%3Cspan%3E%E5%90%AF%E5%8A%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="移送执行" _mubu_text="%3Cspan%3E%E7%A7%BB%E9%80%81%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="执行一般应由当事人依法提出申请" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%80%E8%88%AC%E5%BA%94%E7%94%B1%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BE%9D%E6%B3%95%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="可直接由审判庭移送执行机构执行" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%9B%B4%E6%8E%A5%E7%94%B1%E5%AE%A1%E5%88%A4%E5%BA%AD%E7%A7%BB%E9%80%81%E6%89%A7%E8%A1%8C%E6%9C%BA%E6%9E%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="具有给付赡养费、扶养费、抚养费内容的法律文书" _mubu_text="%3Cspan%3E%E5%85%B7%E6%9C%89%E7%BB%99%E4%BB%98%E8%B5%A1%E5%85%BB%E8%B4%B9%E3%80%81%E6%89%B6%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E5%85%BB%E8%B4%B9%E5%86%85%E5%AE%B9%E7%9A%84%E6%B3%95%E5%BE%8B%E6%96%87%E4%B9%A6%3C/span%3E" _note="权利人为弱势群体" _mubu_note="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E4%B8%BA%E5%BC%B1%E5%8A%BF%E7%BE%A4%E4%BD%93%3C/span%3E"/>
              <outline text="民事制裁决定书" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E5%88%B6%E8%A3%81%E5%86%B3%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="无申请人" _mubu_note="%3Cspan%3E%E6%97%A0%E7%94%B3%E8%AF%B7%E4%BA%BA%3C/span%3E"/>
              <outline text="刑事附带民事判决、裁定、调解书；" _mubu_text="%3Cspan%3E%E5%88%91%E4%BA%8B%E9%99%84%E5%B8%A6%E6%B0%91%E4%BA%8B%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%E3%80%81%E8%B0%83%E8%A7%A3%E4%B9%A6%EF%BC%9B%3C/span%3E" _note="权利人为弱势群体" _mubu_note="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E4%B8%BA%E5%BC%B1%E5%8A%BF%E7%BE%A4%E4%BD%93%3C/span%3E"/>
              <outline text="环保公益诉讼以及检察机关提起的公益诉讼的生效判决、裁定" _mubu_text="%3Cspan%3E%E7%8E%AF%E4%BF%9D%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E4%BB%A5%E5%8F%8A%E6%A3%80%E5%AF%9F%E6%9C%BA%E5%85%B3%E6%8F%90%E8%B5%B7%E7%9A%84%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E7%9A%84%E7%94%9F%E6%95%88%E5%88%A4%E5%86%B3%E3%80%81%E8%A3%81%E5%AE%9A%3C/span%3E" _note="公共利益" _mubu_note="%3Cspan%3E%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%3C/span%3E"/>
            </outline>
          </outline>
          <outline text="申请执行" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="条件" _mubu_text="%3Cspan%3E%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
              <outline text="权利义务主体明确" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E4%B8%BB%E4%BD%93%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="给内容明确" _mubu_text="%3Cspan%3E%E7%BB%99%E5%86%85%E5%AE%B9%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法院应在7日内立案或裁定不予受理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E5%9C%A87%E6%97%A5%E5%86%85%E7%AB%8B%E6%A1%88%E6%88%96%E8%A3%81%E5%AE%9A%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="执行时效" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E6%97%B6%E6%95%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="2年" _mubu_text="%3Cspan%3E2%E5%B9%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="适用中止、中断" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E4%B8%AD%E6%AD%A2%E3%80%81%E4%B8%AD%E6%96%AD%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="超时效申请，法院应受理" _mubu_text="%3Cspan%3E%E8%B6%85%E6%97%B6%E6%95%88%E7%94%B3%E8%AF%B7%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="被执行人有时效抗辩权" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E6%9C%89%E6%97%B6%E6%95%88%E6%8A%97%E8%BE%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="其履行全部或部分后以不知时效届满为由请求执行回转，不予支持" _mubu_text="%3Cspan%3E%E5%85%B6%E5%B1%A5%E8%A1%8C%E5%85%A8%E9%83%A8%E6%88%96%E9%83%A8%E5%88%86%E5%90%8E%E4%BB%A5%E4%B8%8D%E7%9F%A5%E6%97%B6%E6%95%88%E5%B1%8A%E6%BB%A1%E4%B8%BA%E7%94%B1%E8%AF%B7%E6%B1%82%E6%89%A7%E8%A1%8C%E5%9B%9E%E8%BD%AC%EF%BC%8C%E4%B8%8D%E4%BA%88%E6%94%AF%E6%8C%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
          <outline text="诉讼管辖or执行管辖(图)" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E7%AE%A1%E8%BE%96or%E6%89%A7%E8%A1%8C%E7%AE%A1%E8%BE%96(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22cvPlrA2EAR%22%2C%22oh%22%3A1252%2C%22ow%22%3A3196%2C%22uri%22%3A%22document_image%2F40273493-f53a-4bb5-b0b7-848ff04ebca9-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="委托执行" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="被执行人或者被执行财产在外地" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E6%88%96%E8%80%85%E8%A2%AB%E6%89%A7%E8%A1%8C%E8%B4%A2%E4%BA%A7%E5%9C%A8%E5%A4%96%E5%9C%B0%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="委托当地人民法院代为执行" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E5%BD%93%E5%9C%B0%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E4%BB%A3%E4%B8%BA%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="收到委托函件后，必须在15日内开始执行" _mubu_text="%3Cspan%3E%E6%94%B6%E5%88%B0%E5%A7%94%E6%89%98%E5%87%BD%E4%BB%B6%E5%90%8E%EF%BC%8C%E5%BF%85%E9%A1%BB%E5%9C%A815%E6%97%A5%E5%86%85%E5%BC%80%E5%A7%8B%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="不执行的，委托法院可请求受托法院上级指令其执行" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%89%A7%E8%A1%8C%E7%9A%84%EF%BC%8C%E5%A7%94%E6%89%98%E6%B3%95%E9%99%A2%E5%8F%AF%E8%AF%B7%E6%B1%82%E5%8F%97%E6%89%98%E6%B3%95%E9%99%A2%E4%B8%8A%E7%BA%A7%E6%8C%87%E4%BB%A4%E5%85%B6%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="应将执行结果及时函复委托法院" _mubu_text="%3Cspan%3E%E5%BA%94%E5%B0%86%E6%89%A7%E8%A1%8C%E7%BB%93%E6%9E%9C%E5%8F%8A%E6%97%B6%E5%87%BD%E5%A4%8D%E5%A7%94%E6%89%98%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="不得拒绝" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%8B%92%E7%BB%9D%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="通知" _mubu_text="%3Cspan%3E%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note="">
          <outline text="接到申请执行书或者移交执行书后10日内" _mubu_text="%3Cspan%3E%E6%8E%A5%E5%88%B0%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%B9%A6%E6%88%96%E8%80%85%E7%A7%BB%E4%BA%A4%E6%89%A7%E8%A1%8C%E4%B9%A6%E5%90%8E10%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="向被执行人发出执行通知" _mubu_text="%3Cspan%3E%E5%90%91%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%8F%91%E5%87%BA%E6%89%A7%E8%A1%8C%E9%80%9A%E7%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="并可立即采取强制措施" _mubu_text="%3Cspan%3E%E5%B9%B6%E5%8F%AF%E7%AB%8B%E5%8D%B3%E9%87%87%E5%8F%96%E5%BC%BA%E5%88%B6%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="执行阻却" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E9%98%BB%E5%8D%B4%3C/span%3E" _note="" _mubu_note="">
        <outline text="执行担保" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%89%A7%E8%A1%8C%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用条件" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
            <outline text="执行中，被执行人或他人向法院提供担保" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%AD%EF%BC%8C%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E6%88%96%E4%BB%96%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="经申请执行人同意" _mubu_text="%3Cspan%3E%E7%BB%8F%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="应出具书面同意意见" _mubu_text="%3Cspan%3E%E5%BA%94%E5%87%BA%E5%85%B7%E4%B9%A6%E9%9D%A2%E5%90%8C%E6%84%8F%E6%84%8F%E8%A7%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="也可由执行人员将其同意内容记笔录，并由其签名或者盖章" _mubu_text="%3Cspan%3E%E4%B9%9F%E5%8F%AF%E7%94%B1%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%91%98%E5%B0%86%E5%85%B6%E5%90%8C%E6%84%8F%E5%86%85%E5%AE%B9%E8%AE%B0%E7%AC%94%E5%BD%95%EF%BC%8C%E5%B9%B6%E7%94%B1%E5%85%B6%E7%AD%BE%E5%90%8D%E6%88%96%E8%80%85%E7%9B%96%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="法院可决定暂缓执行以及其期限" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E5%86%B3%E5%AE%9A%E6%9A%82%E7%BC%93%E6%89%A7%E8%A1%8C%E4%BB%A5%E5%8F%8A%E5%85%B6%E6%9C%9F%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="恢复执行" _mubu_text="%3Cspan%3E%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="依申请执行人申请" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
              <outline text="期限届满，义务人仍不履行义务" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%EF%BC%8C%E4%B9%89%E5%8A%A1%E4%BA%BA%E4%BB%8D%E4%B8%8D%E5%B1%A5%E8%A1%8C%E4%B9%89%E5%8A%A1%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="暂缓执行期间担保人转移、隐藏、变卖、毁损担保财产" _mubu_text="%3Cspan%3E%E6%9A%82%E7%BC%93%E6%89%A7%E8%A1%8C%E6%9C%9F%E9%97%B4%E6%8B%85%E4%BF%9D%E4%BA%BA%E8%BD%AC%E7%A7%BB%E3%80%81%E9%9A%90%E8%97%8F%E3%80%81%E5%8F%98%E5%8D%96%E3%80%81%E6%AF%81%E6%8D%9F%E6%8B%85%E4%BF%9D%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="直接执行担保(人)财产" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E6%89%A7%E8%A1%8C%E6%8B%85%E4%BF%9D(%E4%BA%BA)%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
              <outline text="无需将担保人变更、追加为被执行人" _mubu_text="%3Cspan%3E%E6%97%A0%E9%9C%80%E5%B0%86%E6%8B%85%E4%BF%9D%E4%BA%BA%E5%8F%98%E6%9B%B4%E3%80%81%E8%BF%BD%E5%8A%A0%E4%B8%BA%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="也无需考虑担保人的先诉抗辩权" _mubu_text="%3Cspan%3E%E4%B9%9F%E6%97%A0%E9%9C%80%E8%80%83%E8%99%91%E6%8B%85%E4%BF%9D%E4%BA%BA%E7%9A%84%E5%85%88%E8%AF%89%E6%8A%97%E8%BE%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="担保人的权利保障" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E4%BA%BA%E7%9A%84%E6%9D%83%E5%88%A9%E4%BF%9D%E9%9A%9C%3C/span%3E" _note="" _mubu_note="">
              <outline text="执行范围限制" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E8%8C%83%E5%9B%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="以担保人应当履行义务部分的财产为限" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%8B%85%E4%BF%9D%E4%BA%BA%E5%BA%94%E5%BD%93%E5%B1%A5%E8%A1%8C%E4%B9%89%E5%8A%A1%E9%83%A8%E5%88%86%E7%9A%84%E8%B4%A2%E4%BA%A7%E4%B8%BA%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="担保期间限制" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E6%9C%9F%E9%97%B4%E9%99%90%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
                <outline text="担保期间届满后，申请执行人不得再申请执行担保(人)财产" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E5%90%8E%EF%BC%8C%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%86%8D%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E6%8B%85%E4%BF%9D(%E4%BA%BA)%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="担保期间(图)" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E6%9C%9F%E9%97%B4(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
                  <outline text="自暂缓执行期间届满之日起算" _mubu_text="%3Cspan%3E%E8%87%AA%E6%9A%82%E7%BC%93%E6%89%A7%E8%A1%8C%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E4%B9%8B%E6%97%A5%E8%B5%B7%E7%AE%97%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22QYNeWBH8ma%22%2C%22oh%22%3A217%2C%22ow%22%3A719%2C%22uri%22%3A%22document_image%2F53f869b3-6096-4b34-aa5c-196d50c8ef00-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
                </outline>
              </outline>
              <outline text="追偿权" _mubu_text="%3Cspan%3E%E8%BF%BD%E5%81%BF%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="执行和解" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%89%A7%E8%A1%8C%E5%92%8C%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
          <outline text="执行中不允许调解" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%AD%E4%B8%8D%E5%85%81%E8%AE%B8%E8%B0%83%E8%A7%A3%3C/span%3E" _note="亦不允许根据和解协议制作调解书" _mubu_note="%3Cspan%3E%E4%BA%A6%E4%B8%8D%E5%85%81%E8%AE%B8%E6%A0%B9%E6%8D%AE%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%88%B6%E4%BD%9C%E8%B0%83%E8%A7%A3%E4%B9%A6%3C/span%3E"/>
          <outline text="形式" _mubu_text="%3Cspan%3E%E5%BD%A2%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="当事人共同向法院提交书面和解协议的" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%85%B1%E5%90%8C%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E4%BA%A4%E4%B9%A6%E9%9D%A2%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一方提交书面和解协议，其他当事人予以认可的" _mubu_text="%3Cspan%3E%E4%B8%80%E6%96%B9%E6%8F%90%E4%BA%A4%E4%B9%A6%E9%9D%A2%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%EF%BC%8C%E5%85%B6%E4%BB%96%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BA%88%E4%BB%A5%E8%AE%A4%E5%8F%AF%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="达成口头和解协议，执行人员将其记入笔录，当事人签名、盖章的" _mubu_text="%3Cspan%3E%E8%BE%BE%E6%88%90%E5%8F%A3%E5%A4%B4%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%EF%BC%8C%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%91%98%E5%B0%86%E5%85%B6%E8%AE%B0%E5%85%A5%E7%AC%94%E5%BD%95%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%AD%BE%E5%90%8D%E3%80%81%E7%9B%96%E7%AB%A0%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="效果" _mubu_text="%3Cspan%3E%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁定中止执行" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%E4%B8%AD%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="申请人可申请解除查、扣、冻" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%8F%AF%E7%94%B3%E8%AF%B7%E8%A7%A3%E9%99%A4%E6%9F%A5%E3%80%81%E6%89%A3%E3%80%81%E5%86%BB%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法律后果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%90%8E%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
            <outline text="和解协议履行完毕" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%B1%A5%E8%A1%8C%E5%AE%8C%E6%AF%95%3C/span%3E" _note="" _mubu_note="">
              <outline text="作执行结案处理，即裁定执行终结" _mubu_text="%3Cspan%3E%E4%BD%9C%E6%89%A7%E8%A1%8C%E7%BB%93%E6%A1%88%E5%A4%84%E7%90%86%EF%BC%8C%E5%8D%B3%E8%A3%81%E5%AE%9A%E6%89%A7%E8%A1%8C%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="因被执行人迟延、瑕疵履行遭受损害的，申请人可向执行法院另诉" _mubu_text="%3Cspan%3E%E5%9B%A0%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E8%BF%9F%E5%BB%B6%E3%80%81%E7%91%95%E7%96%B5%E5%B1%A5%E8%A1%8C%E9%81%AD%E5%8F%97%E6%8D%9F%E5%AE%B3%E7%9A%84%EF%BC%8C%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%8F%AF%E5%90%91%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%E5%8F%A6%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="拒不履行和解协议" _mubu_text="%3Cspan%3E%E6%8B%92%E4%B8%8D%E5%B1%A5%E8%A1%8C%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
              <outline text="申请人可以选择申请恢复执行" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%8F%AF%E4%BB%A5%E9%80%89%E6%8B%A9%E7%94%B3%E8%AF%B7%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="救济：对是否恢复执行的裁定不服，可以向上级法院申请复议一次" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%EF%BC%9A%E5%AF%B9%E6%98%AF%E5%90%A6%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%E7%9A%84%E8%A3%81%E5%AE%9A%E4%B8%8D%E6%9C%8D%EF%BC%8C%E5%8F%AF%E4%BB%A5%E5%90%91%E4%B8%8A%E7%BA%A7%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="执行行为异议" _mubu_note="%3Cspan%3E%E6%89%A7%E8%A1%8C%E8%A1%8C%E4%B8%BA%E5%BC%82%E8%AE%AE%3C/span%3E"/>
                <outline text="申请执行后不得再起诉" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E5%90%8E%E4%B8%8D%E5%BE%97%E5%86%8D%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="或就和解协议起诉" _mubu_text="%3Cspan%3E%E6%88%96%E5%B0%B1%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="只有执行和解协议可就和解协议起诉，其他和解协议不行" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%9C%89%E6%89%A7%E8%A1%8C%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%8F%AF%E5%B0%B1%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E8%B5%B7%E8%AF%89%EF%BC%8C%E5%85%B6%E4%BB%96%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E4%B8%8D%E8%A1%8C%3C/span%3E" _note="赋予执行和解协议可诉性" _mubu_note="%3Cspan%3E%E8%B5%8B%E4%BA%88%E6%89%A7%E8%A1%8C%E5%92%8C%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%8F%AF%E8%AF%89%E6%80%A7%3C/span%3E"/>
                <outline text="由执行法院管辖" _mubu_text="%3Cspan%3E%E7%94%B1%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="给付之诉" _mubu_note="%3Cspan%3E%E7%BB%99%E4%BB%98%E4%B9%8B%E8%AF%89%3C/span%3E"/>
                <outline text="处理" _mubu_text="%3Cspan%3E%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                  <outline text="受理后，裁定终结执行" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%EF%BC%8C%E8%A3%81%E5%AE%9A%E7%BB%88%E7%BB%93%E6%89%A7%E8%A1%8C%3C/span%3E" _note="不得恢复执行" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%BE%97%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%3C/span%3E"/>
                  <outline text="执行中的查扣冻自动转为诉讼中的查扣冻" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%AD%E7%9A%84%E6%9F%A5%E6%89%A3%E5%86%BB%E8%87%AA%E5%8A%A8%E8%BD%AC%E4%B8%BA%E8%AF%89%E8%AE%BC%E4%B8%AD%E7%9A%84%E6%9F%A5%E6%89%A3%E5%86%BB%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="当事人、利害关系人认为该协议无效或应予撤销" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E3%80%81%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E8%AE%A4%E4%B8%BA%E8%AF%A5%E5%8D%8F%E8%AE%AE%E6%97%A0%E6%95%88%E6%88%96%E5%BA%94%E4%BA%88%E6%92%A4%E9%94%80%3C/span%3E" _note="确认、形成之诉" _mubu_note="%3Cspan%3E%E7%A1%AE%E8%AE%A4%E3%80%81%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%3C/span%3E">
                  <outline text="可向执行法院提起诉讼" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="胜诉后，申请执行人可以据此申请恢复执行" _mubu_text="%3Cspan%3E%E8%83%9C%E8%AF%89%E5%90%8E%EF%BC%8C%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%8F%AF%E4%BB%A5%E6%8D%AE%E6%AD%A4%E7%94%B3%E8%AF%B7%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="被执行人以此事由提起诉讼的" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E4%BB%A5%E6%AD%A4%E4%BA%8B%E7%94%B1%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
                    <outline text="不影响申请执行人申请恢复执行" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E7%94%B3%E8%AF%B7%E6%81%A2%E5%A4%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="执行中止与终结" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E4%B8%AD%E6%AD%A2%E4%B8%8E%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="追索赡养费、扶养费、抚养费的权利人死亡，终结执行" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E8%B5%A1%E5%85%BB%E8%B4%B9%E3%80%81%E6%89%B6%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E5%85%BB%E8%B4%B9%E7%9A%84%E6%9D%83%E5%88%A9%E4%BA%BA%E6%AD%BB%E4%BA%A1%EF%BC%8C%E7%BB%88%E7%BB%93%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
            <outline text="义务人死亡，不一定会导致执行终结" _mubu_text="%3Cspan%3E%E4%B9%89%E5%8A%A1%E4%BA%BA%E6%AD%BB%E4%BA%A1%EF%BC%8C%E4%B8%8D%E4%B8%80%E5%AE%9A%E4%BC%9A%E5%AF%BC%E8%87%B4%E6%89%A7%E8%A1%8C%E7%BB%88%E7%BB%93%3C/span%3E" _note="因为权利人尚需继续赡养、扶养、抚养" _mubu_note="%3Cspan%3E%E5%9B%A0%E4%B8%BA%E6%9D%83%E5%88%A9%E4%BA%BA%E5%B0%9A%E9%9C%80%E7%BB%A7%E7%BB%AD%E8%B5%A1%E5%85%BB%E3%80%81%E6%89%B6%E5%85%BB%E3%80%81%E6%8A%9A%E5%85%BB%3C/span%3E"/>
          </outline>
          <outline text="当事人死亡产生的法律效果(图)" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%AD%BB%E4%BA%A1%E4%BA%A7%E7%94%9F%E7%9A%84%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22w0F0papPrh%22%2C%22oh%22%3A822%2C%22ow%22%3A1109%2C%22uri%22%3A%22document_image%2Fe6b78531-76c2-4acf-b079-7a0ed77501d5-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="执行回转" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%9B%9E%E8%BD%AC%3C/span%3E" _note="" _mubu_note="">
          <outline text="执行完毕后" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%AE%8C%E6%AF%95%E5%90%8E%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="据以执行的法律文书确有错误被法院撤销的" _mubu_text="%3Cspan%3E%E6%8D%AE%E4%BB%A5%E6%89%A7%E8%A1%8C%E7%9A%84%E6%B3%95%E5%BE%8B%E6%96%87%E4%B9%A6%E7%A1%AE%E6%9C%89%E9%94%99%E8%AF%AF%E8%A2%AB%E6%B3%95%E9%99%A2%E6%92%A4%E9%94%80%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="已经执行的财产，应裁定责令取得财产人返还" _mubu_text="%3Cspan%3E%E5%B7%B2%E7%BB%8F%E6%89%A7%E8%A1%8C%E7%9A%84%E8%B4%A2%E4%BA%A7%EF%BC%8C%E5%BA%94%E8%A3%81%E5%AE%9A%E8%B4%A3%E4%BB%A4%E5%8F%96%E5%BE%97%E8%B4%A2%E4%BA%A7%E4%BA%BA%E8%BF%94%E8%BF%98%3C/span%3E" _note="拒不返还的，强制执行" _mubu_note="%3Cspan%3E%E6%8B%92%E4%B8%8D%E8%BF%94%E8%BF%98%E7%9A%84%EF%BC%8C%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="执行救济" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
        <outline text="对执行行为的异议" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E5%AF%B9%E6%89%A7%E8%A1%8C%E8%A1%8C%E4%B8%BA%E7%9A%84%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="主体" _mubu_text="%3Cspan%3E%E4%B8%BB%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
            <outline text="当事人和利害关系人" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%92%8C%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="理由" _mubu_text="%3Cspan%3E%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="认为执行行为在程序上违反法律规定" _mubu_text="%3Cspan%3E%E8%AE%A4%E4%B8%BA%E6%89%A7%E8%A1%8C%E8%A1%8C%E4%B8%BA%E5%9C%A8%E7%A8%8B%E5%BA%8F%E4%B8%8A%E8%BF%9D%E5%8F%8D%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="向负责执行的法院提出书面异议" _mubu_text="%3Cspan%3E%E5%90%91%E8%B4%9F%E8%B4%A3%E6%89%A7%E8%A1%8C%E7%9A%84%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E4%B9%A6%E9%9D%A2%E5%BC%82%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="法院处理" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="15日内审查" _mubu_text="%3Cspan%3E15%E6%97%A5%E5%86%85%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="异议成立，裁定撤销或改正；不成立，驳回" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E6%88%90%E7%AB%8B%EF%BC%8C%E8%A3%81%E5%AE%9A%E6%92%A4%E9%94%80%E6%88%96%E6%94%B9%E6%AD%A3%EF%BC%9B%E4%B8%8D%E6%88%90%E7%AB%8B%EF%BC%8C%E9%A9%B3%E5%9B%9E%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
            <outline text="不服裁定，可自送达之日起10日内向上一级法院申请复议" _mubu_text="%3Cspan%3E%E4%B8%8D%E6%9C%8D%E8%A3%81%E5%AE%9A%EF%BC%8C%E5%8F%AF%E8%87%AA%E9%80%81%E8%BE%BE%E4%B9%8B%E6%97%A5%E8%B5%B710%E6%97%A5%E5%86%85%E5%90%91%3C/span%3E%3Cspan%20class=%22bold%22%3E%E4%B8%8A%E4%B8%80%E7%BA%A7%3C/span%3E%3Cspan%3E%E6%B3%95%E9%99%A2%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="异议审查和复议期间，不停止执行" _mubu_text="%3Cspan%3E%E5%BC%82%E8%AE%AE%E5%AE%A1%E6%9F%A5%E5%92%8C%E5%A4%8D%E8%AE%AE%E6%9C%9F%E9%97%B4%EF%BC%8C%E4%B8%8D%E5%81%9C%E6%AD%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="被执行人、利害关系人提供充分有效担保请求停止，可准许" _mubu_text="%3Cspan%3E%E8%A2%AB%E6%89%A7%E8%A1%8C%E4%BA%BA%E3%80%81%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E4%BA%BA%E6%8F%90%E4%BE%9B%E5%85%85%E5%88%86%E6%9C%89%E6%95%88%E6%8B%85%E4%BF%9D%E8%AF%B7%E6%B1%82%E5%81%9C%E6%AD%A2%EF%BC%8C%E5%8F%AF%E5%87%86%E8%AE%B8%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="申请执行人提供充分有效担保，要求继续执行的，应继续执行" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%E4%BA%BA%E6%8F%90%E4%BE%9B%E5%85%85%E5%88%86%E6%9C%89%E6%95%88%E6%8B%85%E4%BF%9D%EF%BC%8C%E8%A6%81%E6%B1%82%E7%BB%A7%E7%BB%AD%E6%89%A7%E8%A1%8C%E7%9A%84%EF%BC%8C%E5%BA%94%E7%BB%A7%E7%BB%AD%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
