          </outline>
        </outline>
        <outline text="二审" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="原则：合议庭" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="独任" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E7%8B%AC%E4%BB%BB%3C/span%3E" _note="" _mubu_note="">
            <outline text="中院" _mubu_text="%3Cspan%3E%E4%B8%AD%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一审适用简易程序或者不服一审裁定提起上诉" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E9%80%82%E7%94%A8%3C/span%3E%3Cspan%20class=%22bold%22%3E%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%3C/span%3E%3Cspan%3E%E6%88%96%E8%80%85%E4%B8%8D%E6%9C%8D%E4%B8%80%E5%AE%A1%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%A3%81%E5%AE%9A%3C/span%3E%3Cspan%3E%E6%8F%90%E8%B5%B7%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="事实清楚、权利义务关系明确" _mubu_text="%3Cspan%3E%E4%BA%8B%E5%AE%9E%E6%B8%85%E6%A5%9A%E3%80%81%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="双方当事人同意" _mubu_text="%3Cspan%3E%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="特别程序" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="原则：独任" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E7%8B%AC%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="合议庭" _mubu_text="%3Cspan%3E%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note="">
            <outline text="选民资格案件" _mubu_text="%3Cspan%3E%E9%80%89%E6%B0%91%E8%B5%84%E6%A0%BC%E6%A1%88%E4%BB%B6%3C/span%3E" _note="且只能由审判员装成" _mubu_note="%3Cspan%3E%E4%B8%94%E5%8F%AA%E8%83%BD%E7%94%B1%E5%AE%A1%E5%88%A4%E5%91%98%E8%A3%85%E6%88%90%3C/span%3E"/>
            <outline text="重大疑难案件" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7%E7%96%91%E9%9A%BE%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="督促程序" _mubu_text="%3Cspan%3E%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="独任审查" _mubu_text="%3Cspan%3E%E7%8B%AC%E4%BB%BB%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="公示催告程序" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="公示催告阶段：独任" _mubu_text="%3Cspan%3E%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E9%98%B6%E6%AE%B5%EF%BC%9A%E7%8B%AC%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="除权判决阶段：合议庭" _mubu_text="%3Cspan%3E%E9%99%A4%E6%9D%83%E5%88%A4%E5%86%B3%E9%98%B6%E6%AE%B5%EF%BC%9A%E5%90%88%E8%AE%AE%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="当事人对独任制有异议权" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%AF%B9%E7%8B%AC%E4%BB%BB%E5%88%B6%E6%9C%89%E5%BC%82%E8%AE%AE%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="回避制度" _mubu_text="%3Cspan%3E%E5%9B%9E%E9%81%BF%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
        <outline text="适用的对象" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E7%9A%84%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="审判人员(包括陪审员)、书记员" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98(%E5%8C%85%E6%8B%AC%E9%99%AA%E5%AE%A1%E5%91%98)%E3%80%81%E4%B9%A6%E8%AE%B0%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="勘验、执行人员、翻译、鉴定" _mubu_text="%3Cspan%3E%E5%8B%98%E9%AA%8C%E3%80%81%E6%89%A7%E8%A1%8C%E4%BA%BA%E5%91%98%E3%80%81%E7%BF%BB%E8%AF%91%E3%80%81%E9%89%B4%E5%AE%9A%3C/span%3E" _note="“看(勘)着(执)犯(翻)贱(鉴)”" _mubu_note="%3Cspan%3E%E2%80%9C%E7%9C%8B(%E5%8B%98)%E7%9D%80(%E6%89%A7)%E7%8A%AF(%E7%BF%BB)%E8%B4%B1(%E9%89%B4)%E2%80%9D%3C/span%3E"/>
          <outline text="证人不适用回避" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E4%B8%8D%E9%80%82%E7%94%A8%E5%9B%9E%E9%81%BF%3C/span%3E" _note="具有不可替代性" _mubu_note="%3Cspan%3E%E5%85%B7%E6%9C%89%E4%B8%8D%E5%8F%AF%E6%9B%BF%E4%BB%A3%E6%80%A7%3C/span%3E"/>
        </outline>
        <outline text="法定原因" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E5%8E%9F%E5%9B%A0%3C/span%3E" _note="" _mubu_note="">
          <outline text="与案件有利害关系" _mubu_text="%3Cspan%3E%E4%B8%8E%E6%A1%88%E4%BB%B6%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="有不当行为" _mubu_text="%3Cspan%3E%E6%9C%89%E4%B8%8D%E5%BD%93%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="为本案当事人推荐代理人、律师" _mubu_text="%3Cspan%3E%E4%B8%BA%E6%9C%AC%E6%A1%88%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8E%A8%E8%8D%90%E4%BB%A3%E7%90%86%E4%BA%BA%E3%80%81%E5%BE%8B%E5%B8%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="违反规定会见当事人" _mubu_text="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E8%A7%84%E5%AE%9A%E4%BC%9A%E8%A7%81%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="非工作时间、地点" _mubu_note="%3Cspan%3E%E9%9D%9E%E5%B7%A5%E4%BD%9C%E6%97%B6%E9%97%B4%E3%80%81%E5%9C%B0%E7%82%B9%3C/span%3E"/>
            <outline text="其他" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="曾参与前序程序" _mubu_text="%3Cspan%3E%E6%9B%BE%E5%8F%82%E4%B8%8E%E5%89%8D%E5%BA%8F%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="发回重审后，又进入二审的，二审不受其限制" _mubu_text="%3Cspan%3E%E5%8F%91%E5%9B%9E%E9%87%8D%E5%AE%A1%E5%90%8E%EF%BC%8C%E5%8F%88%E8%BF%9B%E5%85%A5%E4%BA%8C%E5%AE%A1%E7%9A%84%EF%BC%8C%E4%BA%8C%E5%AE%A1%E4%B8%8D%E5%8F%97%E5%85%B6%E9%99%90%E5%88%B6%3C/span%3E" _note="无实质审理" _mubu_note="%3Cspan%3E%E6%97%A0%E5%AE%9E%E8%B4%A8%E5%AE%A1%E7%90%86%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="回避方式" _mubu_text="%3Cspan%3E%E5%9B%9E%E9%81%BF%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="自行" _mubu_text="%3Cspan%3E%E8%87%AA%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="方式" _mubu_text="%3Cspan%3E%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="口头、书面" _mubu_text="%3Cspan%3E%E5%8F%A3%E5%A4%B4%E3%80%81%E4%B9%A6%E9%9D%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="时间" _mubu_text="%3Cspan%3E%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
              <outline text="开始审理时" _mubu_text="%3Cspan%3E%E5%BC%80%E5%A7%8B%E5%AE%A1%E7%90%86%E6%97%B6%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法庭辩论终结前" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BA%AD%E8%BE%A9%E8%AE%BA%E7%BB%88%E7%BB%93%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="效力" _mubu_text="%3Cspan%3E%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="申请后，决定前，应停止本案工作" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E5%90%8E%EF%BC%8C%E5%86%B3%E5%AE%9A%E5%89%8D%EF%BC%8C%E5%BA%94%E5%81%9C%E6%AD%A2%E6%9C%AC%E6%A1%88%E5%B7%A5%E4%BD%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="紧急情况除外" _mubu_text="%3Cspan%3E%E7%B4%A7%E6%80%A5%E6%83%85%E5%86%B5%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
                  <outline text="紧急保全" _mubu_text="%3Cspan%3E%E7%B4%A7%E6%80%A5%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="制止扰乱法庭秩序的行为" _mubu_text="%3Cspan%3E%E5%88%B6%E6%AD%A2%E6%89%B0%E4%B9%B1%E6%B3%95%E5%BA%AD%E7%A7%A9%E5%BA%8F%E7%9A%84%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="民诉中更换审判人员后程序继续进行，无需重来" _mubu_text="%3Cspan%3E%E6%B0%91%E8%AF%89%E4%B8%AD%E6%9B%B4%E6%8D%A2%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E5%90%8E%E7%A8%8B%E5%BA%8F%E7%BB%A7%E7%BB%AD%E8%BF%9B%E8%A1%8C%EF%BC%8C%E6%97%A0%E9%9C%80%E9%87%8D%E6%9D%A5%3C/span%3E" _note="与刑诉不同" _mubu_note="%3Cspan%3E%E4%B8%8E%E5%88%91%E8%AF%89%E4%B8%8D%E5%90%8C%3C/span%3E"/>
              </outline>
            </outline>
          </outline>
          <outline text="指令" _mubu_text="%3Cspan%3E%E6%8C%87%E4%BB%A4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="回避决定" _mubu_text="%3Cspan%3E%E5%9B%9E%E9%81%BF%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="审判人员的回避由院长决定" _mubu_text="%3Cspan%3E%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E7%9A%84%E5%9B%9E%E9%81%BF%E7%94%B1%E9%99%A2%E9%95%BF%E5%86%B3%E5%AE%9A%3C/span%3E" _note="包括陪审员、书记员" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E9%99%AA%E5%AE%A1%E5%91%98%E3%80%81%E4%B9%A6%E8%AE%B0%E5%91%98%3C/span%3E"/>
          <outline text="其他人员的回避由审判长或者独任审判员决定" _mubu_text="%3Cspan%3E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%91%98%E7%9A%84%E5%9B%9E%E9%81%BF%E7%94%B1%E5%AE%A1%E5%88%A4%E9%95%BF%E6%88%96%E8%80%85%E7%8B%AC%E4%BB%BB%E5%AE%A1%E5%88%A4%E5%91%98%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="院长担任审判长或者独任审判员时的回避由审判委员会决定" _mubu_text="%3Cspan%3E%E9%99%A2%E9%95%BF%E6%8B%85%E4%BB%BB%E5%AE%A1%E5%88%A4%E9%95%BF%E6%88%96%E8%80%85%E7%8B%AC%E4%BB%BB%E5%AE%A1%E5%88%A4%E5%91%98%E6%97%B6%E7%9A%84%E5%9B%9E%E9%81%BF%E7%94%B1%E5%AE%A1%E5%88%A4%E5%A7%94%E5%91%98%E4%BC%9A%E5%86%B3%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="申请人对驳回不服可复议一次" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%AF%B9%E9%A9%B3%E5%9B%9E%E4%B8%8D%E6%9C%8D%E5%8F%AF%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note="">
            <outline text="但若出现新的事实理由可再次提出申请" _mubu_text="%3Cspan%3E%E4%BD%86%E8%8B%A5%E5%87%BA%E7%8E%B0%E6%96%B0%E7%9A%84%E4%BA%8B%E5%AE%9E%E7%90%86%E7%94%B1%E5%8F%AF%E5%86%8D%E6%AC%A1%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被申请回避人员复议期间不停止工作" _mubu_text="%3Cspan%3E%E8%A2%AB%E7%94%B3%E8%AF%B7%E5%9B%9E%E9%81%BF%E4%BA%BA%E5%91%98%E5%A4%8D%E8%AE%AE%E6%9C%9F%E9%97%B4%E4%B8%8D%E5%81%9C%E6%AD%A2%E5%B7%A5%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="被决定回避的人无权申请复议" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%86%B3%E5%AE%9A%E5%9B%9E%E9%81%BF%E7%9A%84%E4%BA%BA%E6%97%A0%E6%9D%83%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="公开审判制度" _mubu_text="%3Cspan%3E%E5%85%AC%E5%BC%80%E5%AE%A1%E5%88%A4%E5%88%B6%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
        <outline text="法定不公开" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E4%B8%8D%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
          <outline text="涉及国家秘密" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E5%AE%B6%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="涉及个人隐私" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E4%B8%AA%E4%BA%BA%E9%9A%90%E7%A7%81%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="法律规定的其他" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%E7%9A%84%E5%85%B6%E4%BB%96%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="未成年案件民诉公开，刑诉不公开" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%88%90%E5%B9%B4%E6%A1%88%E4%BB%B6%E6%B0%91%E8%AF%89%E5%85%AC%E5%BC%80%EF%BC%8C%E5%88%91%E8%AF%89%E4%B8%8D%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="申请不公开" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%B8%8D%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note="">
          <outline text="离婚诉讼" _mubu_text="%3Cspan%3E%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="涉及商业秘密" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="只需要一方当事人申请即可" _mubu_text="%3Cspan%3E%E5%8F%AA%E9%9C%80%E8%A6%81%E4%B8%80%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%B3%E8%AF%B7%E5%8D%B3%E5%8F%AF%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="不论案件是否公开审理，宣判一律公开" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%AE%BA%E6%A1%88%E4%BB%B6%E6%98%AF%E5%90%A6%E5%85%AC%E5%BC%80%E5%AE%A1%E7%90%86%EF%BC%8C%E5%AE%A3%E5%88%A4%E4%B8%80%E5%BE%8B%E5%85%AC%E5%BC%80%3C/span%3E" _note="合议庭评议一律不得公开" _mubu_note="%3Cspan%3E%E5%90%88%E8%AE%AE%E5%BA%AD%E8%AF%84%E8%AE%AE%E4%B8%80%E5%BE%8B%E4%B8%8D%E5%BE%97%E5%85%AC%E5%BC%80%3C/span%3E"/>
      </outline>
      <outline text="两审终审制" _mubu_text="%3Cspan%3E%E4%B8%A4%E5%AE%A1%E7%BB%88%E5%AE%A1%E5%88%B6%3C/span%3E" _note="" _mubu_note="">
        <outline text="一审终审的例外情形" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%E7%9A%84%E4%BE%8B%E5%A4%96%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="最高法审理的案件，一审终审" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%B3%95%E5%AE%A1%E7%90%86%E7%9A%84%E6%A1%88%E4%BB%B6%EF%BC%8C%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="调解书一审终审，不能上诉" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B9%A6%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="一般的裁定书一审终审，不能上诉" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%9A%84%E8%A3%81%E5%AE%9A%E4%B9%A6%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%EF%BC%8C%E4%B8%8D%E8%83%BD%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="例外" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="不予受理裁定" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="驳回起诉裁定" _mubu_text="%3Cspan%3E%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="管辖权异议裁定" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="特别程序、督促程序、公示催告程序一审终审" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E7%A8%8B%E5%BA%8F%E3%80%81%E7%9D%A3%E4%BF%83%E7%A8%8B%E5%BA%8F%E3%80%81%E5%85%AC%E7%A4%BA%E5%82%AC%E5%91%8A%E7%A8%8B%E5%BA%8F%E4%B8%80%E5%AE%A1%E7%BB%88%E5%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="小额诉讼的程序" _mubu_text="%3Cspan%3E%E5%B0%8F%E9%A2%9D%E8%AF%89%E8%AE%BC%E7%9A%84%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
    </outline>
    <outline text="主管与管辖" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%AE%A1%E4%B8%8E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
      <outline text="主管" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%AE%A1%3C/span%3E" _note="" _mubu_note="">
        <outline text="法院主管与人民调解关系" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%BB%E7%AE%A1%E4%B8%8E%E4%BA%BA%E6%B0%91%E8%B0%83%E8%A7%A3%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="人民调解是社会救济" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%B0%91%E8%B0%83%E8%A7%A3%E6%98%AF%E7%A4%BE%E4%BC%9A%E6%95%91%E6%B5%8E%3C/span%3E" _note="与法院调解有本质区别" _mubu_note="%3Cspan%3E%E4%B8%8E%E6%B3%95%E9%99%A2%E8%B0%83%E8%A7%A3%E6%9C%89%E6%9C%AC%E8%B4%A8%E5%8C%BA%E5%88%AB%3C/span%3E"/>
          <outline text="调解不是必经程序" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E4%B8%8D%E6%98%AF%E5%BF%85%E7%BB%8F%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="调解协议具有法律约束力" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E5%85%B7%E6%9C%89%E6%B3%95%E5%BE%8B%E7%BA%A6%E6%9D%9F%E5%8A%9B%3C/span%3E" _note="法律约束力相当于合同" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E7%BA%A6%E6%9D%9F%E5%8A%9B%E7%9B%B8%E5%BD%93%E4%BA%8E%E5%90%88%E5%90%8C%3C/span%3E">
            <outline text="只能就调解协议起诉" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E5%B0%B1%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E8%B5%B7%E8%AF%89%3C/span%3E" _note="不再是就原纠纷" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%86%8D%E6%98%AF%E5%B0%B1%E5%8E%9F%E7%BA%A0%E7%BA%B7%3C/span%3E"/>
          </outline>
          <outline text="调解协议不具强制力" _mubu_text="%3Cspan%3E%E8%B0%83%E8%A7%A3%E5%8D%8F%E8%AE%AE%E4%B8%8D%E5%85%B7%E5%BC%BA%E5%88%B6%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="经法院确认后有强制执行力" _mubu_text="%3Cspan%3E%E7%BB%8F%E6%B3%95%E9%99%A2%E7%A1%AE%E8%AE%A4%E5%90%8E%E6%9C%89%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="法院主管与仲裁委关系" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%BB%E7%AE%A1%E4%B8%8E%E4%BB%B2%E8%A3%81%E5%A7%94%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="法院主管与劳动争议仲裁委关系" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%BB%E7%AE%A1%E4%B8%8E%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E4%BB%B2%E8%A3%81%E5%A7%94%E5%85%B3%E7%B3%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="劳动仲裁≠仲裁" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E2%89%A0%E4%BB%B2%E8%A3%81%3C/span%3E" _note="前者为行政仲裁" _mubu_note="%3Cspan%3E%E5%89%8D%E8%80%85%E4%B8%BA%E8%A1%8C%E6%94%BF%E4%BB%B2%E8%A3%81%3C/span%3E"/>
          <outline text="劳动争议的解决方式" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E7%9A%84%E8%A7%A3%E5%86%B3%E6%96%B9%E5%BC%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="和解、调解、劳动仲裁、诉讼" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%E3%80%81%E8%B0%83%E8%A7%A3%E3%80%81%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E3%80%81%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="和解与调解并非强制性程序，但劳动仲裁是诉讼的前置程序" _mubu_text="%3Cspan%3E%E5%92%8C%E8%A7%A3%E4%B8%8E%E8%B0%83%E8%A7%A3%E5%B9%B6%E9%9D%9E%E5%BC%BA%E5%88%B6%E6%80%A7%E7%A8%8B%E5%BA%8F%EF%BC%8C%E4%BD%86%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E6%98%AF%E8%AF%89%E8%AE%BC%E7%9A%84%E5%89%8D%E7%BD%AE%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="民事仲裁一裁终局，不得再起诉" _mubu_note="%3Cspan%3E%E6%B0%91%E4%BA%8B%E4%BB%B2%E8%A3%81%E4%B8%80%E8%A3%81%E7%BB%88%E5%B1%80%EF%BC%8C%E4%B8%8D%E5%BE%97%E5%86%8D%E8%B5%B7%E8%AF%89%3C/span%3E"/>
          </outline>
          <outline text="民事仲裁须有仲裁协议约定，劳动仲裁直接法定" _mubu_text="%3Cspan%3E%E6%B0%91%E4%BA%8B%E4%BB%B2%E8%A3%81%E9%A1%BB%E6%9C%89%E4%BB%B2%E8%A3%81%E5%8D%8F%E8%AE%AE%E7%BA%A6%E5%AE%9A%EF%BC%8C%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E7%9B%B4%E6%8E%A5%E6%B3%95%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="劳动仲裁" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳动合同履行地或者用人单位所在地的劳动争议仲裁委员会管辖" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E5%B1%A5%E8%A1%8C%E5%9C%B0%E6%88%96%E8%80%85%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E6%89%80%E5%9C%A8%E5%9C%B0%E7%9A%84%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E4%BB%B2%E8%A3%81%E5%A7%94%E5%91%98%E4%BC%9A%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="分别向两地申请仲裁的，由劳动合同履行地管辖" _mubu_text="%3Cspan%3E%E5%88%86%E5%88%AB%E5%90%91%E4%B8%A4%E5%9C%B0%E7%94%B3%E8%AF%B7%E4%BB%B2%E8%A3%81%E7%9A%84%EF%BC%8C%E7%94%B1%E5%8A%B3%E5%8A%A8%E5%90%88%E5%90%8C%E5%B1%A5%E8%A1%8C%E5%9C%B0%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="当事人" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="劳务派遣单位或者用工单位与劳动者发生劳动争议的，劳务派遣单位和用工单位为共同当事人" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E6%88%96%E8%80%85%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E4%B8%8E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%8F%91%E7%94%9F%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E7%9A%84%EF%BC%8C%E5%8A%B3%E5%8A%A1%E6%B4%BE%E9%81%A3%E5%8D%95%E4%BD%8D%E5%92%8C%E7%94%A8%E5%B7%A5%E5%8D%95%E4%BD%8D%E4%B8%BA%E5%85%B1%E5%90%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="仲裁程序" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
              <outline text="公开仲裁原则" _mubu_text="%3Cspan%3E%E5%85%AC%E5%BC%80%E4%BB%B2%E8%A3%81%E5%8E%9F%E5%88%99%3C/span%3E" _note="涉及国、商秘、个人隐私的除外" _mubu_note="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E3%80%81%E5%95%86%E7%A7%98%E3%80%81%E4%B8%AA%E4%BA%BA%E9%9A%90%E7%A7%81%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E"/>
              <outline text="仲裁庭的组成" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%BA%AD%E7%9A%84%E7%BB%84%E6%88%90%3C/span%3E" _note="" _mubu_note="">
                <outline text="仲裁庭由三名仲裁员组成，设首席仲裁员" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%BA%AD%E7%94%B1%E4%B8%89%E5%90%8D%E4%BB%B2%E8%A3%81%E5%91%98%E7%BB%84%E6%88%90%EF%BC%8C%E8%AE%BE%E9%A6%96%E5%B8%AD%E4%BB%B2%E8%A3%81%E5%91%98%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="简单劳动争议案件可以由一名仲裁员独任仲裁" _mubu_text="%3Cspan%3E%E7%AE%80%E5%8D%95%E5%8A%B3%E5%8A%A8%E4%BA%89%E8%AE%AE%E6%A1%88%E4%BB%B6%E5%8F%AF%E4%BB%A5%E7%94%B1%E4%B8%80%E5%90%8D%E4%BB%B2%E8%A3%81%E5%91%98%E7%8B%AC%E4%BB%BB%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="先行调解" _mubu_text="%3Cspan%3E%E5%85%88%E8%A1%8C%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note="">
                <outline text="仲裁庭在作出裁决前，应当先行调解" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%BA%AD%E5%9C%A8%E4%BD%9C%E5%87%BA%E8%A3%81%E5%86%B3%E5%89%8D%EF%BC%8C%E5%BA%94%E5%BD%93%E5%85%88%E8%A1%8C%E8%B0%83%E8%A7%A3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="先予执行" _mubu_text="%3Cspan%3E%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="追索劳动报酬、工伤医疗费、经济补偿或者赔偿金的案件" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E3%80%81%E5%B7%A5%E4%BC%A4%E5%8C%BB%E7%96%97%E8%B4%B9%E3%80%81%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E6%88%96%E8%80%85%E8%B5%94%E5%81%BF%E9%87%91%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="当事人之间权利义务关系明确" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B9%8B%E9%97%B4%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="不先予执行将严重影响申请人生活的" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%E5%B0%86%E4%B8%A5%E9%87%8D%E5%BD%B1%E5%93%8D%E7%94%B3%E8%AF%B7%E4%BA%BA%E7%94%9F%E6%B4%BB%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="根据当事人的申请" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
                  <outline text="劳动仲裁中的先予执行由仲裁委裁决，移送法院执行" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E4%B8%AD%E7%9A%84%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%E7%94%B1%E4%BB%B2%E8%A3%81%E5%A7%94%E8%A3%81%E5%86%B3%EF%BC%8C%E7%A7%BB%E9%80%81%E6%B3%95%E9%99%A2%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="劳动者申请先予执行可以不提供担保" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E7%94%B3%E8%AF%B7%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%E5%8F%AF%E4%BB%A5%E4%B8%8D%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="裁决" _mubu_text="%3Cspan%3E%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
                <outline text="按照多数仲裁员的意见作出" _mubu_text="%3Cspan%3E%E6%8C%89%E7%85%A7%E5%A4%9A%E6%95%B0%E4%BB%B2%E8%A3%81%E5%91%98%E7%9A%84%E6%84%8F%E8%A7%81%E4%BD%9C%E5%87%BA%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="仲裁庭不能形成多数意见时，应当按照首席仲裁员的意见作出裁决" _mubu_text="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%BA%AD%E4%B8%8D%E8%83%BD%E5%BD%A2%E6%88%90%E5%A4%9A%E6%95%B0%E6%84%8F%E8%A7%81%E6%97%B6%EF%BC%8C%E5%BA%94%E5%BD%93%E6%8C%89%E7%85%A7%E9%A6%96%E5%B8%AD%E4%BB%B2%E8%A3%81%E5%91%98%E7%9A%84%E6%84%8F%E8%A7%81%E4%BD%9C%E5%87%BA%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="裁决的效力" _mubu_text="%3Cspan%3E%E8%A3%81%E5%86%B3%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：当事人对劳动仲裁裁决不服的，可以在收到裁决之日起15日内向法院" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%AF%B9%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%E4%B8%8D%E6%9C%8D%E7%9A%84%EF%BC%8C%E5%8F%AF%E4%BB%A5%E5%9C%A8%E6%94%B6%E5%88%B0%E8%A3%81%E5%86%B3%E4%B9%8B%E6%97%A5%E8%B5%B715%E6%97%A5%E5%86%85%E5%90%91%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="片面一裁终局：对用人单位一裁终局，但劳动者不服的仍然可以起诉" _mubu_text="%3Cspan%3E%E7%89%87%E9%9D%A2%E4%B8%80%E8%A3%81%E7%BB%88%E5%B1%80%EF%BC%9A%E5%AF%B9%E7%94%A8%E4%BA%BA%E5%8D%95%E4%BD%8D%E4%B8%80%E8%A3%81%E7%BB%88%E5%B1%80%EF%BC%8C%E4%BD%86%E5%8A%B3%E5%8A%A8%E8%80%85%E4%B8%8D%E6%9C%8D%E7%9A%84%E4%BB%8D%E7%84%B6%E5%8F%AF%E4%BB%A5%E8%B5%B7%E8%AF%89%3C/span%3E" _note="劳动者对所有的劳动仲裁裁决都可以起诉" _mubu_note="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E8%80%85%E5%AF%B9%E6%89%80%E6%9C%89%E7%9A%84%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E8%A3%81%E5%86%B3%E9%83%BD%E5%8F%AF%E4%BB%A5%E8%B5%B7%E8%AF%89%3C/span%3E">
                  <outline text="追索劳动报酬、工伤医疗费、经济补偿或者赔偿金，不超过当地月最低工资标准十二个月金额的争议" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%E3%80%81%E5%B7%A5%E4%BC%A4%E5%8C%BB%E7%96%97%E8%B4%B9%E3%80%81%E7%BB%8F%E6%B5%8E%E8%A1%A5%E5%81%BF%E6%88%96%E8%80%85%E8%B5%94%E5%81%BF%E9%87%91%EF%BC%8C%E4%B8%8D%E8%B6%85%E8%BF%87%E5%BD%93%E5%9C%B0%E6%9C%88%E6%9C%80%E4%BD%8E%E5%B7%A5%E8%B5%84%E6%A0%87%E5%87%86%E5%8D%81%E4%BA%8C%E4%B8%AA%E6%9C%88%E9%87%91%E9%A2%9D%E7%9A%84%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="因执行国家的劳动标准在工作时间、休息休假、社会保险等方面发生的争议" _mubu_text="%3Cspan%3E%E5%9B%A0%E6%89%A7%E8%A1%8C%E5%9B%BD%E5%AE%B6%E7%9A%84%E5%8A%B3%E5%8A%A8%E6%A0%87%E5%87%86%E5%9C%A8%E5%B7%A5%E4%BD%9C%E6%97%B6%E9%97%B4%E3%80%81%E4%BC%91%E6%81%AF%E4%BC%91%E5%81%87%E3%80%81%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%99%A9%E7%AD%89%E6%96%B9%E9%9D%A2%E5%8F%91%E7%94%9F%E7%9A%84%E4%BA%89%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
              </outline>
              <outline text="执行" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
                <outline text="裁决、调解书具有强制执行效力" _mubu_text="%3Cspan%3E%E8%A3%81%E5%86%B3%E3%80%81%E8%B0%83%E8%A7%A3%E4%B9%A6%E5%85%B7%E6%9C%89%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
      </outline>
      <outline text="管辖权恒定原则" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E6%9D%83%E6%81%92%E5%AE%9A%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="起诉时享有管辖权的法院，不因确定管辖的事实变化而影响其管辖权" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E6%97%B6%E4%BA%AB%E6%9C%89%E7%AE%A1%E8%BE%96%E6%9D%83%E7%9A%84%E6%B3%95%E9%99%A2%EF%BC%8C%E4%B8%8D%E5%9B%A0%E7%A1%AE%E5%AE%9A%E7%AE%A1%E8%BE%96%E7%9A%84%E4%BA%8B%E5%AE%9E%E5%8F%98%E5%8C%96%E8%80%8C%E5%BD%B1%E5%93%8D%E5%85%B6%E7%AE%A1%E8%BE%96%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
          <outline text="受理后，不受当事人住所地、经常居住地变更的影响" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%EF%BC%8C%E4%B8%8D%E5%8F%97%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BD%8F%E6%89%80%E5%9C%B0%E3%80%81%E7%BB%8F%E5%B8%B8%E5%B1%85%E4%BD%8F%E5%9C%B0%E5%8F%98%E6%9B%B4%E7%9A%84%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="受理后，不得以行政区域变更为由将案件移送" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%EF%BC%8C%E4%B8%8D%E5%BE%97%E4%BB%A5%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%9F%9F%E5%8F%98%E6%9B%B4%E4%B8%BA%E7%94%B1%E5%B0%86%E6%A1%88%E4%BB%B6%E7%A7%BB%E9%80%81%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="管辖权异议审查后确定有管辖权的，不因当事人提起反诉、增加或者变更诉讼请求等改变管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E5%AE%A1%E6%9F%A5%E5%90%8E%E7%A1%AE%E5%AE%9A%E6%9C%89%E7%AE%A1%E8%BE%96%E6%9D%83%E7%9A%84%EF%BC%8C%E4%B8%8D%E5%9B%A0%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8F%90%E8%B5%B7%E5%8F%8D%E8%AF%89%E3%80%81%E5%A2%9E%E5%8A%A0%E6%88%96%E8%80%85%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E7%AD%89%E6%94%B9%E5%8F%98%E7%AE%A1%E8%BE%96%3C/span%3E" _note="违反级别管辖、专属管辖规定的除外" _mubu_note="%3Cspan%3E%E8%BF%9D%E5%8F%8D%E7%BA%A7%E5%88%AB%E7%AE%A1%E8%BE%96%E3%80%81%E4%B8%93%E5%B1%9E%E7%AE%A1%E8%BE%96%E8%A7%84%E5%AE%9A%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E">
          <outline text="重审或按一审程序再审的案件，提出管辖异议的法院不予审查" _mubu_text="%3Cspan%3E%E9%87%8D%E5%AE%A1%E6%88%96%E6%8C%89%E4%B8%80%E5%AE%A1%E7%A8%8B%E5%BA%8F%E5%86%8D%E5%AE%A1%E7%9A%84%E6%A1%88%E4%BB%B6%EF%BC%8C%E6%8F%90%E5%87%BA%E7%AE%A1%E8%BE%96%E5%BC%82%E8%AE%AE%E7%9A%84%E6%B3%95%E9%99%A2%E4%B8%8D%E4%BA%88%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="诉讼标的额发生变化对级别管辖影响" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E9%A2%9D%E5%8F%91%E7%94%9F%E5%8F%98%E5%8C%96%E5%AF%B9%E7%BA%A7%E5%88%AB%E7%AE%A1%E8%BE%96%E5%BD%B1%E5%93%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="原告增加、变更诉讼请求" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%A2%9E%E5%8A%A0%E3%80%81%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note="">
            <outline text="应调整级别管辖" _mubu_text="%3Cspan%3E%E5%BA%94%E8%B0%83%E6%95%B4%E7%BA%A7%E5%88%AB%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="被告提出反诉" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
            <outline text="不调整级别管辖" _mubu_text="%3Cspan%3E%E4%B8%8D%E8%B0%83%E6%95%B4%E7%BA%A7%E5%88%AB%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="级别管辖" _mubu_text="%3Cspan%3E%E7%BA%A7%E5%88%AB%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
        <outline text="基层法院" _mubu_text="%3Cspan%3E%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="中级法院" _mubu_text="%3Cspan%3E%E4%B8%AD%E7%BA%A7%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="重大+涉外" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%A7+%E6%B6%89%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="本辖区有重大影响的案件" _mubu_text="%3Cspan%3E%E6%9C%AC%E8%BE%96%E5%8C%BA%E6%9C%89%E9%87%8D%E5%A4%A7%E5%BD%B1%E5%93%8D%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="最高法确定由中院管辖的案件" _mubu_text="%3Cspan%3E%E6%9C%80%E9%AB%98%E6%B3%95%E7%A1%AE%E5%AE%9A%E7%94%B1%E4%B8%AD%E9%99%A2%E7%AE%A1%E8%BE%96%E7%9A%84%E6%A1%88%E4%BB%B6%3C/span%3E" _note="”公海专裁“" _mubu_note="%3Cspan%3E%E2%80%9D%E5%85%AC%E6%B5%B7%E4%B8%93%E8%A3%81%E2%80%9C%3C/span%3E">
            <outline text="海事海商案件" _mubu_text="%3Cspan%3E%E6%B5%B7%E4%BA%8B%E6%B5%B7%E5%95%86%E6%A1%88%E4%BB%B6%3C/span%3E" _note="海事法院的级别跟中院相当" _mubu_note="%3Cspan%3E%E6%B5%B7%E4%BA%8B%E6%B3%95%E9%99%A2%E7%9A%84%E7%BA%A7%E5%88%AB%E8%B7%9F%E4%B8%AD%E9%99%A2%E7%9B%B8%E5%BD%93%3C/span%3E"/>
            <outline text="公益诉讼案件" _mubu_text="%3Cspan%3E%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E6%A1%88%E4%BB%B6%3C/span%3E" _note="另约除外" _mubu_note="%3Cspan%3E%E5%8F%A6%E7%BA%A6%E9%99%A4%E5%A4%96%3C/span%3E"/>
            <outline text="专利纠纷案件" _mubu_text="%3Cspan%3E%E4%B8%93%E5%88%A9%E7%BA%A0%E7%BA%B7%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="与仲裁相关的" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%BB%B2%E8%A3%81%E7%9B%B8%E5%85%B3%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="除了国内仲裁的保全和证据保全" _mubu_text="%3Cspan%3E%E9%99%A4%E4%BA%86%E5%9B%BD%E5%86%85%E4%BB%B2%E8%A3%81%E7%9A%84%E4%BF%9D%E5%85%A8%E5%92%8C%E8%AF%81%E6%8D%AE%E4%BF%9D%E5%85%A8%3C/span%3E" _note="由基层法院管辖" _mubu_note="%3Cspan%3E%E7%94%B1%E5%9F%BA%E5%B1%82%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E"/>
            </outline>
          </outline>
        </outline>
        <outline text="高级法院" _mubu_text="%3Cspan%3E%E9%AB%98%E7%BA%A7%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
