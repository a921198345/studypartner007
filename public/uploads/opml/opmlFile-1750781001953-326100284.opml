          </outline>
        </outline>
        <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
          <outline text="原告诉讼时效中断" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E8%AF%89%E8%AE%BC%E6%97%B6%E6%95%88%E4%B8%AD%E6%96%AD%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="受理" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note="">
        <outline text="一事不再理" _mubu_text="%3Cspan%3E%E4%B8%80%E4%BA%8B%E4%B8%8D%E5%86%8D%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="诉讼中或判决后不得重复起诉" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%E6%88%96%E5%88%A4%E5%86%B3%E5%90%8E%E4%B8%8D%E5%BE%97%E9%87%8D%E5%A4%8D%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="重复起诉情形" _mubu_text="%3Cspan%3E%E9%87%8D%E5%A4%8D%E8%B5%B7%E8%AF%89%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="当事人相同" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="诉讼地位相反也应认定为当事人相同" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E5%9C%B0%E4%BD%8D%E7%9B%B8%E5%8F%8D%E4%B9%9F%E5%BA%94%E8%AE%A4%E5%AE%9A%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="诉讼标的相同" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%A0%87%E7%9A%84%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="诉讼请求相同" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E7%9B%B8%E5%90%8C%3C/span%3E" _note="" _mubu_note="">
              <outline text="后诉诉请为前诉所包含也叫相同" _mubu_text="%3Cspan%3E%E5%90%8E%E8%AF%89%E8%AF%89%E8%AF%B7%E4%B8%BA%E5%89%8D%E8%AF%89%E6%89%80%E5%8C%85%E5%90%AB%E4%B9%9F%E5%8F%AB%E7%9B%B8%E5%90%8C%3C/span%3E" _note="一般给付、形成之诉包含确认内容" _mubu_note="%3Cspan%3E%E4%B8%80%E8%88%AC%E7%BB%99%E4%BB%98%E3%80%81%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%E5%8C%85%E5%90%AB%E7%A1%AE%E8%AE%A4%E5%86%85%E5%AE%B9%3C/span%3E"/>
            </outline>
            <outline text="后诉实质上否定前诉" _mubu_text="%3Cspan%3E%E5%90%8E%E8%AF%89%E5%AE%9E%E8%B4%A8%E4%B8%8A%E5%90%A6%E5%AE%9A%E5%89%8D%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="应受理情形" _mubu_text="%3Cspan%3E%E5%BA%94%E5%8F%97%E7%90%86%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="对于裁定不予受理、驳回起诉的" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BA%8E%E8%A3%81%E5%AE%9A%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%E7%9A%84%3C/span%3E" _note="原告再次起诉的，如果符合起诉条件，法院应予受理" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%E7%9A%84%EF%BC%8C%E5%A6%82%E6%9E%9C%E7%AC%A6%E5%90%88%E8%B5%B7%E8%AF%89%E6%9D%A1%E4%BB%B6%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E"/>
            <outline text="当事人撤诉或法院按撤诉处理后" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%92%A4%E8%AF%89%E6%88%96%E6%B3%95%E9%99%A2%E6%8C%89%E6%92%A4%E8%AF%89%E5%A4%84%E7%90%86%E5%90%8E%3C/span%3E" _note="当事人以同一诉讼请求再次起诉的，法院应予受理" _mubu_note="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BB%A5%E5%90%8C%E4%B8%80%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E4%BA%88%E5%8F%97%E7%90%86%3C/span%3E"/>
            <outline text="赡养费、扶养费、抚养费案件，裁判生效后，因新情况、新理由" _mubu_text="%3Cspan%3E%E8%B5%A1%E5%85%BB%E8%B4%B9%E3%80%81%E6%89%B6%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E5%85%BB%E8%B4%B9%E6%A1%88%E4%BB%B6%EF%BC%8C%E8%A3%81%E5%88%A4%E7%94%9F%E6%95%88%E5%90%8E%EF%BC%8C%E5%9B%A0%E6%96%B0%E6%83%85%E5%86%B5%E3%80%81%E6%96%B0%E7%90%86%E7%94%B1%3C/span%3E" _note="增加或者减少费用" _mubu_note="%3Cspan%3E%E5%A2%9E%E5%8A%A0%E6%88%96%E8%80%85%E5%87%8F%E5%B0%91%E8%B4%B9%E7%94%A8%3C/span%3E"/>
            <outline text="撤诉处理、判决不准离、调解和好的离婚案件" _mubu_text="%3Cspan%3E%E6%92%A4%E8%AF%89%E5%A4%84%E7%90%86%E3%80%81%E5%88%A4%E5%86%B3%E4%B8%8D%E5%87%86%E7%A6%BB%E3%80%81%E8%B0%83%E8%A7%A3%E5%92%8C%E5%A5%BD%E7%9A%84%E7%A6%BB%E5%A9%9A%E6%A1%88%E4%BB%B6%3C/span%3E" _note="离婚没离成" _mubu_note="%3Cspan%3E%E7%A6%BB%E5%A9%9A%E6%B2%A1%E7%A6%BB%E6%88%90%3C/span%3E">
              <outline text="原告" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="没有新情况、新理由，6个月后再次起诉" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%E6%96%B0%E6%83%85%E5%86%B5%E3%80%81%E6%96%B0%E7%90%86%E7%94%B1%EF%BC%8C6%E4%B8%AA%E6%9C%88%E5%90%8E%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="有新情况、新理由，6个月内再次起诉" _mubu_text="%3Cspan%3E%E6%9C%89%E6%96%B0%E6%83%85%E5%86%B5%E3%80%81%E6%96%B0%E7%90%86%E7%94%B1%EF%BC%8C6%E4%B8%AA%E6%9C%88%E5%86%85%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="被告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note="">
                <outline text="都可以起诉" _mubu_text="%3Cspan%3E%E9%83%BD%E5%8F%AF%E4%BB%A5%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
        </outline>
        <outline text="有有效的仲裁协议不得受理" _mubu_text="%3Cspan%3E%E6%9C%89%E6%9C%89%E6%95%88%E7%9A%84%E4%BB%B2%E8%A3%81%E5%8D%8F%E8%AE%AE%E4%B8%8D%E5%BE%97%E5%8F%97%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="法律效果" _mubu_text="%3Cspan%3E%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
          <outline text="法院取得案件审判权" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%96%E5%BE%97%E6%A1%88%E4%BB%B6%E5%AE%A1%E5%88%A4%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="法院对案件形成排他管辖权" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%AF%B9%E6%A1%88%E4%BB%B6%E5%BD%A2%E6%88%90%E6%8E%92%E4%BB%96%E7%AE%A1%E8%BE%96%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="当事人之间产生具体的诉讼法律关系" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B9%8B%E9%97%B4%E4%BA%A7%E7%94%9F%E5%85%B7%E4%BD%93%E7%9A%84%E8%AF%89%E8%AE%BC%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%3C/span%3E" _note="双方权利义务" _mubu_note="%3Cspan%3E%E5%8F%8C%E6%96%B9%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="撤诉和缺席审判" _mubu_text="%3Cspan%3E%E6%92%A4%E8%AF%89%E5%92%8C%E7%BC%BA%E5%B8%AD%E5%AE%A1%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
        <outline text="原告可以申请撤诉，是否准许，由法院决定" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E6%92%A4%E8%AF%89%EF%BC%8C%E6%98%AF%E5%90%A6%E5%87%86%E8%AE%B8%EF%BC%8C%E7%94%B1%E6%B3%95%E9%99%A2%E5%86%B3%E5%AE%9A%3C/span%3E" _note="包括有独三和反诉原告" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E6%9C%89%E7%8B%AC%E4%B8%89%E5%92%8C%E5%8F%8D%E8%AF%89%E5%8E%9F%E5%91%8A%3C/span%3E"/>
        <outline text="按撤诉处理" _mubu_text="%3Cspan%3E%E6%8C%89%E6%92%A4%E8%AF%89%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="原告经传票传唤没有正当理由拒不到庭" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%91%8A%E7%BB%8F%E4%BC%A0%E7%A5%A8%E4%BC%A0%E5%94%A4%E6%B2%A1%E6%9C%89%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E6%8B%92%E4%B8%8D%E5%88%B0%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="或未经许可申途退庭的" _mubu_text="%3Cspan%3E%E6%88%96%E6%9C%AA%E7%BB%8F%E8%AE%B8%E5%8F%AF%E7%94%B3%E9%80%94%E9%80%80%E5%BA%AD%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="该预交受理费而不交的" _mubu_text="%3Cspan%3E%E8%AF%A5%E9%A2%84%E4%BA%A4%E5%8F%97%E7%90%86%E8%B4%B9%E8%80%8C%E4%B8%8D%E4%BA%A4%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="撤诉/按撤诉处理可再次起诉" _mubu_text="%3Cspan%3E%E6%92%A4%E8%AF%89/%E6%8C%89%E6%92%A4%E8%AF%89%E5%A4%84%E7%90%86%E5%8F%AF%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
          <outline text="只是在程序上终结诉讼程序，案件并未经过实体处理" _mubu_text="%3Cspan%3E%E5%8F%AA%E6%98%AF%E5%9C%A8%E7%A8%8B%E5%BA%8F%E4%B8%8A%E7%BB%88%E7%BB%93%E8%AF%89%E8%AE%BC%E7%A8%8B%E5%BA%8F%EF%BC%8C%E6%A1%88%E4%BB%B6%E5%B9%B6%E6%9C%AA%E7%BB%8F%E8%BF%87%E5%AE%9E%E4%BD%93%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="但是离婚诉讼会受到一定限制" _mubu_text="%3Cspan%3E%E4%BD%86%E6%98%AF%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%E4%BC%9A%E5%8F%97%E5%88%B0%E4%B8%80%E5%AE%9A%E9%99%90%E5%88%B6%3C/span%3E" _note="6个月内无新情况新理由" _mubu_note="%3Cspan%3E6%E4%B8%AA%E6%9C%88%E5%86%85%E6%97%A0%E6%96%B0%E6%83%85%E5%86%B5%E6%96%B0%E7%90%86%E7%94%B1%3C/span%3E"/>
        </outline>
        <outline text="缺席判决" _mubu_text="%3Cspan%3E%E7%BC%BA%E5%B8%AD%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
          <outline text="被告、无独三或申请撤诉未获准许的原告" _mubu_text="%3Cspan%3E%E8%A2%AB%E5%91%8A%E3%80%81%E6%97%A0%E7%8B%AC%E4%B8%89%E6%88%96%E7%94%B3%E8%AF%B7%E6%92%A4%E8%AF%89%E6%9C%AA%E8%8E%B7%E5%87%86%E8%AE%B8%E7%9A%84%E5%8E%9F%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="经传票传唤，没有正当理由拒不到庭" _mubu_text="%3Cspan%3E%E7%BB%8F%E4%BC%A0%E7%A5%A8%E4%BC%A0%E5%94%A4%EF%BC%8C%E6%B2%A1%E6%9C%89%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E6%8B%92%E4%B8%8D%E5%88%B0%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="或者未经许可中途退庭" _mubu_text="%3Cspan%3E%E6%88%96%E8%80%85%E6%9C%AA%E7%BB%8F%E8%AE%B8%E5%8F%AF%E4%B8%AD%E9%80%94%E9%80%80%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="诉讼障碍" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E8%AF%89%E8%AE%BC%E9%9A%9C%E7%A2%8D%3C/span%3E" _note="" _mubu_note="">
        <outline text="延期审理" _mubu_text="%3Cspan%3E%E5%BB%B6%E6%9C%9F%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="临时性障碍" _mubu_text="%3Cspan%3E%E4%B8%B4%E6%97%B6%E6%80%A7%E9%9A%9C%E7%A2%8D%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="导致无法继续，需推迟或择日" _mubu_text="%3Cspan%3E%E5%AF%BC%E8%87%B4%E6%97%A0%E6%B3%95%E7%BB%A7%E7%BB%AD%EF%BC%8C%E9%9C%80%E6%8E%A8%E8%BF%9F%E6%88%96%E6%8B%A9%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="文书" _mubu_text="%3Cspan%3E%E6%96%87%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="决定" _mubu_text="%3Cspan%3E%E5%86%B3%E5%AE%9A%3C/span%3E" _note="非裁定" _mubu_note="%3Cspan%3E%E9%9D%9E%E8%A3%81%E5%AE%9A%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="诉讼中止" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="对将来的继续处于不确定状态" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%B0%86%E6%9D%A5%E7%9A%84%E7%BB%A7%E7%BB%AD%E5%A4%84%E4%BA%8E%E4%B8%8D%E7%A1%AE%E5%AE%9A%E7%8A%B6%E6%80%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="文书" _mubu_text="%3Cspan%3E%E6%96%87%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁定" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="诉讼终结" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="一方当事人死亡" _mubu_text="%3Cspan%3E%E4%B8%80%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%AD%BB%E4%BA%A1%3C/span%3E" _note="" _mubu_note="">
              <outline text="当事人死亡的处理" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%AD%BB%E4%BA%A1%E7%9A%84%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note="">
                <outline text="起诉前死亡" _mubu_text="%3Cspan%3E%E8%B5%B7%E8%AF%89%E5%89%8D%E6%AD%BB%E4%BA%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="不予受理，受理后发现的裁定驳回起诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%EF%BC%8C%E5%8F%97%E7%90%86%E5%90%8E%E5%8F%91%E7%8E%B0%E7%9A%84%E8%A3%81%E5%AE%9A%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
                </outline>
                <outline text="诉讼中死亡" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%E6%AD%BB%E4%BA%A1%3C/span%3E" _note="" _mubu_note="">
                  <outline text="诉讼中" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%3C/span%3E" _note="" _mubu_note="">
                    <outline text="受理后→判决生效前" _mubu_text="%3Cspan%3E%E5%8F%97%E7%90%86%E5%90%8E%E2%86%92%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%E5%89%8D%3C/span%3E" _note="判决宣告后都仍属于诉讼中" _mubu_note="%3Cspan%3E%E5%88%A4%E5%86%B3%E5%AE%A3%E5%91%8A%E5%90%8E%E9%83%BD%E4%BB%8D%E5%B1%9E%E4%BA%8E%E8%AF%89%E8%AE%BC%E4%B8%AD%3C/span%3E"/>
                  </outline>
                  <outline text="一般诉讼" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
                    <outline text="尚未确定权义继承人，诉讼中止" _mubu_text="%3Cspan%3E%E5%B0%9A%E6%9C%AA%E7%A1%AE%E5%AE%9A%E6%9D%83%E4%B9%89%E7%BB%A7%E6%89%BF%E4%BA%BA%EF%BC%8C%E8%AF%89%E8%AE%BC%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="只要未明确说没有继承人等，就裁定中止" _mubu_note="%3Cspan%3E%E5%8F%AA%E8%A6%81%E6%9C%AA%E6%98%8E%E7%A1%AE%E8%AF%B4%E6%B2%A1%E6%9C%89%E7%BB%A7%E6%89%BF%E4%BA%BA%E7%AD%89%EF%BC%8C%E5%B0%B1%E8%A3%81%E5%AE%9A%E4%B8%AD%E6%AD%A2%3C/span%3E"/>
                    <outline text="没有，诉讼终结" _mubu_text="%3Cspan%3E%E6%B2%A1%E6%9C%89%EF%BC%8C%E8%AF%89%E8%AE%BC%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="离婚诉讼" _mubu_text="%3Cspan%3E%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="包括解除婚姻关系和财产分割" _mubu_note="%3Cspan%3E%E5%8C%85%E6%8B%AC%E8%A7%A3%E9%99%A4%E5%A9%9A%E5%A7%BB%E5%85%B3%E7%B3%BB%E5%92%8C%E8%B4%A2%E4%BA%A7%E5%88%86%E5%89%B2%3C/span%3E">
                    <outline text="直接裁定诉讼终结" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E8%A3%81%E5%AE%9A%E8%AF%89%E8%AE%BC%E7%BB%88%E7%BB%93%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                </outline>
              </outline>
            </outline>
            <outline text="不需、无法继承诉讼权义" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%9C%80%E3%80%81%E6%97%A0%E6%B3%95%E7%BB%A7%E6%89%BF%E8%AF%89%E8%AE%BC%E6%9D%83%E4%B9%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="文书" _mubu_text="%3Cspan%3E%E6%96%87%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="裁定" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="审限" _mubu_text="%3Cspan%3E%E5%AE%A1%E9%99%90%3C/span%3E" _note="" _mubu_note="">
        <outline text="(6+6)个月" _mubu_text="%3Cspan%3E(6+6)%E4%B8%AA%E6%9C%88%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="判裁决" _mubu_text="%3Cspan%3E%E5%88%A4%E8%A3%81%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
        <outline text="判决" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%3C/span%3E" _note="" _mubu_note="">
          <outline text="一律公开宣判" _mubu_text="%3Cspan%3E%E4%B8%80%E5%BE%8B%E5%85%AC%E5%BC%80%E5%AE%A3%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="离婚诉讼" _mubu_text="%3Cspan%3E%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
            <outline text="应当告知双方当事人在判决生效前不得另行结婚" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E5%91%8A%E7%9F%A5%E5%8F%8C%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%E5%89%8D%E4%B8%8D%E5%BE%97%E5%8F%A6%E8%A1%8C%E7%BB%93%E5%A9%9A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="送达判决书" _mubu_text="%3Cspan%3E%E9%80%81%E8%BE%BE%E5%88%A4%E5%86%B3%E4%B9%A6%3C/span%3E" _note="" _mubu_note="">
            <outline text="当庭宣判的" _mubu_text="%3Cspan%3E%E5%BD%93%E5%BA%AD%E5%AE%A3%E5%88%A4%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="10日内" _mubu_text="%3Cspan%3E10%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="定期宣判的" _mubu_text="%3Cspan%3E%E5%AE%9A%E6%9C%9F%E5%AE%A3%E5%88%A4%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
              <outline text="当庭" _mubu_text="%3Cspan%3E%E5%BD%93%E5%BA%AD%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="错误纠正" _mubu_text="%3Cspan%3E%E9%94%99%E8%AF%AF%E7%BA%A0%E6%AD%A3%3C/span%3E" _note="" _mubu_note="">
            <outline text="瑕疵" _mubu_text="%3Cspan%3E%E7%91%95%E7%96%B5%3C/span%3E" _note="" _mubu_note="">
              <outline text="下达裁定书补正" _mubu_text="%3Cspan%3E%E4%B8%8B%E8%BE%BE%E8%A3%81%E5%AE%9A%E4%B9%A6%E8%A1%A5%E6%AD%A3%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="实质内容错误" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E5%86%85%E5%AE%B9%E9%94%99%E8%AF%AF%3C/span%3E" _note="适用法律、认定事实错误" _mubu_note="%3Cspan%3E%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E3%80%81%E8%AE%A4%E5%AE%9A%E4%BA%8B%E5%AE%9E%E9%94%99%E8%AF%AF%3C/span%3E">
              <outline text="当事人上诉" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="二审法院通过二审程序予以纠正" _mubu_text="%3Cspan%3E%E4%BA%8C%E5%AE%A1%E6%B3%95%E9%99%A2%E9%80%9A%E8%BF%87%E4%BA%8C%E5%AE%A1%E7%A8%8B%E5%BA%8F%E4%BA%88%E4%BB%A5%E7%BA%A0%E6%AD%A3%3C/span%3E" _note="原审法院提出原判有误意见，报送二审法院" _mubu_note="%3Cspan%3E%E5%8E%9F%E5%AE%A1%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E5%8E%9F%E5%88%A4%E6%9C%89%E8%AF%AF%E6%84%8F%E8%A7%81%EF%BC%8C%E6%8A%A5%E9%80%81%E4%BA%8C%E5%AE%A1%E6%B3%95%E9%99%A2%3C/span%3E"/>
              </outline>
              <outline text="当事人不上诉" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%8D%E4%B8%8A%E8%AF%89%3C/span%3E" _note="" _mubu_note="">
                <outline text="判决生效" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="按审判监督程序处理" _mubu_text="%3Cspan%3E%E6%8C%89%E5%AE%A1%E5%88%A4%E7%9B%91%E7%9D%A3%E7%A8%8B%E5%BA%8F%E5%A4%84%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
          </outline>
          <outline text="判决的效力" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
            <outline text="既判力" _mubu_text="%3Cspan%20class=%22%20text-color-red%22%3E%E6%97%A2%E5%88%A4%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="既判力就是禁止再诉的效力" _mubu_text="%3Cspan%3E%E6%97%A2%E5%88%A4%E5%8A%9B%E5%B0%B1%E6%98%AF%E7%A6%81%E6%AD%A2%E5%86%8D%E8%AF%89%E7%9A%84%E6%95%88%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="既判力的客观范围" _mubu_text="%3Cspan%3E%E6%97%A2%E5%88%A4%E5%8A%9B%E7%9A%84%E5%AE%A2%E8%A7%82%E8%8C%83%E5%9B%B4%3C/span%3E" _note="法律关系或实体请求权有拘束力" _mubu_note="%3Cspan%3E%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%88%96%E5%AE%9E%E4%BD%93%E8%AF%B7%E6%B1%82%E6%9D%83%E6%9C%89%E6%8B%98%E6%9D%9F%E5%8A%9B%3C/span%3E">
                <outline text="原则：既判力只及于判决的主文" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E6%97%A2%E5%88%A4%E5%8A%9B%E5%8F%AA%E5%8F%8A%E4%BA%8E%E5%88%A4%E5%86%B3%E7%9A%84%E4%B8%BB%E6%96%87%3C/span%3E" _note="不及于判决的理由部分" _mubu_note="%3Cspan%3E%E4%B8%8D%E5%8F%8A%E4%BA%8E%E5%88%A4%E5%86%B3%E7%9A%84%E7%90%86%E7%94%B1%E9%83%A8%E5%88%86%3C/span%3E">
                  <outline text="主文" _mubu_text="%3Cspan%3E%E4%B8%BB%E6%96%87%3C/span%3E" _note="" _mubu_note="">
                    <outline text="判决中对原告诉讼请求作出的支持与否的判断" _mubu_text="%3Cspan%3E%E5%88%A4%E5%86%B3%E4%B8%AD%E5%AF%B9%E5%8E%9F%E5%91%8A%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E4%BD%9C%E5%87%BA%E7%9A%84%E6%94%AF%E6%8C%81%E4%B8%8E%E5%90%A6%E7%9A%84%E5%88%A4%E6%96%AD%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="理由" _mubu_text="%3Cspan%3E%E7%90%86%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
                    <outline text="在判决中对证据的采信、事实的认定、适用法律的分析" _mubu_text="%3Cspan%3E%E5%9C%A8%E5%88%A4%E5%86%B3%E4%B8%AD%E5%AF%B9%E8%AF%81%E6%8D%AE%E7%9A%84%E9%87%87%E4%BF%A1%E3%80%81%E4%BA%8B%E5%AE%9E%E7%9A%84%E8%AE%A4%E5%AE%9A%E3%80%81%E9%80%82%E7%94%A8%E6%B3%95%E5%BE%8B%E7%9A%84%E5%88%86%E6%9E%90%3C/span%3E" _note="" _mubu_note=""/>
                  </outline>
                  <outline text="仅提出抗辩，抗辩内容归属判决理由部分；提出反诉，则归属主文" _mubu_text="%3Cspan%3E%E4%BB%85%E6%8F%90%E5%87%BA%E6%8A%97%E8%BE%A9%EF%BC%8C%E6%8A%97%E8%BE%A9%E5%86%85%E5%AE%B9%E5%BD%92%E5%B1%9E%E5%88%A4%E5%86%B3%E7%90%86%E7%94%B1%E9%83%A8%E5%88%86%EF%BC%9B%E6%8F%90%E5%87%BA%E5%8F%8D%E8%AF%89%EF%BC%8C%E5%88%99%E5%BD%92%E5%B1%9E%E4%B8%BB%E6%96%87%3C/span%3E" _note="反诉构成独立诉讼请求" _mubu_note="%3Cspan%3E%E5%8F%8D%E8%AF%89%E6%9E%84%E6%88%90%E7%8B%AC%E7%AB%8B%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E"/>
                </outline>
                <outline text="权利人在诉讼请求中仅主张部分法律效果" _mubu_text="%3Cspan%3E%E6%9D%83%E5%88%A9%E4%BA%BA%E5%9C%A8%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E4%B8%AD%E4%BB%85%E4%B8%BB%E5%BC%A0%E9%83%A8%E5%88%86%E6%B3%95%E5%BE%8B%E6%95%88%E6%9E%9C%3C/span%3E" _note="" _mubu_note="">
                  <outline text="部分请求产生全部既判力" _mubu_text="%3Cspan%3E%E9%83%A8%E5%88%86%E8%AF%B7%E6%B1%82%E4%BA%A7%E7%94%9F%E5%85%A8%E9%83%A8%E6%97%A2%E5%88%A4%E5%8A%9B%3C/span%3E" _note="否则原告可以将诉讼无限切割" _mubu_note="%3Cspan%3E%E5%90%A6%E5%88%99%E5%8E%9F%E5%91%8A%E5%8F%AF%E4%BB%A5%E5%B0%86%E8%AF%89%E8%AE%BC%E6%97%A0%E9%99%90%E5%88%87%E5%89%B2%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="既判力的主观范围" _mubu_text="%3Cspan%3E%E6%97%A2%E5%88%A4%E5%8A%9B%E7%9A%84%E4%B8%BB%E8%A7%82%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="原则：判决只对判决的当事人产生既判力" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%88%A4%E5%86%B3%E5%8F%AA%E5%AF%B9%E5%88%A4%E5%86%B3%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%BA%A7%E7%94%9F%E6%97%A2%E5%88%A4%E5%8A%9B%3C/span%3E" _note="既判力具有相对性" _mubu_note="%3Cspan%3E%E6%97%A2%E5%88%A4%E5%8A%9B%E5%85%B7%E6%9C%89%E7%9B%B8%E5%AF%B9%E6%80%A7%3C/span%3E"/>
                <outline text="例外：既判力主观范围扩张" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E6%97%A2%E5%88%A4%E5%8A%9B%E4%B8%BB%E8%A7%82%E8%8C%83%E5%9B%B4%E6%89%A9%E5%BC%A0%3C/span%3E" _note="" _mubu_note="">
                  <outline text="作为当事人死亡或合并、分立，其权义承受者受原判决既判力约束" _mubu_text="%3Cspan%3E%E4%BD%9C%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%AD%BB%E4%BA%A1%E6%88%96%E5%90%88%E5%B9%B6%E3%80%81%E5%88%86%E7%AB%8B%EF%BC%8C%E5%85%B6%E6%9D%83%E4%B9%89%E6%89%BF%E5%8F%97%E8%80%85%E5%8F%97%E5%8E%9F%E5%88%A4%E5%86%B3%E6%97%A2%E5%88%A4%E5%8A%9B%E7%BA%A6%E6%9D%9F%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="公司法上的股东会、董事会决议诉讼等，既判力应扩张至其他股东" _mubu_text="%3Cspan%3E%E5%85%AC%E5%8F%B8%E6%B3%95%E4%B8%8A%E7%9A%84%E8%82%A1%E4%B8%9C%E4%BC%9A%E3%80%81%E8%91%A3%E4%BA%8B%E4%BC%9A%E5%86%B3%E8%AE%AE%E8%AF%89%E8%AE%BC%E7%AD%89%EF%BC%8C%E6%97%A2%E5%88%A4%E5%8A%9B%E5%BA%94%E6%89%A9%E5%BC%A0%E8%87%B3%E5%85%B6%E4%BB%96%E8%82%A1%E4%B8%9C%3C/span%3E" _note="以避免不同股东对同一决议反复起诉" _mubu_note="%3Cspan%3E%E4%BB%A5%E9%81%BF%E5%85%8D%E4%B8%8D%E5%90%8C%E8%82%A1%E4%B8%9C%E5%AF%B9%E5%90%8C%E4%B8%80%E5%86%B3%E8%AE%AE%E5%8F%8D%E5%A4%8D%E8%B5%B7%E8%AF%89%3C/span%3E"/>
                </outline>
              </outline>
              <outline text="既判力的时间范围" _mubu_text="%3Cspan%3E%E6%97%A2%E5%88%A4%E5%8A%9B%E7%9A%84%E6%97%B6%E9%97%B4%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
                <outline text="仅对该特定时间点(判决生效)之前发生的事项具有既判力" _mubu_text="%3Cspan%3E%E4%BB%85%E5%AF%B9%E8%AF%A5%E7%89%B9%E5%AE%9A%E6%97%B6%E9%97%B4%E7%82%B9(%E5%88%A4%E5%86%B3%E7%94%9F%E6%95%88)%E4%B9%8B%E5%89%8D%E5%8F%91%E7%94%9F%E7%9A%84%E4%BA%8B%E9%A1%B9%E5%85%B7%E6%9C%89%E6%97%A2%E5%88%A4%E5%8A%9B%3C/span%3E" _note="对之后发生的事项没有既判力，当事人可再次起诉" _mubu_note="%3Cspan%3E%E5%AF%B9%E4%B9%8B%E5%90%8E%E5%8F%91%E7%94%9F%E7%9A%84%E4%BA%8B%E9%A1%B9%E6%B2%A1%E6%9C%89%E6%97%A2%E5%88%A4%E5%8A%9B%EF%BC%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%AF%E5%86%8D%E6%AC%A1%E8%B5%B7%E8%AF%89%3C/span%3E"/>
              </outline>
            </outline>
            <outline text="执行力" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="给付之诉＋原告胜诉＋具有明确给付内容" _mubu_text="%3Cspan%3E%E7%BB%99%E4%BB%98%E4%B9%8B%E8%AF%89%EF%BC%8B%E5%8E%9F%E5%91%8A%E8%83%9C%E8%AF%89%EF%BC%8B%E5%85%B7%E6%9C%89%E6%98%8E%E7%A1%AE%E7%BB%99%E4%BB%98%E5%86%85%E5%AE%B9%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="形成力" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E5%8A%9B%3C/span%3E" _note="" _mubu_note="">
              <outline text="形成之诉＋原告胜诉" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E4%B9%8B%E8%AF%89%EF%BC%8B%E5%8E%9F%E5%91%8A%E8%83%9C%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="图" _mubu_text="%3Cspan%3E%E5%9B%BE%3C/span%3E" _note="" _mubu_note="">
              <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%229ElQ92LeY1%22%2C%22oh%22%3A275%2C%22ow%22%3A832%2C%22uri%22%3A%22document_image%2F5fc19016-94a3-4a66-a80c-2b7ca3cbd0f5-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
            </outline>
          </outline>
        </outline>
        <outline text="裁定" _mubu_text="%3Cspan%3E%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
          <outline text="可上诉裁定" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%B8%8A%E8%AF%89%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
            <outline text="不予受理、管辖权异议、驳回起诉" _mubu_text="%3Cspan%3E%E4%B8%8D%E4%BA%88%E5%8F%97%E7%90%86%E3%80%81%E7%AE%A1%E8%BE%96%E6%9D%83%E5%BC%82%E8%AE%AE%E3%80%81%E9%A9%B3%E5%9B%9E%E8%B5%B7%E8%AF%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="原则书面，存在口头" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B9%A6%E9%9D%A2%EF%BC%8C%E5%AD%98%E5%9C%A8%E5%8F%A3%E5%A4%B4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
