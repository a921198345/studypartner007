          <outline text="拒绝在送达回证上签字" _mubu_text="%3Cspan%3E%E6%8B%92%E7%BB%9D%E5%9C%A8%E9%80%81%E8%BE%BE%E5%9B%9E%E8%AF%81%E4%B8%8A%E7%AD%BE%E5%AD%97%3C/span%3E" _note="" _mubu_note=""/>
