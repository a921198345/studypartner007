              <outline text="采纳+训诫、罚款" _mubu_text="%3Cspan%3E%E9%87%87%E7%BA%B3+%E8%AE%AD%E8%AF%AB%E3%80%81%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="证据与案件基本事实有关" _mubu_note="%3Cspan%3E%E8%AF%81%E6%8D%AE%E4%B8%8E%E6%A1%88%E4%BB%B6%E5%9F%BA%E6%9C%AC%E4%BA%8B%E5%AE%9E%E6%9C%89%E5%85%B3%3C/span%3E"/>
            </outline>
            <outline text="非故意或重大过失" _mubu_text="%3Cspan%3E%E9%9D%9E%E6%95%85%E6%84%8F%E6%88%96%E9%87%8D%E5%A4%A7%E8%BF%87%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
              <outline text="采纳+训诫" _mubu_text="%3Cspan%3E%E9%87%87%E7%BA%B3+%E8%AE%AD%E8%AF%AB%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="另一方可要求逾期方赔偿逾期致使增加的费用" _mubu_text="%3Cspan%3E%E5%8F%A6%E4%B8%80%E6%96%B9%E5%8F%AF%E8%A6%81%E6%B1%82%E9%80%BE%E6%9C%9F%E6%96%B9%E8%B5%94%E5%81%BF%E9%80%BE%E6%9C%9F%E8%87%B4%E4%BD%BF%E5%A2%9E%E5%8A%A0%E7%9A%84%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="应在举证期限届满前进行" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E4%B8%BE%E8%AF%81%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%E5%89%8D%E8%BF%9B%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="凡是和证据证明相关的" _mubu_text="%3Cspan%3E%E5%87%A1%E6%98%AF%E5%92%8C%E8%AF%81%E6%8D%AE%E8%AF%81%E6%98%8E%E7%9B%B8%E5%85%B3%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="法院调查收集证据" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E8%B0%83%E6%9F%A5%E6%94%B6%E9%9B%86%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
        <outline text="依职权" _mubu_text="%3Cspan%3E%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="国家、社会、第三人利益" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E3%80%81%E7%A4%BE%E4%BC%9A%E3%80%81%E7%AC%AC%E4%B8%89%E4%BA%BA%E5%88%A9%E7%9B%8A%3C/span%3E" _note="" _mubu_note="">
              <outline text="涉及可能损害国家利益、社会公共利益的" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%8F%AF%E8%83%BD%E6%8D%9F%E5%AE%B3%E5%9B%BD%E5%AE%B6%E5%88%A9%E7%9B%8A%E3%80%81%E7%A4%BE%E4%BC%9A%E5%85%AC%E5%85%B1%E5%88%A9%E7%9B%8A%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="涉及公益诉讼的" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%85%AC%E7%9B%8A%E8%AF%89%E8%AE%BC%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="当事人有恶意串通损害他人合法权益可能的" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%89%E6%81%B6%E6%84%8F%E4%B8%B2%E9%80%9A%E6%8D%9F%E5%AE%B3%E4%BB%96%E4%BA%BA%E5%90%88%E6%B3%95%E6%9D%83%E7%9B%8A%E5%8F%AF%E8%83%BD%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="身份关系的事实" _mubu_text="%3Cspan%3E%E8%BA%AB%E4%BB%BD%E5%85%B3%E7%B3%BB%E7%9A%84%E4%BA%8B%E5%AE%9E%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="程序性事实" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E6%80%A7%E4%BA%8B%E5%AE%9E%3C/span%3E" _note="" _mubu_note="">
              <outline text="涉及依职权追加当事人、中止诉讼、终结诉讼、回避等程序性事项的" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E4%BE%9D%E8%81%8C%E6%9D%83%E8%BF%BD%E5%8A%A0%E5%BD%93%E4%BA%8B%E4%BA%BA%E3%80%81%E4%B8%AD%E6%AD%A2%E8%AF%89%E8%AE%BC%E3%80%81%E7%BB%88%E7%BB%93%E8%AF%89%E8%AE%BC%E3%80%81%E5%9B%9E%E9%81%BF%E7%AD%89%E7%A8%8B%E5%BA%8F%E6%80%A7%E4%BA%8B%E9%A1%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="以上三类情形在民诉中的相关类似规定" _mubu_text="%3Cspan%3E%E4%BB%A5%E4%B8%8A%E4%B8%89%E7%B1%BB%E6%83%85%E5%BD%A2%E5%9C%A8%E6%B0%91%E8%AF%89%E4%B8%AD%E7%9A%84%E7%9B%B8%E5%85%B3%E7%B1%BB%E4%BC%BC%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
              <outline text="法院依职权调查收集证据情形" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E8%B0%83%E6%9F%A5%E6%94%B6%E9%9B%86%E8%AF%81%E6%8D%AE%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="不适用自认制度情形" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%80%82%E7%94%A8%E8%87%AA%E8%AE%A4%E5%88%B6%E5%BA%A6%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法院依职权通知证人出庭情形" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E9%80%9A%E7%9F%A5%E8%AF%81%E4%BA%BA%E5%87%BA%E5%BA%AD%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="法院依职权决定鉴定情形" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BE%9D%E8%81%8C%E6%9D%83%E5%86%B3%E5%AE%9A%E9%89%B4%E5%AE%9A%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="质证" _mubu_text="%3Cspan%3E%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
            <outline text="不需质证" _mubu_text="%3Cspan%3E%E4%B8%8D%E9%9C%80%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="依申请" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
          <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="该证据属于国家有关部门保存需法院调取" _mubu_text="%3Cspan%3E%E8%AF%A5%E8%AF%81%E6%8D%AE%E5%B1%9E%E4%BA%8E%E5%9B%BD%E5%AE%B6%E6%9C%89%E5%85%B3%E9%83%A8%E9%97%A8%E4%BF%9D%E5%AD%98%E9%9C%80%E6%B3%95%E9%99%A2%E8%B0%83%E5%8F%96%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="涉及国家秘密、商业秘密、个人隐私" _mubu_text="%3Cspan%3E%E6%B6%89%E5%8F%8A%E5%9B%BD%E5%AE%B6%E7%A7%98%E5%AF%86%E3%80%81%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%E3%80%81%E4%B8%AA%E4%BA%BA%E9%9A%90%E7%A7%81%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="因客观原因无法自行收集" _mubu_text="%3Cspan%3E%E5%9B%A0%E5%AE%A2%E8%A7%82%E5%8E%9F%E5%9B%A0%E6%97%A0%E6%B3%95%E8%87%AA%E8%A1%8C%E6%94%B6%E9%9B%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="书面申请" _mubu_text="%3Cspan%3E%E4%B9%A6%E9%9D%A2%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="应质证" _mubu_text="%3Cspan%3E%E5%BA%94%E8%B4%A8%E8%AF%81%3C/span%3E" _note="视为申请方提供的证据质证" _mubu_note="%3Cspan%3E%E8%A7%86%E4%B8%BA%E7%94%B3%E8%AF%B7%E6%96%B9%E6%8F%90%E4%BE%9B%E7%9A%84%E8%AF%81%E6%8D%AE%E8%B4%A8%E8%AF%81%3C/span%3E"/>
          <outline text="法院不予准许的，不能申请复议" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%8D%E4%BA%88%E5%87%86%E8%AE%B8%E7%9A%84%EF%BC%8C%E4%B8%8D%E8%83%BD%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="质证" _mubu_text="%3Cspan%3E%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
        <outline text="未质证不得作为定案根据" _mubu_text="%3Cspan%3E%E6%9C%AA%E8%B4%A8%E8%AF%81%E4%B8%8D%E5%BE%97%E4%BD%9C%E4%B8%BA%E5%AE%9A%E6%A1%88%E6%A0%B9%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="视为质证" _mubu_text="%3Cspan%3E%E8%A7%86%E4%B8%BA%E8%B4%A8%E8%AF%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="审理前的准备阶段认可的证据" _mubu_text="%3Cspan%3E%E5%AE%A1%E7%90%86%E5%89%8D%E7%9A%84%E5%87%86%E5%A4%87%E9%98%B6%E6%AE%B5%E8%AE%A4%E5%8F%AF%E7%9A%84%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="经审判人员庭审中说明" _mubu_text="%3Cspan%3E%E7%BB%8F%E5%AE%A1%E5%88%A4%E4%BA%BA%E5%91%98%E5%BA%AD%E5%AE%A1%E4%B8%AD%E8%AF%B4%E6%98%8E%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="不得公开质证" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%85%AC%E5%BC%80%E8%B4%A8%E8%AF%81%3C/span%3E" _note="依职权" _mubu_note="%3Cspan%3E%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E">
          <outline text="国家秘密" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%AE%B6%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="商业秘密" _mubu_text="%3Cspan%3E%E5%95%86%E4%B8%9A%E7%A7%98%E5%AF%86%3C/span%3E" _note="" _mubu_note="">
            <outline text="依申请不公开审理" _mubu_text="%3Cspan%3E%E4%BE%9D%E7%94%B3%E8%AF%B7%E4%B8%8D%E5%85%AC%E5%BC%80%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="个人隐私" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BA%BA%E9%9A%90%E7%A7%81%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="证据的认定" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E7%9A%84%E8%AE%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
        <outline text="证据裁判原则" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E8%A3%81%E5%88%A4%E5%8E%9F%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="认定案件事实应当以证据为根据" _mubu_text="%3Cspan%3E%E8%AE%A4%E5%AE%9A%E6%A1%88%E4%BB%B6%E4%BA%8B%E5%AE%9E%E5%BA%94%E5%BD%93%E4%BB%A5%E8%AF%81%E6%8D%AE%E4%B8%BA%E6%A0%B9%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="证明力的判断（自由心证）" _mubu_text="%3Cspan%3E%E8%AF%81%E6%98%8E%E5%8A%9B%E7%9A%84%E5%88%A4%E6%96%AD%EF%BC%88%E8%87%AA%E7%94%B1%E5%BF%83%E8%AF%81%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="不得单独作为认定案件事实的证据" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%8D%95%E7%8B%AC%E4%BD%9C%E4%B8%BA%E8%AE%A4%E5%AE%9A%E6%A1%88%E4%BB%B6%E4%BA%8B%E5%AE%9E%E7%9A%84%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="当事人的陈述" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="无、限制民事行为能力人所作的与其年龄、智力或者精神健康状况不相当的证言" _mubu_text="%3Cspan%3E%E6%97%A0%E3%80%81%E9%99%90%E5%88%B6%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E8%83%BD%E5%8A%9B%E4%BA%BA%E6%89%80%E4%BD%9C%E7%9A%84%E4%B8%8E%E5%85%B6%E5%B9%B4%E9%BE%84%E3%80%81%E6%99%BA%E5%8A%9B%E6%88%96%E8%80%85%E7%B2%BE%E7%A5%9E%E5%81%A5%E5%BA%B7%E7%8A%B6%E5%86%B5%E4%B8%8D%E7%9B%B8%E5%BD%93%E7%9A%84%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="与一方当事人或者其代理人有利害关系的证人陈述的证言" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%B8%80%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%88%96%E8%80%85%E5%85%B6%E4%BB%A3%E7%90%86%E4%BA%BA%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E7%9A%84%E8%AF%81%E4%BA%BA%E9%99%88%E8%BF%B0%E7%9A%84%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="存在疑点的视听资料、电子数据" _mubu_text="%3Cspan%3E%E5%AD%98%E5%9C%A8%E7%96%91%E7%82%B9%E7%9A%84%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%E3%80%81%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="无法与原件核对的复印件、复制品" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E4%B8%8E%E5%8E%9F%E4%BB%B6%E6%A0%B8%E5%AF%B9%E7%9A%84%E5%A4%8D%E5%8D%B0%E4%BB%B6%E3%80%81%E5%A4%8D%E5%88%B6%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="非法证据排除" _mubu_text="%3Cspan%3E%E9%9D%9E%E6%B3%95%E8%AF%81%E6%8D%AE%E6%8E%92%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
          <outline text="偷拍/录不是法律禁止的方法" _mubu_text="%3Cspan%3E%E5%81%B7%E6%8B%8D/%E5%BD%95%E4%B8%8D%E6%98%AF%E6%B3%95%E5%BE%8B%E7%A6%81%E6%AD%A2%E7%9A%84%E6%96%B9%E6%B3%95%3C/span%3E" _note="窃听/录则是" _mubu_note="%3Cspan%3E%E7%AA%83%E5%90%AC/%E5%BD%95%E5%88%99%E6%98%AF%3C/span%3E"/>
          <outline text="不得单独作为认定案件事实的证据" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BE%97%E5%8D%95%E7%8B%AC%E4%BD%9C%E4%B8%BA%E8%AE%A4%E5%AE%9A%E6%A1%88%E4%BB%B6%E4%BA%8B%E5%AE%9E%E7%9A%84%E8%AF%81%E6%8D%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="当事人的陈述" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%9A%84%E9%99%88%E8%BF%B0%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无/限民行能人所作的与其年龄、智力或者精神健康状况不相当的证言" _mubu_text="%3Cspan%3E%E6%97%A0/%E9%99%90%E6%B0%91%E8%A1%8C%E8%83%BD%E4%BA%BA%E6%89%80%E4%BD%9C%E7%9A%84%E4%B8%8E%E5%85%B6%E5%B9%B4%E9%BE%84%E3%80%81%E6%99%BA%E5%8A%9B%E6%88%96%E8%80%85%E7%B2%BE%E7%A5%9E%E5%81%A5%E5%BA%B7%E7%8A%B6%E5%86%B5%E4%B8%8D%E7%9B%B8%E5%BD%93%E7%9A%84%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="与一方当事人或者其代理人有利害关系的证人陈述的证言" _mubu_text="%3Cspan%3E%E4%B8%8E%E4%B8%80%E6%96%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%88%96%E8%80%85%E5%85%B6%E4%BB%A3%E7%90%86%E4%BA%BA%E6%9C%89%E5%88%A9%E5%AE%B3%E5%85%B3%E7%B3%BB%E7%9A%84%E8%AF%81%E4%BA%BA%E9%99%88%E8%BF%B0%E7%9A%84%E8%AF%81%E8%A8%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="存在疑点的视听资料、电子数据" _mubu_text="%3Cspan%3E%E5%AD%98%E5%9C%A8%E7%96%91%E7%82%B9%E7%9A%84%E8%A7%86%E5%90%AC%E8%B5%84%E6%96%99%E3%80%81%E7%94%B5%E5%AD%90%E6%95%B0%E6%8D%AE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="无法与原件核对的复印件、复制品" _mubu_text="%3Cspan%3E%E6%97%A0%E6%B3%95%E4%B8%8E%E5%8E%9F%E4%BB%B6%E6%A0%B8%E5%AF%B9%E7%9A%84%E5%A4%8D%E5%8D%B0%E4%BB%B6%E3%80%81%E5%A4%8D%E5%88%B6%E5%93%81%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="法院查明事实与当事人主张的事实不一致" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%9F%A5%E6%98%8E%E4%BA%8B%E5%AE%9E%E4%B8%8E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B8%BB%E5%BC%A0%E7%9A%84%E4%BA%8B%E5%AE%9E%E4%B8%8D%E4%B8%80%E8%87%B4%3C/span%3E" _note="" _mubu_note="">
        <outline text="法院不能" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%8D%E8%83%BD%3C/span%3E" _note="" _mubu_note="">
          <outline text="根据其审理认定直接作出判决" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E5%85%B6%E5%AE%A1%E7%90%86%E8%AE%A4%E5%AE%9A%E7%9B%B4%E6%8E%A5%E4%BD%9C%E5%87%BA%E5%88%A4%E5%86%B3%3C/span%3E" _note="突袭裁判" _mubu_note="%3Cspan%3E%E7%AA%81%E8%A2%AD%E8%A3%81%E5%88%A4%3C/span%3E"/>
          <outline text="告知当事人变更诉讼请求" _mubu_text="%3Cspan%3E%E5%91%8A%E7%9F%A5%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="法院应当" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%BA%94%E5%BD%93%3C/span%3E" _note="" _mubu_note="">
          <outline text="将法律关系性质或者民事行为效力作为焦点问题进行审理" _mubu_text="%3Cspan%3E%E5%B0%86%E6%B3%95%E5%BE%8B%E5%85%B3%E7%B3%BB%E6%80%A7%E8%B4%A8%E6%88%96%E8%80%85%E6%B0%91%E4%BA%8B%E8%A1%8C%E4%B8%BA%E6%95%88%E5%8A%9B%E4%BD%9C%E4%B8%BA%E7%84%A6%E7%82%B9%E9%97%AE%E9%A2%98%E8%BF%9B%E8%A1%8C%E5%AE%A1%E7%90%86%3C/span%3E" _note="以保障当事人辩论权" _mubu_note="%3Cspan%3E%E4%BB%A5%E4%BF%9D%E9%9A%9C%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%BE%A9%E8%AE%BA%E6%9D%83%3C/span%3E">
            <outline text="对裁判结果和理由没有影响，或者相关问题已经当事人充分辩论的除外" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%A3%81%E5%88%A4%E7%BB%93%E6%9E%9C%E5%92%8C%E7%90%86%E7%94%B1%E6%B2%A1%E6%9C%89%E5%BD%B1%E5%93%8D%EF%BC%8C%E6%88%96%E8%80%85%E7%9B%B8%E5%85%B3%E9%97%AE%E9%A2%98%E5%B7%B2%E7%BB%8F%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%85%85%E5%88%86%E8%BE%A9%E8%AE%BA%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="当事人未变更诉请，法院应针对当事人诉请作出判决" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%9C%AA%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AF%B7%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E9%92%88%E5%AF%B9%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%AF%89%E8%AF%B7%E4%BD%9C%E5%87%BA%E5%88%A4%E5%86%B3%3C/span%3E" _note="处分原则" _mubu_note="%3Cspan%3E%E5%A4%84%E5%88%86%E5%8E%9F%E5%88%99%3C/span%3E"/>
          <outline text="当事人变更诉请，法院应准许" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%98%E6%9B%B4%E8%AF%89%E8%AF%B7%EF%BC%8C%E6%B3%95%E9%99%A2%E5%BA%94%E5%87%86%E8%AE%B8%3C/span%3E" _note="可以根据审理需要重新为当事人指定举证期限" _mubu_note="%3Cspan%20class=%22bold%22%3E%E5%8F%AF%E4%BB%A5%3C/span%3E%3Cspan%3E%E6%A0%B9%E6%8D%AE%E5%AE%A1%E7%90%86%E9%9C%80%E8%A6%81%E9%87%8D%E6%96%B0%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8C%87%E5%AE%9A%E4%B8%BE%E8%AF%81%E6%9C%9F%E9%99%90%3C/span%3E"/>
        </outline>
      </outline>
    </outline>
    <outline text="保全" _mubu_text="%3Cspan%3E%E4%BF%9D%E5%85%A8%3C/span%3E" _note="" _mubu_note="">
      <outline text="诉前or诉中(图)" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8Dor%E8%AF%89%E4%B8%AD(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
        <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22XNdhzJOGwZ%22%2C%22oh%22%3A1132%2C%22ow%22%3A3156%2C%22uri%22%3A%22document_image%2F67a2c20b-2729-4648-a7fd-dee640530234-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
        <outline text="诉前保全后向其他有管辖权法院起诉" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8D%E4%BF%9D%E5%85%A8%E5%90%8E%E5%90%91%E5%85%B6%E4%BB%96%E6%9C%89%E7%AE%A1%E8%BE%96%E6%9D%83%E6%B3%95%E9%99%A2%E8%B5%B7%E8%AF%89%3C/span%3E" _note="诉前保全以外的" _mubu_note="%3Cspan%3E%E8%AF%89%E5%89%8D%E4%BF%9D%E5%85%A8%E4%BB%A5%E5%A4%96%E7%9A%84%3C/span%3E">
          <outline text="应将保全手续移送受理案件的法院" _mubu_text="%3Cspan%3E%E5%BA%94%E5%B0%86%E4%BF%9D%E5%85%A8%E6%89%8B%E7%BB%AD%E7%A7%BB%E9%80%81%E5%8F%97%E7%90%86%E6%A1%88%E4%BB%B6%E7%9A%84%E6%B3%95%E9%99%A2%3C/span%3E" _note="保全为诉讼服务" _mubu_note="%3Cspan%3E%E4%BF%9D%E5%85%A8%E4%B8%BA%E8%AF%89%E8%AE%BC%E6%9C%8D%E5%8A%A1%3C/span%3E"/>
          <outline text="诉前保全裁定视为受移送法院作出的裁定" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8D%E4%BF%9D%E5%85%A8%E8%A3%81%E5%AE%9A%E8%A7%86%E4%B8%BA%E5%8F%97%E7%A7%BB%E9%80%81%E6%B3%95%E9%99%A2%E4%BD%9C%E5%87%BA%E7%9A%84%E8%A3%81%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="执行前" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E5%89%8D%3C/span%3E" _note="" _mubu_note="">
        <outline text="申请时间" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="文书生效后，履行期满前" _mubu_text="%3Cspan%3E%E6%96%87%E4%B9%A6%E7%94%9F%E6%95%88%E5%90%8E%EF%BC%8C%E5%B1%A5%E8%A1%8C%E6%9C%9F%E6%BB%A1%E5%89%8D%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="管辖" _mubu_text="%3Cspan%3E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
          <outline text="执行法院" _mubu_text="%3Cspan%3E%E6%89%A7%E8%A1%8C%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note="">
            <outline text="一审法院" _mubu_text="%3Cspan%3E%E4%B8%80%E5%AE%A1%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="或与之同级的被执行财产所在地法院" _mubu_text="%3Cspan%3E%E6%88%96%E4%B8%8E%E4%B9%8B%E5%90%8C%E7%BA%A7%E7%9A%84%E8%A2%AB%E6%89%A7%E8%A1%8C%E8%B4%A2%E4%BA%A7%E6%89%80%E5%9C%A8%E5%9C%B0%E6%B3%95%E9%99%A2%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="解除" _mubu_text="%3Cspan%3E%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note="">
          <outline text="应在履行期限届满后5日内申请执行，否则法院解除" _mubu_text="%3Cspan%3E%E5%BA%94%E5%9C%A8%E5%B1%A5%E8%A1%8C%E6%9C%9F%E9%99%90%E5%B1%8A%E6%BB%A1%E5%90%8E5%E6%97%A5%E5%86%85%E7%94%B3%E8%AF%B7%E6%89%A7%E8%A1%8C%EF%BC%8C%E5%90%A6%E5%88%99%E6%B3%95%E9%99%A2%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="担保" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
          <outline text="可以不责令" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E4%B8%8D%E8%B4%A3%E4%BB%A4%3C/span%3E" _note="当事人间权利义务关系已明确" _mubu_note="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E9%97%B4%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%E5%B7%B2%E6%98%8E%E7%A1%AE%3C/span%3E"/>
        </outline>
      </outline>
      <outline text="共同问题" _mubu_text="%3Cspan%3E%E5%85%B1%E5%90%8C%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
        <outline text="被保全财产的保管" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BF%9D%E5%85%A8%E8%B4%A2%E4%BA%A7%E7%9A%84%E4%BF%9D%E7%AE%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="一般财产" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E8%B4%A2%E4%BA%A7%3C/span%3E" _note="" _mubu_note="">
            <outline text="查扣冻时应当妥善保管" _mubu_text="%3Cspan%3E%E6%9F%A5%E6%89%A3%E5%86%BB%E6%97%B6%E5%BA%94%E5%BD%93%E5%A6%A5%E5%96%84%E4%BF%9D%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不宜由法院保管的可以指定被保全人保管" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AE%9C%E7%94%B1%E6%B3%95%E9%99%A2%E4%BF%9D%E7%AE%A1%E7%9A%84%E5%8F%AF%E4%BB%A5%E6%8C%87%E5%AE%9A%E8%A2%AB%E4%BF%9D%E5%85%A8%E4%BA%BA%E4%BF%9D%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="不宜由被保全人保管的，可委托他人或者申请保全人保管" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AE%9C%E7%94%B1%E8%A2%AB%E4%BF%9D%E5%85%A8%E4%BA%BA%E4%BF%9D%E7%AE%A1%E7%9A%84%EF%BC%8C%E5%8F%AF%E5%A7%94%E6%89%98%E4%BB%96%E4%BA%BA%E6%88%96%E8%80%85%E7%94%B3%E8%AF%B7%E4%BF%9D%E5%85%A8%E4%BA%BA%E4%BF%9D%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="不宜长期保存的" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%AE%9C%E9%95%BF%E6%9C%9F%E4%BF%9D%E5%AD%98%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="可责令当事人及时处理，由法院保存价款" _mubu_text="%3Cspan%3E%E5%8F%AF%E8%B4%A3%E4%BB%A4%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%8A%E6%97%B6%E5%A4%84%E7%90%86%EF%BC%8C%E7%94%B1%E6%B3%95%E9%99%A2%E4%BF%9D%E5%AD%98%E4%BB%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="必要时，人民法院可变卖，保存价款" _mubu_text="%3Cspan%3E%E5%BF%85%E8%A6%81%E6%97%B6%EF%BC%8C%E4%BA%BA%E6%B0%91%E6%B3%95%E9%99%A2%E5%8F%AF%E5%8F%98%E5%8D%96%EF%BC%8C%E4%BF%9D%E5%AD%98%E4%BB%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="对他人到期债权" _mubu_text="%3Cspan%3E%E5%AF%B9%E4%BB%96%E4%BA%BA%E5%88%B0%E6%9C%9F%E5%80%BA%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院可依申请裁定该他人不得向债务人清偿" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BE%9D%E7%94%B3%E8%AF%B7%E8%A3%81%E5%AE%9A%E8%AF%A5%E4%BB%96%E4%BA%BA%E4%B8%8D%E5%BE%97%E5%90%91%E5%80%BA%E5%8A%A1%E4%BA%BA%E6%B8%85%E5%81%BF%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="要求清偿的，由法院提存财物或者价款" _mubu_text="%3Cspan%3E%E8%A6%81%E6%B1%82%E6%B8%85%E5%81%BF%E7%9A%84%EF%BC%8C%E7%94%B1%E6%B3%95%E9%99%A2%E6%8F%90%E5%AD%98%E8%B4%A2%E7%89%A9%E6%88%96%E8%80%85%E4%BB%B7%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="对质押物、留置物" _mubu_text="%3Cspan%3E%E5%AF%B9%E8%B4%A8%E6%8A%BC%E7%89%A9%E3%80%81%E7%95%99%E7%BD%AE%E7%89%A9%3C/span%3E" _note="" _mubu_note="">
            <outline text="不影响抵押、留置、质权人的优先受偿权" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%BD%B1%E5%93%8D%E6%8A%B5%E6%8A%BC%E3%80%81%E7%95%99%E7%BD%AE%E3%80%81%E8%B4%A8%E6%9D%83%E4%BA%BA%E7%9A%84%E4%BC%98%E5%85%88%E5%8F%97%E5%81%BF%E6%9D%83%3C/span%3E" _note="无需经其同意" _mubu_note="%3Cspan%3E%E6%97%A0%E9%9C%80%E7%BB%8F%E5%85%B6%E5%90%8C%E6%84%8F%3C/span%3E"/>
            <outline text="担保物权人占有的，一般由其保管" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E7%89%A9%E6%9D%83%E4%BA%BA%E5%8D%A0%E6%9C%89%E7%9A%84%EF%BC%8C%E4%B8%80%E8%88%AC%E7%94%B1%E5%85%B6%E4%BF%9D%E7%AE%A1%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="法院保管的，质权、留置权不消灭" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%BF%9D%E7%AE%A1%E7%9A%84%EF%BC%8C%E8%B4%A8%E6%9D%83%E3%80%81%E7%95%99%E7%BD%AE%E6%9D%83%E4%B8%8D%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="可以申请复议一次" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="复议不停止原裁定的执行" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%E4%B8%8D%E5%81%9C%E6%AD%A2%E5%8E%9F%E8%A3%81%E5%AE%9A%E7%9A%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="应当解除情形" _mubu_text="%3Cspan%3E%E5%BA%94%E5%BD%93%E8%A7%A3%E9%99%A4%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="保全错误" _mubu_text="%3Cspan%3E%E4%BF%9D%E5%85%A8%E9%94%99%E8%AF%AF%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请人撤回保全申请" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%92%A4%E5%9B%9E%E4%BF%9D%E5%85%A8%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请人的起诉或者诉讼请求被生效裁判驳回的" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E7%9A%84%E8%B5%B7%E8%AF%89%E6%88%96%E8%80%85%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E8%A2%AB%E7%94%9F%E6%95%88%E8%A3%81%E5%88%A4%E9%A9%B3%E5%9B%9E%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="诉前保全30日内不起诉或申请仲裁的" _mubu_text="%3Cspan%3E%E8%AF%89%E5%89%8D%E4%BF%9D%E5%85%A830%E6%97%A5%E5%86%85%E4%B8%8D%E8%B5%B7%E8%AF%89%E6%88%96%E7%94%B3%E8%AF%B7%E4%BB%B2%E8%A3%81%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="财产纠纷案件，被申请人向法院提供担保的" _mubu_text="%3Cspan%3E%E8%B4%A2%E4%BA%A7%E7%BA%A0%E7%BA%B7%E6%A1%88%E4%BB%B6%EF%BC%8C%E8%A2%AB%E7%94%B3%E8%AF%B7%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="变更" _mubu_text="%3Cspan%3E%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="被保全人提供其他等值担保产且有利于执行的，可裁定变更" _mubu_text="%3Cspan%3E%E8%A2%AB%E4%BF%9D%E5%85%A8%E4%BA%BA%E6%8F%90%E4%BE%9B%E5%85%B6%E4%BB%96%E7%AD%89%E5%80%BC%E6%8B%85%E4%BF%9D%E4%BA%A7%E4%B8%94%E6%9C%89%E5%88%A9%E4%BA%8E%E6%89%A7%E8%A1%8C%E7%9A%84%EF%BC%8C%E5%8F%AF%E8%A3%81%E5%AE%9A%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
    </outline>
    <outline text="先予执行" _mubu_text="%3Cspan%3E%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
      <outline text="适用情形" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
        <outline text="追索赡养费、扶养费、抚养费、抚恤金、医疗费等" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E8%B5%A1%E5%85%BB%E8%B4%B9%E3%80%81%E6%89%B6%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E5%85%BB%E8%B4%B9%E3%80%81%E6%8A%9A%E6%81%A4%E9%87%91%E3%80%81%E5%8C%BB%E7%96%97%E8%B4%B9%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="追索劳动报酬" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E5%8A%B3%E5%8A%A8%E6%8A%A5%E9%85%AC%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="情况紧急需要先予执行" _mubu_text="%3Cspan%3E%E6%83%85%E5%86%B5%E7%B4%A7%E6%80%A5%E9%9C%80%E8%A6%81%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
          <outline text="需要立即停止侵害、排除妨碍的" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E7%AB%8B%E5%8D%B3%E5%81%9C%E6%AD%A2%E4%BE%B5%E5%AE%B3%E3%80%81%E6%8E%92%E9%99%A4%E5%A6%A8%E7%A2%8D%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="需要立即制止某项行为的" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E7%AB%8B%E5%8D%B3%E5%88%B6%E6%AD%A2%E6%9F%90%E9%A1%B9%E8%A1%8C%E4%B8%BA%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="追索恢复生产、经营急需的保险理赔费的" _mubu_text="%3Cspan%3E%E8%BF%BD%E7%B4%A2%E6%81%A2%E5%A4%8D%E7%94%9F%E4%BA%A7%E3%80%81%E7%BB%8F%E8%90%A5%E6%80%A5%E9%9C%80%E7%9A%84%E4%BF%9D%E9%99%A9%E7%90%86%E8%B5%94%E8%B4%B9%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="需要立即返还社会保险金、社会救助资金的" _mubu_text="%3Cspan%3E%E9%9C%80%E8%A6%81%E7%AB%8B%E5%8D%B3%E8%BF%94%E8%BF%98%E7%A4%BE%E4%BC%9A%E4%BF%9D%E9%99%A9%E9%87%91%E3%80%81%E7%A4%BE%E4%BC%9A%E6%95%91%E5%8A%A9%E8%B5%84%E9%87%91%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不立即返还款项，将严重影响权利人生活和生产经营的" _mubu_text="%3Cspan%3E%E4%B8%8D%E7%AB%8B%E5%8D%B3%E8%BF%94%E8%BF%98%E6%AC%BE%E9%A1%B9%EF%BC%8C%E5%B0%86%E4%B8%A5%E9%87%8D%E5%BD%B1%E5%93%8D%E6%9D%83%E5%88%A9%E4%BA%BA%E7%94%9F%E6%B4%BB%E5%92%8C%E7%94%9F%E4%BA%A7%E7%BB%8F%E8%90%A5%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="适用条件" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E6%9D%A1%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
        <outline text="当事人之间权利义务关系明确" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E4%B9%8B%E9%97%B4%E6%9D%83%E5%88%A9%E4%B9%89%E5%8A%A1%E5%85%B3%E7%B3%BB%E6%98%8E%E7%A1%AE%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="申请人有实现权利的迫切需要" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%9C%89%E5%AE%9E%E7%8E%B0%E6%9D%83%E5%88%A9%E7%9A%84%E8%BF%AB%E5%88%87%E9%9C%80%E8%A6%81%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="当事人向法院提出申请" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%90%91%E6%B3%95%E9%99%A2%E6%8F%90%E5%87%BA%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="被申请人有履行能力" _mubu_text="%3Cspan%3E%E8%A2%AB%E7%94%B3%E8%AF%B7%E4%BA%BA%E6%9C%89%E5%B1%A5%E8%A1%8C%E8%83%BD%E5%8A%9B%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
        <outline text="申请" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%3C/span%3E" _note="不能依职权" _mubu_note="%3Cspan%3E%E4%B8%8D%E8%83%BD%E4%BE%9D%E8%81%8C%E6%9D%83%3C/span%3E"/>
        <outline text="担保" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note="">
          <outline text="可以责令提供担保" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E8%B4%A3%E4%BB%A4%E6%8F%90%E4%BE%9B%E6%8B%85%E4%BF%9D%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="各类保全和先于执行担保的必要性(图)" _mubu_text="%3Cspan%3E%E5%90%84%E7%B1%BB%E4%BF%9D%E5%85%A8%E5%92%8C%E5%85%88%E4%BA%8E%E6%89%A7%E8%A1%8C%E6%8B%85%E4%BF%9D%E7%9A%84%E5%BF%85%E8%A6%81%E6%80%A7(%E5%9B%BE)%3C/span%3E" _note="" _mubu_note="">
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22VAJY450rFe%22%2C%22oh%22%3A1100%2C%22ow%22%3A1748%2C%22uri%22%3A%22document_image%2F82da123a-b953-42c6-a371-bd976a521c0c-21501259.jpg%22%2C%22w%22%3A400%7D%5D"/>
          </outline>
        </outline>
        <outline text="范围" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note="">
          <outline text="限于当事人诉讼请求的范围" _mubu_text="%3Cspan%3E%E9%99%90%E4%BA%8E%E5%BD%93%E4%BA%8B%E4%BA%BA%E8%AF%89%E8%AE%BC%E8%AF%B7%E6%B1%82%E7%9A%84%E8%8C%83%E5%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="且以当事人生活、生产经营的急需为限" _mubu_text="%3Cspan%3E%E4%B8%94%E4%BB%A5%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%94%9F%E6%B4%BB%E3%80%81%E7%94%9F%E4%BA%A7%E7%BB%8F%E8%90%A5%E7%9A%84%E6%80%A5%E9%9C%80%E4%B8%BA%E9%99%90%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="救济" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%3C/span%3E" _note="" _mubu_note="">
          <outline text="可以申请复议一次" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="同级复议" _mubu_note="%3Cspan%3E%E5%90%8C%E7%BA%A7%E5%A4%8D%E8%AE%AE%3C/span%3E"/>
          <outline text="复议不停止原裁定的执行" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%E4%B8%8D%E5%81%9C%E6%AD%A2%E5%8E%9F%E8%A3%81%E5%AE%9A%E7%9A%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="各种纠纷解决程序中的先予执行" _mubu_text="%3Cspan%3E%E5%90%84%E7%A7%8D%E7%BA%A0%E7%BA%B7%E8%A7%A3%E5%86%B3%E7%A8%8B%E5%BA%8F%E4%B8%AD%E7%9A%84%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note="">
        <outline text="诉讼中可以申请先予执行" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E4%B8%AD%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="仲裁中不存在先予执行的问题" _mubu_text="%3Cspan%20class=%22bold%22%3E%E4%BB%B2%E8%A3%81%3C/span%3E%3Cspan%3E%E4%B8%AD%E4%B8%8D%E5%AD%98%E5%9C%A8%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%E7%9A%84%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="劳动仲裁中可以申请先予执行" _mubu_text="%3Cspan%3E%E5%8A%B3%E5%8A%A8%E4%BB%B2%E8%A3%81%E4%B8%AD%E5%8F%AF%E4%BB%A5%E7%94%B3%E8%AF%B7%E5%85%88%E4%BA%88%E6%89%A7%E8%A1%8C%3C/span%3E" _note="仲裁庭有裁决权无执行权，移交法院执行" _mubu_note="%3Cspan%3E%E4%BB%B2%E8%A3%81%E5%BA%AD%E6%9C%89%E8%A3%81%E5%86%B3%E6%9D%83%E6%97%A0%E6%89%A7%E8%A1%8C%E6%9D%83%EF%BC%8C%E7%A7%BB%E4%BA%A4%E6%B3%95%E9%99%A2%E6%89%A7%E8%A1%8C%3C/span%3E"/>
      </outline>
    </outline>
    <outline text="对妨碍诉讼的强制措施" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%A6%A8%E7%A2%8D%E8%AF%89%E8%AE%BC%E7%9A%84%E5%BC%BA%E5%88%B6%E6%8E%AA%E6%96%BD%3C/span%3E" _note="" _mubu_note="">
      <outline text="拘传" _mubu_text="%3Cspan%3E%E6%8B%98%E4%BC%A0%3C/span%3E" _note="" _mubu_note="">
        <outline text="适用对象" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E5%AF%B9%E8%B1%A1%3C/span%3E" _note="" _mubu_note="">
          <outline text="必须到庭的当事人" _mubu_text="%3Cspan%3E%E5%BF%85%E9%A1%BB%E5%88%B0%E5%BA%AD%E7%9A%84%E5%BD%93%E4%BA%8B%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="负有赡养、抚养、扶养义务的" _mubu_text="%3Cspan%3E%E8%B4%9F%E6%9C%89%E8%B5%A1%E5%85%BB%E3%80%81%E6%8A%9A%E5%85%BB%E3%80%81%E6%89%B6%E5%85%BB%E4%B9%89%E5%8A%A1%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="和不到庭就无法查案情的" _mubu_text="%3Cspan%3E%E5%92%8C%E4%B8%8D%E5%88%B0%E5%BA%AD%E5%B0%B1%E6%97%A0%E6%B3%95%E6%9F%A5%E6%A1%88%E6%83%85%E7%9A%84%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="包括原告和被告" _mubu_text="%3Cspan%3E%E5%8C%85%E6%8B%AC%E5%8E%9F%E5%91%8A%E5%92%8C%E8%A2%AB%E5%91%8A%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="经两次传唤" _mubu_text="%3Cspan%3E%E7%BB%8F%E4%B8%A4%E6%AC%A1%E4%BC%A0%E5%94%A4%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="由院长签发拘传票" _mubu_text="%3Cspan%3E%E7%94%B1%E9%99%A2%E9%95%BF%E7%AD%BE%E5%8F%91%E6%8B%98%E4%BC%A0%E7%A5%A8%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="罚款与拘留" _mubu_text="%3Cspan%3E%E7%BD%9A%E6%AC%BE%E4%B8%8E%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note="">
        <outline text="情形" _mubu_text="%3Cspan%3E%E6%83%85%E5%BD%A2%3C/span%3E" _note="" _mubu_note="">
          <outline text="妨碍诉讼，妨碍执行" _mubu_text="%3Cspan%3E%E5%A6%A8%E7%A2%8D%E8%AF%89%E8%AE%BC%EF%BC%8C%E5%A6%A8%E7%A2%8D%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="协助or配合" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%8A%A9or%E9%85%8D%E5%90%88%3C/span%3E" _note="" _mubu_note="">
            <outline text="协助" _mubu_text="%3Cspan%3E%E5%8D%8F%E5%8A%A9%3C/span%3E" _note="" _mubu_note="">
              <outline text="法院无法凭一己之力完成相关行为，需相关主体提供积极帮助" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E6%97%A0%E6%B3%95%E5%87%AD%E4%B8%80%E5%B7%B1%E4%B9%8B%E5%8A%9B%E5%AE%8C%E6%88%90%E7%9B%B8%E5%85%B3%E8%A1%8C%E4%B8%BA%EF%BC%8C%E9%9C%80%E7%9B%B8%E5%85%B3%E4%B8%BB%E4%BD%93%E6%8F%90%E4%BE%9B%E7%A7%AF%E6%9E%81%E5%B8%AE%E5%8A%A9%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="此时需要向相关主体出具协助执行通知书" _mubu_text="%3Cspan%3E%E6%AD%A4%E6%97%B6%E9%9C%80%E8%A6%81%E5%90%91%E7%9B%B8%E5%85%B3%E4%B8%BB%E4%BD%93%E5%87%BA%E5%85%B7%E5%8D%8F%E5%8A%A9%E6%89%A7%E8%A1%8C%E9%80%9A%E7%9F%A5%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="配合" _mubu_text="%3Cspan%3E%E9%85%8D%E5%90%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="范围很广，任何个人和组织都有义务配合法院、法官执行职务行为" _mubu_text="%3Cspan%3E%E8%8C%83%E5%9B%B4%E5%BE%88%E5%B9%BF%EF%BC%8C%E4%BB%BB%E4%BD%95%E4%B8%AA%E4%BA%BA%E5%92%8C%E7%BB%84%E7%BB%87%E9%83%BD%E6%9C%89%E4%B9%89%E5%8A%A1%E9%85%8D%E5%90%88%E6%B3%95%E9%99%A2%E3%80%81%E6%B3%95%E5%AE%98%E6%89%A7%E8%A1%8C%E8%81%8C%E5%8A%A1%E8%A1%8C%E4%B8%BA%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="无需发出协助执行通知书" _mubu_text="%3Cspan%3E%E6%97%A0%E9%9C%80%E5%8F%91%E5%87%BA%E5%8D%8F%E5%8A%A9%E6%89%A7%E8%A1%8C%E9%80%9A%E7%9F%A5%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="程序" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%3C/span%3E" _note="" _mubu_note="">
          <outline text="由院长批准" _mubu_text="%3Cspan%3E%E7%94%B1%E9%99%A2%E9%95%BF%E6%89%B9%E5%87%86%3C/span%3E" _note="无听证程序要求" _mubu_note="%3Cspan%3E%E6%97%A0%E5%90%AC%E8%AF%81%E7%A8%8B%E5%BA%8F%E8%A6%81%E6%B1%82%3C/span%3E"/>
          <outline text="使用决定书" _mubu_text="%3Cspan%3E%E4%BD%BF%E7%94%A8%E5%86%B3%E5%AE%9A%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="采取拘留后，应24小时内通知其家属" _mubu_text="%3Cspan%3E%E9%87%87%E5%8F%96%E6%8B%98%E7%95%99%E5%90%8E%EF%BC%8C%E5%BA%9424%E5%B0%8F%E6%97%B6%E5%86%85%E9%80%9A%E7%9F%A5%E5%85%B6%E5%AE%B6%E5%B1%9E%3C/span%3E" _note="确实无法(按时)通知的，记录在案" _mubu_note="%3Cspan%3E%E7%A1%AE%E5%AE%9E%E6%97%A0%E6%B3%95(%E6%8C%89%E6%97%B6)%E9%80%9A%E7%9F%A5%E7%9A%84%EF%BC%8C%E8%AE%B0%E5%BD%95%E5%9C%A8%E6%A1%88%3C/span%3E"/>
        </outline>
        <outline text="适用" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%3C/span%3E" _note="" _mubu_note="">
          <outline text="可单独，也可合并" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%8D%95%E7%8B%AC%EF%BC%8C%E4%B9%9F%E5%8F%AF%E5%90%88%E5%B9%B6%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="复议" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
          <outline text="可向上一级法院复议一次" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%90%91%E4%B8%8A%E4%B8%80%E7%BA%A7%E6%B3%95%E9%99%A2%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="复议期间不停止原决定的执行" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%E6%9C%9F%E9%97%B4%E4%B8%8D%E5%81%9C%E6%AD%A2%E5%8E%9F%E5%86%B3%E5%AE%9A%E7%9A%84%E6%89%A7%E8%A1%8C%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="处罚幅度" _mubu_text="%3Cspan%3E%E5%A4%84%E7%BD%9A%E5%B9%85%E5%BA%A6%3C/span%3E" _note="" _mubu_note="">
          <outline text="拘留" _mubu_text="%3Cspan%3E%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note="">
            <outline text="≤15日" _mubu_text="%3Cspan%3E%E2%89%A415%E6%97%A5%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="期间承认并改正错误的，法院可提前解除" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E6%89%BF%E8%AE%A4%E5%B9%B6%E6%94%B9%E6%AD%A3%E9%94%99%E8%AF%AF%E7%9A%84%EF%BC%8C%E6%B3%95%E9%99%A2%E5%8F%AF%E6%8F%90%E5%89%8D%E8%A7%A3%E9%99%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="罚款" _mubu_text="%3Cspan%3E%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note="">
            <outline text="个人" _mubu_text="%3Cspan%3E%E4%B8%AA%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="≤10万" _mubu_text="%3Cspan%3E%E2%89%A410%E4%B8%87%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="单位" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%3C/span%3E" _note="" _mubu_note="">
              <outline text="5万≤ ≤100万" _mubu_text="%3Cspan%3E5%E4%B8%87%E2%89%A4%20%E2%89%A4100%E4%B8%87%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
        </outline>
        <outline text="单位妨碍" _mubu_text="%3Cspan%3E%E5%8D%95%E4%BD%8D%E5%A6%A8%E7%A2%8D%3C/span%3E" _note="" _mubu_note="">
          <outline text="对单位进行罚款" _mubu_text="%3Cspan%3E%E5%AF%B9%E5%8D%95%E4%BD%8D%E8%BF%9B%E8%A1%8C%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="并可对其主要负责人或者直接责任人员予以罚款、拘留" _mubu_text="%3Cspan%3E%E5%B9%B6%E5%8F%AF%E5%AF%B9%E5%85%B6%E4%B8%BB%E8%A6%81%E8%B4%9F%E8%B4%A3%E4%BA%BA%E6%88%96%E8%80%85%E7%9B%B4%E6%8E%A5%E8%B4%A3%E4%BB%BB%E4%BA%BA%E5%91%98%E4%BA%88%E4%BB%A5%E7%BD%9A%E6%AC%BE%E3%80%81%E6%8B%98%E7%95%99%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="构成犯罪的，依法追究刑事责任" _mubu_text="%3Cspan%3E%E6%9E%84%E6%88%90%E7%8A%AF%E7%BD%AA%E7%9A%84%EF%BC%8C%E4%BE%9D%E6%B3%95%E8%BF%BD%E7%A9%B6%E5%88%91%E4%BA%8B%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
    </outline>
    <outline text="期间与送达" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E4%B8%8E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
      <outline text="期间" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
        <outline text="分类" _mubu_text="%3Cspan%3E%E5%88%86%E7%B1%BB%3C/span%3E" _note="" _mubu_note="">
          <outline text="法定期间" _mubu_text="%3Cspan%3E%E6%B3%95%E5%AE%9A%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="绝对不可变期" _mubu_text="%3Cspan%3E%E7%BB%9D%E5%AF%B9%E4%B8%8D%E5%8F%AF%E5%8F%98%E6%9C%9F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="相对不可变期" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E4%B8%8D%E5%8F%AF%E5%8F%98%E6%9C%9F%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="指定期间" _mubu_text="%3Cspan%3E%E6%8C%87%E5%AE%9A%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="法院为当事人指定的期间" _mubu_text="%3Cspan%3E%E6%B3%95%E9%99%A2%E4%B8%BA%E5%BD%93%E4%BA%8B%E4%BA%BA%E6%8C%87%E5%AE%9A%E7%9A%84%E6%9C%9F%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="一般情形不能任意变更，特殊情形下法院可依职变更" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E6%83%85%E5%BD%A2%E4%B8%8D%E8%83%BD%E4%BB%BB%E6%84%8F%E5%8F%98%E6%9B%B4%EF%BC%8C%E7%89%B9%E6%AE%8A%E6%83%85%E5%BD%A2%E4%B8%8B%E6%B3%95%E9%99%A2%E5%8F%AF%E4%BE%9D%E8%81%8C%E5%8F%98%E6%9B%B4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="计算" _mubu_text="%3Cspan%3E%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note="">
          <outline text="以时、日、月、年计算" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%97%B6%E3%80%81%E6%97%A5%E3%80%81%E6%9C%88%E3%80%81%E5%B9%B4%E8%AE%A1%E7%AE%97%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="期间开始的时和日不计算在内" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E5%BC%80%E5%A7%8B%E7%9A%84%E6%97%B6%E5%92%8C%E6%97%A5%E4%B8%8D%E8%AE%A1%E7%AE%97%E5%9C%A8%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="期间届满最后一日是法定休假日的" _mubu_text="%3Cspan%3E%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E6%9C%80%E5%90%8E%E4%B8%80%E6%97%A5%E6%98%AF%E6%B3%95%E5%AE%9A%E4%BC%91%E5%81%87%E6%97%A5%E7%9A%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="以法定休假日后的第一个工作日为期间届满日" _mubu_text="%3Cspan%3E%E4%BB%A5%E6%B3%95%E5%AE%9A%E4%BC%91%E5%81%87%E6%97%A5%E5%90%8E%E7%9A%84%E7%AC%AC%E4%B8%80%E4%B8%AA%E5%B7%A5%E4%BD%9C%E6%97%A5%E4%B8%BA%E6%9C%9F%E9%97%B4%E5%B1%8A%E6%BB%A1%E6%97%A5%3C/span%3E" _note="期间开始或者期间中有法定休假日的不能扣除顺延" _mubu_note="%3Cspan%3E%E6%9C%9F%E9%97%B4%E5%BC%80%E5%A7%8B%E6%88%96%E8%80%85%E6%9C%9F%E9%97%B4%E4%B8%AD%E6%9C%89%E6%B3%95%E5%AE%9A%E4%BC%91%E5%81%87%E6%97%A5%E7%9A%84%E4%B8%8D%E8%83%BD%E6%89%A3%E9%99%A4%E9%A1%BA%E5%BB%B6%3C/span%3E"/>
          </outline>
          <outline text="在途期间不包括在内" _mubu_text="%3Cspan%3E%E5%9C%A8%E9%80%94%E6%9C%9F%E9%97%B4%E4%B8%8D%E5%8C%85%E6%8B%AC%E5%9C%A8%E5%86%85%3C/span%3E" _note="" _mubu_note="">
            <outline text="诉讼文书在期满前交邮的，不算过期" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E6%96%87%E4%B9%A6%E5%9C%A8%E6%9C%9F%E6%BB%A1%E5%89%8D%E4%BA%A4%E9%82%AE%E7%9A%84%EF%BC%8C%E4%B8%8D%E7%AE%97%E8%BF%87%E6%9C%9F%3C/span%3E" _note="仅指文书在途期间，而当事人参诉在途时间要计算在内" _mubu_note="%3Cspan%3E%E4%BB%85%E6%8C%87%E6%96%87%E4%B9%A6%E5%9C%A8%E9%80%94%E6%9C%9F%E9%97%B4%EF%BC%8C%E8%80%8C%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%8F%82%E8%AF%89%E5%9C%A8%E9%80%94%E6%97%B6%E9%97%B4%E8%A6%81%E8%AE%A1%E7%AE%97%E5%9C%A8%E5%86%85%3C/span%3E"/>
          </outline>
        </outline>
        <outline text="耽误与顺延" _mubu_text="%3Cspan%3E%E8%80%BD%E8%AF%AF%E4%B8%8E%E9%A1%BA%E5%BB%B6%3C/span%3E" _note="" _mubu_note="">
          <outline text="事由" _mubu_text="%3Cspan%3E%E4%BA%8B%E7%94%B1%3C/span%3E" _note="" _mubu_note="">
            <outline text="不可抗拒的事由或其他正当理由耽误" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E6%8A%97%E6%8B%92%E7%9A%84%E4%BA%8B%E7%94%B1%E6%88%96%E5%85%B6%E4%BB%96%E6%AD%A3%E5%BD%93%E7%90%86%E7%94%B1%E8%80%BD%E8%AF%AF%3C/span%3E" _note="适用于所有期间类型" _mubu_note="%3Cspan%3E%E9%80%82%E7%94%A8%E4%BA%8E%E6%89%80%E6%9C%89%E6%9C%9F%E9%97%B4%E7%B1%BB%E5%9E%8B%3C/span%3E"/>
          </outline>
          <outline text="只能依申请" _mubu_text="%3Cspan%3E%E5%8F%AA%E8%83%BD%E4%BE%9D%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="申请时间" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="障碍消除后10日内" _mubu_text="%3Cspan%3E%E9%9A%9C%E7%A2%8D%E6%B6%88%E9%99%A4%E5%90%8E10%E6%97%A5%E5%86%85%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="送达" _mubu_text="%3Cspan%3E%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
        <outline text="直接送达" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E9%80%81%E8%BE%BE%3C/span%3E" _note="" _mubu_note="">
          <outline text="交受送达人或其同住成年家属" _mubu_text="%3Cspan%3E%E4%BA%A4%E5%8F%97%E9%80%81%E8%BE%BE%E4%BA%BA%E6%88%96%E5%85%B6%E5%90%8C%E4%BD%8F%E6%88%90%E5%B9%B4%E5%AE%B6%E5%B1%9E%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="离婚诉讼" _mubu_text="%3Cspan%3E%E7%A6%BB%E5%A9%9A%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
