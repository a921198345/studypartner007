            </outline>
            <outline text="赔偿只赔直接损失" _mubu_text="%3Cspan%3E%E8%B5%94%E5%81%BF%E5%8F%AA%E8%B5%94%E7%9B%B4%E6%8E%A5%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
              <outline text="直接损失" _mubu_text="%3Cspan%3E%E7%9B%B4%E6%8E%A5%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note="">
                <outline text="①存款利息、贷款利息、现金利息" _mubu_text="%3Cspan%3E%E2%91%A0%E5%AD%98%E6%AC%BE%E5%88%A9%E6%81%AF%E3%80%81%E8%B4%B7%E6%AC%BE%E5%88%A9%E6%81%AF%E3%80%81%E7%8E%B0%E9%87%91%E5%88%A9%E6%81%AF%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="②机动车停运期间的营运损失" _mubu_text="%3Cspan%3E%E2%91%A1%E6%9C%BA%E5%8A%A8%E8%BD%A6%E5%81%9C%E8%BF%90%E6%9C%9F%E9%97%B4%E7%9A%84%E8%90%A5%E8%BF%90%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="③通过行政补偿程序依法应当获得的奖励、补贴等" _mubu_text="%3Cspan%3E%E2%91%A2%E9%80%9A%E8%BF%87%E8%A1%8C%E6%94%BF%E8%A1%A5%E5%81%BF%E7%A8%8B%E5%BA%8F%E4%BE%9D%E6%B3%95%E5%BA%94%E5%BD%93%E8%8E%B7%E5%BE%97%E7%9A%84%E5%A5%96%E5%8A%B1%E3%80%81%E8%A1%A5%E8%B4%B4%E7%AD%89%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="预期利润不属于直接损失" _mubu_text="%3Cspan%3E%E9%A2%84%E6%9C%9F%E5%88%A9%E6%B6%A6%E4%B8%8D%E5%B1%9E%E4%BA%8E%E7%9B%B4%E6%8E%A5%E6%8D%9F%E5%A4%B1%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="费用的支付" _mubu_text="%3Cspan%3E%E8%B4%B9%E7%94%A8%E7%9A%84%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note="">
            <outline text="由各级财政部门拨款，由赔偿义务机关向请求权人支付" _mubu_text="%3Cspan%3E%E7%94%B1%E5%90%84%E7%BA%A7%E8%B4%A2%E6%94%BF%E9%83%A8%E9%97%A8%E6%8B%A8%E6%AC%BE%EF%BC%8C%E7%94%B1%E8%B5%94%E5%81%BF%E4%B9%89%E5%8A%A1%E6%9C%BA%E5%85%B3%E5%90%91%E8%AF%B7%E6%B1%82%E6%9D%83%E4%BA%BA%E6%94%AF%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
    </outline>
    <outline text="进阶篇 " _mubu_text="%3Cspan%3E%E8%BF%9B%E9%98%B6%E7%AF%87%20%3C/span%3E" _note="" _mubu_note="">
      <outline text="行政与刑事交叉" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E4%B8%8E%E5%88%91%E4%BA%8B%E4%BA%A4%E5%8F%89%3C/span%3E" _note="" _mubu_note="">
        <outline text="有相关性——移送且中止" _mubu_text="%3Cspan%3E%E6%9C%89%E7%9B%B8%E5%85%B3%E6%80%A7%E2%80%94%E2%80%94%E7%A7%BB%E9%80%81%E4%B8%94%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="无相关性——移送不中止" _mubu_text="%3Cspan%3E%E6%97%A0%E7%9B%B8%E5%85%B3%E6%80%A7%E2%80%94%E2%80%94%E7%A7%BB%E9%80%81%E4%B8%8D%E4%B8%AD%E6%AD%A2%3C/span%3E" _note="" _mubu_note=""/>
      </outline>
      <outline text="行政诉讼与民事诉讼交叉案件" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%AF%89%E8%AE%BC%E4%B8%8E%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%E4%BA%A4%E5%8F%89%E6%A1%88%E4%BB%B6%3C/span%3E" _note="" _mubu_note="">
        <outline text="分别审理" _mubu_text="%3Cspan%3E%E5%88%86%E5%88%AB%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note=""/>
        <outline text="合并审理" _mubu_text="%3Cspan%3E%E5%90%88%E5%B9%B6%E5%AE%A1%E7%90%86%3C/span%3E" _note="" _mubu_note="">
          <outline text="适用前提" _mubu_text="%3Cspan%3E%E9%80%82%E7%94%A8%E5%89%8D%E6%8F%90%3C/span%3E" _note="" _mubu_note="">
            <outline text="行政行为与民事纠纷具有关联性" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%A1%8C%E4%B8%BA%E4%B8%8E%E6%B0%91%E4%BA%8B%E7%BA%A0%E7%BA%B7%E5%85%B7%E6%9C%89%E5%85%B3%E8%81%94%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="当事人在一审开庭前提出或在审理案件中同意" _mubu_text="%3Cspan%3E%E5%BD%93%E4%BA%8B%E4%BA%BA%E5%9C%A8%E4%B8%80%E5%AE%A1%E5%BC%80%E5%BA%AD%E5%89%8D%E6%8F%90%E5%87%BA%E6%88%96%E5%9C%A8%E5%AE%A1%E7%90%86%E6%A1%88%E4%BB%B6%E4%B8%AD%E5%90%8C%E6%84%8F%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="同一法院管辖" _mubu_text="%3Cspan%3E%E5%90%8C%E4%B8%80%E6%B3%95%E9%99%A2%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note="">
              <outline text="行政立案没有专属管辖" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E7%AB%8B%E6%A1%88%E6%B2%A1%E6%9C%89%E4%B8%93%E5%B1%9E%E7%AE%A1%E8%BE%96%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未约定仲裁" _mubu_text="%3Cspan%3E%E6%9C%AA%E7%BA%A6%E5%AE%9A%E4%BB%B2%E8%A3%81%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="未提起民事诉讼" _mubu_text="%3Cspan%3E%E6%9C%AA%E6%8F%90%E8%B5%B7%E6%B0%91%E4%BA%8B%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="救济途径" _mubu_text="%3Cspan%3E%E6%95%91%E6%B5%8E%E9%80%94%E5%BE%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="可申请复议一次" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%B3%E8%AF%B7%E5%A4%8D%E8%AE%AE%E4%B8%80%E6%AC%A1%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="立案审理裁判" _mubu_text="%3Cspan%3E%E7%AB%8B%E6%A1%88%E5%AE%A1%E7%90%86%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note="">
            <outline text="原则：分别立案、合并审理、分别裁判" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%EF%BC%9A%E5%88%86%E5%88%AB%E7%AB%8B%E6%A1%88%E3%80%81%E5%90%88%E5%B9%B6%E5%AE%A1%E7%90%86%E3%80%81%E5%88%86%E5%88%AB%E8%A3%81%E5%88%A4%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="例外：合并立案、合并审理、合并裁判（行政裁决）" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A4%96%EF%BC%9A%E5%90%88%E5%B9%B6%E7%AB%8B%E6%A1%88%E3%80%81%E5%90%88%E5%B9%B6%E5%AE%A1%E7%90%86%E3%80%81%E5%90%88%E5%B9%B6%E8%A3%81%E5%88%A4%EF%BC%88%E8%A1%8C%E6%94%BF%E8%A3%81%E5%86%B3%EF%BC%89%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
      </outline>
      <outline text="细节归纳" _mubu_text="%3Cspan%3E%E7%BB%86%E8%8A%82%E5%BD%92%E7%BA%B3%3C/span%3E" _note="" _mubu_note="">
        <outline text="收费问题" _mubu_text="%3Cspan%3E%E6%94%B6%E8%B4%B9%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
          <outline text="行政机关可收费3⃣️" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E5%8F%AF%E6%94%B6%E8%B4%B93%E2%83%A3%EF%B8%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="行政强制执行的合理费用" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%BC%BA%E5%88%B6%E6%89%A7%E8%A1%8C%E7%9A%84%E5%90%88%E7%90%86%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="行政许可的实施和监督不收费，但法律、行政法规另有规定的除外" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E8%AE%B8%E5%8F%AF%E7%9A%84%E5%AE%9E%E6%96%BD%E5%92%8C%E7%9B%91%E7%9D%A3%E4%B8%8D%E6%94%B6%E8%B4%B9%EF%BC%8C%E4%BD%86%E6%B3%95%E5%BE%8B%E3%80%81%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%E5%8F%A6%E6%9C%89%E8%A7%84%E5%AE%9A%E7%9A%84%E9%99%A4%E5%A4%96%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="信息公开不收费，但数量超过合理范围的收信息处理费" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%81%AF%E5%85%AC%E5%BC%80%E4%B8%8D%E6%94%B6%E8%B4%B9%EF%BC%8C%E4%BD%86%E6%95%B0%E9%87%8F%E8%B6%85%E8%BF%87%E5%90%88%E7%90%86%E8%8C%83%E5%9B%B4%E7%9A%84%E6%94%B6%E4%BF%A1%E6%81%AF%E5%A4%84%E7%90%86%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="证据制度可收费3⃣️" _mubu_text="%3Cspan%3E%E8%AF%81%E6%8D%AE%E5%88%B6%E5%BA%A6%E5%8F%AF%E6%94%B6%E8%B4%B93%E2%83%A3%EF%B8%8F%3C/span%3E" _note="" _mubu_note="">
            <outline text="证人费用，败诉方承担" _mubu_text="%3Cspan%3E%E8%AF%81%E4%BA%BA%E8%B4%B9%E7%94%A8%EF%BC%8C%E8%B4%A5%E8%AF%89%E6%96%B9%E6%89%BF%E6%8B%85%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="鉴定费用" _mubu_text="%3Cspan%3E%E9%89%B4%E5%AE%9A%E8%B4%B9%E7%94%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="申请法院责令行政机关提交证据产生的费用，由申请人预付" _mubu_text="%3Cspan%3E%E7%94%B3%E8%AF%B7%E6%B3%95%E9%99%A2%E8%B4%A3%E4%BB%A4%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E6%8F%90%E4%BA%A4%E8%AF%81%E6%8D%AE%E4%BA%A7%E7%94%9F%E7%9A%84%E8%B4%B9%E7%94%A8%EF%BC%8C%E7%94%B1%E7%94%B3%E8%AF%B7%E4%BA%BA%E9%A2%84%E4%BB%98%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="三大救济途径" _mubu_text="%3Cspan%3E%E4%B8%89%E5%A4%A7%E6%95%91%E6%B5%8E%E9%80%94%E5%BE%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="复议不收费" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%E4%B8%8D%E6%94%B6%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="国赔不收费" _mubu_text="%3Cspan%3E%E5%9B%BD%E8%B5%94%E4%B8%8D%E6%94%B6%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="诉讼受理费" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E5%8F%97%E7%90%86%E8%B4%B9%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="口头问题" _mubu_text="%3Cspan%3E%E5%8F%A3%E5%A4%B4%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
          <outline text="行政机关唯一口头" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%E5%94%AF%E4%B8%80%E5%8F%A3%E5%A4%B4%3C/span%3E" _note="" _mubu_note="">
            <outline text="现场发现违法，口头传唤" _mubu_text="%3Cspan%3E%E7%8E%B0%E5%9C%BA%E5%8F%91%E7%8E%B0%E8%BF%9D%E6%B3%95%EF%BC%8C%E5%8F%A3%E5%A4%B4%E4%BC%A0%E5%94%A4%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="口头申请" _mubu_text="%3Cspan%3E%E5%8F%A3%E5%A4%B4%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note="">
            <outline text="信息公开" _mubu_text="%3Cspan%3E%E4%BF%A1%E6%81%AF%E5%85%AC%E5%BC%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="复议申请" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="诉讼申请" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="国赔申请" _mubu_text="%3Cspan%3E%E5%9B%BD%E8%B5%94%E7%94%B3%E8%AF%B7%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="复议" _mubu_text="%3Cspan%3E%E5%A4%8D%E8%AE%AE%3C/span%3E" _note="" _mubu_note="">
            <outline text="公民口头委托代理人" _mubu_text="%3Cspan%3E%E5%85%AC%E6%B0%91%E5%8F%A3%E5%A4%B4%E5%A7%94%E6%89%98%E4%BB%A3%E7%90%86%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
          <outline text="诉讼" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note="">
            <outline text="被限制人身自由，可口头委托近亲属以公民名义提起诉讼" _mubu_text="%3Cspan%3E%E8%A2%AB%E9%99%90%E5%88%B6%E4%BA%BA%E8%BA%AB%E8%87%AA%E7%94%B1%EF%BC%8C%E5%8F%AF%E5%8F%A3%E5%A4%B4%E5%A7%94%E6%89%98%E8%BF%91%E4%BA%B2%E5%B1%9E%E4%BB%A5%E5%85%AC%E6%B0%91%E5%90%8D%E4%B9%89%E6%8F%90%E8%B5%B7%E8%AF%89%E8%AE%BC%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="简易程序可送达裁判文书以外的诉讼文书" _mubu_text="%3Cspan%3E%E7%AE%80%E6%98%93%E7%A8%8B%E5%BA%8F%E5%8F%AF%E9%80%81%E8%BE%BE%E8%A3%81%E5%88%A4%E6%96%87%E4%B9%A6%E4%BB%A5%E5%A4%96%E7%9A%84%E8%AF%89%E8%AE%BC%E6%96%87%E4%B9%A6%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="人数问题" _mubu_text="%3Cspan%3E%E4%BA%BA%E6%95%B0%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
          <outline text="3个两人" _mubu_text="%3Cspan%3E3%E4%B8%AA%E4%B8%A4%E4%BA%BA%3C/span%3E" _note="" _mubu_note="">
            <outline text="实质审查2人" _mubu_text="%3Cspan%3E%E5%AE%9E%E8%B4%A8%E5%AE%A1%E6%9F%A52%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="调取证据2人" _mubu_text="%3Cspan%3E%E8%B0%83%E5%8F%96%E8%AF%81%E6%8D%AE2%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="委托代理人1～2人" _mubu_text="%3Cspan%3E%E5%A7%94%E6%89%98%E4%BB%A3%E7%90%86%E4%BA%BA1%EF%BD%9E2%E4%BA%BA%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="公告问题" _mubu_text="%3Cspan%3E%E5%85%AC%E5%91%8A%E9%97%AE%E9%A2%98%3C/span%3E" _note="" _mubu_note="">
          <outline text="行政处罚5⃣️" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%A4%84%E7%BD%9A5%E2%83%A3%EF%B8%8F%3C/span%3E" _note="" _mubu_note="">
