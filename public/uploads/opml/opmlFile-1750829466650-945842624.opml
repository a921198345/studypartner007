            <outline text="行政委托：无行政主体资格，行政主体作出" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E5%A7%94%E6%89%98%EF%BC%9A%E6%97%A0%E8%A1%8C%E6%94%BF%E4%B8%BB%E4%BD%93%E8%B5%84%E6%A0%BC%EF%BC%8C%E8%A1%8C%E6%94%BF%E4%B8%BB%E4%BD%93%E4%BD%9C%E5%87%BA%3C/span%3E" _note="" _mubu_note="">
              <outline text="处罚：事业组织" _mubu_text="%3Cspan%3E%E5%A4%84%E7%BD%9A%EF%BC%9A%E4%BA%8B%E4%B8%9A%E7%BB%84%E7%BB%87%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="许可：行政机关" _mubu_text="%3Cspan%3E%E8%AE%B8%E5%8F%AF%EF%BC%9A%E8%A1%8C%E6%94%BF%E6%9C%BA%E5%85%B3%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="强制措施：无" _mubu_text="%3Cspan%3E%E5%BC%BA%E5%88%B6%E6%8E%AA%E6%96%BD%EF%BC%9A%E6%97%A0%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
          </outline>
          <outline text="行政主体的判定" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E4%B8%BB%E4%BD%93%E7%9A%84%E5%88%A4%E5%AE%9A%3C/span%3E" _note="" _mubu_note="">
