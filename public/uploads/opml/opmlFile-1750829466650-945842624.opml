              <outline text="一般严重：总额的50%以下" _mubu_text="%3Cspan%3E%E4%B8%80%E8%88%AC%E4%B8%A5%E9%87%8D%EF%BC%9A%E6%80%BB%E9%A2%9D%E7%9A%8450%25%E4%BB%A5%E4%B8%8B%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="特别严重：总额的50%以上" _mubu_text="%3Cspan%3E%E7%89%B9%E5%88%AB%E4%B8%A5%E9%87%8D%EF%BC%9A%E6%80%BB%E9%A2%9D%E7%9A%8450%25%E4%BB%A5%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
