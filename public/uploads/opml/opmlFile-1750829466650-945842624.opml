            <outline text="公布载体" _mubu_text="%3Cspan%3E%E5%85%AC%E5%B8%83%E8%BD%BD%E4%BD%93%3C/span%3E" _note="" _mubu_note="">
              <outline text="规章" _mubu_text="%3Cspan%3E%E8%A7%84%E7%AB%A0%3C/span%3E" _note="" _mubu_note="">
                <outline text="部门规章：国务院公报或部门公报、中国政府法制信息网、全国范围内发行的报纸上" _mubu_text="%3Cspan%3E%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%EF%BC%9A%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%85%AC%E6%8A%A5%E6%88%96%E9%83%A8%E9%97%A8%E5%85%AC%E6%8A%A5%E3%80%81%E4%B8%AD%E5%9B%BD%E6%94%BF%E5%BA%9C%E6%B3%95%E5%88%B6%E4%BF%A1%E6%81%AF%E7%BD%91%E3%80%81%E5%85%A8%E5%9B%BD%E8%8C%83%E5%9B%B4%E5%86%85%E5%8F%91%E8%A1%8C%E7%9A%84%E6%8A%A5%E7%BA%B8%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="地方政府规章：本级政府公报、中国政府法制信息网、本行政区域内发行的报纸上" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%94%BF%E5%BA%9C%E8%A7%84%E7%AB%A0%EF%BC%9A%E6%9C%AC%E7%BA%A7%E6%94%BF%E5%BA%9C%E5%85%AC%E6%8A%A5%E3%80%81%E4%B8%AD%E5%9B%BD%E6%94%BF%E5%BA%9C%E6%B3%95%E5%88%B6%E4%BF%A1%E6%81%AF%E7%BD%91%E3%80%81%E6%9C%AC%E8%A1%8C%E6%94%BF%E5%8C%BA%E5%9F%9F%E5%86%85%E5%8F%91%E8%A1%8C%E7%9A%84%E6%8A%A5%E7%BA%B8%E4%B8%8A%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="行政法规" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%3C/span%3E" _note="" _mubu_note="">
                <outline text="国务院公报、中国政府法制信息网、全国范围发行报纸" _mubu_text="%3Cspan%3E%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%85%AC%E6%8A%A5%E3%80%81%E4%B8%AD%E5%9B%BD%E6%94%BF%E5%BA%9C%E6%B3%95%E5%88%B6%E4%BF%A1%E6%81%AF%E7%BD%91%E3%80%81%E5%85%A8%E5%9B%BD%E8%8C%83%E5%9B%B4%E5%8F%91%E8%A1%8C%E6%8A%A5%E7%BA%B8%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="标准文本" _mubu_text="%3Cspan%3E%E6%A0%87%E5%87%86%E6%96%87%E6%9C%AC%3C/span%3E" _note="" _mubu_note="">
              <outline text="部门规章：部门公报或国务院公报" _mubu_text="%3Cspan%3E%E9%83%A8%E9%97%A8%E8%A7%84%E7%AB%A0%EF%BC%9A%E9%83%A8%E9%97%A8%E5%85%AC%E6%8A%A5%E6%88%96%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%85%AC%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="地方政府规章：地方政府公报" _mubu_text="%3Cspan%3E%E5%9C%B0%E6%96%B9%E6%94%BF%E5%BA%9C%E8%A7%84%E7%AB%A0%EF%BC%9A%E5%9C%B0%E6%96%B9%E6%94%BF%E5%BA%9C%E5%85%AC%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="行政法规：国务院公报" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%EF%BC%9A%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%85%AC%E6%8A%A5%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
            <outline text="备案" _mubu_text="%3Cspan%3E%E5%A4%87%E6%A1%88%3C/span%3E" _note="" _mubu_note="">
              <outline text="规章：制定机关法制机构" _mubu_text="%3Cspan%3E%E8%A7%84%E7%AB%A0%EF%BC%9A%E5%88%B6%E5%AE%9A%E6%9C%BA%E5%85%B3%E6%B3%95%E5%88%B6%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note=""/>
              <outline text="行政法规：国务院办公厅" _mubu_text="%3Cspan%3E%E8%A1%8C%E6%94%BF%E6%B3%95%E8%A7%84%EF%BC%9A%E5%9B%BD%E5%8A%A1%E9%99%A2%E5%8A%9E%E5%85%AC%E5%8E%85%3C/span%3E" _note="" _mubu_note=""/>
            </outline>
