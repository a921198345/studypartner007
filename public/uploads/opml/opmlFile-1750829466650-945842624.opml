            <outline text="诉讼：参照规章" _mubu_text="%3Cspan%3E%E8%AF%89%E8%AE%BC%EF%BC%9A%E5%8F%82%E7%85%A7%E8%A7%84%E7%AB%A0%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
        </outline>
        <outline text="对抽象行政行为附带性审查" _mubu_text="%3Cspan%3E%E5%AF%B9%E6%8A%BD%E8%B1%A1%E8%A1%8C%E6%94%BF%E8%A1%8C%E4%B8%BA%E9%99%84%E5%B8%A6%E6%80%A7%E5%AE%A1%E6%9F%A5%3C/span%3E" _note="" _mubu_note="">
