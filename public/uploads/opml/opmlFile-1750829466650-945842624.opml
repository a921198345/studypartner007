          </outline>
          <outline text="派出机构" _mubu_text="%3Cspan%3E%E6%B4%BE%E5%87%BA%E6%9C%BA%E6%9E%84%3C/span%3E" _note="" _mubu_note="">
            <outline text="派出所：警告、500 以下罚款" _mubu_text="%3Cspan%3E%E6%B4%BE%E5%87%BA%E6%89%80%EF%BC%9A%E8%AD%A6%E5%91%8A%E3%80%81500%20%E4%BB%A5%E4%B8%8B%E7%BD%9A%E6%AC%BE%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="工商所" _mubu_text="%3Cspan%3E%E5%B7%A5%E5%95%86%E6%89%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="税务所" _mubu_text="%3Cspan%3E%E7%A8%8E%E5%8A%A1%E6%89%80%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="原则上不做行政主体，例外时可以" _mubu_text="%3Cspan%3E%E5%8E%9F%E5%88%99%E4%B8%8A%E4%B8%8D%E5%81%9A%E8%A1%8C%E6%94%BF%E4%B8%BB%E4%BD%93%EF%BC%8C%E4%BE%8B%E5%A4%96%E6%97%B6%E5%8F%AF%E4%BB%A5%3C/span%3E" _note="" _mubu_note=""/>
          </outline>
