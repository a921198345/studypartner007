                <outline text="物权" _mubu_text="%3Cspan%3E%E7%89%A9%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="人身权" _mubu_text="%3Cspan%3E%E4%BA%BA%E8%BA%AB%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="知识产权" _mubu_text="%3Cspan%3E%E7%9F%A5%E8%AF%86%E4%BA%A7%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="形成权" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E6%9D%83%3C/span%3E" _note="" _mubu_note="">
                <outline text="单方意思表示即可使权利发生，变更或者消灭" _mubu_text="%3Cspan%3E%E5%8D%95%E6%96%B9%E6%84%8F%E6%80%9D%E8%A1%A8%E7%A4%BA%E5%8D%B3%E5%8F%AF%E4%BD%BF%E6%9D%83%E5%88%A9%E5%8F%91%E7%94%9F%EF%BC%8C%E5%8F%98%E6%9B%B4%E6%88%96%E8%80%85%E6%B6%88%E7%81%AD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="形成权可以由当事人约定，也可由法律规定" _mubu_text="%3Cspan%3E%E5%BD%A2%E6%88%90%E6%9D%83%E5%8F%AF%E4%BB%A5%E7%94%B1%E5%BD%93%E4%BA%8B%E4%BA%BA%E7%BA%A6%E5%AE%9A%EF%BC%8C%E4%B9%9F%E5%8F%AF%E7%94%B1%E6%B3%95%E5%BE%8B%E8%A7%84%E5%AE%9A%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="类型" _mubu_text="%3Cspan%3E%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note="">
                  <outline text="善意相对人的撤销权" _mubu_text="%3Cspan%3E%E5%96%84%E6%84%8F%E7%9B%B8%E5%AF%B9%E4%BA%BA%E7%9A%84%E6%92%A4%E9%94%80%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
