                <outline text="（1）建筑物和其他土地附着物" _mubu_text="%3Cspan%3E%EF%BC%881%EF%BC%89%E5%BB%BA%E7%AD%91%E7%89%A9%E5%92%8C%E5%85%B6%E4%BB%96%E5%9C%9F%E5%9C%B0%E9%99%84%E7%9D%80%E7%89%A9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="（2）建设用地 使用权" _mubu_text="%3Cspan%3E%EF%BC%882%EF%BC%89%E5%BB%BA%E8%AE%BE%E7%94%A8%E5%9C%B0%20%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="（3）海域使用权" _mubu_text="%3Cspan%3E%EF%BC%883%EF%BC%89%E6%B5%B7%E5%9F%9F%E4%BD%BF%E7%94%A8%E6%9D%83%3C/span%3E" _note="" _mubu_note=""/>
