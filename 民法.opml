                <outline text="特定权利人请求特定义务人为或不为特定行为的权利" _mubu_text="%3Cspan%3E%E7%89%B9%E5%AE%9A%E6%9D%83%E5%88%A9%E4%BA%BA%E8%AF%B7%E6%B1%82%E7%89%B9%E5%AE%9A%E4%B9%89%E5%8A%A1%E4%BA%BA%E4%B8%BA%E6%88%96%E4%B8%8D%E4%B8%BA%E7%89%B9%E5%AE%9A%E8%A1%8C%E4%B8%BA%E7%9A%84%E6%9D%83%E5%88%A9%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
                  <outline text="相对性" _mubu_text="%3Cspan%3E%E7%9B%B8%E5%AF%B9%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
