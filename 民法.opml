                  <outline text="无过错" _mubu_text="%3Cspan%3E%E6%97%A0%E8%BF%87%E9%94%99%3C/span%3E" _note="" _mubu_note="">
                    <outline text="担保人不承担赔偿责任" _mubu_text="%3Cspan%3E%E6%8B%85%E4%BF%9D%E4%BA%BA%E4%B8%8D%E6%89%BF%E6%8B%85%E8%B5%94%E5%81%BF%E8%B4%A3%E4%BB%BB%3C/span%3E" _note="" _mubu_note=""/>
